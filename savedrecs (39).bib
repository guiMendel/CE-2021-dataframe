@article{ WOS:000232519200012,
Author = {Abbott, M and Arthur, A and Walker, L and Doody, G},
Title = {{The challenge of recruiting people with schizophrenia to a health
   promotion trial}},
Journal = {{BRITISH JOURNAL OF GENERAL PRACTICE}},
Year = {{2005}},
Volume = {{55}},
Number = {{517}},
Pages = {{634-636}},
Month = {{AUG}},
Abstract = {{People with schizophrenia have an increased risk of coronary heart
   disease. This pilot study tested the feasibility of carrying out a
   randomised controlled trial to compare coronary heart disease prevention
   for this population through an enhanced occupational therapy support
   intervention versus a practice-based intervention. Difficulty in
   deciding whether to take part meant that 123 visits were made to 25
   people with 12 ultimately providing informed consent. Participants'
   discussion at a subsequent focus group (n = 3) suggested a poor
   understanding of the study process. Distrust of randomisation suggests
   that randomised controlled trials may not be the best way to evaluate
   community-based interventions for people with schizophrenia.}},
