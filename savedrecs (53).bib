@article{ WOS:A1995TR99100005,
Author = {Tanaka, K and Furukawa, S and Hirao, K and Soga, N},
Title = {{Elastic anomaly and structure of phosphate glasses containing divalent
   cations}},
Journal = {{GLASS PHYSICS AND CHEMISTRY}},
Year = {{1995}},
Volume = {{21}},
Number = {{6}},
Pages = {{420-426}},
Month = {{NOV-DEC}},
Abstract = {{Sound velocities and their temperature coefficients of RO-P2O5 glasses
   where R is Zn, Mg, and Co have been determined by using cube resonance
   method from 7 K to 300 K. The temperature coefficients of longitudinal
   and transverse sound velocities are positive above 140 K for 50ZnO .
   50P(2)O(5) glass, indicating that so-called elastic anomaly takes place
   in this glass. For 50MgO . 50P(2)O(5) and 50CoO . 50P(2)O(5) glasses,
   the sound velocities decrease monotonically with an increase in
   temperature as expected for normal solids. For 45ZnO . 55P(2)O(5) and
   55ZnO . 45P(2)O(5) glasses, the temperature coefficients of sound
   velocities are negative, but their magnitudes are very small above 140
   K. The average atomic volume indicates that the volume of voids in 50ZnO
   . 50P(2)O(5) glass is much larger than those in 50MgO . 50P(2)O(5) and
   50CoO . 50P(O)O(5) glasses, and is slightly larger than those in 45ZnO .
   55P(2)O(5) and 55ZnO . 45P(2)O(5) glasses. These results are compatible
   with the previous suggestion that the elastic anomaly is apt to occur in
   a glass that has open structure and tetrahedrally coordinated cations.
   It is considered that the elastic anomaly is caused by the transverse
   movements of oxygen atoms.}},
