@article{ WOS:000376417800034,
Author = {Behrndt, Jussi and Rohleder, Jonathan},
Title = {{Spectral analysis of selfadjoint elliptic differential operators,
   Dirichlet-to-Neumann maps, and abstract Weyl functions}},
Journal = {{ADVANCES IN MATHEMATICS}},
Year = {{2015}},
Volume = {{285}},
Pages = {{1301-1338}},
Month = {{NOV 5}},
Abstract = {{The spectrum of a selfadjoint second order elliptic differential
   operator in L-2(R-n) is described in terms of the limiting behavior of
   Dirichlet-to-Neumann maps, which arise in a multi-dimensional Glazman
   decomposition and correspond to an interior and an exterior boundary
   value problem. This leads to PDE analogs of renowned facts in spectral
   theory of ODEs. The main results in this paper are first derived in the
   more abstract context of extension theory of symmetric operators and
   corresponding Weyl functions, and are applied to the PDE setting
   afterwards. (C) 2015 The Authors. Published by Elsevier Inc.}},
