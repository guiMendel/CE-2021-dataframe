Journal = {{BLOOD}},
Year = {{1995}},
Volume = {{86}},
Number = {{9}},
Pages = {{3461-3467}},
Month = {{NOV 1}},
Abstract = {{We sought to determine whether intracellular or extracellular events
   contribute to the decrease in circulating antithrombin (AT) levels that
   is seen in subjects with the Utah mutation (Pro 407 to Leu),
