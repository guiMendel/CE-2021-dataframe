@article{ WOS:000166507300067,
Author = {Vojta, M and Zhang, Y and Sachdev, S},
Title = {{Renormalization group analysis of quantum critical points in d-wave
   superconductors}},
Journal = {{INTERNATIONAL JOURNAL OF MODERN PHYSICS B}},
Year = {{2000}},
Volume = {{14}},
Number = {{29-31}},
Pages = {{3719-3734}},
Month = {{DEC 20}},
Note = {{3rd International Conference on Stripes and High Tc Superconductivity
   (STRIPES 2000), UNIV ROME LA SPIENZA, ROME, ITALY, SEP 25-30, 2000}},
Organization = {{Univ Roma La Sapienza; Ist Nazl Fis Materia; CNR, Grp Nazl Struttura Mat}},
Abstract = {{We describe a search for renormalization group fixed points which
   control a second-order quantum phase transition between a d(x2-y2)
   superconductor and some other superconducting ground state. Only a few
   candidate fixed points are found. In the finite temperature (T)
   quantum-critical region of some of these fixed points, the fermion
   quasiparticle lifetime is very short and the spectral function has an
   energy width of order k(B)T near the Fermi points. Under the same
   conditions, the thermal conductivity is infinite in the scaling limit.
   We thus provide simple, explicit, examples of quantum theories in two
   dimensions for which a purely fermionic quasiparticle description of
   transport is badly violated.}},
