@article{ WOS:000089788000050,
Author = {Hanke, L and Frohlich, D and Ivanov, AL and Littlewood, PB and Stolz, H},
Title = {{Spectroscopy of LA-phonoritons in Cu2O}},
Journal = {{PHYSICA STATUS SOLIDI B-BASIC RESEARCH}},
Year = {{2000}},
Volume = {{221}},
Number = {{1}},
Pages = {{287-293}},
Month = {{SEP}},
Note = {{6th International Workshop on Nonlinear Optics and Excitation Kinetics
   in Semiconductors (NOEKS 2000), MARBURG, GERMANY, APR 10-13, 2000}},
Organization = {{Deutsch Forsch GmbH}},
Abstract = {{Photon, exciton, and phonon states, resonantly interacting in a
   direct-band-gap semiconductor in the presence of a coherent pump light,
   can be approximated by transient three-component eigenstates, the
