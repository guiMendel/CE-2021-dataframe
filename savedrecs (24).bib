@article{ WOS:000283209400039,
Author = {Liu, G. B. and Wang, X. Q. and Kuang, X. J. and He, A. L.},
Title = {{First-principles study on the electronic properties of BaCu2S2}},
Journal = {{PHYSICA B-CONDENSED MATTER}},
Year = {{2010}},
Volume = {{405}},
Number = {{21}},
Pages = {{4582-4585}},
Month = {{NOV 1}},
Abstract = {{Using the pseudopotential plane-wave method under the density functional
   theory (DFT), the electronic properties of BaCu2S2 in alpha and beta
   phases are calculated. The result shows that the bottom conduction band
   is mainly made of Ba 5d state, while the upper valence band is formed by
   S 3p and Cu 4d states, but both of them for beta-BaCu2S2 are wider than
   those for alpha-BaCu2S2. The electron and hole effective masses for
   BaCu2S2 are also analyzed in detail. It is found that beta-BaCu2S2 has
   very small hole effective masses, which implies that beta-BaCu2S2 may
   possess a high hole mobility. Moreover, its hole effective masses are
   extremely anisotropic along ab plane and c axis. These properties are
   helpful to enhance its Seebeck coefficient, and means that beta-BaCu2S2
   could be a good candidate of thermoelectric materials. (C) 2010 Elsevier
   B.V. All rights reserved.}},
