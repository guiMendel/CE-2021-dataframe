@article{ WOS:000368126300024,
Author = {Abca, Elif E. and Evrendilek, Gulsun Akdemir},
Title = {{PROCESSING OF RED WINE BY PULSED ELECTRIC FIELDS WITH RESPECT TO QUALITY
   PARAMETERS}},
Journal = {{JOURNAL OF FOOD PROCESSING AND PRESERVATION}},
Year = {{2015}},
Volume = {{39}},
Number = {{6}},
Pages = {{758-767}},
Month = {{DEC}},
Abstract = {{Processing of red wine by pulsed electric fields (PEFs) with
   determination of physical, chemical and sensory properties as well as
   inactivation of Escherichia coli O157: H7, Lactobacillus delbrueckii
   ssp. bulgaricus, Candida lipolytica, Saccharomyces cerevisiae and
   Hansenula anomala revealed that increased electric field strength and
   treatment temperature did not cause significant difference on measured
   attributes (P > 0.05); however, initial number of inoculated
   microorganisms significantly decreased (P <= 0.05). Response surface
   methodology was used to design the experiments, and a central composite
   design with quadratic model fitted to explain microbial inactivation. It
   is concluded that processing of red wine with no significant changes and
   microbial stability can be ensured with PEF treatment.}},
