@article{ WOS:000234001000015,
Author = {Pegoraro, LF and Scolaro, JM and Conti, PC and Telles, D and Pegoraro,
   TA},
Title = {{Noncarious cervical lesions in adults - Prevalence and occlusal aspects}},
Journal = {{JOURNAL OF THE AMERICAN DENTAL ASSOCIATION}},
Year = {{2005}},
Volume = {{136}},
Number = {{12}},
Pages = {{1694-1700}},
Month = {{DEC}},
Abstract = {{Background. The factors that induce te formation of noncarious lesions
   are not understood fully, particularly those that are related to
   occlusal aspects. The authors conducted a study to evaluate the
   prevalence of noncarious cervical lesions in adults and their
   association with occlusal aspects.
   Methods. The authors examined 70 people (35 men and 35 women) aged 25 to
   45 to determine the presence and type of noncarious cervical lesions,
   wear facets, tooth contacts in minimal intercuspal position, and lateral
   and protrusive movements. The assessment involved a questionnaire and
   clinical examinations.
   Results. Among the teeth the authors evaluated, 17.23 percent had
   cervical lesions, 80.28 percent of which had wear facets (P < .01). The
   authors found a significant difference between the prevalence of
   noncarious lesions and the presence of wear facets (P = .0484)
   Conclusions: The authors found that cervical lesion were related
   significantly to wear facets. These findings strenghten evidence for the
   role of occlusal forces as an etiologic factor for noncarious lesions.
   Clinical Implications. The presence of wear facets should be considered
   in the treatment of noncarious cervical lesions.}},
