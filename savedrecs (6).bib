@article{ WOS:000591235900001,
Author = {Hagiwara, Makoto and Hanato, Shunsuke},
Title = {{A strategic justification of the constrained equal awards rule through a
   procedurally fair multilateral bargaining game}},
Journal = {{THEORY AND DECISION}},
Year = {{2021}},
Volume = {{90}},
Number = {{2}},
Pages = {{233-243}},
Month = {{MAR}},
Abstract = {{We propose a new game to strategically justify the constrained equal
   awards rule in claims problems. Our game is ``procedurally fair{''}
   (claimants are treated equally) and ``multilateral{''} (all claimants
   negotiate simultaneously). In addition, even if claimants cannot reach
   an agreement in any period, they can renegotiate in the next period. We
   show that, for each claims problem, the awards vector chosen by the
   constrained equal awards rule achieved at period 1 is the unique subgame
   perfect equilibrium outcome of the game.}},
