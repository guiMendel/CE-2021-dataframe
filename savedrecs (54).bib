@article{ WOS:A1995RZ84400047,
Author = {NISHIKAWA, M and UETAKE, M and TANAKA, KI and SHIRAISHI, T},
Title = {{MASS-TRANSFER COEFFICIENTS IN CRYOSORPTION OF HYDROGEN ISOTOPES ON
   MOLECULAR-SIEVES OR ACTIVATED CARBON AT 77 K}},
Journal = {{FUSION TECHNOLOGY}},
