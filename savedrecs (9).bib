@article{ WOS:000618703300005,
Author = {Faris, Robert and Felmlee, Diane and McMillan, Cassie},
Title = {{With Friends Like These: Aggression from Amity and Equivalence(1)}},
Journal = {{AMERICAN JOURNAL OF SOCIOLOGY}},
Year = {{2020}},
Volume = {{126}},
Number = {{3}},
Pages = {{673-713}},
Month = {{NOV 1}},
Abstract = {{Some teenagers are willing to bully, harass, and torment their
   schoolmates in order to achieve popularity and other goals. But whom do
   they bully? Here, we extend the logic of instrumental aggression to
   answer this question. To the extent that friendships are the currency of
   social status, we should expect social aspirants to target their own
   friends, their friends' friends, and other structurally equivalent
   schoolmates. This tendency, we argue, extends beyond what would be
   explained by propinquity, and we expect that victimization by friends
   will be particularly distressing. We test these hypotheses using panel
   social network data from 14 middle and high schools at two time points
   during a school year. Findings from temporal exponential random graph
   models suggest that our expectations are correct: the tendency to be
   cruel to friends is not significantly influenced by propinquity, and
   victimization by friends has adverse consequences for mental health.}},
