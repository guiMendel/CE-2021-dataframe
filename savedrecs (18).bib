@article{ WOS:000363334100010,
Author = {Wang, Hui and Ye, Zhuo-peng and Huang, Zhen-chao and Luo, Lun and Chen,
   Chuan and Guo, Ying},
Title = {{Intraoperative Ultrasonography Combined with Indocyanine Green
