@article{ WOS:000365464100005,
Author = {Preimesberger, Matthew R. and Majumdar, Ananya and Rice, Selena L. and
   Que, Lauren and Lecomte, Juliette T. J.},
Title = {{Helix-Capping Histidines: Diversity of N-H center dot center dot center
   dot N Hydrogen Bond Strength Revealed by (2h)J(NN) Scalar Couplings}},
Journal = {{BIOCHEMISTRY}},
Year = {{2015}},
Volume = {{54}},
Number = {{46}},
Pages = {{6896-6908}},
Month = {{NOV 24}},
Abstract = {{In addition to its well-known roles as an electrophile and general acid,
   the side chain of histidine often serves as a hydrogen bond (H-bond)
   acceptor. These H-bonds provide a convenient pH-dependent switch for
   local structure and functional motifs. In hundreds of instances, a
   histidine caps the N-terminus of alpha- and 3(10)-helices by forming a
   backbone NH center dot center dot center dot N delta 1 H-bond. To
   characterize the resilience and dynamics of the histidine cap, we
   measured the trans H-bond scalar coupling constant, (2h)J(NN), in
   several forms of Group 1 truncated hemoglobins and cytochrome b(5). The
   set of 19 measured (2h)J(NN) values were between 4.0 and 5.4 Hz,
   generally smaller than in nucleic acids (similar to 6-10 Hz) and
   indicative of longer, weaker bonds in the studied proteins. A positive
   linear correlation between (2h)J(NN) and the difference in imidazole
   ring N-15 chemical shift (Delta N-15 = vertical bar delta N-15 delta 1 -
   delta N-15 epsilon 2 vertical bar) was found to be consistent with
   variable H-bond length and variable cap population related to the
   ionization of histidine in the capping and noncapping states. The
   relative ease of (2h)J(NN) detection suggests that this parameter can
   become part of the standard arsenal for describing histidines in helix
   caps and other key structural and catalytic elements involving NH center
   dot center dot center dot N H-bonds. The combined nucleic acid and
   protein data extend the utility of (2h)J(NN) as a sensitive marker of
   local structural, dynamic, and thermodynamic properties in biomolecules.}},
