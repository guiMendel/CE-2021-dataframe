@article{ WOS:000363049100004,
Author = {Lin, Yi Hsin and Huang, Kuancheng},
Title = {{Customer Loyalty under the Influence of Revenue Management: The Case of
   Taiwanese Hotel Customers}},
Journal = {{ASIA PACIFIC JOURNAL OF TOURISM RESEARCH}},
Year = {{2015}},
Volume = {{20}},
Number = {{12}},
Pages = {{1374-1388}},
Month = {{DEC 2}},
Abstract = {{Revenue management (RM), a common practice for the hotel industry, has
   been found to be very effective in generating extra revenue. However,
   prior studies have shown that the implementation of RM may potentially
   be in conflict with customer loyalty, the basis for long-term
   profitability. With a view to alleviating the impact of RM on customer
   loyalty, this study investigates the relationship between customer
   loyalty and RM knowledge level, and examines the moderating effect of
   the fairness perception for pricing based on a survey and the related
   statistical analyses. The findings suggest that hotel operators should
   facilitate the RM knowledge and the fairness perception of their
   customers so as to result in a potential win-win situation, in which
   profit-seeking hotels increase their revenue through better resource
   utilization, and heterogeneous customers benefit from more diversified
   services.}},
