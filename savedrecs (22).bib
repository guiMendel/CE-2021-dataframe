@article{ WOS:000285583500037,
Author = {Rodriguez, Ivan D. and Sierra, German},
Title = {{Entanglement entropy of integer quantum Hall states in polygonal domains}},
Journal = {{JOURNAL OF STATISTICAL MECHANICS-THEORY AND EXPERIMENT}},
Year = {{2010}},
Month = {{DEC}},
Abstract = {{The entanglement entropy of the integer quantum Hall states satisfies
   the area law for smooth domains with a vanishing topological term. In
   this paper we consider polygonal domains for which the area law acquires
   a constant term that only depends on the angles of the vertices and we
   give a general expression for it. We study also the dependence of the
   entanglement spectrum on the geometry and give it a simple physical
   interpretation.}},
