@article{ WOS:000597752000001,
Author = {Vranova, Valerie and Holik, Ladislav},
Title = {{Alteration in the amount of Glomalin in transition from forest to
   field/meadow}},
Journal = {{SOIL USE AND MANAGEMENT}},
Abstract = {{Glomalin produced by fungal mycelium represents a significant
   stabilizing element of soil aggregates and a relatively stable carbon
   reservoir in the soil. The aim of our research was to investigate the
   changes in the amount of glomalin-related soil protein (GRSP) associated
   with the transition from forest to meadow or field conditions at the
