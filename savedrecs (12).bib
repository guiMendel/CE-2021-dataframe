@article{ WOS:000377735600011,
Author = {Jung, Kwang-Ho and Park, Bum-Geun and Jung, Seung-Boo},
Title = {{Effects of Polyvinylpyrrolidone Molecular Weight on Adhesion Strength of
   Screen-Printed Cu Circuits to Polyimide Substrate}},
Journal = {{NANOSCIENCE AND NANOTECHNOLOGY LETTERS}},
Year = {{2015}},
Volume = {{7}},
Number = {{12}},
Pages = {{999-1003}},
Month = {{DEC}},
Abstract = {{Characteristics of the printable materials, such as pH, viscosity and
   heterogeneity can be controlled by materials and their composition.
   Optimization of manufacturing printable material is a compulsory step to
   achieve high performance circuits. We fabricated various stabilizers
   containing polyvinylpyrrolidone (PVP) of three different molecular
   weights (MW = 40,000, 360,000, and 1,300,000) dissolved in
   diethylenglycol (DEG) for dispersing and protecting Cu nanoparticles in
   paste. Cu paste was screen-printed on polyimide (PI) substrates and
   sintered at 200 degrees C and 300 degrees C, respectively. Adhesion
   strength between a Cu circuit and a PI substrate was evaluated by a
   roll-type 90 peel test. The fracture surface was observed by FE-SEM.
   Thermal behavior of stabilizers analyzed with DSC and TGA showed
   increase decomposition temperatures with decreasing PVP MWs. The
   adhesion strength and the electrical conductivity was inversely
   proportional to molecular weight of PVP at the sintering temperature of
   200 degrees C. The Cu paste containing 40,000 PVP MW showed the greatest
   adhesion strength of 0.11 N/mm and the lowest electrical resistivity of
   47.3 mu Omega . cm. Fracture surfaces show cohesive fracture modes at
   the lower PVP MWs after peel tests.}},
