@article{ WOS:000628953100001,
Author = {Di Vita, Antonella and Procacci, Mario Augusto and Bellagamba, Martina
   and Jacomini, Maria and Massicci, Roberta and Ciurli, Maria Paola},
Title = {{Psychotherapy and Art Therapy: A pilot study of group treatment for
   patients with traumatic brain injury}},
Journal = {{JOURNAL OF HEALTH PSYCHOLOGY}},
Abstract = {{Here we investigated the efficacy of group Psychotherapy and Art Therapy
   in rebuilding self-identity after traumatic brain injury (TBI),
   assessing their effects on psychological aspects in twelve patients with
   severe TBI. Overall, the results show an increased ability to adapt to
   the experiences of life as well as a higher level of awareness, social
   functioning and emotional autoregulation. Furthermore, patients
   exhibited a reduction in depressive symptoms and in the perception of
   physical problems. Results highlight the importance of treatments
   focusing on the patients' emotional needs, in addition to the classic
   rehabilitation (i.e. physiotherapy, cognitive therapy).}},
