@article{ WOS:000231945700002,
Author = {Monago, KO},
Title = {{An equation of state for gaseous argon determined from the speed of
   sound}},
Journal = {{CHEMICAL PHYSICS}},
Year = {{2005}},
Volume = {{316}},
Number = {{1-3}},
Pages = {{9-19}},
Month = {{SEP 19}},
Abstract = {{An equation of state to determine the thermodynamic properties of a
   gaseous substance from precise experimental data is described and
   applied to argon. The method is based on a truncated virial equation in
   which the fourth coefficient is exact at the level of graphs that are at
   most linear in non-additive three-body forces. For argon, the method
   predicted the ordinary virial coefficients B and C and their acoustic
   analogues beta and gamma; in addition, reasonably accurate values of the
   fourth virial coefficient and its acoustic parallel 8 were obtained. The
   pair- and triplet-potential parameters for argon derived from the model
   are in close agreement with other current estimates.
   The resulting equation of state predicted all interesting thermodynamic
   properties of gaseous argon over a wide range of temperatures at
   densities of at least one-third the critical value with very good
   accuracy, though not always to their experimental uncertainties. (C)
   2005 Elsevier B.V. All rights reserved.}},
