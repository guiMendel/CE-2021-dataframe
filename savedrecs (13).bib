@article{ WOS:000367460400097,
Author = {Popel, S. I. and Zelenyi, L. M. and Atamaniuk, B.},
Title = {{Dusty plasma sheath-like structure in the region of lunar terminator}},
Journal = {{PHYSICS OF PLASMAS}},
Year = {{2015}},
Volume = {{22}},
Number = {{12}},
Month = {{DEC}},
Abstract = {{The main properties of the dusty plasma layer near the surface over the
   illuminated and dark parts of the Moon are described. They are used to
   realize dusty plasma behaviour and to determine electric fields over the
   terminator region. Possibility of the existence of a dusty plasma
   sheath-like structure in the region of lunar terminator is shown. The
   electric fields excited in the terminator region are demonstrated to be
   on the order of 300 V/m. These electric fields can result in rise of
   dust particles of the size of 2-3 mu m up to an altitude of about 30 cm
   over the lunar surface that explains the effect of ``horizon glow{''}
   observed at the terminator by Surveyor lunar lander. (c) 2015 AIP
   Publishing LLC.}},
