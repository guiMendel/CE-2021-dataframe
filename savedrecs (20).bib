@article{ WOS:000284518800047,
Author = {Deng, Liang and Majumdar, Amit and Lo, Wayne and Holm, R. H.},
Title = {{Stabilization of 3:1 Site-Differentiated Cubane-Type Clusters in the
   {[}Fe4S4](1+) Core Oxidation State by Tertiary Phosphine Ligation:
   Synthesis, Core Structural Diversity, and S=1/2 Ground States}},
Journal = {{INORGANIC CHEMISTRY}},
Year = {{2010}},
Volume = {{49}},
Number = {{23}},
Pages = {{11118-11126}},
Month = {{DEC 6}},
Abstract = {{An extensive series of 3:1 site-differentiated cubane-type clusters
   {[}Fe4S4(PPr3i)(3)L] (L = Cl-, Br-, I-, RO-, RS-, RSe-) has been
   prepared in 40-80\% yield by two methods: ligand substitution of
   {[}Fe4S4(PPr3i)(4)](1+) in tetrahydrofuran (THF)/acetonitrile by
   reaction with monoanions, and reductive cleavage of ligand substrates
   (RSSR, RSeSeR, 12) by the all-ferrous clusters
   {[}Fe8S8(PPr3i)(6)]/{[}Fe16S16(PPR3i)(8)] in THF. These neutral clusters
   are stable and do not undergo ligand redistribution reactions involving
   charged species in benzene and THF solutions. X-ray structural studies
   confirm the cubane stereochemistry but with substantial and variable
   distortions of the {[}Fe4S4](1+) core from idealized cubic core
   geometry. Based on Fe-S bond lengths, seven clusters were found to have
   compressed tetragonal distortions (4 short and 8 long bonds), and the
   remaining seven display other types of distortions with different
   combinations of long, short, and intermediate bond lengths. These
   results further emphasize the facile deformabililty of this core
   oxidation state previously observed in {[}Fe4S4(SR)(4)](3-) clusters.
   The Fe2.25+ mean oxidation state was demonstrated from Fe-57 isomer
   shifts, and the appearance of two quadrupole doublets arises from the
   spin-coupled l9/2,4,1/2 > state. The S = 1/2 ground state was further
   supported by electron paramagnetic resonance spectra and magnetic
   susceptibility data.}},
Publisher = {{AMER CHEMICAL SOC}},
Address = {{1155 16TH ST, NW, WASHINGTON, DC 20036 USA}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Holm, RH (Corresponding Author), Harvard Univ, Dept Chem \& Chem Biol, Cambridge, MA 02138 USA.
   Deng, Liang; Majumdar, Amit; Lo, Wayne; Holm, R. H., Harvard Univ, Dept Chem \& Chem Biol, Cambridge, MA 02138 USA.}},
DOI = {{10.1021/ic101702b}},
ISSN = {{0020-1669}},
EISSN = {{1520-510X}},
