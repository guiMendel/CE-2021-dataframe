@article{ WOS:000599877200014,
Author = {De Florio, Vincenzo},
Title = {{Social translucence as a transition-inducing mechanism in complex
   socio-ecological systems}},
Journal = {{ECOLOGICAL COMPLEXITY}},
Year = {{2020}},
Volume = {{44}},
Month = {{DEC}},
Abstract = {{As well-known, the interplay between human beings and their environment
   is complex and non-linear, with changes to initial conditions and local
   behaviors often being associated with the emergence of dramatically
   different global (macroscopic) changes. This paper discusses the problem
   of inducing significant behavioral changes in ecologically biocomplex
   systems. The role of social translucence in this context is hypothesized
   and reasoned upon. Elements of a theory of social translucence are
   provided, and requirements are derived from them. Two architectural
   models matching those requirements and ranging from the micro to the
   macro social scale are discussed.}},
