@article{ WOS:A1995TL59500004,
Author = {Galdikiene, RP and Petrauskas, AV},
Title = {{Anodic behavior of Ni electrodeposits under potentiodynamic conditions}},
Journal = {{PROTECTION OF METALS}},
Year = {{1995}},
Volume = {{31}},
Number = {{6}},
Pages = {{525-530}},
Month = {{NOV-DEC}},
Abstract = {{The potentiodynamic anodic behavior of Ni electrodeposits is studied as
   a function of electrolyte composition, polarization conditions, and
   foreign inclusions. The deposits are prone to passivation even in the
   absence of dissolved oxygen and, after being washed in H2O, are
   completely blocked by a hydroxide film. In the anodic polarization
   curves measured in the Watts electrolyte at pH 5.7, freshly deposited
   nickel demonstrates an anodic current peak, in the vicinity of which it
   is oxidized with the formation of a passivating film containing two to
   three atomic layers of nickel. As the concentration of H+ or Cl- ions in
   the electrolyte increases, the film thickness increases up to 10 times.
   Deposits with foreign inclusions are more active, are passivated more
   quickly than pure deposits, and demonstrate the anodic current peak of
   Ni oxidation only at a high rate of potential increase.}},
