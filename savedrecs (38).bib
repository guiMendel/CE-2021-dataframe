@article{ WOS:000231708500011,
Author = {O'Donnell, SM and Hansberger, MW and Connolly, JL and Chappell, JD and
   Watson, MJ and Pierces, JM and Wetzel, JD and Han, W and Barton, ES and
   Forrest, JC and Valyi-Nagy, T and Yull, FE and Blackwell, TS and
   Rottman, JN and Sherry, B and Dermody, TS},
Title = {{Organ-specific roles for transcription factor NF-kappa B in
   reovirus-induced apoptosis and disease}},
Journal = {{JOURNAL OF CLINICAL INVESTIGATION}},
Year = {{2005}},
Volume = {{115}},
Number = {{9}},
Pages = {{2341-2350}},
Month = {{SEP}},
Abstract = {{Reovirus induces apoptosis in cultured cells and in vivo. In cell
   culture models, apoptosis is contingent upon a mechanism involving
   reovirus-induced activation of transcription factor NF-kappa B complexes
   containing p50 and p65/Re1A subunits. To explore the in vivo role of
   NF-kappa B in this process, we tested the capacity of reovirus to induce
   apoptosis in mice lacking a functional nfkb1/p50 gene. The genetic
   defect had no apparent effect on reovirus replication in the intestine
   or dissemination to secondary sites of infection. In comparison to what
   was observed in wild-type controls, apoptosis was significantly
   diminished in the CNS of p50-null mice following reovirus infection. In
   sharp contrast, the loss of p50 was associated with massive
   reovirus-induced apoptosis and uncontrolled reovirus replication in the
   heart. Levels of IFN-beta mRNA were markedly increased in the hearts of
   wild-type animals but not p50-null animals infected with reovirus.
   Treatment of p50-null mice with IFN-beta substantially diminished
   reovirus replication and apoptosis, which suggests that IFN-beta
   induction by NF-kappa B protects against reovirus-induced myocarditis.
   These findings reveal an organ-specific role for NF-kappa B in the
   regulation of reovirus-induced apoptosis, which modulates encephalitis
   and myocarditis associated with reovirus infection.}},
