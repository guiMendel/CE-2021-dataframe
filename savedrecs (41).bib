@article{ WOS:000165380400016,
Author = {Mattila, HR and Otis, GW},
Title = {{The efficacy of Apiguard against varroa and tracheal mites, and its
   effect on honey production: 1999 trial}},
Journal = {{AMERICAN BEE JOURNAL}},
Year = {{2000}},
Volume = {{140}},
Number = {{12}},
Pages = {{969-973}},
Month = {{DEC}},
Abstract = {{In 1999, we conducted a second trial of Apiguard, a gel formulation of
   thymol, in which we treated colonies earlier and longer than in our 1998
   trial. For 30 days (May 19-June 18), colonies were either treated with
   Apiguard or left untreated (controls). Varroa mortality due to treatment
   with Apiguard was 76.2\% compared to 23.5\% natural mite mortality in
   control colonies. There was no improvement in the efficacy of Apiguard
   against varroa in 1999 compared to 1998. HBTM (honey bce tracheal mites)
   infestations did not differ between treated and untreated colonies after
   the Apiguard treatments. In both treated and control colonies, HBTM
   infestations had increased substantially by Fall. Honey production
   during treatment with Apiguard was reduced significantly, although the
   treatment period overlapped only slightly with the honey flows,
   therefore the average honey crop For the entire season did not differ
   significantly between treatment groups.}},
