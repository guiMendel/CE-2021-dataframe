@article{ WOS:000165371800002,
Author = {Huang, CY and Yuan, PB and Lin, CW and Wang, TK and Chang, CP},
Title = {{Geodynamic processes of Taiwan arc-continent collision and comparison
   with analogs in Timor, Papua New Guinea, Urals and Corsica}},
Journal = {{TECTONOPHYSICS}},
Year = {{2000}},
Volume = {{325}},
Number = {{1-2}},
Pages = {{1-21}},
Month = {{OCT 15}},
Note = {{Symposium on Arc-Continent Collision Past and Present Held at the 10th
   Biennial Meeting of the European-Union-of-Geoscience, STRASBOURG,
   FRANCE, MAR 29-APR 01, 1999}},
Organization = {{European Union Geosci}},
Abstract = {{The Taiwan are-continent collision involves four geodynamic processes:
   intra-oceanic subduction; initial arc-continent collision; advanced
   are-continent collision; and are collapse/subduction. These processes
   now occur simultaneously in 19-24 degrees 30'N but have operated
   sequentially southward since the Late Miocene. Although the geological
   and geophysical features appear to change progressively from north to
   south across the island, they are distinct within individual tectonic
   regimes. Using the present scenario as a basis for comparison, it is
   suggested that Timer is at the initial are-continent collision stage,
   while Papua New Guinea and Urals proceeded to the advanced are-continent
   collision stage. However, Corsica represents an even more advanced stage
   than in Taiwan, where a previously accreted are has collapsed and been
   totally removed. (C) 2000 Elsevier Science B.V. All rights reserved.}},
