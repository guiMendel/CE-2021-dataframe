@article{ WOS:000234038100017,
Author = {Xu, YH and Eilers, G and Borgstrom, M and Pan, JX and Abrahamsson, M and
   Magnuson, A and Lomoth, R and Bergquist, J and Polivka, T and Sun, LC
   and Sundstrom, V and Styring, S and Hammarstrom, L and Akermark, B},
Title = {{Synthesis and characterization of dinuclear ruthenium complexes
   covalently linked to Ru-II tris-bipyridine: An approach to mimics of the
   donor side of photosystem II}},
Journal = {{CHEMISTRY-A EUROPEAN JOURNAL}},
Year = {{2005}},
Volume = {{11}},
Number = {{24}},
Pages = {{7305-7314}},
Month = {{DEC 9}},
Abstract = {{To mimic the electron-donor side of photosystem II (PSII), three
   trinuclear ruthenium complexes (2, 2a, 2b) were synthesized. In these
   compl plexes, a mixed-valent dinuclear Ru-2(II,III) moiety with one
   phenoxy and two acetato bridges is covalently linked to a Ru-II
   tris-bipyridine photosensitizer. The properties and photoinduced
   electron/energy transfer of these complexes were studied. The results
   show that the Ru-2(II,III) moieties in the complexes readily undergo
   reversible one-electron reduction and one-electron oxidation to give the
   Ru-2(II,II) and Ru-2(II,III) states, respectively. This could allow for
   photooxidation of the sensitizer part with an external acceptor and
   subsequent electron transfer from the dinuclear ruthenium moiety to
   regenerate the sensitizer. However, all trinuclear ruthenium complexes
   have a very short excited-state lifetime, in the range of a few
   nanoseconds to less than 100 ps. Studies by femtosecond time-resolved
   techniques suggest that a mixture of intramolecular energy and electron
   transfer between the dinuclear ruthenium moiety and the excited
   {[}Ru(bpy)(3)](2+) photosensitizer is responsible for the short
   lifetimes. This problem is overcome by anchoring the complexes with
   ester- or carboxyl-substituted bipyridine ligands (2a, 2b) to
   nanocrystalline TiO2, and the desired electron transfer from the excited
   state of the {[}Ru(bPY)(3)](2+) moiety to the conduction band of TiO2,
   followed by intramolecular electron transfer from the dinuclear
   Ru-2(II,III) moiety to photogenerated Ru-III was observed. The resulting
   long-lived Ru-2(III,III) state decays on the millisecond timescale.}},
