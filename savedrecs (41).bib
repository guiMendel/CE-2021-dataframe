@article{ WOS:000166413700004,
Author = {Jagadish, HV and Kapitskaia, O and Ng, RT and Srivastava, D},
Title = {{One-dimensional and multi-dimensional substring selectivity estimation}},
Journal = {{VLDB JOURNAL}},
Year = {{2000}},
Volume = {{9}},
