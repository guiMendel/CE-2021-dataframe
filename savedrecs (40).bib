@article{ WOS:000165908600044,
Author = {Chung, SJ and Cha, OH and Hong, CH and Suh, EK and Lee, HJ and Kim, YS
   and Kim, BH},
Title = {{Emission mechanism of the yellow luminescence in undoped GaN}},
Journal = {{JOURNAL OF THE KOREAN PHYSICAL SOCIETY}},
Year = {{2000}},
Volume = {{37}},
Number = {{6}},
Pages = {{1003-1006}},
Month = {{DEC}},
Abstract = {{The temperature-dependent behavior of the yellow-band (YB) luminescence
   in n-type GaN epitaxial layers grown by metalorganic chemical-vapor
   deposition have been investigated. The YB luminescence peak position
   initially shows a blueshift in the temperature range from 5 to 40 K and
   then a redshift in the temperature range from 40 to 280 K. The
   temperature dependence of the peak intensity, as well as the integrated
   intensity. also shows an abrupt change at 40 K. above which the
   intensities increase with the temperature until they reach a maximum
   value at around 120 K and then decrease monotonically. The optical
   absorption spectrum associated with the transition related with the YB
   luminescence was observed at temperatures around 100 K and 150 K at
   which the intensity of the luminescence approached maximum values.
   According to the Lucovsky theory, the optical absorption related to the
   YB is best described by a transition from an impurity level to the
   conduction band. This result indicates that the YB luminescence is due
   to a transition from shallow to deep donor states.}},
