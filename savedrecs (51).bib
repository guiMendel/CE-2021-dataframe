@article{ WOS:A1995UN80200012,
Author = {Yadav, OP and Manga, VK},
Title = {{Visual evaluation for seed yield in pearl millet}},
Journal = {{ANNALS OF ARID ZONE}},
Year = {{1995}},
Volume = {{34}},
Number = {{4}},
Pages = {{301-303}},
Month = {{DEC}},
Abstract = {{The effectiveness of visual discrimination for seed yield was studied in
   pearl millet (Pennisteum glaucum). Results indicated that it is possible
   to visually discriminate the lines for seed yield in case the
   differences among them were large but not when such differences were
   small. Plots intermediate in yield were more prone to misjudgment during
   visual evaluation in comparison to high or low-yielding ones. The
   observer's perception of yield was influenced by plant height and
   earliness resulting into overestimation of actual yield for certain
   plots and underestimation for others. In general, correctly classified
   lines were taller, while no particular trend was observed with respect
   to earliness.}},
