@article{ WOS:000364983300018,
Author = {Piper, B. J. and Farrell, P. T.},
Title = {{Hereditary antithrombin III deficiency and neuraxial anaesthesia}},
Journal = {{ANAESTHESIA AND INTENSIVE CARE}},
Year = {{2015}},
Volume = {{43}},
Number = {{6}},
Pages = {{782-785}},
Month = {{NOV}},
Abstract = {{Antithrombin III (ATIII) deficiency offers unique challenges to the
   anaesthetist in the perioperative setting due to the inherent
   thrombophilia, the anticoagulant therapies instituted and replacement of
   the deficient intrinsic natural anticoagulant. A particular challenge is
   the use of intrathecal anaesthesia, which requires a safe level of
   coagulation at the time of subarachnoid puncture. We describe the use of
   intrathecal anaesthesia on two occasions in a patient with ATIII
   deficiency who received human-derived ATIII concentrate as part of a
   perioperative anticoagulant regimen. The patient sustained no thrombotic
   or bleeding events. Our experience suggests that ATIII deficiency does
   not preclude the use of regional anaesthetic techniques so long as there
   is timely referral to a multidisciplinary perioperative service for
   anticoagulant management and that ATIII concentrate is used to ensure
   safe levels of ATIII throughout the perioperative period.}},
