@article{ WOS:000087553100005,
Author = {Anandacoomaraswamy, A and De Costa, WAJM and Shyamalie, HW and Campbell,
   GS},
Title = {{Factors controlling transpiration of mature field-grown tea and its
   relationship with yield}},
Journal = {{AGRICULTURAL AND FOREST METEOROLOGY}},
Year = {{2000}},
Volume = {{103}},
Number = {{4}},
Pages = {{375-386}},
Month = {{JUL 1}},
Abstract = {{The objective of this experiment was to determine the factors
   influencing the transpiration rates of mature, clonal tea (Camellia
   sinensis L.) and estimate its transpiration efficiency. The heat pulse
   technique was used to measure transpiration rates of tea plants growing
   in the field as part of extensive canopies at Talawakelle, Sri Lanka
   during the period between 1 January and 19 February 1997. irrigation and
   shading treatments were used to determine the influence of soil water
   content (S) and irradiance on transpiration rate. The transpiration rate
   declined only slightly when S decreased from field capacity (44\%) to
   33\%. However, when S declined below 33\%, the transpiration rate showed
   a rapid decline from 1.6 to 0.71 per plant per day at 15\% S. When S was
   near field capacity, maximum transpiration rates of 0.53-0.931 plant(-1)
   h(-1) occurred between 1000 and 1500 h. The corresponding maxima when
   the S was near permanent wilting point (i.e. at -1.5 MPa matric
   potential) were 0.27-0.531 plant(-1) h(-1). Transpiration decreased
   linearly with decreasing irradiance throughout the range of radiation
   levels tested (i.e. from 100 to 15\% of full sunlight) at a rate of
   0.0311 per plant per day per \% reduction in solar irradiance. The daily
   transpiration rates of tea plants (0.42-1.071 per plant per day) under
   the natural shade of Grevillea robusta were considerably lower than the
   value of tea plants in the open, 3.5111 per plant per day. Spraying of
   an antitranspirant, Kaolin, decreased canopy temperature by 2-4 degrees
   C and especially around mid-day. Kaolin also decreased transpiration
   slightly during the period between 1000 and 1500 h. Transpiration
   efficiency (T-E) was 9.637 kg ha(-1) (made tea) mm(-1) of water
   transpired. The relationship between total dry matter yield and the
   ratio between transpiration and mean saturation vapour pressure deficit
   also was linear with a proportionality constant of 6.9 g kg(-1) kPa. (C)
   2000 Elsevier Science B.V. All rights reserved.}},
Publisher = {{ELSEVIER SCIENCE BV}},
Address = {{PO BOX 211, 1000 AE AMSTERDAM, NETHERLANDS}},
