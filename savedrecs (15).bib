Title = {{Angular coefficients of Z bosons produced in pp collisions at root S=8
   TeV and decaying to mu(+)mu(-) as a function of transverse momentum and
   rapidity}},
Journal = {{PHYSICS LETTERS B}},
Year = {{2015}},
Volume = {{750}},
