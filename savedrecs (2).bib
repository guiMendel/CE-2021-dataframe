@article{ WOS:000595898100001,
Author = {Wu, Zhen-Yu and Zhang, Hong},
Title = {{The different resonant modes for the special 2D/1D hybrid structures in
   the visible-light region}},
Journal = {{INTERNATIONAL JOURNAL OF QUANTUM CHEMISTRY}},
Year = {{2021}},
Volume = {{121}},
Number = {{7}},
Month = {{APR 5}},
Abstract = {{Low-dimensional nanostructures have demonstrated abundant novelty
   characteristics in the quantum field. Inspired by this, we design a
   2D/1D hybrid structure based on the graphene-like material (BC3 cluster)
   and noble metal (Ag chain) to study its optical response feature. The
   absorption spectrum illustrates that the structure obtains excellent
   performance such as high absorption strength and distinguish double
   peaks in the visible energy range (1.61-3.10 eV). Further calculations
   of induced charge distribution show that there are obviously resonant
   surface plasmon mode for the 2D/1D hybrid structure and origin surface
   plasmon mode mainly come from the 2D structure in the low energy region.
   To find out the similar characteristics of other 2D/1D hybrid structure,
   we perform the corresponding calculations of C3N and Ag hybrid
   structures. The results have proved the uniform performances for the two
   special hybrid structures in optical response, which provides attractive
   light-matter interactions in the low-dimensional nanostructures and
   tremendous potential in the developments of photosensor and optical
   energy devices.}},
