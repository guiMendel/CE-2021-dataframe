@article{ WOS:000283641200002,
Author = {Xia, Changsuo and Li, Yajuan and Cao, Wen and Yu, Zhaohua},
Title = {{Platelet-rich plasma-induced bone marrow mesenchymal stem cells versus
   autologous nerve grafting for sciatic nerve repair}},
Journal = {{NEURAL REGENERATION RESEARCH}},
Year = {{2010}},
Volume = {{5}},
Number = {{17}},
Pages = {{1291-1295}},
Month = {{SEP 15}},
Abstract = {{Autologous nerve grafting is the gold standard of peripheral nerve
   repair. We previously showed that autologous platelet-rich plasma (PAP)
   contains high concentrations of growth factors and can induce in vitro
   cultured bone marrow mesenchymal stem cells (BMSCs) to differentiate
   into Schwann cells. Here we used PAP-induced BMSCs combined with
   chemically extracted acellular nerves to repair sciatic nerve defects
   and compared the effect with autologous nerve grafting. The BMSCs and
   chemically extracted acellular nerve promoted target muscle wet weight
   restoration, motor nerve conduction velocity, and axonal and myelin
   sheath regeneration, with similar effectiveness to autologous nerve
   grafting. This finding suggests that PAP induced BMSCs can be used to
   repair peripheral nerve defects.}},
