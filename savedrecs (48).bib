@article{ WOS:000088553000005,
Author = {Niwa, O and Kurita, R and Hayashi, K and Horiuchi, T and Torimitsu, K
   and Maeyama, K and Tanizawa, K},
Title = {{Continuous measurement of histamine from rat basophilic leukemia cells
