@article{ WOS:000231025000018,
Author = {Fujieda, A and Katayama, N and Ohishi, K and Yamamura, K and Shibasaki,
   T and Sugimoto, Y and Miyata, E and Nishii, K and Masuya, M and Ueda, H
   and Nakajima, H and Shiku, H},
Title = {{A putative role for histone deacetylase in the differentiation of human
   erythroid cells}},
Journal = {{INTERNATIONAL JOURNAL OF ONCOLOGY}},
Year = {{2005}},
Volume = {{27}},
Number = {{3}},
Pages = {{743-748}},
Month = {{SEP}},
Abstract = {{Histone acetylation controls the expression of specific genes in
   eukaryotic cells. We investigated the role of historic deacetylases
   (HDACs) in the differentiation of human erythroid cells, using
   pharmacological approaches. When CD36(+) erythroid precursor cells,
   generated from CD34(+) cells with stem cell factor, flt-3 ligand,
   thrombopoietin, interleukin-3, interleukin-6, and erythropoietin, were
   cultured with an HDAC inhibitor FK228 (depsipeptide) at a specified dose
   in the presence of erythropoietin, their differentiation was inhibited,
   as determined by the expression of CD45 and glycophorin A. Addition of
   the same dose of FK228 to cultures did not affect the growth of CD36(+)
   cells. Regardless of the presence or absence of FK228, cultured CD36(+)
   cells displayed similar proliferation kinetics. Analysis of acetylated
   histories revealed that FK228 upregulated the acetylation status of
   histories H3 and H4 in CD36(+) cells. The inhibition of CD36(+) cell
   differentiation was restored by removal of FK228 from the culture,
   indicating that the modification of CD36(+) cell differentiation by
   FK228 is reversible. Furthermore, interference with historic
   deacetylation by FK228 inhibited the generation of CD36(+) erythroid
   cells from CD34(+) hematopoietic progenitor cells. Our results indicate
   the possible involvement of HDACs in human erythropoiesis, especially
   the regulation of erythroid cell differentiation.}},
