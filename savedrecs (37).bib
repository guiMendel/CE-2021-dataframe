@article{ WOS:000232977700001,
Author = {Drudi, F and Massa, M},
Title = {{Price manipulation in parallel markets with different transparency}},
Journal = {{JOURNAL OF BUSINESS}},
Year = {{2005}},
Volume = {{78}},
Number = {{5}},
Pages = {{1625-1658}},
Month = {{SEP}},
Abstract = {{We provide a unique test of trading behavior under asymmetric
   information with parallel markets characterized by different degrees of
   transparency for the same asset. We consider the Treasury bond market
   and show that the informed dealers simultaneously place bids in the
   primary market and sell in the secondary market, repurchasing when the
   primary market closes. Price manipulation increases market depth in the
   more transparent market when the more opaque market is open. This
   supports the experimental findings of Bloomfield and O'Hara and shows
   how the existence of less transparent markets may increase the liquidity
   of the more transparent ones.}},
