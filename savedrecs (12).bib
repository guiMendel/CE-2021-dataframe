@article{ WOS:000364537600001,
Author = {Gwiazda, P. and Wittbold, P. and Wroblewska-Kaminska, A. and Zimmermann,
   A.},
Title = {{Renormalized solutions to nonlinear parabolic problems in generalized
   Musielak-Orlicz spaces}},
Journal = {{NONLINEAR ANALYSIS-THEORY METHODS \& APPLICATIONS}},
Year = {{2015}},
Volume = {{129}},
Pages = {{1-36}},
Month = {{DEC}},
Abstract = {{We will present the proof of existence of renormalized solutions to a
   nonlinear parabolic problem partial derivative(t)u - diva(., Du) = f
   with right-hand side f and initial data u(0) in L-1. The growth and
   coercivity conditions on the monotone vector field a are prescribed by a
   generalized Ar-function M which is anisotropic and inhomogeneous with
   respect to the space variable. In particular, M does not have to satisfy
   an upper growth bound described by a Delta(2)-condition. Therefore we
   work with generalized Musielak-Orlicz spaces which are not necessarily
   reflexive. Moreover we provide a weak sequential stability result for a
   more general problem: partial derivative(t)beta(., u)-div (a(., Du) +
   F(u)) = f, where beta is a monotone function with respect to the second
   variable and F is locally Lipschitz continuous. Within the proof we use
   truncation methods, Young measure techniques, the integration by parts
   formula and monotonicity arguments which have been adapted to
   nonreflexive Musielak-Orlicz spaces. (C) 2015 Elsevier Ltd. All rights
   reserved.}},
