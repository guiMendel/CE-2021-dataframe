@article{ WOS:000233343400048,
Author = {Turner, A and Beales, J and Faure, S and Dunford, RP and Laurie, DA},
Title = {{The pseudo-response regulator Ppd-H1 provides adaptation to photoperiod
   in barley}},
Journal = {{SCIENCE}},
Year = {{2005}},
Volume = {{310}},
Number = {{5750}},
Pages = {{1031-1034}},
Month = {{NOV 11}},
Abstract = {{Plants commonly use photoperiod (day length) to control the timing of
   flowering during the year, and variation in photoperiod response has
   been selected in many crops to provide adaptation to different
   environments and farming practices. Positional cloning identified
   Ppd-H1, the major determinant of barley photoperiod response, as a
   pseudo-response regulator, a class of genes involved in circadian clock
   function. Reduced photoperiod responsiveness of the ppd-H1 mutant, which
   is highly advantageous in spring-sown varieties, is explained by altered
   circadian expression of the photoperiod pathway gene CONSTANS and
   reduced expression of its downstream target, FT, a key regulator of
   flowering.}},
