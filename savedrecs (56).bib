@article{ WOS:A1995RV58500003,
Author = {ROUAULT, Y and BASCHNAGEL, J and BINDER, K},
Title = {{PHASE-SEPARATION OF SYMMETRICAL POLYMER MIXTURES IN THIN-FILM GEOMETRY}},
