@article{ WOS:A1995TL18200022,
Author = {Ball, P and Beneke, M and Braun, VM},
Title = {{Resummation of running coupling effects in semileptonic B meson decays
   and extraction of vertical bar V-cb vertical bar}},
Journal = {{PHYSICAL REVIEW D}},
Year = {{1995}},
Volume = {{52}},
Number = {{7}},
Pages = {{3929-3948}},
Month = {{OCT 1}},
Abstract = {{We present a determination of \textbackslash{}V-cb\textbackslash{} from
   semileptonic B decays that includes resummation of supposedly large
   perturbative corrections, originating hom the running of the strong
   coupling. We argue that the low value of the BLM scale found previously
   for inclusive decays is a manifestation of the renormalon divergence of
   the perturbative series starting already in third order. A reliable
   determination of \textbackslash{}V-cb\textbackslash{} from inclusive
   decays is possible if one either uses a short-distance b quark mass or
   eliminates all unphysical mass parameters in terms of measured
   observables, such that all infrared contributions of order 1/m(b) cancel
   explicitly. We find that using the <(MS)over bar> running mass
   significantly reduces the perturbative coefficients already in low
   orders. For a semileptonic branching ratio of 10.9\% we obtain
   \textbackslash{}V-cb\textbackslash{}(tau(B)/1.50 ps)(1/2) = 0.041 +/-
   0.002 from inclusive decays, in good agreement with the value extracted
   from exclusive decays.}},
