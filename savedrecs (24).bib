@article{ WOS:000285034600010,
Author = {Krook, Lennart and Whalen, Joseph P.},
Title = {{Nutritional secondary hyperparathyroidism in the animal kingdom report
   of two cases}},
Journal = {{CLINICAL IMAGING}},
Year = {{2010}},
Volume = {{34}},
Number = {{6}},
Pages = {{458-461}},
Month = {{NOV-DEC}},
Abstract = {{This report describes two cases of marked bone loss (osteopenia)
   occurring in a 9-week-old German shepherd puppy and in a 6-month-old
   tiger In both cases the animals were fed a diet which was exclusively
   boneless meat The diets in both cases contained approximately 40 mg of
   calcium and 1000 mg of phosphorus per pound resulting in both calcium
   deficiency and phosphorus excess, resulting in a phosphorus-to-calcium
   ratio of 25 1, well beyond the amounts known to cause marked loss of
   bone experimentally This has been termed nutritional secondary
   hyperparathyroichsm (NSH) Both animals presented with severe bone pain,
   difficulty in ambulation, and difficulty in chewing food Radiographs
   showed marked osteopenia and spontaneous fractures Both responded
   clinically and radiographically to calcium supplementation and a diet
   with an appropriate phosphorus-to-calcium ratio The importance of
   calcium and phosphorus in the human diet is briefly discussed (C) 2010
   Elsevier Inc All rights reserved}},
