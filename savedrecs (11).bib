@article{ WOS:000363776800008,
Author = {Chango, Joanna M. and Allen, Joseph P. and Szwedo, David and Schad,
   Megan M.},
Title = {{Early Adolescent Peer Foundations of Late Adolescent and Young Adult
   Psychological Adjustment}},
Journal = {{JOURNAL OF RESEARCH ON ADOLESCENCE}},
Year = {{2015}},
Volume = {{25}},
Number = {{4}},
Pages = {{685-699}},
Month = {{DEC}},
Abstract = {{The long-term impacts of failing to establish autonomy and relatedness
   within close friendships are poorly understood. Adolescent behaviors
   undermining autonomy and relatedness in friendships at 13 were examined
   as predictors of friendship competence at 18 and depressive symptoms and
   social withdrawal at 21. A diverse community sample of 184 adolescents
   participated in self-, peer, and observational assessments. Teens'
   inability to establish autonomy and connection with friends at 13
   predicted decreases in friendship competence at 18 (=-.20, p=.02).
   Direct links to increase in depressive symptoms (=.34, p<.001) and
   social withdrawal (=.18, p=.03) were observed, with friendship
   competence partially mediating these relations. Results highlight the
   importance of problematic adolescent peer relationships as risk factors
   for the development of young adult internalizing symptoms.}},
