@article{ WOS:000088302500044,
Author = {Wu, YT and Du, DL},
Title = {{On the Lie-Poisson structure of the nonlinearized discrete eigenvalue
   problem}},
Journal = {{JOURNAL OF MATHEMATICAL PHYSICS}},
Year = {{2000}},
Volume = {{41}},
Number = {{8}},
Pages = {{5832-5848}},
Month = {{AUG}},
Abstract = {{A 3x3 discrete eigenvalue problem and corresponding discrete soliton
   equations are proposed. Under a constraint between the potentials and
   eigenfunctions, the 3x3 discrete eigenvalue problem is nonlinearized
   into an integrable Poisson map with a Lie-Poisson structure. Further, a
   reduction of the Lie-Poisson structure on the co-adjoint orbit yields
   the standard symplectic structure. The Poisson map is reduced to the
   usual symplectic map when it is restricted on the leaves of the
   symplectic foliation. (C) 2000 American Institute of Physics.
