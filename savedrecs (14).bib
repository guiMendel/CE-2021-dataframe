@article{ WOS:000367057500032,
Author = {Ying, Gui-shuang and Maguire, Maureen G. and Daniel, Ebenezer and
   Ferris, Frederick L. and Jaffe, Glenn J. and Grunwald, Juan E. and Toth,
   Cynthia A. and Huang, Jiayan and Martin, Daniel F. and Comparison
   Age-Related Macular},
Title = {{Association of Baseline Characteristics and Early Vision Response with
   2-Year Vision Outcomes in the Comparison of AMD Treatments Trials (CATT)}},
Journal = {{OPHTHALMOLOGY}},
