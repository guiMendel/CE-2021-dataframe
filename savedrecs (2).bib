@article{ WOS:000594648600011,
Author = {Ahn, Cheolmin and Hong, Jaehyeong and Shin, Jongshin and Kim, Byungsub
   and Park, Hong-June and Sim, Jae-Yoon},
Title = {{An 18-Gb/s NRZ Transceiver With a Channel-Included 2-UI Impulse-Response
   Filtering FFE and 1-Tap DFE Compensating up to 32-dB Loss}},
Journal = {{IEEE TRANSACTIONS ON CIRCUITS AND SYSTEMS II-EXPRESS BRIEFS}},
Year = {{2020}},
Volume = {{67}},
Number = {{12}},
Pages = {{2863-2867}},
Month = {{DEC}},
Abstract = {{This brief presents a channel-included filtering feed-forward equalizer
   (FFE) scheme suitable for data transmission through a heavily-dispersive
   lossy channel. The proposed scheme adjusts the tap weights so that the
   impulse response seen at the receiver has only a 2-unit interval
   (UI)-long impulse response with other parts negligible. The confinement
   of each bit response to a 2-UI period effectively achieves a lossless
   duobinary-like eye opening and enables data recovery with only a 1-tap
   decision-feedback equalizer (DFE). The proposed transceiver, implemented
   in 28-nm LP CMOS technology, achieved data recovery of 18 Gb/s up to
   32-dB loss with only 3-tap FFE at the transmitter and 1-tap DFE at the
   receiver.}},
