@article{ WOS:000089593400010,
Author = {Zhao, YF and Yang, HQ and Pang, SJ},
Title = {{Si(100)-c(4X8) reconstruction formed in a highly nonequilibrium process}},
Journal = {{PHYSICAL REVIEW B}},
Year = {{2000}},
Volume = {{62}},
Number = {{12}},
Pages = {{R7715-R7718}},
Month = {{SEP 15}},
Abstract = {{A Si(100)-c(4 x 8) reconstruction is formed using a two-step solid phase
   epitaxy (SPE) technique. Calculations indicate that the reconstruction
   has a high surface energy (0.4 eV per surface atom above 2 x 1). Strain
   releasing mechanisms play a dominant role within the local energy scope
   around the surface energy. Dynamic simulations show that the system is
   kinetically pushed into this energy scope. Analytical modeling of SPE
   discloses a highly nonequilibrium thermodynamic process in which the
   surface atoms obtain high kinetic energy by self-overheating and are
   trapped in the high-energy state by instant self-quenching.}},
