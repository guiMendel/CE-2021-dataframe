@article{ WOS:000374425500011,
Author = {Ziaforoughi, Amin and Esfahani, Javad Abolfazli and Razavi, Seyed M. A.},
Title = {{Infrared Thin-Layer Drying of Quince Slice: Mathematical Modeling and
   Dehydration Characteristics}},
Journal = {{PHILIPPINE AGRICULTURAL SCIENTIST}},
Year = {{2015}},
Volume = {{98}},
Number = {{4}},
Pages = {{429-437}},
Month = {{DEC}},
Abstract = {{Infrared drying of thin-layer quince slices as thick as 5 +/- 0.24 mm
   was investigated at four levels of radiation power which was regulated
   to attain final temperatures of 50, 60, 70 and 80 degrees C. The
   temperature evolution of samples revealed the different stages of
   drying. The drying rate curves showed that the entire drying process
   took place in the falling rate period, except for about 7\% of initial
   drying time when the drying rate increased while the slice center
   temperature curves showed a small constant rate period. The rehydration
   ratio curves of the dried samples showed a rise in the final temperature
   of the samples which enhanced their rehydrability. Ten well-known
   thin-layer models were used to predict drying kinetics by nonlinear
   analysis of regression. The Midilli model best fitted the experimental
   data for the whole range of temperatures. The moisture diffusivity
   coefficient at each temperature was determined by Fick's second law of
   diffusion, in which their value varied from 1.02436 x 10(-9) to 2.92514
   x 10(-9) m(2)/s over the mentioned temperature range. The dependence of
   effective diffusivity coefficient on temperature was expressed by an
   Arrhenius type equation. The calculated value of the activation energy
   of moisture diffusion was 33.25 kJ/mol.}},
