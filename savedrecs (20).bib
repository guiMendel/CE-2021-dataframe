@article{ WOS:000285530300002,
Author = {Cho, Young-Uk and Park, Chan-Jeoung and Cha, Choong-Hwan and Chi,
   Hyun-Sook and Jang, Seongsoo and Kim, Mi-Jung and Lee, Kyoo-Hyung and
   Lee, Je-Hwan and Lee, Jung-Hee and Seo, Jong Jin and Im, Ho Joon},
Title = {{Minimal Residual Disease Detection in Acute Leukemia Patients by Flow
   Cytometric Assay of Cross-lineage Antigen Expression}},
Journal = {{KOREAN JOURNAL OF LABORATORY MEDICINE}},
Year = {{2010}},
Volume = {{30}},
Number = {{6}},
Pages = {{533-539}},
Month = {{DEC}},
Abstract = {{Background : It has been demonstrated that flow cytometric detection of
   minimal residual disease (MRD) has a prognostic significance in the
   treatment of patients with acute leukemia. We investigated the
   significance of flow cytometric MRD detection for the first time in
   Korea.
   Methods : We analyzed the results of MAD detection in morphologically
   complete remission bone marrow aspirates from 89 patients with
   newly-diagnosed or relapsed acute leukemia, in which leukemic cells had
   cross-lineage antigen expression. Patients were grouped based on MRD
   frequencies: >= 1.0\%, high MAD; <1.0\%, low MRD.
   Results : Forty-seven ALL patients consisted of 10 with high and 37 with
   low MRD levels. Patients with high MRD levels showed a tendency of more
   frequent relapse than those with low MRD levels (40.0\% and 13.5\%,
   respectively) (P=0.08). High MAD group showed a tendency of short
   relapse-free survival (RFS) and overall survival (OS), although the
   differences were not statistically significant. Forty-two AML patients
   consisted of 16 with high and 26 with low MAD levels. There were no
   correlations between the MRD levels and relapse rate, RFS or OS. AML
   patients with high MRD levels showed significantly higher rate of
   unfavorable cytogenetic risk categories and lower rate of favorable risk
   categories (P=0.03).
   Conclusions : MRD detection by flow cytometric assay of cross-lineage
   antigen expression would be useful in predicting treatment outcome in
   patients with ALL rather than AML. We expect that the establishment of
   the standardization of methods, time to test or antibody combination
   would be achieved through further trials in this country. (Korean J Lab
   Med 2010;30:533-9)}},
