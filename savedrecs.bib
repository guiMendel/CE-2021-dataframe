@article{ WOS:000659356900001,
Author = {Campbell, Roderick B.},
Title = {{Beyond meaning: skeuomorphy and the mediation of Shang things}},
Journal = {{WORLD ARCHAEOLOGY}},
Year = {{2020}},
Volume = {{52}},
Number = {{3, SI}},
Pages = {{376-394}},
Month = {{MAY 26}},
Abstract = {{Not only does the ontological turn not necessarily imply a turn away
   from semiotics but that rethought as systems of mediation, semiotics are
   both a necessary aspect of ontological analysis and necessarily reshaped
   by the endeavour. At the same time, if the `decentering of the human'
   implied by a turn to things and to ontology has tended to `reassemble
   the social' apolitically as well as symmetrically, at least in
   archaeology, this is not due to a lack of potential. In this article I
   will investigate the ontology and semiotics of Shang visual culture and
   its participation in structuring and manifesting local hierarchies of
   being. I will use the phenomenon of skeumorphy to both problematize
   common Western assumptions about the nature of representation and to
   explore early Chinese patterns of power and mediation.}},
