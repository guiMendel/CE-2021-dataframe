@article{ WOS:000364611500006,
Author = {Huang, Yanming and Liao, Qiong and Yuan, Rongdi},
Title = {{Management and visual outcome of SCH in vitrectomized eye}},
Journal = {{MEDICAL HYPOTHESES}},
Year = {{2015}},
Volume = {{85}},
Number = {{5}},
Pages = {{548-550}},
Month = {{NOV}},
Abstract = {{Suprachoroidal hemorrhage is a rare complication of all types of
   intraocular surgery and is always associated with poor visual outcome or
   even eye enucleation. Better understanding of SCH during various types
   of intraocular surgery can help surgeons to avoid this devastating
   complication and to optimally treat patients. We encountered three cases
   of SCH in vitrectomized eyes and found that there were some common
   characteristics compared to SCH developed in other intraocular
   surgeries. We hypothesized that SCH in vitrectomized eye might be
   localized, not severe and ended up comparably good without surgical
   intervention. Then we analyzed the risk factors and incidence,
   characteristics and severity, management and prognosis of SCH in
   vitrectomized eye to evaluate the hypothesis. (C) 2015 Elsevier Ltd. All
   rights reserved.}},
