@article{ WOS:000286026200113,
Author = {Suzuki, Sachiko and Kobayashi, Makoto and Kurata, Rie and Wang, Wanjing
   and Fujii, Toshiyuki and Yamana, Hajimu and Feng, Kaiming and Oya,
   Yasuhisa and Okuno, Kenji},
Title = {{Elucidation of annihilation processes of defects induced by
   gamma-irradiation in Li2TiO3}},
Journal = {{FUSION ENGINEERING AND DESIGN}},
Year = {{2010}},
Volume = {{85}},
Number = {{10-12}},
Pages = {{2331-2333}},
Month = {{DEC}},
Note = {{9th International Symposium on Fusion Nuclear Technology, Dalian,
   PEOPLES R CHINA, OCT 11-16, 2009}},
Abstract = {{Annihilation behaviors of irradiation defects induced by gamma-ray in
   Li2TiO3 were studied by means of ESR and compared to those by thermal
   neutron. The ESR spectra indicated that E `- and O--centers were formed
   after the gamma-ray irradiation and isochronal annealing experiments
   showed that the amounts of E `-and O--centers, which are Frenkel pair,
   decreased in the temperature range of 550-650 K for the gamma-ray
   irradiated sample, while for the thermal neutron irradiated one, those
   decreased in the lower temperature range of 400-500K. The activation
   energies of recombination reaction of E `-center and O--center were 0.68
   +/- 0.01 and 0.31 +/- 0.04 eV for the gamma-ray and the thermal neutron
   irradiated samples, respectively. In the gamma-ray irradiated sample,
   the formation of defects is resulted from electron-excitations but that
   in thermal neutron irradiated one is mainly resulted from collision of
   energetic particles. Comparing with the gamma-ray irradiated sample, it
   was found for the thermal neutron irradiated sample that O-2(-)-center
   would be easily formed and the recombination process of E `- and
   O-2(-)-centers would also take place. The processes of different defect
   formations like electron-excitation and collision processes, would
   produce the different defects such as O-- and O-2(-)-centers, leading to
   the different annihilation behaviors of irradiation defects. (C) 2010
   Elsevier B.V. All rights reserved.}},
Publisher = {{ELSEVIER SCIENCE SA}},
Address = {{PO BOX 564, 1001 LAUSANNE, SWITZERLAND}},
