@article{ WOS:000282347400001,
Author = {Talamo, Giampaolo and Castellani, William and Dolloff, Nathan G.},
Title = {{Prozone effect of serum IgE levels in a case of plasma cell leukemia}},
Journal = {{JOURNAL OF HEMATOLOGY \& ONCOLOGY}},
Year = {{2010}},
Volume = {{3}},
Month = {{SEP 10}},
Abstract = {{We describe a case of multiple myeloma (MM) and secondary plasma cell
   leukemia (PCL) secreting IgE-kappa immunoglobulin. To our knowledge,
   only 2 cases of IgE-producing secondary PCL have been reported in the
   medical literature. In our patient, the only tumor marker available for
   monitoring the therapeutic response to chemotherapy and allogeneic stem
   cell transplantation was the quantitative M component at serum protein
   electrophoresis (SPEP), because serum free light chains were in the
   normal range, Bence-Jones proteinuria was absent, and quantitative serum
   IgE levels provided inaccurate and erratic results, due to the prozone
   effect. This is a laboratory phenomenon that occurs when antigen excess
   interferes with antibody-based methods requiring immune complex
   formation for detection. It is important to recognize the presence of a
   prozone effect, because it can produce falsely normal results, and
   therefore it could lead clinicians to incorrect assessment of the
   response to therapy.}},
