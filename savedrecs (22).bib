@article{ WOS:000285073200012,
Author = {Simakov, S. K.},
Title = {{Metastable nanosized diamond formation from a C-H-O fluid system}},
Journal = {{JOURNAL OF MATERIALS RESEARCH}},
Year = {{2010}},
Volume = {{25}},
Number = {{12}},
Pages = {{2336-2340}},
Month = {{DEC}},
Abstract = {{The model of nanosized diamond particles formation at metastable P-T
   parameters from a C-H-O fluid system is presented It explains the
   hydrothermal formation and growth of diamond and the specifics of
   chemical vapor deposition (CVD) diamond synthesis gas mixtures at low
   P-T parameters Further, the model explains the genesis of interstellar
   nanodiamond formations in space and the genesis of metamorphic
   microdiamonds in shallow depth Earth rocks In contrast to models where
   many possible reactions are considered, the present model makes the
   simplest possible assumptions about the key processes, and is then able
   to account for various tendencies seen in experimental data}},
