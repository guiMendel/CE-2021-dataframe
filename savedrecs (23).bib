@article{ WOS:000283848800011,
Author = {Chisholm, Matthew F. and Luo, Weidong and Oxley, Mark P. and Pantelides,
   Sokrates T. and Lee, Ho Nyung},
Title = {{Atomic-Scale Compensation Phenomena at Polar Interfaces}},
Journal = {{PHYSICAL REVIEW LETTERS}},
Year = {{2010}},
Volume = {{105}},
Number = {{19}},
Month = {{NOV 4}},
Abstract = {{The interfacial screening charge that arises to compensate electric
   fields of dielectric or ferroelectric thin films is now recognized as
   the most important factor in determining the capacitance or polarization
   of ultrathin ferroelectrics. Here we investigate using
   aberration-corrected electron microscopy and density-functional theory
   to show how interfaces cope with the need to terminate ferroelectric
   polarization. In one case, we show evidence for ionic screening, which
   has been predicted by theory but never observed. For a ferroelectric
   film on an insulating substrate, we found that compensation can be
   mediated by an interfacial charge generated, for example, by oxygen
   vacancies.}},
