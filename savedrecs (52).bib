@article{ WOS:A1995TJ24800018,
Author = {MOUCHATY, S and COOK, JA and SHIELDS, GF},
Title = {{PHYLOGENETIC ANALYSIS OF NORTHERN HAIR SEALS BASED ON
   NUCLEOTIDE-SEQUENCES OF THE MITOCHONDRIAL CYTOCHROME-B GENE}},
Journal = {{JOURNAL OF MAMMALOGY}},
Year = {{1995}},
Volume = {{76}},
Number = {{4}},
Pages = {{1178-1185}},
Month = {{NOV}},
Abstract = {{Phylogenetic analysis was performed on a 458-base pair sequence from the
   mitochondrial cytochrome b gene of northern hair seals (Phocidae:
   Phocinae) including Phoca hispida, Phoca largha, Phoca vitulina, Phoca
