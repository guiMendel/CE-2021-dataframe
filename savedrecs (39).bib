@article{ WOS:000208317800033,
Author = {Lu, Yi-Ying and Cheung, Yun-Chung and Ko, Sheung-Fat and Ng, Shu-Hang},
Title = {{Calcified reticulate rind sign: A characteristic feature of gossypiboma
   on computed tomography}},
Journal = {{WORLD JOURNAL OF GASTROENTEROLOGY}},
Year = {{2005}},
Volume = {{11}},
Number = {{31}},
Pages = {{4927-4929}},
Month = {{AUG 21}},
Abstract = {{We herein report a gossypiboma resulting from a retained surgical swab,
   which had been left in peritoneum for 20 years after appendectomy. CT
   revealed a cystic mass with a calcified reticulate rind. Subsequent
   surgery and pathological examination showed a gossypiboma. A simple
   experiment, using a barium-soaked surgical swab demonstrating similar CT
   appearance, supported our postulation that calcium deposition on the
   reticulated fibers of a surgical swab could generate such a
   characteristic ``calcified reticulate rind{''} sign. We believe that
   identification of this CT sign facilitates the diagnosis of
   gossypibomas. (C) 2005 The WJG Press and Elsevier Inc. All rights
