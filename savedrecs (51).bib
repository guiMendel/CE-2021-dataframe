@article{ WOS:A1995TG42900024,
Author = {NISHII, Y and TANABE, Y},
Title = {{SEQUENTIAL AND REGIOSELECTIVE FRIEDEL-CRAFTS REACTIONS OF
   GEM-DIHALOCYCLOPROPANECARBONYL CHLORIDES WITH BENZENES FOR THE SYNTHESIS
   OF 4-ARYL-1-NAPHTHOL DERIVATIVES}},
Journal = {{TETRAHEDRON LETTERS}},
Year = {{1995}},
Volume = {{36}},
Number = {{48}},
Pages = {{8803-8806}},
Month = {{NOV 27}},
Abstract = {{Two types of novel, sequential and regioselective Friedel-Crafts
   reactions of gem-dihalocyclopropanecarbonyl chlorides with benzenes
   proceeded in a one-pot manner; E-3-aryl-2,2-dihalo-cyclopropanecarbonyl
