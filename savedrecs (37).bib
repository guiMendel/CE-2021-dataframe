@article{ WOS:000234570000038,
Author = {Al-Bashir, RF and Al-Awadi, NA and El-Dusouqui, OME},
Title = {{Synthesis of novel sulfonyl-stabilized phosphorus ylides, and the
   kinetics and mechanism of their conventional and flash vacuum pyrolysis
   reactions}},
Journal = {{CANADIAN JOURNAL OF CHEMISTRY}},
Year = {{2005}},
Volume = {{83}},
Number = {{9}},
Pages = {{1543-1553}},
Month = {{SEP}},
Abstract = {{Nine Substituted sulfonyl-stabilized phosphorus ylides were prepared by
   treating their intermediate ylide analogues with phenylinethanesulfonyl
   fluoride. The stoichiometric ratio of the reactants for each preparation
   needed to be adjusted according to the basicity of each ylide
   intermediate. The nine ylide compounds were then subjected to
   conventional (sealed-tube) gas-phase pyrolysis at 470-545 K. The
   pyrolytic reactions were homogeneous and obeyed a first-order rate
   equation. The values of the Arrhenius log A (s(-1)) and E-a (kJ mol(-1))
   obtained for these reactions averaged 11, 12 +/- 2.00 and 13 1.8 +/-
   24.4, respectively. Analysis Of the pyrolysates front conventional
   pyrolysis and from flash Vacuum pyrolysis at 600 K showed the products
   to be complex Mixtures of triphenylphosphine, triphenylphosphine oxide,
   triphenylphosphine sulfide, and symmetric and unsymmetric alkenes.
   Conventional pyrolysis also gave novel mixed sulfones and, for the
   p-methoxyaryl substituent, p-anisaildehyde. The products of the
   reactions under study are explained oil the basis of a mechanism
   involving a sulfonyl carbene intermediate, and the reaction mechanism is
   used to rationalize the kinetic results and molecular reactivities.}},
