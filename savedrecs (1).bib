@article{ WOS:000612225400058,
Author = {Perez-Obiol, A. and Gazda, D. and Friedman, E. and Gal, A.},
Title = {{Revisiting the hypertriton lifetime puzzle}},
Journal = {{PHYSICS LETTERS B}},
Year = {{2020}},
Volume = {{811}},
Month = {{DEC 10}},
Abstract = {{Conflicting values of the hypertriton (H-3(Lambda)) lifetime were
   extracted in recent relativistic heavy-ion collision experiments. The
   ALICE Collaboration's reported (3)(Lambda H) lifetime tau(H-3(A)) is
   compatible within measurement uncertainties with the free Lambda
   lifetime tau(Lambda), as naively expected for a loosely bound Lambda
   hyperon in tau(Lambda), whereas STAR's reported range of
   tau(H-3(Lambda)) values is considerably shorter: tau(STAR)
   (H-3(Lambda))similar to(0.4-0.7)tau(Lambda). This H-3(Lambda) lifetime
   puzzle is revisited theoretically, using H-3(Lambda) three-body
   wavefunctions generated in a chiral effective field theory approach to
   calculate the decay rate Gamma(H-3(Lambda) -> He-3 + pi(-)). Significant
   but opposing contributions arise from Sigma NN admixtures in H-3(Lambda)
   and from pi(-)-He-3 final-state interaction. Evaluating the inclusive
   pi(-) decay rate Gamma(pi)-(H-3(Lambda)) via a branching ratio
   Gamma(H-3(Lambda)-> He-3+pi(-))/Gamma(pi)- (H-3(Lambda)) determined in
   helium bubble-chamber experiments, and adding Gamma(pi 0)(H-3(Lambda))
   through the Delta I = 1/2 rule, we derive tau(H-3(Lambda)) assuming
   several different values of the Lambda separation energy B-Lambda
   (H-3(Lambda)). It is concluded that each of ALICE and STAR reported
   tau(H-3(Lambda)) intervals implies its own constraint on B-Lambda
   (H-3(Lambda)): B-Lambda less than or similar to 0.1 MeV for ALICE,
   B-Lambda greater than or similar to 0.2 MeV for STAR. (C) 2020 The
   Authors. Published by Elsevier B.V.}},
