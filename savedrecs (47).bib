@article{ WOS:000088799100009,
Author = {Xu, WC and Yi, Y and Qiu, L and Shuaib, A},
Title = {{Neuroprotective activity of tiagabine in a focal embolic model of
   cerebral ischemia}},
Journal = {{BRAIN RESEARCH}},
Year = {{2000}},
Volume = {{874}},
Number = {{1}},
Pages = {{75-77}},
Month = {{AUG 18}},
Abstract = {{Gamma aminobutyric acid (GABA) agonists have been shown to have
   neuroprotective effects when used after focal or global cerebral
   ischemia. In this study, we evaluated the neuroprotective effects of a
   GABA re-uptake inhibitory agent, tiagabine, on focal ischemic brain
   injury in an embolic model in rats. Tiagabine, injected at 1 h after
   embolization, significantly reduced brain infarction volume, measured
   with 2,3,5-triphenyltetrazolium chloride (TTC) histological assay. There
   were varying degrees of neuroprotective effect exhibited in the other
   experimental groups however this did not reach significance. These
   results suggest that tiagabine is neuroprotective when administrated at
   an early period after the ischemic brain injury. (C) 2000 Elsevier
   Science B.V. All rights reserved.}},
