@article{ WOS:000234524000003,
Author = {Toyama, K and Imaizumi, T and Yoshifuji, K and Miyata, K and Kawamura, M
   and Kohama, I},
Title = {{Study on hemosiderin deposition after intracerebral hemorrhage}},
Journal = {{NEUROLOGICAL SURGERY}},
Year = {{2005}},
Volume = {{33}},
Number = {{12}},
Pages = {{1177-1181}},
Month = {{DEC}},
Abstract = {{Background and Purpose Hemosiderin degenerated from intracerebral
   hematoma (ICH) can be visualized as low intensity on gradient-echo
   T2{*}-weighted (T2{*}-w) MRI, which is sensitive for hemosiderin. We
   studied the factors associated with the hemosiderin deposition on
   T2{*}-w MRI after ICH.
   Methods We analyzed T2{*}-w MRIs in 72 outpatients with past ICH (46
   males, 26 females, 28 - 89 (60.0 +/- 9.8) years old) consecutively came
   to our hospital. The odds ratio (OR) for the apparent thickness; >= 2.5
   nun of low intensity surrounding ICH cavity was estimated, using the
   time after the onset of IGH (17 weeks - 26 years), and other factors.
   Results AD of old ICHs were visible as low intensities on T2{*}-w MRI.
   Multivariate logistic regression analysis revealed that an elevated
   ratio of the apparent thickness >= 2.5 nun of hemosiderin was found for
   the patients with time a 5 years between T2{*}-w MRI and the onset (odds
   ratio (OR): 0.24, 95\% confidence interval (CI): 0.06-0.99),
   intraventricular or subarachnoid hemorrhage related to ICH (OR: 0.16,
   95\% Cl: 0.03 - 0,77), and the diameter of ICH >= 2cm (OR: 33.7, 95\%
   Cl: 4.6 - 245).
   Conclusion Though small sample size limited the power of analyses, our
   findings suggest that the amount of hemosiderin deposition after IGH may
   be associated with the time after the onset, intraventricular or
   subarachnoid hemorrhage related to ICH, and the diameter of ICH.}},
