@article{ WOS:000285438900018,
Author = {Gallego, Isaac and Oncins, Gerard and Sisquella, Xavier and
   Fernandez-Busquets, Xavier and Daban, Joan-Ramon},
Title = {{Nanotribology Results Show that DNA Forms a Mechanically Resistant 2D
   Network in Metaphase Chromatin Plates}},
Journal = {{BIOPHYSICAL JOURNAL}},
Year = {{2010}},
Volume = {{99}},
Number = {{12}},
Pages = {{3951-3958}},
Month = {{DEC 15}},
Abstract = {{In a previous study, we found that metaphase chromosomes are formed by
   thin plates, and here we have applied atomic force microscopy (AFM) and
   friction force measurements at the nanoscale (nanotribology) to analyze
   the properties of these planar structures in aqueous media at room
   temperature. Our results show that high concentrations of NaCl and EDTA
   and extensive digestion with protease and nuclease enzymes cause plate
   denaturation. Nanotribology studies show that native plates under
   structuring conditions (5 mM Mg2+) have a relatively high friction
   coefficient (mu approximate to 0.3), which is markedly reduced when high
   concentrations of NaCl or EDTA are added (mu approximate to 0.1). This
   lubricant effect can be interpreted considering the electrostatic
   repulsion between DNA phosphate groups and the AFM tip. Protease
   digestion increases the friction coefficient (mu approximate to 0.5),
   but the highest friction is observed when DNA is cleaved by micrococcal
   nuclease (mu approximate to 0.9), indicating that DNA is the main
   structural element of plates. Whereas nuclease-digested plates are
   irreversibly damaged after the friction measurement, native plates can
   absorb kinetic energy from the AFM tip without suffering any damage.
   These results suggest that plates are formed by a flexible and
   mechanically resistant two-dimensional network which allows the safe
   storage of DNA during mitosis.}},
