@article{ WOS:000619797700004,
Author = {Hannon, Jr., Michael J.},
Title = {{Fast and Accurate Core Analysis by the Full-Immersion Pressure-Pulse
   Decay: Part 1-Theory}},
Journal = {{SPE RESERVOIR EVALUATION \& ENGINEERING}},
