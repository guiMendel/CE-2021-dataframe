@article{ WOS:000599599300009,
Author = {Arbuzov, Yuri D. and Evdokimov, Vladimir M. and Shepovalova, V, Olga},
Title = {{Theory of concentration distribution over the surface of axisymmetrical
   receiver in cylindrical parabolic mirror solar energy concentrator}},
