@article{ WOS:000365469600001,
Author = {Schottelius, Margret and Wirtz, Martina and Eiber, Matthias and Maurer,
   Tobias and Wester, Hans-Juergen},
Title = {{{[}In-111]PSMA-I\&T: expanding the spectrum of PSMA-I\&T applications
   towards SPECT and radioguided surgery}},
Journal = {{EJNMMI RESEARCH}},
Year = {{2015}},
Volume = {{5}},
Month = {{NOV 25}},
Abstract = {{Background: The relevance of prostate-specific membrane antigen (PSMA)
   targeting in the clinical management of prostate cancer (PCa) is
   continually increasing, entailing the development of PSMA-targeted
   molecular probes. Recently, a first PSMA-targeted theranostic concept
   has been successfully implemented by {[}Ga-68/Lu-177]PSMA-I\&T. To
   further exploit the excellent PSMA-targeting characteristics and in vivo
   performance of the PSMA-I\&T platform, {[}In-111]PSMA-I\&T was evaluated
   as a complementary probe for radioguided surgery and SPECT imaging.
   Findings: Compared to {[}Ga-68/Lu-177]PSMA-I\&T, {[}In-111]PSMA-I\&T
   showed unchangedly high PSMA-affinity and enhanced internalization into
   PSMA-expressing LNCaP PCa cells. Biodistribution studies in LNCaP
   xenograft-bearing mice (1 h p.i.) revealed slightly reduced background
   accumulation of {[}In-111]PSMA-I\&T compared to {[}Lu-177]PSMA-I\& T and
   identical tumor uptake of both compounds, leading to increased
   tumor/background ratios for {[}In-111]PSMAI\&T. An exemplary patient
   with metastatic PCa underwent preoperative {[}Ga-68]HBED-CC-PSMA PET/CT
   (1 h p.i.) and {[}In-111]PSMA-I\&T SPECT/CT (4 h p.i.), followed by
   prostatectomy and radioguided extended pelvic lymphadenectomy (24 h
   p.i.). In {[}In-111]PSMA-I\&T SPECT/CT, the previously identified PCa
   lesions ({[}Ga-68]HBED-CC-PSMA PET/CT) showed high tracer accumulation
   and were also detectable using planar scintigraphy. The intraoperative
   use of a hand-held gamma probe allowed detection and resection of all
   {[}In-111]PSMA-I\&T-accumulating lesions. The presence of PSMA-positive
   tumor tissue in the resected specimens was confirmed histopathologically
   and via {[}In-111]PSMA-I\&T autoradiography.
   Conclusions: {[}In-111]PSMA-I\&T shows efficient PSMA targeting in vitro
   and in vivo, combined with low background accumulation. In an exemplary
   PCa patient, {[}In-111]PSMA-I\&T was successfully applied for
   preoperative SPECT/CT visualization and radioguided resection of
   PSMA-positive lesions, hinting towards a high value of
   {[}In-111]PSMA-I\&T as a complementary tool to {[}Ga-68/Lu-177]PSMA-I\&T
   in the clinical management of prostate cancer.}},
