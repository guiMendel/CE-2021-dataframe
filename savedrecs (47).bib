@article{ WOS:000089598400002,
Author = {Miyazaki, S},
Title = {{Anomalous time scaling of the mean square distance in on-off diffusion}},
Journal = {{JOURNAL OF THE PHYSICAL SOCIETY OF JAPAN}},
Year = {{2000}},
Volume = {{69}},
Number = {{9}},
Pages = {{2719-2722}},
Month = {{SEP}},
Abstract = {{The mean square distance sigma(2)(t) of the diffusion induced by on-off
   intermittency is derived based on the continuous-time random walk
   theory. It obeys a scaling law sigma(2)(t) = 2Dt phi(t/tau) with
   diffusion constant D and characteristic time tau, which is confirmed by
   the use of numerical iterations of a specific periodic map. The scaling
   function phi and the power spectrum of the on-off intermittency variable
   I(omega) are analytically obtained from the distribution function of the
   laminar duration. Normal diffusion (phi(z) similar to 1) and slow
   diffusion (phi(z) proportional to 1/root z) are observed, respectively,
   for t much greater than tau and t much less than tau. The former and
   latter correspond to the flat part (I(omega) similar to const) and the
   power law (I(omega) proportional to 1/root omega), respectively,for the
   power spectrum.}},
