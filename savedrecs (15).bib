@article{ WOS:000364414400014,
Author = {Andres, Beatrice and Christ, Marc and Gahl, Cornelius and Wietstruk,
   Marko and Weinelt, Martin and Kirschner, Juergen},
Title = {{Separating Exchange Splitting from Spin Mixing in Gadolinium by
   Femtosecond Laser Excitation}},
Journal = {{PHYSICAL REVIEW LETTERS}},
Year = {{2015}},
Volume = {{115}},
Number = {{20}},
Month = {{NOV 10}},
Abstract = {{Employing spin-, time-, and energy-resolved photoemission spectroscopy,
   we present the first study on the spin polarization of a single
   electronic state after ultrafast optical excitation. Our investigation
   concentrates on the majority-spin component of the d-band-derived
   Gd(0001) surface state d(z2)(up arrow). While its binding energy shows a
   rapid Stoner-like shift by 90 meV with an exponential time constant of
   tau(E) = 0.6 +/- 0.1 ps, the d(z2)(up arrow) spin polarization remains
   nearly constant within the first picoseconds and decays with tau(S) = 15
   +/- 8 ps. This behavior is in clear contrast to the equilibrium phase
   transition, where the spin polarization vanishes at the Curie
   temperature.}},
