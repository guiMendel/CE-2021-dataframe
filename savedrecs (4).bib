@article{ WOS:000595173100024,
Author = {Li, Jiao and Pan, Yue and Yang, Andi and Lei, Lang},
Title = {{Treatment in a patient with congenital loss of both mandibular lateral
   incisors and severely retroclined maxillary central incisors}},
Journal = {{AMERICAN JOURNAL OF ORTHODONTICS AND DENTOFACIAL ORTHOPEDICS}},
Year = {{2020}},
Volume = {{158}},
Number = {{6}},
Pages = {{868-877}},
Month = {{DEC}},
Abstract = {{This case report describes the treatment of a patient with congenital
   loss of both mandibular lateral incisors and severely retroclined
   maxillary incisors. The treatment included bilateral extraction of the
   maxillary first premolars, accurate 3-digital setup, a 2-stage torque
   control strategy, and intricate mechanic management. The dilemma of
   tooth-size discrepancy was solved by ideal torque control to avoid
   interproximal enamel reduction. An ideal Class I molar and canine
   relation, as well as canine guidance in the lateral excursion movement,
   was achieved with good stability.}},
