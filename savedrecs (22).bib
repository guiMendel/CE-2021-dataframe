@article{ WOS:000283384400023,
Author = {Shah, Syed Qaiser and Khan, Aakif Ullah and Khan, Muhammad Rafiullah},
Title = {{Radiosynthesis and biodistribution of 99(m)Tc-rifampicin: A novel
   radiotracer for in-vivo infection imaging}},
Journal = {{APPLIED RADIATION AND ISOTOPES}},
Year = {{2010}},
Volume = {{68}},
Number = {{12}},
Pages = {{2255-2260}},
Month = {{DEC}},
Abstract = {{Tc-99m-rifampicin (Tc-99m-RMP) a new radioantibiotic complex was
   synthesized specifically for the infection localization caused by
   methicillin-resistant Staphylococcus aureus (MRSA). The in-vitro
   radiochemical purity (RCP) yield. in-vivo biodistribution behavior in
   artificially infected rats (AIT) and scintigraphic accuracy in
   artificially infected rabbit (AIB) of the 99(m)Tc-RMP complex was
   investigated using different concentration of the RMP, sodium
   pertechnetate ((NaTcO4)-Tc-99m), stannous chloride dihydrate (SnCl2
   center dot 2H(2)O) at different pH ranges 5-6. The best RCP yield
   observed at 30, 60, 90 and 120 min after labeling was: 98.95 +/- 0 20,
   98 15 +/- 0.24. 96.50 +/- 0.27 and 91.55 +/- 0.22\%, respectively. using
   1.5 mg RMP, 175 mu L of SnCl2 2H(2)O (1 mu g/mu L in 0.01 N HCl), 3 mu
   of (NaTcO4)-Tc-99m, at pH 5.6. Initially in the infected muscle (INM) of
   the AIT the activity was lower but after 90 mm it went up to 18.35 +/-
   0.20\% from 595 +/- 0 25\%. The activity in the inflamed muscle (IMM),
   normal (NM) muscle, blood, liver and spleen was initially high that
   decreased with time. The ratios of the INM/NM and IMM/NM were 7.34 +/-
   074 and 1.20 +/- 0.85, respectively. The whole body static (WBS) imaging
   of the MRSA infected rabbit confirmed the usefulness of the Tc-99m-RMP
   as a precise radiotracer for MRSA infection imaging. On the basis of
   in-vitro RCP, in-vivo biodistribution and scintigraphic precision, we
   recommend the Tc-99m-RMP complex prepared aseptically for in-vivo
   assessment of MRSA infection. (C) 2010 Elsevier Ltd All rights reserved.}},
