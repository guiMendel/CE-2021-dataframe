@article{ WOS:000587994800005,
Author = {Bavula, Volodymyr V. and Lu, Tao},
Title = {{THE PRIME IDEALS AND SIMPLE MODULES OF THE UNIVERSAL ENVELOPING ALGEBRA
   U(b x V-2)}},
Journal = {{GLASGOW MATHEMATICAL JOURNAL}},
Year = {{2020}},
Volume = {{62}},
Number = {{1, SI}},
Pages = {{S77-S98}},
Month = {{DEC}},
Abstract = {{Let b be the Borel subalgebra of the Lie algebra sl(2) and V-2 be the
   simple two-dimensional sl(2)-module. For the universal enveloping
   algebra A:= U(b x V-2) of the semi-direct product b x V-2 of Lie
   algebras, the prime, primitive and maximal spectra are classified. The
   sets of completely prime ideals of A are described. The simple
   unfaithful A-modules are classified and an explicit description of all
   prime factor algebras of A is given. The following classes of simple U(b
   x V-2)-modules are classified: the Whittaker modules, the K{[}X]-torsion
   modules and the K{[}E]-torsion modules.}},
