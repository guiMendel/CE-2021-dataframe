@article{ WOS:A1995TK21000004,
Author = {WAND, Y and MONARCHI, DE and PARSONS, J and WOO, CC},
Title = {{THEORETICAL FOUNDATIONS FOR CONCEPTUAL MODELING IN INFORMATION-SYSTEMS
   DEVELOPMENT}},
