@article{ WOS:000361771500062,
Author = {Knor, Martin and Skrekovski, Riste and Tepeh, Aleksandra},
Title = {{An inequality between the edge-Wiener index and the Wiener index of a
   graph}},
Journal = {{APPLIED MATHEMATICS AND COMPUTATION}},
Year = {{2015}},
Volume = {{269}},
Pages = {{714-721}},
Month = {{OCT 15}},
Abstract = {{The Wiener index W(G) of a connected graph G is defined to be the sum
   Sigma(u, v)d(u, v) of distances between all unordered pairs of vertices
   in G. Similarly, the edge-Wiener index W-e(G) of G is defined to be the
   sum Sigma(e,f)d(e,f) of distances between all unordered pairs of edges
   in G, or equivalently, the Wiener index of the line graph L(G). Wu
   (2010) showed that W-e(G) >= W(G) for graphs of minimum degree 2, where
   equality holds only when G is a cycle. Similarly, in Knor et al. (2014),
   it was shown that W-e(G) >= delta(2)-1/4W(G) where delta denotes the
   minimum degree in G. In this paper, we extend/improve these two results
   by showing that W-e(G) >= delta(2)/4 W(G) with equality satisfied only
   if G is a path on 3 vertices or a cycle. Besides this, we also consider
   the upper bound for W-e(G) as well as the ratio W-e(G)/W(G). We show
   that among graphs Con n vertices attains its minimum for the star. (C)
   2015 Elsevier Inc. All rights reserved.}},
