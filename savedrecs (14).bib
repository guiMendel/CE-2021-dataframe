@article{ WOS:000366321100011,
Author = {Wind, Julia and Auckett, Josie E. and Withers, Ray L. and Piltz, Ross O.
   and Maljuk, Andrey and Ling, Chris D.},
Title = {{Type II Bi-1 (-) xWxO1.5 (+) (1.5x): a (3+3)-dimensional commensurate
   modulation that stabilizes the fast-ion conducting delta phase of
   bismuth oxide}},
Journal = {{ACTA CRYSTALLOGRAPHICA SECTION B-STRUCTURAL SCIENCE CRYSTAL ENGINEERING
   AND MATERIALS}},
