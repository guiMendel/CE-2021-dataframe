@article{ WOS:000283954800133,
Author = {von Ballmoos, Peter and Takahashi, Tadayuki and Boggs, Steven E. and
   Broad Int Collaboration},
Title = {{A DUAL mission for nuclear astrophysics}},
Journal = {{NUCLEAR INSTRUMENTS \& METHODS IN PHYSICS RESEARCH SECTION
