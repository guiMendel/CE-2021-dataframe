@article{ WOS:000281183900006,
Author = {Addison, Timothy and Cayre, Olivier J. and Biggs, Simon and Armes,
   Steven P. and York, David},
Title = {{Multi-layer films of block copolymer micelles adsorbed to colloidal
   templates}},
Journal = {{PHILOSOPHICAL TRANSACTIONS OF THE ROYAL SOCIETY A-MATHEMATICAL PHYSICAL
   AND ENGINEERING SCIENCES}},
Year = {{2010}},
Volume = {{368}},
Number = {{1927}},
Pages = {{4293-4311}},
