@article{ WOS:A1995RQ29900004,
Author = {COCHRAN, SD},
Title = {{ADAPTIVE VERGENCE FOR THE STEREO MATCHING OF OBLIQUE IMAGERY}},
Journal = {{ISPRS JOURNAL OF PHOTOGRAMMETRY AND REMOTE SENSING}},
Year = {{1995}},
Volume = {{50}},
Number = {{4}},
Pages = {{21-28}},
