@article{ WOS:000283925300007,
Author = {Yamada, Kei and Asada, Hideki},
Title = {{Collinear solution to the general relativistic three-body problem}},
Journal = {{PHYSICAL REVIEW D}},
Year = {{2010}},
Volume = {{82}},
Number = {{10}},
Month = {{NOV 8}},
Abstract = {{The three-body problem is reexamined in the framework of general
   relativity. The Newtonian three-body problem admits Euler's collinear
   solution, where three bodies move around the common center of mass with
   the same orbital period and always line up. The solution is unstable.
   Hence, it is unlikely that such a simple configuration would exist owing
   to general relativistic forces dependent not only on the masses but also
   on the velocity of each body. However, we show that the collinear
   solution remains true with a correction to the spatial separation
   between masses. Relativistic corrections to the Sun-Jupiter Lagrange
   points L-1, L-2, and L-3 are also evaluated.}},
