@article{ WOS:000364967700001,
Author = {Javier Garcia-Pacheco, Francisco},
Title = {{Extremal properties of the set of vector-valued Banach limits}},
Journal = {{OPEN MATHEMATICS}},
Year = {{2015}},
Volume = {{13}},
Pages = {{757-767}},
Month = {{NOV 3}},
Abstract = {{In this manuscript we find another class of real Banach spaces which
   admit vector-valued Banach limits different from the classes found in
   {[}6, 7]. We also characterize the separating subsets of l(infinity)(X).
   For this we first need to study when the space of almost convergent
   sequences is closed in the space of bounded sequences, which turns out
   to happen only when the underlying space is complete. Finally, a study
   on the extremal structure of the set of vector-valued Banach limits is
   conducted when the underlying normed space is a Hilbert space. We also
   reach the conclusion that the set of vector-valued Banach limits is not
   a convex component of B-CL(l infinity(X).X), provided that X is a
   1-injective Banach space satisfying that the underlying compact
   Hausdorff topological space has isolated points.}},
