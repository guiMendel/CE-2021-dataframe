@article{ WOS:000360255000021,
Author = {Barnett, Michael D. and Martinez, Beatriz},
Title = {{Optimists: It could have been worse; Pessimists: It could have been
   better Dispositional optimism and pessimism and counterfactual thinking}},
Journal = {{PERSONALITY AND INDIVIDUAL DIFFERENCES}},
Year = {{2015}},
Volume = {{86}},
Pages = {{122-125}},
Month = {{NOV}},
Abstract = {{Optimism and pessimism are cognitive expectancies regarding future
   events, whereas counterfactual thinking is the cognitive process of
   imagining alternatives to events that occurred in the past. The purpose
   of this study was to conceptualize dispositional optimism and pessimism
   within the context of norm theory and examine relationships between
   dispositional optimism and pessimism and counterfactual thinking.
   Undergraduate students (N = 833) completed measures of counterfactual
   thinking and optimism and pessimism. After controlling for the effects
   of positive and negative affect, it was found that downward
   counterfactual thinking (imagining how things could have been worse) was
   associated with optimism and that upward styles of counterfactual
   thinking (imagining how things could have been better) were associated
   with pessimism. These results suggest that thinking about past events is
   consistent with expectations about the future. (C) 2015 Elsevier Ltd.
   All rights-reserved.}},
