@article{ WOS:A1995TX42100002,
Author = {Stein, P},
Title = {{Custom in Roman and medieval civil law}},
Journal = {{CONTINUITY AND CHANGE}},
Year = {{1995}},
Volume = {{10}},
Number = {{3}},
Pages = {{337-\&}},
Month = {{DEC}},
Abstract = {{Roman rhetorical literature recognized custom as the basis of all law
   which did not derive from legislation, whereas juristic writing
   originally recognized only local custom which supplemented the general
   law. The jurist Julian argued that popular consent, which was the basis
   of legislation, could be expressed also in customary practice, but the
   Emperor Constantine held that custom could only be valid if it did not
   conflict with either reason or written law. Medieval lawyers had to
   reconcile these two texts and English civil lawyers in particular used
   the debate to justify the validity of the new common law.}},
