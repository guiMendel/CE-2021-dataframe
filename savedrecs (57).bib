@article{ WOS:A1995RN36500017,
Author = {PERKINS, AT and ONDO, W},
Title = {{WHEN TO WORRY ABOUT HEADACHE - HEAD PAIN AS A CLUE TO INTRACRANIAL
   DISEASE}},
