@article{ WOS:000234534800026,
Author = {Wang, JH and Zhong, K and Ding, H and Li, R and Gao, MX and Pan, HG},
Title = {{Structure and electrochemical properties of La-Mg-Ni system hydrogen
   storage alloys with different Co contents}},
Journal = {{TRANSACTIONS OF NONFERROUS METALS SOCIETY OF CHINA}},
Year = {{2005}},
Volume = {{15}},
Number = {{6}},
Pages = {{1351-1355}},
Month = {{DEC}},
Abstract = {{The structure and electrochemical properties of the
   La0.7Mg0.3Ni3.4-xMn0.1 Co-x (x=0 - 1.05) hydrogen storage alloys were
   investigated. The crystal structure and the lattice parameters of the
   alloys were analyzed by Xray diffractometry and Rietveld method.
   Electrochemical properties of the alloys including p-c-t curves,
   discharge capacity, discharge capacity retention were studied. The
   results show that (La, Mg) Ni-3 and LaNi5 are the main phases of all the
   alloys. The plateau pressure for hydrogen absorption/desorption
   decreases and the hydrogen storage capacity firstly increases and
   subsequently decreases with increasing Cc content. The values of the
   maximum discharge capacity of the alloy electrodes remain in range of
   395.3 - 403.1 mA(.)h/g in spite of the change of Cc content. The cycling
   stability of the alloy electrodes is greatly improved with increasing Co
   content, which is attributed to the suppression of the cell volume
   expansion during hydriding, leading the pulverization of the alloy
   particles lowered and the oxidation/corrosion of the active elements
   reduced.}},
