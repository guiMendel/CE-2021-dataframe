@article{ WOS:000618892300012,
Author = {Herrick, Lauren P. and Goh, Jensen and Menke, Walter and Campbell,
   Marilyn S. and Fleenor, Bradley S. and Abel, Mark G. and Bergstrom,
   Haley C.},
Title = {{Effects of Curcumin and Fenugreek Soluble Fiber on the Physical Working
   Capacity at the Fatigue Threshold, Peak Oxygen Consumption, and Time to
   Exhaustion}},
Journal = {{JOURNAL OF STRENGTH AND CONDITIONING RESEARCH}},
Year = {{2020}},
Volume = {{34}},
Number = {{12}},
Pages = {{3346-3355}},
Month = {{DEC}},
Abstract = {{The purpose of this study was to examine the effects of curcumin in
   combination with fenugreek soluble fiber (CUR + FEN) or fenugreek
   soluble fiber alone (FEN) on the neuromuscular fatigue threshold
   (PWCFT), peak oxygen consumption ((V) over doto(2)peak), and time to
   exhaustion (T-lim) on a graded exercise test (GXT), in untrained
   subjects. The PWCFT estimates the highest power output that can be
   maintained without evidence of neuromuscular fatigue. Forty-seven
   untrained, college-aged subjects were randomly assigned to one of 3
   supplementation groups; placebo (PLA, n515), CUR + FEN (500 mg.d(-1),
   n=18), or FEN (300 mg.d(-1), n=14). The subjects completed a maximal GXT
   on a cycle ergometer to determine the PWCFT, (V) over doto(2)peak, and
   T-lim before (PRE) and after (POST) 28 days of daily supplementation.
   Surface electromyographic signals-were recorded from a bipolar electrode
   arrangement on the vastus lateralis of the right leg during each test.
   Separate one-way analysis of covariances were used to determine if there
   were between-group differences for adjusted POST-PWCFT, POST-(V) over
   doto(2)peak, and POST-T-lim values, covaried for the respective PRE-test
   scores. The adjusted POST-PWCFT for the CUR + FEN group (mean +/- SD:
   196 +/- 58 W) was greater (p = 0.016) than the PLA group (168 +/- 49 W)
   but the FEN group (185 +/- 32 W) was not different from the CUR + FEN or
   PLA groups (p > 0.05). There were no differences for adjusted POST-(V)
   over doto(2)peak (p=0.612) or POST-T-lim (p=0.508) among the groups.
   These findings suggested curcumin combined with fenugreek soluble fiber
   might delay neuromuscular fatigue.}},
