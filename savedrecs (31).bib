@article{ WOS:000234119600016,
Author = {Mochizuki, H and Mizokuro, T and Tanigaki, N and Hiraga, T},
Title = {{Formation and characteristics of dispersion layers of organofluorine
   compounds in poly(methyl methacrylate) using a vacuum process}},
Journal = {{JOURNAL OF APPLIED PHYSICS}},
Year = {{2005}},
Volume = {{98}},
Number = {{11}},
Month = {{DEC 1}},
Abstract = {{Dispersion layers of organofluorine compounds in poly(methyl
   methacrylate) (PMMA) were formed by a vacuum process, and their
   thickness increased with treatment temperature and time. Generally,
   dispersion of an additive into a polymer seems to follow Fick's Law
   based on thermal diffusion. However, the dispersion layers formed in the
   present study could not have been governed by Fick's Law. The
   concentration of organofluorine compounds was uniform throughout the
   dispersion layer, which showed a lower refractive index than the initial
   PMMA because the organofluorine compound used here showed a lower
   refractive index. The refractive index could be controlled by treatment
   temperature. The process used in the present study was favorable for
   preparation of the waveguide. (c) 2005 American Institute of Physics.}},
