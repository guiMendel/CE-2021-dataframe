@article{ WOS:000232332600034,
Author = {Fu, H and Tu, MJ and Chen, YG and Zhang, TB and Zhang, LC},
Title = {{Effect of impurities on the structure and magnetic entopy changes of
   Gd-5(SixGe1-x)(4)(x approximate to 0.5) alloys}},
Journal = {{RARE METAL MATERIALS AND ENGINEERING}},
Year = {{2005}},
Volume = {{34}},
Number = {{9}},
Pages = {{1481-1484}},
Month = {{SEP}},
Abstract = {{The Gd5S1.9Ge2.1 and Gd5Si1.72Ge2.28 alloys with different Gd starting
   materials were prepared to investigate the effect of impurites of Gd on
   the magnetocaloric properties of the Gds(SixGe1-x)(4) alloys. The
   structure of alloys was analyzed by X-ray diffraction (XRD) based on the
   X'Pert Highscore software package and the PDF2-2003 database. It was
   found that the alloys with high purity Gd is single Gd5Si2Ge2 phase.
   Whereas the alloys with low purity Gd is double phases, i.e. the
   Gd5Si2Ge2 phase and the Gd-5(Si,Ge)(3) phase. The magnetic measurements
   showed that the impurities have no visible effect on the interaction of
   the ferromagnetic Gd5Si2Ge2 phase. The main reason for the decrease of
   magnetic entropy changes of the Gd5Si1.9Ge2.1 and Gd5Si1.72Ge2.28 alloys
