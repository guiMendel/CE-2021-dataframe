@article{ WOS:000593114000018,
Author = {Gvozdjan, Kristina and Casey, Heather and Mowery, Carrie and Kumer,
   Lorie and Fisher, Carolyn and Tyler, Jennifer and Bayerl, Mike G. and
   Malysz, Jozef and Naik, Seema and Rybka, Witold and Ehmann, Christopher
   and Claxton, David and Mineishi, Shin and Baker, Maria and Hong, Zheng
   and Shike, Hiroko},
Title = {{Unexpected Short-Tandem-Repeat Patterns in Posttransplant Chimerism
   Testing: Investigation of 3 Cases with Help from Forensic Science}},
Journal = {{LABORATORY MEDICINE}},
Year = {{2020}},
Volume = {{51}},
Number = {{6}},
Pages = {{635-641}},
Month = {{NOV}},
Abstract = {{Chimerism testing by short tandem repeats (STRs) is used to monitor
   engraftment after allogeneic hematopoietic stem cell transplantation
   (HSCT). Generally, STR alleles are stable and transferred from parent to
   child or from donor to recipient. However, 3 cases did not follow this
   norm. Additional work-up with help from forensic literature solved these
   mysteries.
   In case 1, the patient received HSCT from his son. The son shared STR
   alleles in 22/23 loci except Penta E, which was explained by repeat
   expansion in the son.
   In case 2, the patient had been in remission for 14 years after HSCT for
   lymphoma and developed repeat expansion in CSF1P0 in granulocytes.
   In case 3, a pre-HSCT patient demonstrated 3 alleles, with 2 peaks
   taller than the third, in the FGA locus (chromosome 4). A combination of
   a triallelic variant and leukemia-associated trisomy 4 explained the
   finding. STR number variants are rare and clinically inconsequential but
   can overlap malignancy-associated, clinically significant changes.}},
