@article{ WOS:000603189600002,
Author = {Wang, Hanchen and Chen, Jilei and Yu, Tao and Liu, Chuanpu and Guo,
   Chenyang and Liu, Song and Shen, Ka and Jia, Hao and Liu, Tao and Zhang,
   Jianyu and Cabero, Marco A. and Song, Qiuming and Tu, Sa and Wu,
   Mingzhong and Han, Xiufeng and Xia, Ke and Yu, Dapeng and Bauer, Gerrit
   E. W. and Yu, Haiming},
Title = {{Nonreciprocal coherent coupling of nanomagnets by exchange spin waves}},
Journal = {{NANO RESEARCH}},
Year = {{2021}},
Volume = {{14}},
Number = {{7}},
Pages = {{2133-2138}},
Month = {{JUL}},
Abstract = {{Nanomagnets are widely used to store information in non-volatile
   spintronic devices. Spin waves can transfer information with low-power
   consumption as their propagations are independent of charge transport.
   However, to dynamically couple two distant nanomagnets via spin waves
   remains a major challenge for magnonics. Here we experimentally
   demonstrate coherent coupling of two distant Co nanowires by fast
   propagating spin waves in an yttrium iron garnet thin film with sub-50
   nm wavelengths. Magnons in two nanomagnets are unidirectionally
   phase-locked with phase shifts controlled by magnon spin torque and
   spin-wave propagation. The coupled system is finally formulated by an
   analytical theory in terms of an effective non-Hermitian Hamiltonian.
   Our results are attractive for analog neuromorphic computing that
   requires unidirectional information transmission.}},
