@article{ WOS:000362921500018,
Author = {Beckingham, Bryan S. and Sanoja, Gabriel E. and Lynd, Nathaniel A.},
Title = {{Simple and Accurate Determination of Reactivity Ratios Using a
   Nonterminal Model of Chain Copolymerization}},
Journal = {{MACROMOLECULES}},
Year = {{2015}},
Volume = {{48}},
Number = {{19}},
Pages = {{6922-6930}},
Month = {{OCT 13}},
Abstract = {{We propose a new method for the determination of reactivity ratios based
   on a nonterminal model of copolymerization kinetics. Within the context
   of this model, we derive simple, reactivity-ratio-dependent expressions
   whose solution relies solely on monomer consumption information spanning
   the full range of conversion. Utilizing this method, reactivity ratios
   are obtained for the aluminum chelate-catalyzed copolymerization of
   phenyl glycidyl ether and allyl glycidyl ether (r(PGE) = 1.56 +/- 0.01
   and r(AGE) = 0.66 +/- 0.03) with monomer consumption monitored by in
