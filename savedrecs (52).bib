@article{ WOS:A1995TV55200044,
Author = {Deakin, J and Prunier, V and Wood, GC and Stott, FH},
Title = {{The effects of reactive elements on the oxidation of alumina-forming
   iron-chromium-aluminium alloys at high temperature}},
