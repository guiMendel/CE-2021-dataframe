@article{ WOS:A1995TJ47700008,
Author = {CHMUTOVA, MK and IVANOVA, LA and MYASOEDOV, BF},
Title = {{SOLUBILITY OF DIARY{[}DIALKYLCARBAMOYLMETHYL]PHOSPHINE OXIDES IN
   NITRIC-ACID SOLUTIONS}},
Journal = {{RADIOCHEMISTRY}},
