@article{ WOS:A1995RZ31400024,
Author = {BUZATU, D and LEV, FM},
Title = {{OKUBO-ZWEIG-IIZUKA RULE VIOLATION IN THE REACTION
   (P)OVER-BAR-P-]F'(2)PI(0)}},
Journal = {{PHYSICS LETTERS B}},
Year = {{1995}},
Volume = {{359}},
Number = {{3-4}},
Pages = {{393-396}},
Month = {{OCT 12}},
Abstract = {{The interpretation of the OZI-rule violation in the reaction <(p)over
   bar p> --> phi pi(0) encounters difficulties since one cannot exclude
   the possibility that the violation is a consequence of the rescattering
   in the final state. For this reason it is important to seek reactions in
   which the OZI-rule violation cannot be explained by conventional
   mechanisms. We compute the rescattering contribution in the reaction
   <(p)over bar p> --> f'(2) pi(0) and show that this contribution is
   negligible. Therefore any substantial OZI-rule violation in the process
   <(p)over bar p> --> f'(2) pi(0) would undoubtedly mean that some unusual
   physics plays an important role in this process.}},
