@article{ WOS:000367859500005,
Author = {Codecasa, Lorenzo and d'Alessandro, Vincenzo and Magnani, Alessandro and
   Rinaldi, Niccolo},
Title = {{Structure-preserving approach to multi-port dynamic compact models of
   nonlinear heat conduction}},
Journal = {{MICROELECTRONICS JOURNAL}},
Year = {{2015}},
Volume = {{46}},
Number = {{12, A}},
Pages = {{1129-1137}},
Month = {{DEC}},
Abstract = {{A novel approach for constructing multi-port dynamic compact thermal
   models of nonlinear heat diffusion in electronic components is
   presented, extending a technique previously conceived for the one-port
   case. The approach allows achieving high levels of accuracy even for
   small state-space dimensions of the model. It is also very efficient
   since it requires the solutions to a few linear heat diffusion problems
   in the frequency domain. The multi-port compact models exhibit structure
   similar to the one-port counterparts and can be used to accurately
   approximate not only the junction temperatures, but also the whole
   space-time temperature distribution within the electronic components.
   (C) 2015 Elsevier Ltd. All rights reserved.}},
Publisher = {{ELSEVIER SCI LTD}},
