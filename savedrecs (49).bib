@article{ WOS:000088405200027,
Author = {Romanski, SA and Nelson, RM and Jensen, MD},
Title = {{Meal fatty acid uptake in adipose tissue: gender effects in nonobese
   humans}},
Journal = {{AMERICAN JOURNAL OF PHYSIOLOGY-ENDOCRINOLOGY AND METABOLISM}},
Year = {{2000}},
Volume = {{279}},
Number = {{2}},
Pages = {{E455-E462}},
Month = {{AUG}},
Abstract = {{We tested for gender differences in dietary fatty acid metabolism in 12
   nonobese men and 12 nonobese women using the meal fatty acid
   tracer/adipose tissue biopsy study design. In addition to determining
   body composition, measurements of regional adipose tissue lipoprotein
   lipase activity, blood flow, and fat cell size were performed to place
   the meal fatty acid kinetic studies in perspective. Twenty-four hours
   after ingesting the test meal, the concentration of meal fatty acids was
   greater (P < 0.05) in abdominal subcutaneous than in thigh adipose
   tissue in both men (0.61 +/- 0.12 vs. 0.45 +/- 0.09 mg/g) and women
   (0.59 +/- 0.10 vs. 0.43 +/- 0.05) but was not different between men and
   women. A greater percentage of dietary fat was stored in subcutaneous
   adipose tissue in women than in men (38 +/- 3 vs. 24 +/- 3\%,
   respectively, P < 0.05), and a greater portion of meal fatty acid
   disposal was unaccounted for in men. Significant gender differences in
   regional adipose tissue blood flow after meal ingestion were noted; the
   differences were in the direction that could support greater nutrient
   storage in lower body fat in women.}},
