@article{ WOS:000231310700041,
Author = {Qi, YJ and Lu, CJ and Zhu, J and Chen, XB and Song, HL and Zhang, HJ and
   Xu, XG},
Title = {{Ferroelectric and dielectric properties of Ca0.28Ba0.72Nb2O6 single
   crystals of tungsten bronzes structure}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2005}},
Volume = {{87}},
Number = {{8}},
Month = {{AUG 22}},
Abstract = {{Large Ca0.28Ba0.72Nb2O6 (CBN-28) single crystals exhibit saturated
   ferroelectric hysteresis loops under an electric field of 115 kV/cm
   along the {[}001] direction. Their spontaneous polarization, remanent
   polarization and coercive field are 35.3 mu C/cm(2), 32.2 mu C/cm(2),
   and 38.1 kV/cm, respectively. The dielectric constant and dielectric
   loss tan delta of the crystals are 195 and 0.32 at 10 kHz, respectively.
   A diffused dielectric anomaly with relaxor characteristic was observed
   in the range of 325-500 degrees C at low frequencies, while the CBN-28
   crystals experienced a first-order normal-relaxor ferroelectric phase
   transition around 252 degrees C on heating. A broad dielectric loss peak
   appears around 120 degrees C and it is interpreted in terms of the
   migration of oxygen vacancies. The conductance activation energy was
   determined to be 1.33 eV in the high temperature regime (500-560 degrees
   C). (c) 2005 American Institute of Physics.}},
