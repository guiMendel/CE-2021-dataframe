@article{ WOS:000364770500235,
Author = {Zhang, Zhizhi and Lv, Haolin and Feng, Zekun and Nie, Yan},
Title = {{Study on the Magnetically Tunable Filters Based on Mnn+ and Al3+
   Co-Doped YIG Ferrite}},
Journal = {{IEEE TRANSACTIONS ON MAGNETICS}},
Year = {{2015}},
Volume = {{51}},
