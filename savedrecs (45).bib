@article{ WOS:000089749100025,
Author = {Barz, C and Abahji, TN and Trulzsch, K and Heesemann, J},
Title = {{The Yersinia Ser/Thr protein kinase YpkA/YopO directly interacts with
   the small GTPases RhoA and Rac-1}},
Journal = {{FEBS LETTERS}},
Year = {{2000}},
Volume = {{482}},
Number = {{1-2}},
Pages = {{139-143}},
Month = {{SEP 29}},
Abstract = {{Pathogenic bacteria of the genus Yersinia counteract host defense by
   interfering with eukaryotic signal transduction pathways. YpkA of
   Yersinia pseudotuberculosis shares significant homology with eukaryotic
   Ser/Thr protein kinases, is translocated into the host cell and has been
   shown to be an essential virulence factor in a mouse infection model. In
   this study, we identify the small GTPases RhoA and Rac-1 as eukaryotic
   binding partners of YpkA and its homolog YopO of Yersinia
   enterocolitica. We demonstrate that the interaction is independent of
   phosphorylation of YpkA and nucleotide loading state of the GTPases. The
   interaction with RhoA and Rac-1 might provide an important clue to how
   YpkA interferes with eukaryotic signaling on a molecular level, (C) 2000
   Federation of European Biochemical Societies. Published by Elsevier
   Science B.V. All rights reserved.}},
