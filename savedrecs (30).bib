@article{ WOS:000234161100019,
Author = {Zhao, G and Duan, CS and Zhou, ZY and Wu, QY and Lu, JF},
Title = {{Density functional theory study of the hydrogen bonding interaction of 1
   : 1 complexes of formamide with cysteine}},
Journal = {{JOURNAL OF MOLECULAR STRUCTURE-THEOCHEM}},
Year = {{2005}},
Volume = {{756}},
Number = {{1-3}},
Pages = {{143-149}},
Month = {{DEC 9}},
Abstract = {{The hydrogen bonding of 1:1 complexes formed between formamide and
   cysteine molecules has been completely studied in the present paper
   using density functional theory (DFT), second-order Moller-Plesset
   Perturbation (MP2) method and the Hartree-Fock (HF) method at varied
   basis set levels from 6-3 1 g to 6-31 + g(d). Nine reasonable geometries
   on the potential energy hypersurface of formamide and cysteine system
   are considered with the global minimum, eight of them are cyclic
   double-hydrogen bonded structures and only one is single-hydrogen bonded
   structure. The optimized geometric parameters and interaction energies
   for various isomers at different levels are estimated. The infrared
   spectrum frequencies, IR intensities and the vibrational frequency
   shifts are reported. (c) 2005 Elsevier B.V. All rights reserved.}},
