@article{ WOS:A1995RT10100004,
Author = {YAMAMOTO, K and AKITA, T and IKEUCHI, H and KITA, Y},
Title = {{EXPERIMENTAL-STUDY OF THE FLOW IN A HELICAL CIRCULAR TUBE}},
Journal = {{FLUID DYNAMICS RESEARCH}},
Year = {{1995}},
Volume = {{16}},
Number = {{4}},
Pages = {{237-249}},
Month = {{SEP}},
Abstract = {{The effect of torsion on the flow in a helical tube of circular
   cross-section is experimentally investigated over a range of Reynolds
   numbers from about 500 to 20000. Three cases of dimensionless curvature
   delta were studied, i.e. delta = 0.01, 0.05 and 0.1. The torsional
   parameter beta(0), which is defined as beta(0) = tau/root 2 delta with
   dimensionless torsion tau, was considered for seven cases between 0.45
   and 1.72. The results reveal rather large effect of torsion on the flow:
   the friction factor of the flow at a fixed delta deviates from that of
   the toroidally curved tube without torsion as beta(0) increases, and it
   decreases toward that of the straight tube as beta(0) further increases.
   It is also found that the torsion has a destabilizing effect on the
   flow. The critical Reynolds number at the onset of turbulence varies
   with beta(0) and has a minimum at beta(0) similar or equal to 1.3. The
   minimum critical Reynolds number at delta = 0.1 is about 800.}},
