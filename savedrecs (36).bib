@article{ WOS:000232542100009,
Author = {Ostermann, E},
Title = {{The wild-become subject. Christian Moosbrugger and the imagination of
   wildness in Musil's `Mann Ohne Eigenschaften'}},
Journal = {{NEOPHILOLOGUS}},
Year = {{2005}},
Volume = {{89}},
Number = {{4}},
Pages = {{605-623}},
Month = {{OCT}},
Abstract = {{Since Rousseau the noble savage and the imagination of wildness have
   become ideals that serve as harmonic counterparts to the experience of
   alienation in modern life. In Robert Musil's great novel Der Mann ohne
   Eigenschaften (The man without qualities), which was written in view of
   the catastrophe of World War I with it's breakdown of all traditional
   values, the situation has changed radically. Wildness has lost its
   utopian qualities and is characterized now as an uncontrollable and
   irreducible force that appears in crime, madness, and (common) ecstasy.
   Since it is able to destroy the structures of personal identity just as
   those of social life when ignored and repressed, Musil also looks for
   possibilities to integrate it into life. The essay describes several
   aspects of wildness in Musil's novel while focussing on the psychotic
   murderer Christian Moosbrugger, who can be understood as a modern
   successor of the noble savage.}},
