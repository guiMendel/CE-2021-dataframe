@article{ WOS:000209675800008,
Author = {Fafandel, M. and Ravlic, S. and Smodlaka, M. and Bihari, N.},
Title = {{Deoxyribonucleases in the cortex and endosome of the marine sponge
   Tethya aurantium.}},
Journal = {{BIOLOGIYA MORYA-MARINE BIOLOGY}},
Year = {{2010}},
Volume = {{36}},
Number = {{5}},
Pages = {{377-383}},
Month = {{SEP-OCT}},
Abstract = {{The presence and activity of deoxyribonucleases (DNases) in the cortex
   and endosome regions of the sea orange Tethya aurantium were
   investigated. Maximum enzyme activity in the sponge homogenate was
   detected at pH values of 4.27, 7.0, and 8.5-8.75. Among different
   specimens, several distinct patterns of neutral DNase isozymes were
   observed in the cortex region. In each investigated specimen, the
   highest neutral DNase activity was associated with high molecular weight
   proteins (up to75 kDa). The acid DNases showed a low level of activity.
   In the endosome, the acid DNase activity was up to 10 times higher than
   in the cortex and the presence of a DNase II-like protein was detected.
   Neutral DNase exhibiting the highest activity in all investigated
   specimens had a molecular weight of 20 kDa and belonged to the DNase
   I-like family. The results indicate that the activity of neutral and
   acid DNases is related to the region of the sponge and its biological
   functions. The cortex region communicating with the environment showed a
   high interindividual variability and heterogeneity of neutral DNases,
   while the endosome, where the intracellular digestion occurs, is a site
   of high acid DNase activity.}},
