@article{ WOS:000624243000001,
Author = {Zhao, Qianwei and Cepeda, Alice and Chou, Chih-Ping and Valdez, Avelardo},
Title = {{Incarceration Trajectories of Women Who Are Mothers: A Nationally
   Representative Study of State and Federal Prisoners}},
Journal = {{CRIME \& DELINQUENCY}},
Year = {{2021}},
Volume = {{67}},
Number = {{10}},
Pages = {{1513-1535}},
Month = {{SEP}},
Abstract = {{Despite growing research on women's offending trajectories, knowledge on
   the imprisonment experiences of women who are mothers is limited. This
   study used a nationally representative dataset of state and federal
   prisoners to identify and characterize subgroups of mothers based on
   incarceration histories. Group-based trajectory modeling identified four
   groups with distinct incarceration trajectories: stable escalating
   group, moderate declining group, adolescence-peak group, and
   young-adulthood-peak group. Bivariate analyses then suggested that
   different incarceration trajectories were associated with adverse life
   experiences (i.e., foster care placement, sexual abuse experience) and
   confinement experiences (i.e., number of episodes of prison
   confinement). Discussed are the future research directions and
   implications for criminal justice policies and interventions targeting
   imprisoned mothers.}},
