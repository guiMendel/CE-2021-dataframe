@article{ WOS:000367630000107,
Author = {Alavianmehr, Mohammad Mehdi and Hosseini, Sayed Mostafa and Akbari,
   Falamarz},
Title = {{Modeling volumetric properties of amorphous and molten polymers using
   new perturbed-chain equation of state}},
Journal = {{JOURNAL OF MOLECULAR LIQUIDS}},
Year = {{2015}},
Volume = {{212}},
Pages = {{900-906}},
Month = {{DEC}},
Abstract = {{A perturbed hard-chain (PHC) equation of state (EOS) has been previously
   employed to model the volumetric properties of molten polymers. The aim
   of this paper is to develop a new version of the PHC EOS for 25 polymers
   which employs a trimer expression from the statistical associating fluid
   theory-trimer (SAFT-T) as the reference physical model perturbed by the
   van der Waals dispersion forces. The significance of the proposed
   perturbed-hard-trimer-chain (PHTC) EOS is its capability of predicting
   the volumetric properties of polymers ranging from the amorphous state
   to above the melting temperature. Another feature of this model is the
   number of temperature-dependent parameters appeared in the EOS, for
   which only one temperature-dependent parameter is appeared to be the
   universal function of the reduced temperature using the molecular
   scaling constants: the dispersive energy among the pair-wise trimers,
   epsilon and the hard-core diameter, a. The performance of the PHTC EOS
   has been checked by predicting 3262 and 910 volumetric data related to
   the respectively, molten and amorphous/glassy polymers over pressure
   range within 0.1-200 MPa and temperature ranging from 294 to 608 K. The
   average absolute deviation (MD in \%) of the predicted specific volumes
   of studied amorphous and molten polymers were found to be 038\% and
   0.23\%, respectively, which represents lower AADs than those obtained
   from the preceding PHC EOS. (C) 2015 Elsevier B.V. All rights reserved.}},
