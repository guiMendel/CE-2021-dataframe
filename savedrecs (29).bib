@article{ WOS:000281644900003,
Author = {von Loewenich, Clemens and Benner, Hartmut and Just, Wolfram},
Title = {{Experimental verification of Pyragas-Scholl-Fiedler control}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2010}},
Volume = {{82}},
Number = {{3, 2}},
Month = {{SEP 7}},
Abstract = {{We present an experimental realization of time-delayed feedback control
   proposed by Scholl and Fiedler. The scheme enables us to stabilize
   torsion-free periodic orbits in autonomous systems, and to overcome the
   so-called odd number limitation. The experimental control performance is
   in quantitative agreement with the bifurcation analysis of simple model
   systems. The results uncover some general features of the control scheme
   which are deemed to be relevant for a large class of setups.}},
