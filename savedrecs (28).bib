@article{ WOS:000282667400012,
Author = {Wang, Weizhi and Zhang, Qikai and Ding, Lianhui and Zheng, Ying},
Title = {{SIMULATION OF HYDRODESULFURIZATION USING ARTIFICIAL NEURAL NETWORK}},
Journal = {{CANADIAN JOURNAL OF CHEMICAL ENGINEERING}},
Year = {{2010}},
Volume = {{88}},
Number = {{5}},
Pages = {{801-807}},
Month = {{OCT}},
Abstract = {{Artificial neural network (ANN) is applied to investigate the
   hydrodesulfurization (HDS) process with light-cycle oil as feed and
   NiMo/Al2O3 as catalyst. ANN models frequently work as a ``black box{''}
   which makes the model invisible to users and always need significant
   data for training. In this work, a new ANN is proposed. The
   Langmuir-Hinshelwood kinetic mechanism is incorporated into the model so
   that the proposed ANN model is forced to follow the given reaction
   mechanisms. Both advantages of self-learning ability of ANN and the
   existing knowledge of HDS were taken into account. Lengthy training
