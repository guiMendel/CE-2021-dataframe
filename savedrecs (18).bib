@article{ WOS:000362492300006,
Author = {Stein, Paul D. and Matta, Fadi and Hughes, Patrick G. and Ghiardi,
   Martina and Marsh, John H. and Khwarg, Juewon and Brandon, Michael S.
   and Fowkes, Hope A. N. and Kazan, Viviane and Wiepking, Matthew and
   Keyes, Daniel C. and Kakish, Edward J. and Hughes, Mary J.},
Title = {{Home Treatment of Deep Venous Thrombosis in the Era of New Oral
   Anticoagulants}},
Journal = {{CLINICAL AND APPLIED THROMBOSIS-HEMOSTASIS}},
Year = {{2015}},
Volume = {{21}},
Number = {{8}},
Pages = {{729-732}},
Month = {{NOV}},
Abstract = {{This is a retrospective cohort study of adults with a primary diagnosis
   of deep venous thrombosis (DVT) unaccompanied by pulmonary embolism
   (PE), seen in 4 emergency departments in 2013 and part of 2014. The
   purpose was to assess the prevalence of home treatment of DVT in the
   present era of new oral anticoagulants. Among 96 patients with DVT and
   no PE, 85 (88.5\%) were hospitalized and 11 (11.5\%) were discharged to
   home. Most of the patients discharged to home received
   low-molecular-weight heparin, 9 (81.8\%) of 11. None were prescribed new
   oral anticoagulants. Early discharge in 2 days occurred 28 (32.9\%) of
   85 patients. Most (64.3\%) received enoxaparin and/or warfarin at early
   discharge. Rivaroxaban was prescribed in 7 (25.0\%) of those discharged
   in 2 days. We conclude that in some emergency departments, patients with
   DVT are uncommonly discharged to home even though new oral
   anticoagulants are available.}},
