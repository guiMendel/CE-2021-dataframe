@article{ WOS:000362575800007,
Author = {Melnikov, P. and Arkhangelsky, I. V. and Nascimento, V. A. and Silva, A.
   F. and de Oliveira, L. C. S. and Zanoni Consolo, L. Z. and Herrero, A.
   S.},
Title = {{Thermolysis mechanism of dysprosium hexahydrate nitrate Dy(NO3)(3)center
   dot 6H(2)O and modeling of intermediate decomposition products}},
Journal = {{JOURNAL OF THERMAL ANALYSIS AND CALORIMETRY}},
Year = {{2015}},
Volume = {{122}},
Number = {{2}},
Pages = {{571-578}},
Month = {{NOV}},
Abstract = {{The thermal decomposition of dysprosium nitrate is a complex process
   which begins with the simultaneous condensation of the initial monomer
   Dy(NO3)(3)center dot 6H(2)O into a cyclic hexamer 6{[}Dy(NO3)(3)center
   dot 6H(2)O]. This cluster gradually loses water and nitric acid, and
   intermediate oxynitrates Dy6O7(NO3)(4)center dot 6H(2)O and
   Dy6O8(NO3)(2)center dot 5H(2)O are formed. At higher temperatures, these
   products undergo further degradation, lose nitrogen dioxide, water, and
   oxygen, and finally, after having lost lattice water, are transformed
   into the cubic form of dysprosium oxide. The models of intermediate
   oxynitrates represent a reasonably good approximation to the real
   structures and a proper interpretation of experimental data. The
   comparison of the potential energies of consecutive oxynitrates
   permitted an evaluation of their stability. The mechanism was studied by
   thermal analysis, differential scanning calorimetry, infrared
   spectroscopy, and X-ray diffraction. The mass losses were related to the
   vibrational energy levels of the evolved gases.}},
