@article{ WOS:000595631500032,
Author = {Verdier, Nicolas and Papy, Jean Michel and Renard, Jean Baptiste and
   Lefevre, Maximilien and Agrapart, Clemence},
Title = {{Enhanced detection and sizing algorithm to improve LOAC optical particle
   counter performances}},
Journal = {{APPLIED OPTICS}},
Year = {{2020}},
Volume = {{59}},
Number = {{34}},
Pages = {{10892-10901}},
Month = {{DEC 1}},
Abstract = {{The light optical aerosols counter (LOAC) instrument is an optical
   aerosol counter that allows atmospheric partides from 0.2 to 50 mu m to
   be individually counted and classified by size. The scattered light due
   to the interaction between a particle and a laser beam is captured by
   photodiodes and produces an electric pulse. Detecting these pulses and
   measuring their amplitude is a key process to measure the particle size.
   This paper first details the current method to achieve such estimation
   and proposes a correlation algorithm to improve the detection and the
   assessment of particle sizes. In the last section, some test results are
   presented to demonstrate the performance of this new algorithm. (C) 2020
   Optical Society of America}},
