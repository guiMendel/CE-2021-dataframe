@article{ WOS:000367305100004,
Author = {Ehlebracht-Koenig, I. and Siemienik, K. and Dorn, M. and Reusch, A.},
Title = {{Patient Education in Fibromyalgia - Participatory Development Taking
   into Account the S3 Guideline}},
Journal = {{PHYSIKALISCHE MEDIZIN REHABILITATIONSMEDIZIN KURORTMEDIZIN}},
Year = {{2015}},
Volume = {{25}},
Number = {{6}},
Pages = {{310-318}},
Month = {{DEC}},
Abstract = {{Patient education is recommended in the German S3-guideline released in
   2012 by the AWMF as a basic component of the treatment for fibromyalgia
   syndrome. Accordingly, we have reviewed previous patient education
   approaches published in both the national and international literature.
   Moreover, to identify participants' needs from the patients'
   perspective, a qualitative assessment was carried out using focus
   groups. Building on this work, we refined an existing standardized
   education program, which already was based on a curricular structure.
   The statements regarding diagnosis and treatment incorporated in the new
   program were made in close accordance with the S3-guideline mentioned
   above, while its didactic conception and design comply with the quality
   criteria and recommendations as issued by the Center for Patient
   Education, Wuerzburg. The overarching goal of the new education program
   is to enhance patients' self-management of their condition. The
   program's focus is not only on valid disease-specific information
   delivered by interactive and patient-oriented methods, but also on
   lifestyle changes and practical action planning. The evaluation of
   acceptance and results of the heiQ-Program questionnaire showed high
   levels of satisfaction with the new program, which may be used in both
   inpatient and outpatient settings.}},
