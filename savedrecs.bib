@article{ WOS:000602696500001,
Author = {Gopalan, Parikshit and Yehudayoff, Amir},
Title = {{Concentration for Limited Independence via Inequalities for the
   Elementary Symmetric Polynomials}},
Journal = {{THEORY OF COMPUTING}},
Year = {{2020}},
Volume = {{16}},
Month = {{DEC 24}},
Abstract = {{We study the extent of independence needed to approximate the product of
   bounded random variables in expectation. This natural question has
   applications in pseudorandomness and min-wise independent hashing.
   For random variables with absolute value bounded by 1, we give an error
   bound of the form sigma(Omega(k)) when the input is k-wise independent
   and sigma(2) is the variance of their sum. Previously, known bounds only
   applied in more restricted settings, and were quantitatively weaker.
   Our proof relies on a new analytic inequality for the elementary
   symmetric polynomials S-k(x) for x is an element of R-n. We show that if
   vertical bar S-k(X)vertical bar, vertical bar Sk+1(X)vertical bar are
   small relative to vertical bar Sk-1(X)vertical bar for some k > 0 then
   vertical bar S-l(x)vertical bar is also small for all l > k.
   We use this to give a simpler and more modular analysis of a
   construction of min-wise independent hash functions and pseudorandom
   generators for combinatorial rectangles due to Gopalan et al., which
   also improves the overall seed-length.}},
Publisher = {{UNIV CHICAGO, DEPT COMPUTER SCIENCE}},
Address = {{1100 E 58TH ST, CHICAGO, IL 60637 USA}},
Type = {{Article}},
