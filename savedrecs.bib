@article{ WOS:000604634500001,
Author = {Carbery, Anthony and Iliopoulou, Marina},
Title = {{Joints formed by lines and a k-plane, and a discrete estimate of Kakeya
   type}},
Journal = {{DISCRETE ANALYSIS}},
Year = {{2020}},
Month = {{DEC 29}},
Abstract = {{Let L be a family of lines and let P be a family of k-planes in F-n
   where F is a field. In our first result we show that the number of
   joints formed by a k-plane in P together with (n - k) lines in L is
   O-n(vertical bar jL parallel to P vertical bar(1/(n k))). This is the
   first sharp result for joints involving higher-dimensional affine
   subspaces, and it holds in the setting of arbitrary fields F. In
   contrast, for our second result, we work in the three-dimensional
   Euclidean space R-3, and we establish the Kakeya-type estimate
   Sigma(x is an element of J)(Sigma(l is an element of L)chi(l)(x))(3/2)
   less than or similar to vertical bar L vertical bar(3/2)
   where J is the set of joints formed by L; such an estimate fails in the
   setting of arbitrary fields. This result strengthens the known estimates
   for joints, including those counting multiplicities. Additionally, our
   techniques yield significant structural information on quasi-extremisers
   for this inequality.}},
