@article{ WOS:A1995TW62900015,
Author = {Montravers, P and Maulin, L},
Title = {{Antimicrobial therapy in peritonitis: Lessons from experimental models}},
Journal = {{MEDECINE ET MALADIES INFECTIEUSES}},
Year = {{1995}},
Volume = {{25}},
Number = {{1}},
Pages = {{121-126}},
Month = {{DEC}},
Note = {{National Congress of BECAR on Peritonitis - Medical and Surgical Focus,
   PARIS, FRANCE, JAN 12, 1996}},
Abstract = {{Many models of experimental peritonitis have been made. The models using
   the implantation of a foreign body combined with pathogens are close to
   the clinical feature. These models demonstrated that early lethality was
   caused by enterobacteriaceae whereas intra-abdominal abscesses were due
   to anaerobes. Optimal results are achieved when treatment is directed
   against both enterobacteriaceae and anaerobes, such as combination of
   clindamycin and aminoglycosides. Many combinations with similar spectrum
   of activity provided similar results. In infections involving
   extended-spectrum beta-lactamase-producing Gram negative bacilli,
   imipenem appears to be the best choice. Although enterococci seem to
