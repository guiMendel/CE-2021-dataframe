@article{ WOS:000367105000029,
Author = {Huenemoerder, David P. and Gayley, K. G. and Hamann, W. -R. and Ignace,
   R. and Nichols, J. S. and Oskinova, L. and Pollock, A. M. T. and Schulz,
   N. S. and Shenar, T.},
Title = {{PROBING WOLF-RAYET WINDS: CHANDRA/HETG X-RAY SPECTRA OF WR 6}},
Journal = {{ASTROPHYSICAL JOURNAL}},
Year = {{2015}},
Volume = {{815}},
Number = {{1}},
Month = {{DEC 10}},
Abstract = {{With a deep Chandra/HETGS exposure of WR 6, we have resolved emission
   lines whose profiles show that the X-rays originate from a uniformly
   expanding spherical wind of high X-ray-continuum optical depth. The
   presence of strong helium-like forbidden lines places the source of
   X-ray emission at tens to hundreds of stellar radii from the
   photosphere. Variability was present in X-rays and simultaneous optical
   photometry, but neither were correlated with the known period of the
   system or with each other. An enhanced abundance of sodium revealed
   nuclear-processed material, a quantity related to the evolutionary state
   of the star. The characterization of the extent and nature of the hot
   plasma in WR 6 will help to pave the way to a more fundamental
   theoretical understanding of the winds and evolution of massive stars.}},
