Title = {{OSIRIS observations of meter-sized exposures of H2O ice at the surface
   of 67P/Churyumov-Gerasimenko and interpretation using laboratory
   experiments}},
Journal = {{ASTRONOMY \& ASTROPHYSICS}},
Year = {{2015}},
Volume = {{583}},
