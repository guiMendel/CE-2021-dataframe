@article{ WOS:000592490800071,
Author = {Chen, Ximiao and Gu, Mingbao and Jin, Jie and Ren, Chenghao and Pan,
   Zongyou and Wu, Yaosen and Tian, Naifeng and Wu, Aimin and Sun, Liaojun
   and Gao, Weiyang and Wang, Xiangyang and Bei, Chaoyong and Zhou, Yifei
   and Zhang, Xiaolei},
Title = {{beta-Hydroxyisovalerylshikonin inhibits IL-1 beta-induced chondrocyte
   inflammation via Nrf2 and retards osteoarthritis in mice}},
Journal = {{FOOD \& FUNCTION}},
Year = {{2020}},
Volume = {{11}},
Number = {{11}},
Pages = {{10219-10230}},
Month = {{NOV 1}},
Abstract = {{Osteoarthritis is a chronic degenerative disease characterized by
   cartilage destruction. It is the fourth most disabling disease worldwide
   and is currently incurable. Inflammation and extracellular matrix (ECM)
   degradation are considered to be substantial reasons for accelerating
   the progression of OA. beta-Hydroxyisoamylshikonin (beta-HIVS) is a
   natural naphthoquinone compound with anti-inflammatory and antioxidant
   activity. However, the effect of beta-HIVS on OA is still unclear. In
   this study, we found that beta-HIVS can down-regulate the expression of
   NO, PEG2, IL-6, TNF-alpha, COX-2, and iNOS, suggesting its
   anti-inflammatory effects in chondrocytes; we also found that beta-HIVS
   may down-regulate the expression of ADAMTS5 and MMP13 and up-regulate
   the expression of aggrecan and collagen II to inhibit the degradation of
   ECM. Mechanistically, beta-HIVS inhibited the NF kappa B pathway by
   activating the Nrf2/HO-1 axis, thereby exerting its anti-inflammatory
   and inhibitory effects on ECM degradation. In vivo experiments also
   proved the therapeutic effects of beta-HIVS on OA in mice, and Nrf2 is
   the target of beta-HIVS. These findings indicate that beta-HIVS may
   become a new drug for the treatment of OA.}},
