@article{ WOS:A1995TC04500004,
Author = {CRENSHAW, EM},
Title = {{DEMOCRACY AND DEMOGRAPHIC INHERITANCE - THE INFLUENCE OF MODERNITY AND
   PROTO-MODERNITY ON POLITICAL AND CIVIL-RIGHTS, 1965 TO 1980}},
Journal = {{AMERICAN SOCIOLOGICAL REVIEW}},
Year = {{1995}},
Volume = {{60}},
Number = {{5}},
Pages = {{702-718}},
Month = {{OCT}},
Abstract = {{Macrosocial theories of democratization point to the pluralism attendant
   on industrialization, class conflicts and alliances, the state and its
   social environment, and transnational linkages as influential in the
   growth of political and civil rights. Unfortunately, these theories
   neglect the ecological and technological histories of many contemporary
   states. Proto-modernity, defined as a rich demographic and institutional
   inheritance rooted in the preindustrial past, is theorized to lay the
   foundations of pluralism and class structure, in essence allowing some
   states to democratize more easily than others. In this longitudinal,
   cross-national analysis using data from a maximum of 83 countries, I
   find robust support for the importance of modernity and proto-modernity
   to the growth of democracy. By contrast, results are inconclusive on
   whether stratification affects democratization.}},
