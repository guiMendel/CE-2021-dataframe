@article{ WOS:000608072700029,
Author = {Guo Minchao and Wang Mingdi and Zhang Shengjiang and Lin Yao and Wang
   Xinyue and Yin Zihang and Wang Xianbao},
Title = {{Techniques for Femtosecond Laser Processing of Micro-Holes in FR-4
   Copper Clad Laminate}},
Journal = {{CHINESE JOURNAL OF LASERS-ZHONGGUO JIGUANG}},
Year = {{2020}},
Volume = {{47}},
Number = {{12}},
Month = {{DEC}},
Abstract = {{As for the high-precision micro-hole formation in FR-1 copper clad
   laminate, the traditional mechanical drilling process is cumbersome and
   the degree of precision is low. The femtosecond laser is used to perform
   the single factor test and the orthogonal test on the copper clad
   laminate. The inlet diameter, outlet diameter and taper arc used as the
   evaluation indexes to discuss the influence of the process parameters on
   the quality of micro-holes. The results show that the single pulse
   energy has the greatest impact on the quality of micro-holes and the
   repetition rate has the least influence. The optimal process parameter
   combination is set as single pulse energy of 29 mu J, repetition rate of
   92 kHz, pulse number of 2112, and defocusing amount of 0.01 mm. After
   these optimal process parameters arc adopted, high-quality and
   high-precision drilling of FR-1 copper clad laminate can be performed to
   realize the rapid and efficient production of printed circuit boards.}},
