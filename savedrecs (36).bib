@article{ WOS:000233543100020,
Author = {Zinser, GM and Suckow, M and Welsh, J},
Title = {{Vitamin D receptor (VDR) ablation alters carcinogen-induced
   tumorigenesis in mammary gland, epidermis and lymphoid tissues}},
Journal = {{JOURNAL OF STEROID BIOCHEMISTRY AND MOLECULAR BIOLOGY}},
