@article{ WOS:000409408800059,
Author = {Lathadevi, H. T. and Guggarigoudar, S. P.},
Title = {{Difficulties in Management of a Sessile Subglottic Polyp}},
Journal = {{JOURNAL OF CLINICAL AND DIAGNOSTIC RESEARCH}},
Year = {{2015}},
Volume = {{9}},
Number = {{12}},
Pages = {{MD01-MD02}},
Month = {{DEC}},
Abstract = {{Benign laryngeal polyps are usually managed with micro- laryngeal
   surgery. Occasionally surgery becomes challenging because of size of the
   polyp or its location. Maintaining the anaesthesia and the airway
   becomes difficult either during immediate management or during excision.
   Upper airway obstruction still remains the major indication for
   tracheostomy in many centers. Nowadays Laryngeal tumour has become the
   main indication of tracheostomy. Conditions like infections, trauma,
   benign lesions and prolonged intubation were leading indications
   previously. Otolaryngologist has to decide a method, often on the spot.
   Here we are presenting such a case where emergency tracheostomy was the
   only choice.}},
