Title = {{SYNTHESIS AND LIQUID CRYSTALLINITY OF POLYMETHACRYLATE SYSTEMS
   CONTAINING BOTH ELECTRON-DONATING (QUINOLINYLMETHYLENE)ANILINE AND
   ELECTRON-ACCEPTING (4'-NITROBENZYLIDENE)ANILINE GROUPS}},
Journal = {{MACROMOLECULES}},
