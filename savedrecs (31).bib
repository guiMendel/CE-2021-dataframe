@article{ WOS:000233722500004,
Author = {Khrennikov, A},
Title = {{Generalizations of quantum mechanics induced by classical statistical
   field theory}},
Journal = {{FOUNDATIONS OF PHYSICS LETTERS}},
Year = {{2005}},
Volume = {{18}},
Number = {{7}},
Pages = {{637-650}},
Month = {{DEC}},
Abstract = {{We show that the Dirac-von Neumann formalism for quantum mechanics can
   be obtained as an approximation of classical statistical field theory.
   This approximation is based on the Taylor expansion (up to terms of the
   second order) of classical physical variables-maps f:Omega -> R, where
   Omega is the infinite-dimensional Hilbert space. The space of classical
   statistical states consists of Gaussian measures rho on Omega having
   zero mean value and dispersion sigma(2)(rho)approximate to h. This
   viewpoint to the conventional quantum formalism gives the possibility to
   create generalized quantum formalisms based on expansions of classical
   physical variables in the Taylor series up to terms of nth order and
   considering statistical states. having dispersion sigma(2)(rho)=h(n)
   (for n=2 we obtain the conventional quantum formalism).}},
