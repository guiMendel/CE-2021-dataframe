@article{ WOS:000232562200002,
Author = {Ricken, AXC and Bennett, SJ and Savelsbergh, GJP},
Title = {{Coordination of reaching in children with spastic hemiparetic cerebral
   palsy under different task demands}},
Journal = {{MOTOR CONTROL}},
Year = {{2005}},
Volume = {{9}},
Number = {{4}},
Pages = {{357-371}},
Month = {{OCT}},
Note = {{International Workshop on Advances in Motor Research in Hemiparetic
   Cerebral Palsy, Univ Leeds, Sch Sport \& Exercise Sci, Leeds, ENGLAND,
   OCT 14-15, 2004}},
Organization = {{UK-Netherlands Partnership Programme Sci}},
Abstract = {{Coordination of reaching with the impaired and non-impaired arm in 10
   children with spastic hemiparetic cerebral palsy (SHCP) was examined in
   a stationary ball and moving ball context. Kinematic data on trunk, arm,
   and wrist movements, and coordination patterns between joint angles of
   elbow, shoulder, and trunk, were analyzed to determine how reaching was
   influenced by impairment and object motion. Results showed longer
   deceleration time and movement time and greater trunk contribution
   following decreased elbow and shoulder excursion when reaching with the
   impaired arm compared to the non-impaired arm. The coordination of joint
   angle pairs showed little linearity for the impaired arm, indicating
   more segmented movements of shoulder and elbow. It was also found that
   coordination patterns between elbow, shoulder, and trunk displayed less
   similarity when reaching with the impaired arm compared to the
   non-impaired arm in both stationary and moving ball conditions.
   Regardless of the timing constraints, children with SHCP could make
   successful interceptions using the impaired arm, indicating that they
   coordinated and controlled the degrees of freedom within their own
   functional possibilities.}},
