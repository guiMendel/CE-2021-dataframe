@article{ WOS:000590573600008,
Author = {Tkachev, Valeriy V. and Utenyshev, Andrey N. and Kovalchukova, Olga V.},
Title = {{Structural and theoretical study of
   (4E,5Z)-4,5-dibenzylidene-1,2,3,6,7,8-hexahydroacridine}},
Journal = {{CHEMISTRYSELECT}},
Year = {{2020}},
Volume = {{5}},
Number = {{43}},
Pages = {{13487-13491}},
Month = {{NOV 20}},
Abstract = {{The crystal and molecular structure of
   (4E,5Z)-4,5-dibenzylidene-1,2,3,6,7,8-hexahydroacridine as a
   biologically active substance with potential antioxidant,
   anti-inflammatory and anti-microbial activity is reported. The title
   compound is found to be non-planar. The rings are twisted around the
   central fragment of the molecule by 54.0 and 32.5 degrees. The dihedral
   angle between the two phenyl rings is 23 degrees. In crystal, the
   molecules are packed in head-to-head stacks along the b-axis with no
   intermolecular H-bonds. DFT B3LYP theoretical modeling with the LANL2DZ
