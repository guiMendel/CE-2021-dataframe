@article{ WOS:000366151400021,
Author = {Esselman, Brian J. and Emmert, III, Frank L. and Wiederhold, Andrew J.
   and Thompson, Stephanie J. and Slipchenko, Lyudmila V. and McMahon,
   Robert J.},
Title = {{Thermal Isomerizations of Diethynyl Cyclobutadienes and Implications for
   Fullerene Formation}},
Journal = {{JOURNAL OF ORGANIC CHEMISTRY}},
Year = {{2015}},
Volume = {{80}},
Number = {{23}},
Pages = {{11863-11868}},
Month = {{DEC 4}},
Abstract = {{The mechanism by which carbon condenses to form PAHs or fullerenes is a
   problem that has garnered considerable theoretical and experimental
   attention. The ring-coalescence and annealing model for the formation of
   C-60 involves a {[}2 + 2] cycloaddition reaction of a cydopolyyne to
   form a tetraalkynyl cydobuta-1,3-diene intermediate, followed by a
   Bergman cydoaromatization reaction of the enediyne moiety.
   Intra-molecular trapping of the incipient p-benzyne diradical across a
   diyne moiety of the macrocyclic ring affords an aromatic ring that must
   undergo further intramolecular reactions via polyradical intermediates
   to produce a condensed graphitic structure or fullerene. Computational
   studies of a model system for the intriguing
   tetraalkynylcyclobuta-1,3-diene intermediate, however, reveal that the
   corresponding p-benzyne diradical lies in a shallow minimum with a very
   low barrier to ring opening to cydooctadienediyne. This pathway has not
   been previously considered in the mechanism for carbon condensation.}},
