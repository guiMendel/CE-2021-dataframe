@article{ WOS:000560366800005,
Author = {Mondal, Manisha and Rai, Vineet Kumar},
Title = {{Multiple ratiometric thermometry: Enhanced sensing behaviour via Stark
   sublevels}},
Journal = {{JOURNAL OF ALLOYS AND COMPOUNDS}},
Year = {{2020}},
Volume = {{844}},
Month = {{DEC 5}},
Abstract = {{The monoclinic phase Er3+/Er3+-Yb3+:MgMoO4 phosphors prepared via
   solid-state reaction method reveal Stark sublevels of green emitting
   levels of Er3+ under 980 nm diode laser excitation. The developed
   phosphors have been characterized through X-ray diffraction (XRD), field
   emission scanning electron microscopy (FESEM) and diffuse reflectance
   studies. The dipole-dipole interaction was found responsible for the
   concentration quenching. The mechanism behind the upconversion (UC)
   emission study, energy level diagram of Er3+-Yb3+ system have been
   explored in detail. Moreover, the temperature dependent behaviour have
   been studied based on single and multiple ratiometric thermometry on the
   basis of four schemes of fluorescence intensity ratio (FIR) that consist
   of two component peaks of H-2(11/2) -> I-4(15/2) transition (similar to
   524 \& similar to 530 nm) and two component peaks of S-4(3/2) ->
   I-4(15/2) transition (similar to 544 \& similar to 552 nm). All the four
   thermally coupled levels exhibit strong green emission intensity as well
   as efficient sensing performance with better sensitivity and temperature
   range. CIE colour coordinates confirms the pure green colour perception
   of the developed phosphors. (c) 2020 Elsevier B.V. All rights reserved.}},
