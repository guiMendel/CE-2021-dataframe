@article{ WOS:000600251000001,
Author = {Kim, Younghun and Kang, Philjae and Jeon, Yonggoon and Cho, Hyeon Mo and
   Choi, Moon-Gun},
Title = {{Anion-Templated Self-Assembly of Silver(I) Frameworks Bridged by mu-,
   mu(3)-, mu(4)-1,2,4,5-Tetrazine}},
Journal = {{BULLETIN OF THE KOREAN CHEMICAL SOCIETY}},
