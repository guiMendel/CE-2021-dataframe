@article{ WOS:000088782100005,
Author = {Burns, T and Perkins, R},
Title = {{The future of case management}},
Journal = {{INTERNATIONAL REVIEW OF PSYCHIATRY}},
Year = {{2000}},
Volume = {{12}},
Number = {{3}},
Pages = {{212-218}},
Month = {{AUG}},
Abstract = {{Case management was originally a specific, tightly defined, role
   developed for the care of de-institutionalised psychiatric patients in
   the US. The term rapidly caught on and, as a consequence of its success,
   is now widely used throughout mental health care (and beyond).
   Predictably it is now sur rounded by ambiguity and controversy. This
   paper explores its history-from the age of innocence, through that of
   controversy, to consolidation. It attempts to identify what, if
   anything, is unique to the approach by examining the reported features
   of its most developed form (Assertive Community Treatment). It goes on
   to speculate on whether such a `generic' role will survive the
   increasing range of evidence based mental health interventions that are
   in use.}},
