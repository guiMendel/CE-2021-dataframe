@article{ WOS:A1995TH12700004,
Author = {SYCHEVA, SA},
Title = {{EVOLUTION OF LATE PLEISTOCENE CATENAS OF THE CENTRAL RUSSIAN UPLAND IN A
   FULL GLACIATION INTERGLACIAL CLIMATE CYCLE}},
Journal = {{EURASIAN SOIL SCIENCE}},
Year = {{1995}},
Volume = {{27}},
Number = {{8}},
Pages = {{34-51}},
Month = {{AUG}},
Abstract = {{Two cross sections of the same buried linear erosion form are studied in
   the upper course of the Mikulinskiy ravine, revealed in a quarry wall of
   the Mikhaylovskiy Mining and Dressing Combine. Polygenetic catenas are
   developed on the slopes and in the bottom of the ravine. In the history
   of existence of these catenas, four soil-forming phases have been
   identified, along with the cryomorpholithogenic phases which appear and
   are preserved differently in the studied catenas. Based on study of Late
   Pleistocene catenas, the direction has been established not only for
   soil-forming processes, but also processes of cryo- and
   morpholithogenesis in the glaciation-interglacial cycle. The concept of
   `'complete evolution of catenas and geosystems'' is introduced: the
   process of differentiation of space from homogeneous, represented by a
   single elementary soil area (ESA) or a single elementary geosystem, to
   the most complex, differentiated space, consisting of several ESA, and
   integration of space again to homogeneous. The obtained results can be
   used to solve problems of the evolution of modern soils and predict the
   development of Holocene catenas and geosystems in the future.}},
