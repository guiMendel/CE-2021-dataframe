@article{ WOS:000284419900038,
Author = {Boche, Holger and Moenich, Ullrich J.},
Title = {{Approximation of Wide-Sense Stationary Stochastic Processes by Shannon
   Sampling Series}},
Journal = {{IEEE TRANSACTIONS ON INFORMATION THEORY}},
Year = {{2010}},
Volume = {{56}},
Number = {{12}},
Pages = {{6459-6469}},
Month = {{DEC}},
Abstract = {{In this paper, the convergence behavior of the symmetric and the
   nonsymmetric Shannon sampling series is analyzed for bandlimited
   continuous-time wide-sense stationary stochastic processes that have
   absolutely continuous spectral measure. It is shown that the
   nonsymmetric sampling series converges in the mean-square sense
   uniformly on compact subsets of the real axis if and only if the power
   spectral density of the process fulfills a certain integrability
   condition. Moreover, if this condition is not fulfilled, then the
   pointwise mean-square approximation error of the nonsymmetric sampling
   series and the supremum of the mean-square approximation error over the
   real axis of the symmetric sampling series both diverge. This shows that
   there is a significant difference between the convergence behavior of
   the symmetric and the nonsymmetric sampling series.}},
