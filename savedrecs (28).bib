@article{ WOS:000282579300004,
Author = {Vences, Miguel and Koehler, Joern and Pabijan, Maciej and Glaw, Frank},
Title = {{Two syntopic and microendemic new frogs of the genus Blommersia from the
   east coast of Madagascar}},
Journal = {{AFRICAN JOURNAL OF HERPETOLOGY}},
Year = {{2010}},
Volume = {{59}},
Number = {{2}},
Pages = {{133-156}},
Month = {{OCT}},
Abstract = {{We describe two new species of semiarboreal frogs from the northern
   central east coast of Madagascar which occur syntopically, at least on
   the island of Nosy Boraha. The two species are morphologically closest
   to Blommersia wittei, but differ in advertisement calls and molecular
   phylogenetic relationships. One of the new species has a remarkable
   femoral gland structure, as these are shifted towards the distal part of
   the thigh, close to the knee joint. The new species are apparently
   microendemic to a small stretch of Madagascar's east coast and have so
   far not been found at higher elevations. Since both new species are
   tolerant of habitat disturbance and occur in anthropogenically altered
   habitats, such as plantations and villages, we propose an IUCN threat
   status of Least Concern despite their restricted extent of occurrence.}},
Publisher = {{TAYLOR \& FRANCIS LTD}},
Address = {{2-4 PARK SQUARE, MILTON PARK, ABINGDON OR14 4RN, OXON, ENGLAND}},
