@article{ WOS:A1995RX26700014,
Author = {TAKAHARA, J and NOMURA, A and GAMO, K and TAKAOKA, S and MURASE, K and
   AHMED, H},
Title = {{MAGNETOTRANSPORT IN HEXAGONAL AND RECTANGULAR ANTIDOT LATTICES}},
Journal = {{JAPANESE JOURNAL OF APPLIED PHYSICS PART 1-REGULAR PAPERS SHORT NOTES \&
   REVIEW PAPERS}},
Year = {{1995}},
Volume = {{34}},
Number = {{8B}},
Pages = {{4325-4328}},
Month = {{AUG}},
Note = {{International Workshop on Mesoscopic Physics and Electronics, UNIV
   TOKYO, ROPPONGI CAMPUS, TOKYO, JAPAN, MAR 04-06, 1995}},
Organization = {{Univ Tokyo, Inst Ind Sci; Sci Res Project Prior Areas QCEPT}},
Abstract = {{In order to investigate anisotropic transport in various types of
   lattices, two-dimensional electron gas in the hexagonal and rectangular
   antidot lattices are measured. Magnetoresistance is isotropic in
   hexagonal lattice, but anisotropic in the rectangular lattice with
   circular antidots. The influence of antidot shapes upon anisotropic
   transport is also investigated in the rectangular lattice. It is found
   that the anisotropic transport is sensitive to the arrangement and the
   shape of antidot potential and anomalous peaks are observed in
   rectangular lattice with ellipsoidal antidots. The origin of the
   anomalous peaks is discussed. In both lattices quenching of the flail
   effect is observed and the positions of commensurate peaks in Hall
   resistance shift to higher field than that in magnetoresistance. These
   effects cannot be explained by the pinned orbit model.}},
Publisher = {{JAPAN J APPLIED PHYSICS}},
Address = {{DAINI TOYOKAIJI BLDG 24-8 SHINBASHI 4-CHOME, MINATO-KU TOKYO 105, JAPAN}},
