Author = {Decandio, Carla C. and Vassiliades, Sandra V. and Gerbelli, Barbara B.
   and Aguilar, Andrea M. and Alves, Wendel A.},
Title = {{Hybrid Hydrogels Based on Polyethylene Glycol Bioconjugated with
   Silylated-Amyloidogenic Peptides}},
Journal = {{JOURNAL OF THE BRAZILIAN CHEMICAL SOCIETY}},
