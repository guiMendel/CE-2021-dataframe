@article{ WOS:000243596000009,
Author = {Jeong, Sun-Young and Lim, Jung-Sun and Hwang, Ji-Yoon and Park, Han-Jin
   and Cho, Jae-Woo and Song, Chang-Woo and Kim, Yang-Seok and Lee,
   Wan-Seon and Moon, Jin-Hee and Han, Sang-Seop and Yoon, Seokjoo},
Title = {{Toxicogenomics study on carbon tetrachloride-induced hepatotoxicity in
   mice}},
Journal = {{MOLECULAR \& CELLULAR TOXICOLOGY}},
Year = {{2005}},
Volume = {{1}},
Number = {{4}},
Pages = {{275-280}},
Month = {{DEC 30}},
Abstract = {{Carbon tetrachloride (CCl4) is well known hepatotoxicant. Its overdose
   cause severe centrilobular hepatic necrosis in human and experimental
   animals. We administered CCl4 at low (0.2 mL/kg p.o.) and high (2 mL/kg
   p.o.) doses to mice. Mice were sacrificed at 24 h after administration.
   We evaluated liver toxicity by serum AST and ALT level and by
   microscopic observation. Using cDNA chip, we conducted gene expression
   analysis in liver. Mean serum activities of the hepatocellular leakage
   enzymes, ALT and AST, were significantly increased compare to control,
   respectively, in the low and high dose groups. H\&E evaluation of
   stained liver sections revealed CCl4-related histopathological findings
   in mice. Moderate centrilobular hepatocellular necrosis was present in
   all CCl4 treated mice. We found that gene expression pattern was very
   similar between low and high dose group. However, some stress related
   genes were differently expressed. These results could be a molecular
   signature for the degree of liver injury. Our data suggest that the
   degree of severity could be figure out by gene expression profiling.}},
