@article{ WOS:000367120100011,
Author = {Katano, Hajime and Kasahara, Yoshihiro and Ushimaru, Kazunori and
   Maruyama, Chitose and Hamano, Yoshimitsu},
Title = {{Separation and Purification of epsilon-Poly-L-lysine with Its
   Colorimetric Determination Using Dipicrylamine}},
Journal = {{ANALYTICAL SCIENCES}},
Year = {{2015}},
Volume = {{31}},
Number = {{12}},
Pages = {{1273-1277}},
Month = {{DEC}},
Abstract = {{A separation method for a biopolymer, epsilon-poly-L-lysine (epsilon
   PL), with its colorimetric determination using a yellow anionic dye,
   dipicrylamine anion (DPA(-)), is presented. The epsilon PL-producing
   culture broth was mixed with a NaDPA solution to precipitate the epsilon
   PL in polycationic form with the DPA(-) anion. The precipitate was
   dissolved into acetonitrile (AN). The AN solution was yellowish, and
   gave an absorption maximum at around 420 nm. Thus, epsilon PL in the
   culture broth can be assayed colorimetrically a with multiwell
   microtiter plate. By the addition of
   bis(triphenylphosphoranylidene)ammonium chloride, the polycationic
   epsilon PL was precipitated from the AN solution as the hydrochloride
   salt. On the other hand, the DPA(-) anion remained in the AN solution as
   the quaternary ammonium salt. Thus, epsilon PL larger than tetramer can
   be separated and purified after the high-throughput screening of the
   synthetic enzyme.}},
