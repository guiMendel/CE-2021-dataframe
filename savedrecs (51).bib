@article{ WOS:A1995TN96700007,
Author = {Borg, A and Hunderi, O and Richter, W and Rumberg, J and Venvik, HJ},
Title = {{The optical anisotropy of Pt(100) studied by reflection anisotropy
   spectroscopy}},
Journal = {{PHYSICA STATUS SOLIDI A-APPLIED RESEARCH}},
Year = {{1995}},
Volume = {{152}},
Number = {{1}},
Pages = {{77-84}},
Month = {{NOV 16}},
Note = {{US/European Workshop on Optical Characterization of Electronic Materils,
   HALLE, GERMANY, OCT 05-07, 1994}},
Abstract = {{The optical anisotropy of the Pt(100) hex-R0.7 degrees reconstructed
   phase is studied with reflection anisotropy spectroscopy. The transition
   from the hex-R0.7 degrees to the Pt(1 x 1) structure is also studied.
   The spectrum of Pt(100) hex-R0.7 degrees is found to be dominated by
   surface polariton-like absorption due to the wavy nature of the first
   layer superstructure, but an additional absorption is also observed in
   the reconstructed layer. After exposure to CO the surface transforms to
   a Pt(1 x 1) surface covered with Pt islands. The growth of the (1 x 1)
   phase is highly anisotropic, i.e. the growth being much faster along the
   {[}<0(1) over bar 1>] direction than along {[}011]. The islands are
   elongated in the fast growth direction and an optical anisotropy from
   the island covered surface is observed which is comparable to that of
   the Pt(100) hex-R0.7 degrees surface.}},
