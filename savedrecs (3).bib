@article{ WOS:000600021000052,
Author = {Hooda, Ekta and Verma, Urmil and Hooda, B. K.},
Title = {{STATE SPACE MODELS FOR SUGARCANE YIELD IN EASTERN DISTRICTS OF HARYANA
   USING WEATHER PARAMETERS AS EXOGENOUS INPUT}},
Journal = {{INTERNATIONAL JOURNAL OF AGRICULTURAL AND STATISTICAL SCIENCES}},
Year = {{2020}},
Volume = {{16}},
Number = {{2}},
Pages = {{911-919}},
Month = {{DEC}},
Abstract = {{Parameter constancy is a fundamental issue for empirical models to be
   useful for forecasting, analyzing, or testing any theory. Unlike
   classical regression analysis, the state space models are time-varying
   parameter models as they allow for known changes in the structure of the
   system over time and provide a flexible class of dynamic and structural
   time series models. The paper deals with the development of state space
   models with the weather as exogenous input for sugarcane yield
   prediction in Kurukshetra, Panipat, and Yamunanagar districts of
   Haryana. The state space models with weather as exogenous input using
   different types of growth trends viz., polynomial splines of order-1, 2,
   and 3 have been developed. The growth trend represented by polynomial
   splines of order-2 with the weather as exogenous input was selected as
   the best-suited model for sugarcane yield prediction in all the three
   districts under study.}},
