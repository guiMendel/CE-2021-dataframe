@article{ WOS:000231987200008,
Author = {Fu, XC and Chen, CX and Wang, GP and Liang, WQ and Yu, QS},
Title = {{Prediction of human intestinal absorption using an artificial neural
   network}},
Journal = {{PHARMAZIE}},
Year = {{2005}},
Volume = {{60}},
Number = {{9}},
Pages = {{674-676}},
Month = {{SEP}},
Abstract = {{An artificial neural network model is developed to predict percent human
   intestinal absorption (\%FA) of compounds from their molecular
   structural parameters. These parameters are the polar molecular surface
   area (PSA), the fraction of polar molecular surface area (FPSA, polar
   molecular surface area/ molecular surface area), the sum of the net
   atomic charges of oxygen atoms (Q(O)), the sum of the net atomic charges
   of nitrogen atoms with net negative atomic charges (Q(N)), the sum of
   the net atomic charges of hydrogen atoms attached to oxygen or nitrogen
   atoms (Q(H)), and the number of carboxyls (n(COOH)). For a training set
   of 85 compounds and a test set of 10 compounds, root mean squared errors
   (RMSE) between experimental \%FA values and calculated/predicted \%FA
   values are 8.86\% and 14.1\%, respectively.}},
