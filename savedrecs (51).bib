@article{ WOS:A1995TP40900003,
Author = {Vigneswaran, R and OLoughlin, JA and McDonald, HM},
Title = {{Prevention of early onset group B streptococcal sepsis in the newborn}},
Journal = {{JOURNAL OF PAEDIATRICS AND CHILD HEALTH}},
Year = {{1995}},
Volume = {{31}},
Number = {{6}},
Pages = {{493-494}},
Month = {{DEC}},
Abstract = {{There is an urgent need for strategies to prevent early onset group B
   streptococcal sepsis in the newborn. The most effective mechanism is the
   identification of maternal carriers of the organism and interruption of
   transmission during labour. Vaginal culture is currently the most
   reliable method for the identification of carriers. Antibiotic
   prophylaxis for known carriers in labour has been demonstrated to be
   effective as standard management practice in a number of Australian
   institutions and is the best available strategy at this stage.}},
