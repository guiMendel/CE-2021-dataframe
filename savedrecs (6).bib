@article{ WOS:000593404600001,
Author = {Baimyrzaeva, Mahabat and Meyer, Carolyn Taylor},
Title = {{Megatrends and the future of work for public sector leaders and
   educators}},
Journal = {{INTERNATIONAL JOURNAL OF PUBLIC LEADERSHIP}},
Year = {{2021}},
