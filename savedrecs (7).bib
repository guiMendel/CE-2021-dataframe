@article{ WOS:000571224200008,
Author = {Ren, Zhaokun and Liu, Liwu and Liu, Yanju and Leng, Jinsong},
Title = {{Electric power repair tools made of shape memory composites: Analysis
   and investigation}},
Journal = {{COMPOSITE STRUCTURES}},
Year = {{2020}},
Volume = {{252}},
Month = {{NOV 15}},
Abstract = {{Shape memory polymers (SMPs) and shape memory polymer composites (SMPCs)
   are widely applied to devices in many fields due to their
   characteristics of large deformation and shape recovery. Epoxy resin
   (EP), which has excellent dielectric properties, is extensively used in
   the field of power and electronic devices. In this paper, the mechanical
   and dielectric properties of EP-SMPs and EP composites toughened by
   carbon fiber (CF)/glass fiber (GF) fabric were investigated based on the
   large deformation of SMPCs and the intrinsic electrical properties.
   SMPCs and EP-SMPs can withstand a high voltage to some extent. The
   addition of CF and GF fabric improved the mechanical properties of the
   composites. Hence, the hinged SMPC tools that could repair the damaged
   cable were prepared, which would promote the application and development
   of SMPs in the field of electric power.}},
