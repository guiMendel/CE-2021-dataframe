@article{ WOS:A1995RT33400035,
Author = {MORRIS, TR},
Title = {{NONCOMPACT PURE GAUGE QED IN 3D IS FREE}},
Journal = {{PHYSICS LETTERS B}},
Year = {{1995}},
Volume = {{357}},
Number = {{1-2}},
Pages = {{225-231}},
Month = {{AUG 31}},
Abstract = {{For all Poincare invariant Lagrangians of the form L = f(F-mu nu), in
   three Euclidean dimensions, where f is any invariant function of a
   non-compact U(1) field strength F-mu nu, we find that the only continuum
   limit (described by just such a gauge field) is that of free field
   theory: First we approximate a gauge invariant version of Wilson's
   renormalization group by neglecting all higher derivative terms similar
   to partial derivative(n) F in L, but allowing for a general
   non-vanishing anomalous dimension. Then we prove analytically that the
   resulting flow equation has only one acceptable fixed point: the
   Gaussian fixed point. The possible relevance to high-T-c
   superconductivity is briefly discussed.}},
