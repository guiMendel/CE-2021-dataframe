@article{ WOS:000369553900073,
Author = {Bashir, Willayat and Borkar, V. T. and Dangat, V. T.},
Title = {{A Quantitative Assessment of the Nucleophilicity of Cytosine in Aqueous
   Solution by Polarography Complemented by Hydrodynamic Voltammetry}},
Journal = {{ORIENTAL JOURNAL OF CHEMISTRY}},
Year = {{2015}},
Volume = {{31}},
Number = {{4}},
Pages = {{2421-2425}},
Month = {{DEC}},
Abstract = {{The nucleophilicity of the nucleobase cytosine has been investigated by
   polarographic studies complemented by fast reaction kinetic data.
   Increasing half-wave potentials with pH are observed for the
   bio-molecule using the dropping mercury electrode (DME). The bromination
   of the nucleobase in aqueous solution is found to be a rapid reaction
   necessitating a special technique to follow the kinetics. Herein we have
   adopted the hydrodynamic voltammetry technique. The reaction is found to
   be of the second order, the specific reaction rate of which increases
   with pH. The complementary data in the two-pronged study reveals the
   reactivity of the nucleobase in this aromatic electrophilic substitution
   reaction in aqueous medium on a quantitative scaffold.}},
