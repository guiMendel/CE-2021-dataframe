Title = {{RESEARCH AND DEVELOPMENT OF gamma `' AND gamma ` STRENGTHENED Ni-Fe BASE
   SUPERALLOY GH4169}},
Journal = {{ACTA METALLURGICA SINICA}},
Year = {{2010}},
Volume = {{46}},
