@article{ WOS:A1995TG21800007,
Author = {NI, S and WANG, XG},
Title = {{BALANCED GOVERNMENT BUDGETS VERSUS DEFICIT FINANCE IN A GROWTH ECONOMY}},
Journal = {{CANADIAN JOURNAL OF ECONOMICS-REVUE CANADIENNE D ECONOMIQUE}},
Year = {{1995}},
Volume = {{28}},
Number = {{4B}},
Pages = {{1120-1134}},
Month = {{NOV}},
Abstract = {{This paper analyses the effects of balanced-budgets and deficit-finance
   policies on the resulting steady-state capital stock in a simple
   neoclassical growth model. It is shown that the steady-state capital
   stock may be higher or lower if the government is required to balance
   its budget in each period than if it uses deficit finance, depending on
   the level of the initial capital stock and the curvature of the utility
