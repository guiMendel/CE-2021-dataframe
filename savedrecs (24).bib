@article{ WOS:000280344400022,
Author = {Li, Xiangyong and Tian, Peng and Aneja, Y. P.},
Title = {{An adaptive memory programming metaheuristic for the heterogeneous fixed
   fleet vehicle routing problem}},
Journal = {{TRANSPORTATION RESEARCH PART E-LOGISTICS AND TRANSPORTATION REVIEW}},
Year = {{2010}},
Volume = {{46}},
Number = {{6}},
Pages = {{1111-1127}},
Month = {{NOV}},
Abstract = {{This paper studies the heterogeneous fixed fleet vehicle routing problem
   (HFFVRP), in which the fleet is composed of a fixed number of vehicles
   with different capacities, fixed costs, and variable costs. Given the
   fleet composition, the HFFVRP is to determine a vehicle scheduling
   strategy with the objective of minimizing the total transportation cost.
   We propose a multistart adaptive memory programming (MAMP) and path
   relinking algorithm to solve this problem. Through the search memory.
   MAMP at each iteration constructs multiple provisional solutions, which
   are further improved by a modified tabu search. As an intensification
   strategy, path relinking is integrated to enhance the performance of
   MAMP. We conduct a series of experiments to evaluate and demonstrate the
   effectiveness of the proposed algorithm. (C) 2010 Elsevier Ltd. All
   rights reserved.}},
