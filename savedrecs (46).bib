@article{ WOS:000089412200002,
Author = {Hwang, CY and Yang, SF},
Title = {{Robust stability and performance analysis of feedback control for plants
   with uncertain parameters in an ellipsoid}},
Journal = {{JOURNAL OF THE CHINESE INSTITUTE OF ENGINEERS}},
Year = {{2000}},
Volume = {{23}},
Number = {{5}},
Pages = {{537-548}},
Month = {{SEP}},
Abstract = {{In this paper, we consider the problems of robust stability analysis and
   H(infinity)-norm computation for a feedback control system in which the
   plant is subjected to parametric variations in an m-dimensional
   ellipsoid (m-ellipsoid) E. It is shown that the problems can be solved
   in a unified framework of constructing the image or value set f(B) of an
   m-box B under a differentiable complex-valued mapping f. The value set
   f(B) is constructed through finding the set of the generalized
   principal-point (GPP) manifolds G of dimension one whose image f(G)
   converts the value-set boundary partial derivative f(B). The set G
   consists of connecting GPP manifolds, which have a connection with the
   edges of B, and isolated GPP manifolds which are closed curves on the
   open faces of B. The set of connecting GPP manifolds G(c) are traced out
   by a spherical algorithm such that the computed outer border of f(G(c))
   is epsilon-connected, i.e., the distance of the nearest neighbour is
   less than epsilon. This allows the isolated GPP manifolds whose images
   may have a substantial contribution to the value-set boundary partial
   derivative f(B) to be almost detected and then found.}},
