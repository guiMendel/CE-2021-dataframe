@article{ WOS:000208482600038,
Author = {Charitos, Efstratios I. and Nienaber, Christoph A. and Sievers,
   Hans-Hinrich},
Title = {{A diagnostic odyssey and successful repair of a progressive aneurysmal
   expansion after complex aortic surgery and stent grafting of the
   descending aorta}},
Journal = {{INTERACTIVE CARDIOVASCULAR AND THORACIC SURGERY}},
Year = {{2010}},
Volume = {{11}},
Number = {{3}},
Pages = {{351-353}},
Month = {{SEP}},
Abstract = {{A 48-year-old patient with Marfan syndrome presented with an ascending
   aortic and arch periprosthesis hematoma (14.0x9.0 cm) and a descending
   aortic aneurysm (8.7x9.1 cm) developed over the last 2.5 years, after
   several operations on the ascending aorta and arch as well as
   stent-graft placement in the descending aorta. After a diagnostic
   odyssey, the small true aortic lumen was identified as the source for
   the ascending aortic periprosthesis hematoma and descending aortic
   aneurysm expansion. The present case report underlines several important
