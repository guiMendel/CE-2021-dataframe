@article{ WOS:000588827500001,
Author = {Yamamoto, Akito and Shirai, Toshihiro and Hirai, Keita and Tanaka, Yuko
   and Watanabe, Hirofumi and Endo, Yoshinari and Shimoda, Yukiko and
   Suzuki, Takahito and Noguchi, Rie and Mochizuki, Eisuke and Sakurai,
   Shogo and Saigusa, Mika and Akamatsu, Taisuke and Shishido, Yuichiro and
   Akita, Takefumi and Morita, Satoru and Asada, Kazuhiro},
Title = {{Oscillometry as a Predictor of Exercise Tolerance in COPD}},
Journal = {{COPD-JOURNAL OF CHRONIC OBSTRUCTIVE PULMONARY DISEASE}},
Year = {{2020}},
Volume = {{17}},
Number = {{6}},
Pages = {{647-654}},
Month = {{NOV 1}},
Abstract = {{The usefulness of the oscillometry, known as forced oscillation
   technique, for predicting exercise tolerance in subjects with COPD is
   unknown. To test the hypothesis, we investigated whether oscillometry
   could predict a 6-minute walking distance (6MWD) <350 m in the 6-minute
   walk test (6MWT).
   This was a prospective, observational study. Fifty-seven subjects with
   COPD who attended outpatient clinics for routine checkups at Shizuoka
   General Hospital between April 2015 and April 2016 (54 males; median
   age, 70 years; and \%FEV1, 61.0\%). Modified MRC dyspnea scale (mMRC),
   COPD Assessment Test (CAT), oscillometry, spirometry, and 6MWT were
   performed in a stable condition. The participants were classified into
   subjects with 6MWD >= 350 m or 6MWD <350 m, and the predictor of 6MWD
   <350 m was assessed.
   Of the 57 total subjects, 43 (75.4\%) had a 6MWD >= 350 m, and 14
   (24.6\%) had a 6MWD <350 m. Between the two groups, there were
   significant differences in mMRC scale, GOLD stages, CAT scores, FEV1,
   IC, 6MWD, lowest SpO(2), maximum Borg scale, respiratory resistance
   (Rrs), and reactance (Xrs). In multivariate regression analysis, a 6MWD
   <350 m was independently predicted by CAT scores (OR 1.15, 95\% CI:
   1.01-1.30) and inspiratory R5 (OR 6.01, 95\% CI: 1.09-33.30). In
   receiver operating characteristic curves, the area under the curve was
   0.76, 0.78, and 0.85 for CAT scores, R5, and CAT scores + R5,
   respectively, with the best cutoff value of 17 and 2.82 cmH(2)0/L/s. In
   conclusion, the oscillatory parameter, inspiratory R5, predicted low
   exercise tolerance in COPD subjects.}},
