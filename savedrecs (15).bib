@article{ WOS:000365066500012,
Author = {Abraham, Michael K. and Aquino, Patrick R. and Kuo, Dick C.},
Title = {{Special Considerations in Trauma Patients}},
Journal = {{EMERGENCY MEDICINE CLINICS OF NORTH AMERICA}},
Year = {{2015}},
Volume = {{33}},
Number = {{4}},
Pages = {{853+}},
Month = {{NOV}},
Abstract = {{The emergent management of a traumatic injury can be an extremely
   intense situation. These assessments can be even more difficult when
   patients have an underlying psychiatric condition. After a protocoled
   evaluation of the traumatic injuries, the psychological manifestation of
   diseases can be addressed. The appropriate use of physical or chemical
   restraints to facilitate the work-up is paramount in the ability of the
   provider to protect patients and staff from agitated and traumatized
   patients. The emergency medicine provider should have a low threshold
   for including psychiatry in the treatment plans, as the long-term
   sequelae of these entities require specialized treatment.}},
