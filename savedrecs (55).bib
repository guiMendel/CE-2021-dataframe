@article{ WOS:A1995RV16000015,
Author = {JARABO, S and REBOLLEDO, MA},
Title = {{ANALYTIC MODELING OF ERBIUM-DOPED FIBER AMPLIFIERS ON THE BASIS OF
   INTENSITY-DEPENDENT OVERLAPPING FACTORS}},
Journal = {{APPLIED OPTICS}},
Year = {{1995}},
Volume = {{34}},
Number = {{27}},
Pages = {{6158-6163}},
Month = {{SEP 20}},
Abstract = {{Rate equations based on intensity-dependent overlapping factors are
   integrated to obtain analytic solutions for pump, signal, and amplified
   spontaneous emission (ASE), even when the coupled signal varies with
   time. The equations can be applied without the imposition of any
   restraints on the values of the pump, signal, and ASE powers, the
   excited-state-absorption cross section, the erbium-density distribution,
   or other parameters that characterize the fiber. The methods used to
   calculate pump, signal, and ASE powers are discussed. Experimental
   techniques to characterize the doped fiber that were based on these
   analytic expressions are introduced.}},
Publisher = {{OPTICAL SOC AMER}},
Address = {{2010 MASSACHUSETTS AVE NW, WASHINGTON, DC 20036}},
