@article{ WOS:000283630800008,
Author = {Morsanyi, Kinga and Handley, Simon J. and Evans, Jonathan S. B. T.},
Title = {{Decontextualised Minds: Adolescents with Autism are Less Susceptible to
   the Conjunction Fallacy than Typically Developing Adolescents}},
Journal = {{JOURNAL OF AUTISM AND DEVELOPMENTAL DISORDERS}},
Year = {{2010}},
Volume = {{40}},
Number = {{11}},
Pages = {{1378-1388}},
Month = {{NOV}},
Abstract = {{The conjunction fallacy has been cited as a classic example of the
   automatic contextualisation of problems. In two experiments we compared
   the performance of autistic and typically developing adolescents on a
   set of conjunction fallacy tasks. Participants with autism were less
   susceptible to the conjunction fallacy. Experiment 2 also demonstrated
   that the difference between the groups did not result from increased
   sensitivity to the conjunction rule, or from impaired processing of
   social materials amongst the autistic participants. Although adolescents
   with autism showed less bias in their reasoning they were not more
   logical than the control group in a normative sense. The findings are
   discussed in the light of accounts which emphasise differences in
   contextual processing between typical and autistic populations.}},
