@article{ WOS:000514145500002,
Author = {Long, Adrienne H. and Lee, Daniel W. and Mackall, Crystal L.},
Title = {{Chimeric Antigen Receptors for Cancer: Progress and Challenges}},
Journal = {{CURRENT STEM CELL REPORTS}},
Year = {{2015}},
Volume = {{1}},
Number = {{4}},
Pages = {{187-196}},
Month = {{DEC}},
Abstract = {{Chimeric antigen receptors (CARs) genetically link an antigen-binding
   domain with cell-signaling domains to redirect immune cell specificity
   toward antigens expressed on the surface of cancer cells. Progress in
   CAR engineering over the last two decades has elucidated fundamental
   principles impacting CAR potency, and today CARs can be readily
   generated toward essentially any cell surface target on cancer cells.
   Efficacy thus far has been most impressive using CD19-CAR expressing T
   cells to treat B cell lymphoblastic leukemia, although clear activity
   has also been observed using CD19-CARs in patients with chronic
   lymphocytic leukemic and B cell lymphoma. Much less data is available
   regarding CAR efficacy for solid tumors. Here, we summarize current
   concepts of CAR design, with a focus on the relationship between
   structure and function, a review of the clinical results reported thus
   far, and the challenges to be addressed in future studies.}},
