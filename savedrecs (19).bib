@article{ WOS:000364403600090,
Author = {Akou, H. and Hamedi, M.},
Title = {{High energy micro electron beam generation using chirped laser pulse in
   the presence of an axial magnetic field}},
Journal = {{PHYSICS OF PLASMAS}},
Year = {{2015}},
Volume = {{22}},
Number = {{10}},
Month = {{OCT}},
Abstract = {{In this paper, the generation of high-quality and high-energy micro
   electron beam in vacuum by a chirped Gaussian laser pulse in the
   presence of an axial magnetic field is numerically investigated. The
   features of energy and angular spectra, emittances, and position
   distribution of electron beam are compared in two cases, i.e., in the
   presence and absence of an external magnetic field. The electron beam is
   accelerated with higher energy and qualified in spatial distribution in
   the presence of the magnetic field. The presence of an axial magnetic
   field improves electron beam spatial quality as well as its gained
   energy through keeping the electron motion parallel to the direction of
   propagation for longer distances. It has been found that a 64 mu m
   electron bunch with about MeV initial energy becomes a 20 mu m electron
   beam with high energy of the order of GeV, after interacting with a
   laser pulse in the presence of an external magnetic field. (C) 2015 AIP
   Publishing LLC.}},
