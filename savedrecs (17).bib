@article{ WOS:000364271300011,
Author = {Nandong, Jobrun},
Title = {{Heuristic-based multi-scale control procedure of simultaneous multi-loop
   PID tuning for multivariable processes}},
Journal = {{JOURNAL OF PROCESS CONTROL}},
Year = {{2015}},
Volume = {{35}},
Pages = {{101-112}},
Month = {{NOV}},
Abstract = {{A multi-loop PID control system is widely used in process industry where
   finding good values for the controller parameters is very challenging
   without systematic procedures. In this paper, based on the multi-scale
   control scheme a simultaneous multi-loop tuning method for an arbitrary
   n x n MIMO processes is proposed. The salient feature of the method is
   that it reduces the multi-loop PID tuning parameters from 4n to only 4
   common multi-scale control parameters. Both heuristic- and
   optimization-based procedures are proposed in order to apply the
   simultaneous tuning method, which should substantially simplify
   difficult multi-loop tuning involving numerous controller parameters. To
   ensure adequate robustness of the multi-loop control system, the
   balanced sensitivity function is used with the tuning method. Numerical
   studies based on 2 x 2,3 x 3,4 x 4 and 5 x 5 MIMO processes demonstrate
   the effectiveness of the tuning method. (C) 2015 Elsevier Ltd. All
   rights reserved.}},
