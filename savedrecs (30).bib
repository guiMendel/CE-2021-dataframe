@article{ WOS:000233733300015,
Author = {Doyle, BL and Pollo, MJ and Pekar, AH and Roy, ML and Thomas, BA and
   Brader, ML},
Title = {{Biophysical signatures of noncovalent aggregates formed by a
   glucagonlike peptide-1 analog: A prototypical example of
   biopharmaceutical aggregation}},
Journal = {{JOURNAL OF PHARMACEUTICAL SCIENCES}},
Year = {{2005}},
Volume = {{94}},
Number = {{12}},
Pages = {{2749-2763}},
Month = {{DEC}},
Abstract = {{LY307161 is a 31 amino acid analog of glucagonlike peptide-1(7-37)OH
   susceptible to physical instability associated with pharmaceutical
   processing. Orthogonal biophysical studies were conducted to explore the
   origins of this physical instability and to distinguish pharmaceutically
   desirable states of this aggregating peptide from undesirable ones.
   Equilibrium sedimentation analysis established that LY307161 exists as a
   monomer at pH 3, and reversibly self-associates in the pH range
   7.5-10.5. Causative factors for physical instability related to
   lyophilization conditions were investigated. Solution pH, acetonitrile
   content, and concentration of the peptide prior to lyophilization each
   impacted physicochemical properties of the resultant powders. A
   comparative study of two powder samples exhibiting physicochemically
   disparate properties established that LY307161 forms soluble noncovalent
   aggregates. FT-IR analyses in the solid and solution states identified a
   prominent band at 1657-1659 cm(-1) attributed to alpha-helix structure.
   Noncovalent soluble aggregate exhibited characteristic bands at 1615 and
   1698 cm(-1) indicative of intermolecular beta-sheet structure. An
   agitation-induced, precipitated solid form of LY307161 exhibited a
   different FT-IR signature indicative of a conformationally distinct
   species. Circular dichroism and fluorescence spectroscopy, together with
   dynamic light scattering measurements and dye-aggregate complexation,
   provided additional insights into the distinctions between aggregated
   and native LY307161. (c) 2005 Wiley-Liss, Inc. and the American
   Pharmacists Association.}},
