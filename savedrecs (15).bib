@article{ WOS:000358459300007,
Author = {Li, Min and Chen, Zhibing and Ruan, Xiaoqing and Yong, Xuerong},
Title = {{The formulas for the number of spanning trees in circulant graphs}},
Journal = {{DISCRETE MATHEMATICS}},
Year = {{2015}},
Volume = {{338}},
Number = {{11}},
Pages = {{1883-1906}},
Month = {{NOV 6}},
Abstract = {{The circulant graph C-n(s1,s2, ...,st) is the 2t regular graph with n
   vertices labeled 0, 1, 2, ..., n - 1, where each vertex i has the 2t
   neighbors i +/- s(1), i +/- s(2), ..., i +/- s(t) in which all the
   operations are modulo n. Golin et al. (2010) derive several closed
   integral formulas for the asymptotic limit
   (n ->infinity)lim T (C-n (s1,s2, ..., sk, left perpendicularn/d1right
   perpendicular+e1,leftperpendicularn/d2right perpendicular+e2, ..., left
   perpendicularn/dlright perpendicular+el))(1/n)
   as a function of s(i), d(j) and e(k), where T(G) is the number of
   spanning trees in graph G. In this paper we derive simple and explicit
   formulas for the number of spanning trees in circulant graphs
   C-pn(1,a1n,a2n, ...a1n). Following from the formulas we show that
   (n ->infinity)lim T (C-pn(1,a1n,a2n, ...,a1n))(1/n)
   = (t=0)Pi(k-1)(root 1+(i=1)Sigma(l)sin(2) pi a(i)t/p + root(i=1)Sigma(l)
   pi a(i)t/p)(2p/k),
   where k = lcm(p/a(1), p/a(2), ..., p/a(l)), and lcm denotes the least
   common multiple. The asymptotic limit represents the average growth rate
   of the number of spanning trees.
   The research is continuation of the previous work (Golin et al., 2010;
   Zhang et al., 2000; Zhang et al., 2005). Published by Elsevier B.V.}},
