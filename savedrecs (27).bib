@article{ WOS:000281066100025,
Author = {Garcia, F. and Net, M. and Garcia-Archilla, B. and Sanchez, J.},
Title = {{A comparison of high-order time integrators for thermal convection in
   rotating spherical shells}},
Journal = {{JOURNAL OF COMPUTATIONAL PHYSICS}},
Year = {{2010}},
