@article{ WOS:000284618300019,
Author = {Hadjadj, A. and Djellouli, G. and Jbara, O.},
Title = {{In situ ellipsometry study of the kinetics of hydrogen plasma
   interaction with a-Si:H thin films: A particular temperature-dependence}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2010}},
Volume = {{97}},
Number = {{21}},
Month = {{NOV 22}},
Abstract = {{We performed H-2 plasma treatment of hydrogenated amorphous silicon
   (a-Si:H) thin films and followed by in situ spectroscopic ellipsometry
   measurements the kinetics of hydrogen-induced film modifications at
   temperatures varying from 100 to 250 degrees C. The time-dependence of
   the H-modified layer thickness d(H)(t) follows an exponential relation
   of the form d(H)(0){[}1-exp(-t/tau)]. The temperature-dependence of
   d(H)(0) and tau shows a discontinuity at T=200 degrees C. While the
   activation energy of d(H)(0) changes from 0.19 eV at T<200 degrees C to
   0.05 eV at higher temperatures, the activation energy of tau suddenly
   changes from 0.15 eV to a negative value (-0.26 eV). Such a
   discontinuity should be linked to the thermal equilibrium temperature of
   undoped a-Si:H. Moreover, we found that the rate of formation of the
   H-modified layer r(H)=d(H)(0)/tau varies in inverse proportion to the
   etching rate, indicating a balance between hydrogen insertion and film
   etching at the steady state. (C) 2010 American Institute of Physics.
   {[}doi:10.1063/1.3517495]}},
