@article{ WOS:A1995RW12100019,
Author = {DARRIGOL, O},
Title = {{COHN,EMIL ELECTRODYNAMICS OF MOVING BODIES}},
Journal = {{AMERICAN JOURNAL OF PHYSICS}},
Year = {{1995}},
Volume = {{63}},
Number = {{10}},
Pages = {{908-915}},
Month = {{OCT}},
Abstract = {{A now forgotten figure, Emil Cohn was one of the leading experts in
   electrodynamics at the turn of the century. Unsatisfied with Lorentz's
   electron theory, he proposed an alternative electrodynamics of moving
   bodies based on a modification of Maxwell's macroscopic field equations
   that managed to account for all relevant electrodynamic and optical
   experiments, including that of Michelson and Morley. Some of his
   insights foreshadow aspects of Einstein's relativity theory, especially
   the elimination of the ether and the criticism of Lorentz's implicit
   distinction between true and measured coordinates of space and time.
   However, Cohn did not believe in the general validity of the relativity
   principle, and did not require a revision of the usual concepts of space
   and time. (C) 1995 American Association of Physics Teachers.}},
