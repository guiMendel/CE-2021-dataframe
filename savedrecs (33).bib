@article{ WOS:000232705800008,
Author = {Bokov, PM and Ridikas, D and Slessarev, I and Koberl, P},
Title = {{Core subcriticality as a tool of safety enhancement}},
Journal = {{NUCLEAR SCIENCE AND ENGINEERING}},
Year = {{2005}},
Volume = {{151}},
Number = {{3}},
Pages = {{335-343}},
Month = {{NOV}},
Abstract = {{Core subcriticality can play an important role if the safety enhancement
   of a nuclear system is necessary, in particular, when minor actinides
   submitted for transmutation cause essential degradation of the
   reactivity feedback effects or/and significant reduction of the delayed
   neutron fraction. The present work shows that core subcriticality
   together with thermohydraulics optimization can compensate for the
   possible degradation of the Doppler effect and the reduction of the
   delayed neutron fraction. The particular dependence of the spallation
   neutron yield allows the creation of a supplementary negative feedback
   effect in case of accelerator coupled hybrid systems. A number of
   quantitative examples are provided in this context.}},
