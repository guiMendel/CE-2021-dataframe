@article{ WOS:000232939200048,
Author = {Brunet, P and Amberg, G and Alfredsson, PH},
Title = {{Control of thermocapillary instabilities far from threshold}},
Journal = {{PHYSICS OF FLUIDS}},
Year = {{2005}},
Volume = {{17}},
Number = {{10}},
Month = {{OCT}},
Abstract = {{We report experiments on control of thermocapillary instabilities at
   high temperature differences, in an annular geometry. Previous studies
   {[}Phys. Fluids 14, 3039 (2002)] showed that a reasonable control of
   oscillatory instability could be achieved by optimizing a local heating
   feedback process. We conducted experiments with a basic flow converging
   from periphery to center. This constitutes a more unstable configuration
   than previously, and enables appearance of higher-order instabilities
   and chaos. Applying successfully local feedback control to the periodic
   state close to the threshold, we extend the process to higher
   temperature differences, where nonlinear as well as
   proportional/derivative control laws are necessary to obtain a
   significant decrease of the temperature fluctuations. Finally,
   proportional control allows us to synchronize a chaotic state, to a
   periodic one. (c) 2005 American Institute of Physics.}},
