@article{ WOS:000165091200013,
Author = {Knetter, C and Muller-Hartmann, E and Uhrig, GS},
Title = {{Symmetries and triplet dispersion in a modified Shastry-Sutherland model
   for SrCu2(BO3)(2)}},
Journal = {{JOURNAL OF PHYSICS-CONDENSED MATTER}},
Year = {{2000}},
Volume = {{12}},
Number = {{42}},
Pages = {{9069-9083}},
Month = {{OCT 23}},
Abstract = {{We investigate the one-triplet dispersion in a modified
   Shastry-Sutherland model for SrCu2(BO3)(2) by means of a series
   expansion about the limit of strong dimerization. Our perturbative
   method is based on a continuous unitary transformation that maps the
   original Hamiltonian to an effective, energy-quanta-conserving block
   diagonal Hamiltonian H-eff. The dispersion splits into two branches
   which are nearly degenerate. We analyse thr symmetries of the model and
   show that space group operations are necessary to explain the degeneracy
   of the dispersion at k = 0 and at the border of the magnetic Brillouin
   zone. Moreover, we investigate the behaviour of the dispersion for small
   /k/ and compare our results to inelastic neutron scattering data.}},
