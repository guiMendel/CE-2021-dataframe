@article{ WOS:000362919500015,
Author = {Barker, Andrew T. and Stoll, Martin},
Title = {{Domain decomposition in time for PDE-constrained optimization}},
Journal = {{COMPUTER PHYSICS COMMUNICATIONS}},
Year = {{2015}},
Volume = {{197}},
Pages = {{136-143}},
Month = {{DEC}},
Abstract = {{PDE-constrained optimization problems have a wide range of applications,
   but they lead to very large and ill-conditioned linear systems,
   especially if the problems are time dependent. In this paper we outline
   an approach for dealing with such problems by decomposing them in time
   and applying an additive Schwarz preconditioner in time, so that we can
   take advantage of parallel computers to deal with the very large linear
   systems. We then illustrate the performance of our method on a variety
   of problems. (C) 2015 Elsevier B.V. All rights reserved.}},
