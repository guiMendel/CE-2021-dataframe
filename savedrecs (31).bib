@article{ WOS:000233544000009,
Author = {Liu, XY and Yu, WD and Yu, WD},
Title = {{Static torsion and torsion fatigue of UHMW-PE and aramid filaments}},
Journal = {{HIGH PERFORMANCE POLYMERS}},
Year = {{2005}},
Volume = {{17}},
Number = {{4}},
Pages = {{593-603}},
Month = {{DEC}},
Abstract = {{The effects of pre-tension and torsion angle on the torsion fatigue
   lifetimes of high performance fibers (HPFs), including three p-aramid
   fibers (Twaron (R) 2000, Kevlar (R) 29 and Kevlar (R) 129) and ultra
   high molecular weight polyethylene fiber (UHMW-PE), were studied by
   means of a self-made torsion fatigue tester. The torsion fatigue
   performance of the HPFs were characterized and compared. The
   fracture-end morphology of these fibers after the fatigue tests was
   observed by scanning electron microscopy (SEM) to seek to explain the
   fracture mechanism of the fiber fatigue. The experimental results
   indicate that there was a linear relationship between the logarithm of
   the fatigue lifetime and the pre-tension or torsion angle for the high
   performance p-aramid fibers, and the order of torsional fatigue lifetime
   for the four fibers was UHMW-PE > Kevlar (R) 129 > Kevlar (R) 29 >
   Twaron (R) 2000. Furthermore, the torsion-to-rupture test showed that
   the property of torsion to rupture relies on the twist transfer along
   the fibers, and the degree of twist transfer (DTT) for the UHMW-PE and
   aramid fibers was also in the order of UHMW-PE > Kevlar (R) 129 > Kevlar
   (R) 29 > Twaron (R) 2000.}},
