@article{ WOS:000580355300025,
Author = {Boronat-Belda, Talia and Ferrero, Hilda and Al-Abdulla, Ruba and
   Quesada, Ivan and Gustafsson, Jan-Ake and Nadal, Angel and
   Alonso-Magdalena, Paloma},
Title = {{Bisphenol-A exposure during pregnancy alters pancreatic beta-cell
   division and mass in male mice offspring: A role for ER beta}},
Journal = {{FOOD AND CHEMICAL TOXICOLOGY}},
Year = {{2020}},
Volume = {{145}},
Month = {{NOV}},
Abstract = {{Bisphenol-A (BPA) is a widespread endocrine disrupting chemical that
   constitutes a risk factor for type 2 diabetes mellitus (T2DM). Data from
   animal and human studies have demonstrated that early exposure to BPA
   results in adverse metabolic outcomes in adult life.
   In the present work, we exposed pregnant heterozygous estrogen receptor
   beta (ER beta) knock out (BERKO) mice to 10 mu g/kg/day BPA, during days
   9-16 of pregnancy, and measured beta-cell mass and proliferation in
   wildtype (WT) and BERKO male offspring at postnatal day 30. We observed
   increased pancreatic beta-cell proliferation and mass in WT, yet no
   effect was produced in BERKO mice. Dispersed islet cells in primary
   culture treated with 1 nM BPA showed an enhanced pancreatic beta-cell
   replication rate, which was blunted in pancreatic beta-cells from BERKO
   mice and mimicked by the selective ER beta agonist WAY200070. This
   increased beta-cell proliferation was found in male adult as well as in
   neonate pancreatic beta-cells, suggesting that BPA directly impacts
   beta-cell division at earliest stages of life. These findings strongly
   indicate that BPA during pregnancy upregulates pancreatic beta-cell
   division and mass in an ER beta-dependent manner. Thus, other natural or
   artificial chemicals may use this ER beta-mediated pathway to promote
   similar effects.}},
