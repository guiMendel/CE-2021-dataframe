@article{ WOS:000089460600018,
Author = {Wong, IHN and Chan, AT and Johnson, PJ},
Title = {{Molecular analysis of circulating tumor cells in peripheral blood from
   patients with germ cell tumor: A quantitative approach}},
Journal = {{INTERNATIONAL JOURNAL OF MOLECULAR MEDICINE}},
Year = {{2000}},
Volume = {{6}},
Number = {{4}},
Pages = {{491-494}},
Month = {{OCT}},
Abstract = {{Germ cell tumor (GCT) is one of the few malignancies in which recurrent
   patients would have a good chance of cure by chemotherapy. Using
   semiquantitative reverse transcription polymerase chain reaction for
   a-fetoprotein (afp) mRNA, we measured the circulating tumor load in 54
   peripheral blood samples from 7 GCT patients at diagnosis and 47 healthy
   controls. Clinicopathological information of GCT patients was obtained
   during a 12-month follow-up. As compared to the upper limit among
   healthy controls, afp mRNA levels increased 23-3.4x10(3)-fold
   pretreatment in all 7 patients, and 2.8x10(3)-1.4x10(4)-fold
   posttreatment in 2 patients studied. All 7 patients with substantially
   elevated afp mRNA levels developed recurrence or metastasis within 12
   months of diagnosis. We present evidence that afp-expressing tumor cells
   disseminating in GCT patients before treatment may possibly be the
   source of recurrence or metastasis. Sequential quantification of afp
   mRNA during the clinical course may provide crucial information for
   identifying GCT patients at high risk for metastasis.}},
