@article{ WOS:000587945800001,
Author = {Riccio, Alessandra and Coletti, Alice and Dolciami, Daniela and Mammoli,
   Andrea and Cerra, Bruno and Moretti, Sonia and Gioiello, Antimo and
   Ferlin, Simone and Puxeddu, Efisio and Macchiarulo, Antonio},
Title = {{The Stone Guest: How Does pH Affect Binding Properties of PD-1/PD-L1
   Inhibitors?}},
Journal = {{CHEMMEDCHEM}},
Year = {{2021}},
Volume = {{16}},
Number = {{3}},
Pages = {{568-577}},
Month = {{FEB 4}},
Abstract = {{The interaction between programmed cell death-1 (PD-1) and its ligand
   PD-L1 activates a coinhibitory signal that blocks T-cell activation,
   promoting the immune escape process in the tumor microenvironment.
   Development of monoclonal antibodies targeting and inhibiting PD-1/PD-L1
   interaction as anticancer immunotherapies has proved successful in
   multiple clinical settings and for various types of cancer.
   Notwithstanding, limitations exist with the use of these biologics,
   including drug resistance and narrow therapeutic response rate in a
   majority of patients, that demand for the design of more efficacious
   small molecule-based immunotherapies. Alteration of pH in the tumor
   microenvironment is a key factor that is involved in promoting drug
   resistance, tumor survival and progression. In this study, we have
   investigated the effect of pH shifts on binding properties of distinct
   classes of PD-L1 inhibitors, including macrocyclic peptide and small
   molecules. Results expand structure-activity relationships of PD-L1
   inhibitors, providing insights into structural features and
   physicochemical properties that are useful for the design of ligands
   that may escape a drug resistance mechanism associated to variable pH
   conditions of tumor microenvironment.}},
