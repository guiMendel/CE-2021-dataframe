@article{ WOS:000088854900003,
Author = {Cappelaere, E and Cressely, R},
Title = {{Influence of NaClO3 on the rheological behaviour of a micellar solution
   of CPCl}},
Journal = {{RHEOLOGICA ACTA}},
Year = {{2000}},
Volume = {{39}},
Number = {{4}},
Pages = {{346-353}},
Month = {{AUG}},
Abstract = {{It is now well know that a small addition of salt to a micellar solution
   often increases the zero-shear viscosity eta(o) of the solution, the
   understanding of the behaviour at high salt contest is more
   questionable. In this situation, addition of more salt induces a
   decrease of eta(o). In this experimental work we investigate the linear
   and non-linear rheological behaviour of a new micellar system: CPCl
   (surfactant)/NaClO3 (salt). Studies of the evolution of Ilo as well as
   Go (the elastic modulus) or tau(R) (the relaxation time) are in
   agreement with the hypothesis of a diminution of the mean micellar
   length when, after the maximum go, the salt content increases. In the
   non-linear behaviour (non-Newtonian viscosity) the evolution of (gamma)
   over dot(c) (which defines the occurrence of the shear thinning) with
   salt concentration C-s is also in agreement with such a hypothesis.}},
