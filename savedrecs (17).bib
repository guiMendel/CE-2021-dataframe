@article{ WOS:000366440100025,
Author = {Rangel Gonzalez, Erick and Torre Cepeda, Leonardo E.},
Title = {{Determinants of the cost of starting a business in Mexico}},
Journal = {{NORTH AMERICAN JOURNAL OF ECONOMICS AND FINANCE}},
Year = {{2015}},
Volume = {{34}},
Number = {{SI}},
Pages = {{430-449}},
Month = {{NOV}},
Abstract = {{We construct a panel data at the state level in Mexico for the years
   2006, 2008 and 2011 in order to investigate the impact that entry
   deregulation efforts, states' financial strength, crime level, and
   potential market profitability have on the cost of opening new
   businesses in Mexico, where this cost is taken from the Doing Business
   in Mexico report of the World Bank. Using a model that controls for both
   regional fixed effects and endogenous effects, we find that our measures
   of entry deregulation and states' financial strength are associated with
   a reduction in the cost of opening a new business, while higher levels
   of crime and higher GDP per capita tend to increase it. (C) 2015
   Elsevier Inc. All rights reserved.}},
