@article{ WOS:000090137000002,
Author = {Anzenbacher, P and Try, AC and Miyaji, H and Jursikova, K and Lynch, VM
   and Marquez, M and Sessler, JL},
Title = {{Fluorinated calix{[}4]pyrrole and dipyrrolylquinoxaline: Neutral anion
   receptors with augmented affinities and enhanced selectivities}},
Journal = {{JOURNAL OF THE AMERICAN CHEMICAL SOCIETY}},
Year = {{2000}},
Volume = {{122}},
Number = {{42}},
Pages = {{10268-10272}},
Month = {{OCT 25}},
Abstract = {{The use of the 3,4-difluoro-1H-pyrrole as a building block for the
   preparation of octamethyloctafluorocalix{[}4]pyrrole and
   2,3-di(3',4'-difluoropyrrol-2'yl)quinoxaline is described. These latter
   two entities act as neutral anion receptors and were found to bind
   anions such as fluoride, chloride, or dihydrogen phosphate with an
   enhanced affinity compared to their non-fluorinated congeners as judged
   from H-1 NMR, F-19 NMR, and fluorescence emission spectroscopic
   analyses. The increase in affinity was especially high in case of
   chloride and dihydrogen phosphate anion, with the
   2,3-di(3',4'-difluoropyrrol-2'-yl)quinoxaoline system, in particular,
   displaying an affinity for H2PO4- that was improved by 3 orders of
   magnitude as compared to its nun-fluorinated congener. This improvement
   in the affinity for the dihydrogen phosphate is accompanied by change of
   color from pale yellow to orange, thus allowing the use of such
   compounds as naked-eye sensors for phosphate anion. In the case of the
   octafluorocalix{[}4]pyrrole system X-ray diffraction analyses revealed
   the presence of four different macrocyclic conformations in the solid
   state, as well as close intermolecular contacts mediated by apparent CF-
   -HN hydrogen bonds.}},
