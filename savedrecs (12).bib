@article{ WOS:000365817700011,
Author = {Li, Bin and Chen, Jun-hong and Su, Jin-dong and Yan, Ming-wei and Sun,
   Jia-lin and Li, Yong},
Title = {{Morphology of alpha-Si3N4 in Fe-Si3N4 prepared via flash combustion}},
Journal = {{INTERNATIONAL JOURNAL OF MINERALS METALLURGY AND MATERIALS}},
Year = {{2015}},
Volume = {{22}},
Number = {{12}},
Pages = {{1322-1327}},
Month = {{DEC}},
Abstract = {{The state and formation mechanism of alpha-Si3N4 in Fe-Si3N4 prepared by
   flash combustion were investigated by X-ray diffraction, scanning
   electron microscopy, and transmission electron microscopy. The results
   indicate that alpha-Si3N4 crystals exist only in the Fe-Si3N4 dense
   areas. When FeSi75 particles react with N-2, which generates substantial
   heat, a large number of Si solid particles evaporate. The product
   between Si gas and N-2 is a mixture of alpha-Si3N4 and beta-Si3N4. At
   the later stage of the flash combustion process, alpha-Si3N4 crystals
   dissolve and reprecipitate as beta-Si3N4 and the beta-Si3N4 crystals
   grow outward from the dense areas in the product pool. As the
   temperature decreases, the alpha-Si3N4 crystals cool before transforming
   into beta-Si3N4 crystals in the dense areas of Fe-Si3N4. The phase
   composition of flash-combustion-synthesized Fe-Si3N4 is controllable
   through manipulation of the gas-phase reaction in the early stage and
   the alpha ->beta transformation in the later stage.}},
