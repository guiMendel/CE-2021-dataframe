@article{ WOS:000281738000018,
Author = {Fu, Licai and Yang, Jun and Bi, Qinling and Liu, Weimin},
Title = {{Deformation behavior of bulk nanocrystalline Fe88Si12 alloy}},
Journal = {{JOURNAL OF ALLOYS AND COMPOUNDS}},
Year = {{2010}},
Volume = {{505}},
Number = {{2}},
Pages = {{456-458}},
Month = {{SEP 3}},
Abstract = {{The deformation behavior of bulk nanocrystalline Fe88Si12 alloy has been
   investigated with compression tests at different strain rates. The
   activity volume nu{*} is 6b(3) which indicates that the deformation
   mechanism is controlled by the dislocation slip. On the other hand, the
   critical size of the nanocrystalline Fe88Si12 alloy for deformation
   mechanism is calculated to be about 5.3 nm. It is smaller than the grain
   size of the nanocrystalline Fe88Si12 alloy (10 nm), which also indicates
   that deformation mechanism of the nanocrystalline Fe88Si12 alloy is
   controlled by the dislocation pile-up. (C) 2009 Elsevier B.V. All rights
