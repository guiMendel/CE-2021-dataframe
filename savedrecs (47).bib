@article{ WOS:000088449100008,
Author = {Zeisler, SK and Becker, DW and Pavan, RA and Moschel, R and Ruhle, H},
Title = {{A water-cooled spherical niobium target for the production of
   {[}F-18]fluoride}},
Journal = {{APPLIED RADIATION AND ISOTOPES}},
Year = {{2000}},
Volume = {{53}},
Number = {{3}},
Pages = {{449-453}},
Month = {{SEP}},
Abstract = {{A new automated target system for the routine production of
   {[}F-18]fluoride from O-18-enriched water has been constructed. It
   consists of a small spherical niobium target chamber mounted into a
   special holder, which provides rapid cooling by water Row around the
   sphere. The target is irradiated with 21 MeV protons; the incident
   energy in the target chamber is 13 MeV. The system is operated without
   external over-pressure and has been tested for beam currents up to 50 mu
   A. 95\% of the theoretical yield of {[}F-18]fluoride has been extracted
   and used for the synthesis of {[}F-18]2-fluoro-2-deoxy-D-glucose. (C)
