@article{ WOS:000368273500011,
Author = {Tang, Yani and Yan, Guobei and Song, Xin and Wu, Kuangpei and Li, Zhen
   and Yang, Chao and Deng, Tanggang and Sun, Yang and Hu, Xiaoxiao and
   Yang, Cai and Bai, Huarong and Li, Hui and Tan, Weihong and Ye, Mao and
   Liu, Jing},
Title = {{STIP overexpression confers oncogenic potential to human non-small cell
   lung cancer cells by regulating cell cycle and apoptosis}},
Journal = {{JOURNAL OF CELLULAR AND MOLECULAR MEDICINE}},
Year = {{2015}},
Volume = {{19}},
Number = {{12}},
Pages = {{2806-2817}},
Month = {{DEC}},
Abstract = {{Sip1/tuftelin-interacting protein (STIP), a multidomain nuclear protein,
   is a novel factor associated with the spliceosome, yet its role and
   molecular function in cancer remain unknown. In this study, we show, for
   the first time, that STIP is overexpressed in non-small cell lung cancer
   (NSCLC) tissues compared to adjacent normal lung tissues. The depletion
   of endogenous STIP inhibited NSCLC cell proliferation in vitro and in
   vivo, caused cell cycle arrest and induced apoptosis. Cell cycle arrest
   at the G2/M phase was associated with the expression and activity of the
   cyclin B1-CDK1 (cyclin-dependent kinase 1) complex. We also provide
   evidence that STIP knockdown induced apoptosis by activating both
   caspase-9 and caspase-3 and by altering the Bcl-2/Bax expression ratio.
   RNA sequencing data indicated that the MAPK mitogen-activated protein
   kinases, Wnt, PI3K/AKT, and NF-kappa B (nuclear factor
   kappa-light-chain-enhancer of activated B cells) signalling pathways
   might be involved in STIP-mediated tumour regulation. Collectively,
   these results suggest that STIP may be a novel potential diagnostic and
   therapeutic target for NSCLC.}},
