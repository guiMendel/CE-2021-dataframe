@article{ WOS:A1995TD27000002,
Author = {ARAKAWA, O},
Title = {{EFFECTS OF REPEATED ADMINISTRATION OF LOW-DOSES OF APOMORPHINE ON
   SINGLE-RATS AND PAIRED-RATS OPEN-FIELD BEHAVIOR}},
Journal = {{NEUROSCIENCES-JAPAN}},
Year = {{1995}},
Volume = {{21}},
Number = {{3}},
Pages = {{105-113}},
Month = {{AUG}},
Abstract = {{The present study was conducted to investigate the effects of repeated
   administration of low-dose apomorphine on single- and paired-rats
   open-field behavior by using a video analysis system. Rats were
   pretreated with saline or apomorphine (0.04, 0.2, 1.0 mg/kg, SC) 7 times
   before the experiment began. On the day of the experiment, the rats were
   injected with saline (1 ml/kg, SC) or apomorphine (0.005, 0.05 mg/kg,
   SC) and placed in an open field for 30 min, individually or paired with
   another rat. The rats' movements were plotted on XY coordinates by video
   analysis every 0.5 sec, and analyzed by distance factors. There was no
   effect on rat behaviors of repeated saline pretreatment after acute, but
   low-dose apomorphine pretreatment definitely suppressed rat behaviors.
   However, there was no difference in the effect of apomorphine
   pretreatment on single or paired behaviors acid no dose-dependent
   relationship was found. In order to distinguish the effects of repeated
   administration, this experiment should be repeated with different
   dosages and intervals of administration.}},
