@article{ WOS:000233998000018,
Author = {Ganguli, M and Hendrie, HC},
Title = {{Screening for cognitive impairment and depression in ethnically diverse
   older populations}},
Journal = {{ALZHEIMER DISEASE \& ASSOCIATED DISORDERS}},
