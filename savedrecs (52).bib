@article{ WOS:A1995TN10000003,
Author = {Garrett, MJ},
Title = {{The future of health futures - Another perspective}},
Journal = {{FUTURES}},
Year = {{1995}},
Volume = {{27}},
Number = {{9-10}},
Pages = {{927-933}},
Month = {{NOV-DEC}},
Abstract = {{A good `market' for health futures and easy access to relevant
   information are among the reasons that health futures is currently
   centred in the wealthy nations. Interest in health futures is growing in
   the less developed countries, however, in part because of efforts by WHO
   and its regional office. Many benefits can be expected if the field
   becomes more international, including an influx of fresh ideas about
   health futures study designs and about innovative approaches to health
   care. A shift to a more global orientation is also imperative simply
   because health futures deals with the well-being of human beings, and
   most human beings live in the less developed countries of the world.}},
