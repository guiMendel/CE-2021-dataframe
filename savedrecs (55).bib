@article{ WOS:A1995RW12800016,
Author = {COSTAFERNANDEZ, JM and PEREIROGARCIA, R and SANZMEDEL, A and
   BORDELGARCIA, N},
Title = {{EFFECT OF PLASMA PRESSURE ON THE DETERMINATION OF MERCURY BY
   MICROWAVE-INDUCED PLASMA-ATOMIC EMISSION-SPECTROMETRY}},
Journal = {{JOURNAL OF ANALYTICAL ATOMIC SPECTROMETRY}},
