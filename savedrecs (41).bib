@article{ WOS:000165666600003,
Author = {Seifert, G and Kaempfe, M and Berg, KJ and Graener, H},
Title = {{Femtosecond pump-probe investigation of ultrafast silver nanoparticle
   deformation in a glass matrix}},
Journal = {{APPLIED PHYSICS B-LASERS AND OPTICS}},
Year = {{2000}},
Volume = {{71}},
Number = {{6}},
Pages = {{795-800}},
Month = {{DEC}},
Abstract = {{Single-color femtosecond pump-probe experiments were performed to
   investigate the time dependence of laser-induced ultrafast desorption
   and deformation processes of silver nanoparticles in glass. After laser
   excitation at wavelengths close to the surface plasmon resonance,
   transient extinction changes were found to exhibit dynamics on quite
   different time scales ranging from sub-picoseconds to some hundred ps.
   The slowest observed decay component is identified as characteristic for
   the deformation/desorption processes. Possible mechanisms for these
   processes are discussed.}},
Publisher = {{SPRINGER}},
