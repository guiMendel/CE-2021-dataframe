EISSN = {{2169-9291}},
Keywords-Plus = {{TEMPORAL VARIABILITY; RADIATION PROPERTIES; ARCTIC SURFACE; ENERGY
   BUDGET; CLOUD; FLUX; PARAMETERIZATIONS; ALBEDO; COVER; OCEAN}},
Research-Areas = {{Oceanography}},
