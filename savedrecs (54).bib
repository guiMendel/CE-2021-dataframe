@article{ WOS:A1995RZ47500010,
Author = {ORAZI, A and JING, BD and LEE, CH and ENGLISH, GW and CATTORETTI, G and
   JOHN, K and NEIMAN, RS},
Title = {{CORRELATION BETWEEN PRESENCE OF CLONAL REARRANGEMENTS OF IMMUNOGLOBULIN
   HEAVY-CHAIN GENES AND B-CELL ANTIGEN EXPRESSION IN HODGKINS-DISEASE}},
Journal = {{AMERICAN JOURNAL OF CLINICAL PATHOLOGY}},
