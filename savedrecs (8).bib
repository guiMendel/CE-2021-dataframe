@article{ WOS:000580389100001,
Author = {Berry, V, M.},
Title = {{Integrals whose saddle-point expansions terminate}},
Journal = {{JOURNAL OF PHYSICS A-MATHEMATICAL AND THEORETICAL}},
Year = {{2020}},
Volume = {{53}},
Number = {{44}},
Month = {{NOV 6}},
Abstract = {{The saddle-point expansion for integrals with integrand exp(-kf(x)) is a
   series in powers of 1/k. Usually this series diverges, but there is a
   family of exponent functionsf(x), defining a family of canonical
   integrals, for which the series terminates and the saddle-point
   expansion is exact. For this family, the transformationx -> Xsuch
   thatf(x) =X(2)possesses a Jacobian that is a polynomial inX, whose
   coefficients parameterise the canonical integrals.}},
