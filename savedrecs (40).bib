@article{ WOS:000166489400012,
Author = {Del Prete, R and Fumarola, D and Fumarola, L and Miragliotta, G},
Title = {{Detection of Bartonella henselae and Afipia felis DNA by polymerase
   chain reaction in specimens from patients with cat scratch disease}},
Journal = {{EUROPEAN JOURNAL OF CLINICAL MICROBIOLOGY \& INFECTIOUS DISEASES}},
Year = {{2000}},
Volume = {{19}},
Number = {{12}},
Pages = {{964-967}},
Month = {{DEC}},
Abstract = {{Polymerase chain reaction (PCR) amplification and colorimetric
   identification of amplicons were performed to detect Bartonella henselae
   and Afipia felis DNA in specimens from patients who were clinically and
   histologically suspected of having cat scratch disease. PCR products
   were revealed using 2\% ethidium bromide agarose-gel electrophoresis and
   identified with specific probes in a commercial colorimetric
   hybridization assay (DEIA) (GEN-ETI-K; DiaSorin, Italy). Six
   paraffin-embedded lymph node biopsies from 18 patients as well as 18
   samples of peripheral whole blood and 18 sera were investigated.
   Bartonella henselae DNA was recovered from the whole blood of four
   patients, and Bartonella henselae and Afipia felis DNA were detected in
   one patient's lymph node biopsy. This study suggests that PCR-DEIA is
   sufficiently sensitive to be considered feasible for the molecular
