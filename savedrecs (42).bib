@article{ WOS:000165998800002,
Author = {Avresky, DR and Cunningham, C and Ravichanran, H},
Title = {{Fault-tolerant routing for wormhole routed two-dimensional meshes}},
Journal = {{COMPUTER SYSTEMS SCIENCE AND ENGINEERING}},
Year = {{2000}},
Volume = {{15}},
Number = {{6}},
Pages = {{385-397}},
Month = {{NOV}},
Abstract = {{In this paper, the authors present a livelock- and deadlock-free
   fault-tolerant wormhole routing strategy known as NAFTA that
   successfully routes messages in the presence of up to N- 1 faults in an
   N x N two-dimensional mesh, while requiring only four virtual channels
   per physical link. NAFTA does not require any global network information
   but uses nine status definitions that describe the status of only the
   neighboring nodes. NAFTA can dynamically route messages around faulty
   nodes without incurring significant performance degradation. Through
   extensive simulation studies, it is shown that for a 256 x 256 mesh, the
   first fault has the most significant impact on the throughput and
   message passing latency of the mesh. However, successive faults cause
   only a slight reduction in message passing performance.}},
