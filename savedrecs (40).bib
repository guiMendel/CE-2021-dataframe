@article{ WOS:000165800000041,
Author = {Perebeinos, V and Allen, PB},
Title = {{Franck-condon-broadened angle-resolved photoemission spectra predicted
   in LaMnO3}},
Journal = {{PHYSICAL REVIEW LETTERS}},
Year = {{2000}},
Volume = {{85}},
Number = {{24}},
Pages = {{5178-5181}},
Month = {{DEC 11}},
Abstract = {{The sudden photohole of least energy created in the photoemission
   process is a vibrationally excited state of a small polaron. Therefore
   the photoemission spectrum in LaMnO3 is predicted to have multiple
   Franck-Condon vibrational sidebands. This generates an intrinsic fine
   broadening approximate to0.5 eV. The photoemission spectral function has
   two peaks whose central energies disperse with bandwidth approximate
   to1.2 eV. Signatures of these phenomena are predicted to appear in
   angle-resolved photoemission spectra.}},
