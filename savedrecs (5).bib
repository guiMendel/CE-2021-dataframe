@article{ WOS:000559952700010,
Author = {Uttermark, Matthew J.},
Title = {{Revisiting the Relationship Between Federal Design and Economic Growth}},
Journal = {{PUBLIUS-THE JOURNAL OF FEDERALISM}},
Year = {{2020}},
Volume = {{50}},
Number = {{1}},
Pages = {{135-155}},
Month = {{WIN}},
Abstract = {{Federal design matters-but how? Federalism research has addressed the
   longstanding question whether federalism is linked with economic growth,
   but with differing results. In this study, I address the possible
   linkage using measures from the empirically rich Regional Authority
   Index. Examining federalism over time across approximately seventy
   nations, I evaluate the conceptually distinct expectations of self-rule
   and shared-rule federalism on several measures of economic development.
   The results fail to find an association between either self-rule or
   shared-rule and economic development, supporting the argument that the
   linkage is weak or even nonexistent.}},
