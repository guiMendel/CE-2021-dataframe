@article{ WOS:000359165500070,
Author = {Zhang, Xi and Chen, Jixin and Zhang, Jie and Wan, Detian and Zhou,
   Yanchun},
Title = {{High-temperature mechanical and thermal properties of h-BN/30 vol\%
   Y2SiO5 composite}},
Journal = {{CERAMICS INTERNATIONAL}},
