@article{ WOS:000600888300017,
Author = {Gubchenko, V. M.},
Title = {{Source of Electromagnetic Radiation in Corona and Impedance of the
   Stationary Magnetic Reconnection Area}},
Journal = {{GEOMAGNETISM AND AERONOMY}},
Year = {{2020}},
Volume = {{60}},
Number = {{7}},
Pages = {{896-903}},
Month = {{DEC}},
Abstract = {{A source of high-frequency electromagnetic radiation is formed by
   energetic particles in the magnetic structures of plasma corona.
   Energetic particles are formed in the electric and magnetic fields of
   the dissipative structures of the magnetic reconnection caused by the
   low-frequency induction process in a hot, collisionless plasma expanding
   over the magnetization regions. The stationary process of kinetic
   acceleration refers only to the group of resonant particles in the phase
   space of velocities and determines the dissipation. Impedance
   characteristics, which make it possible to calculate the power of the
   acceleration process and the energy of electromagnetic fields formed by
   current systems and to determine the internal oscillation and relaxation
   times of current systems (CSs) of magnetic structures, are introduced
   for the process.}},
