@article{ WOS:000595618300009,
Author = {Chowdhury, Ratul and Grisewood, Matthew J. and Boorla, Veda Sheersh and
   Yan, Qiang and Pfleger, Brian F. and Maranas, Costas D.},
Title = {{IPRO plus /-: Computational Protein Design Tool Allowing for Insertions
   and Deletions}},
Journal = {{STRUCTURE}},
Year = {{2020}},
Volume = {{28}},
Number = {{12}},
Pages = {{1344+}},
Month = {{DEC 1}},
Abstract = {{Insertions and deletions (indels) in protein sequences alter the residue
   spacing along the polypeptide backbone and consequently open up
   possibilities for tuning protein function in a way that is inaccessible
   by amino acid substitution alone. We describe an optimization-based
   computational protein redesign approach centered around predicting
   beneficial combinations of indels along with substitutions and also
   obtain putative substrate-docked structures for these protein variants.
   This modified algorithmic capability would be of interest for enzyme
   engineering and broadly inform other protein design tasks. We highlight
   this capability by (1) identifying active variants of a bacterial
   thioesterase enzyme ('TesA) with experimental corroboration, (2)
   recapitulating existing active TEM-1 beta-Lactamase sequences of
   different sizes, and (3) identifying shorter 4-Coumarate:CoA ligases
   with enhanced in vitro activities toward non-native substrates. A
   separate PyRosetta-based open-source tool, Indel-Maker
   (http://www.maranasgroup.com/software.htm), has also been created to
   construct computational models of user-defined protein variants with
   specific indels and substitutions.}},
