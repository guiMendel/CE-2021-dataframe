@article{ WOS:A1995RN07800006,
Author = {YAVNEH, I},
Title = {{MULTIGRID SMOOTHING FACTORS FOR RED-BLACK GAUSS-SEIDEL RELAXATION
   APPLIED TO A CLASS OF ELLIPTIC-OPERATORS}},
Journal = {{SIAM JOURNAL ON NUMERICAL ANALYSIS}},
Year = {{1995}},
Volume = {{32}},
Number = {{4}},
Pages = {{1126-1138}},
Month = {{AUG}},
Abstract = {{Analytic formulae are obtained for the smoothing factors yielded by
   Gauss-Seidel relaxation in two-color ordering for a class of scalar
   elliptic operators. Block and point relaxations, in conjunction with
   full or partial coarsening, are encompassed for operators with general
   (constant, positive) coefficients in general dimensions and for an
   arbitrary number of relaxation sweeps. It is found that there is no
   direct dependence of the smoothing factors on the dimension, and that
   the effect of the number of relaxation sweeps on the smoothing factor is
   usually independent of the operator coefficients and of the relaxation
   scheme. The results are compared with computed results of two-level
   analyses. Smoothing strategies implied by the formulae are discussed.}},
