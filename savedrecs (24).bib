@article{ WOS:000283926200002,
Author = {Fletcher, Alan J.},
Title = {{WHAT DID ADAM PYNKHURST (NOT) WRITE?: A REPLY TO DR HOROBIN}},
Journal = {{REVIEW OF ENGLISH STUDIES}},
Year = {{2010}},
Volume = {{61}},
Number = {{252}},
Pages = {{690-710}},
Month = {{NOV}},
Abstract = {{Dr Simon Horobin has recently written an interesting critique of an
   earlier essay of the present author that argued the value of reflecting
   on whether one of the scribal hands in Dublin, Trinity College, MS 244,
   may have been that of Adam Pynkhurst or not, the man now celebrated as
   the copyist, amongst other things, of the Ellesmere Chaucer manuscript.
   This essay exposes deficiencies in both the content and argument of his
   critique that dispose of much of its credibility. Furthermore, and more
   importantly, the essay invites wider reflection upon the question of how
   palaeographers and historical linguists decide that one and the same
   scribal hand is present in more than one manuscript. Thus the issues at
   stake, though raised in the local context of a discussion about what
   Adam Pynkhurst may or may not have written, have a far more general and
   far reaching application and concern.}},
