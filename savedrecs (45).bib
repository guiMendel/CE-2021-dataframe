@article{ WOS:000089688000049,
Author = {Sankaranarayanan, S and De Angelis, D and Rothman, JE and Ryan, TA},
Title = {{The use of pHluorins for optical measurements of presynaptic activity}},
Journal = {{BIOPHYSICAL JOURNAL}},
Year = {{2000}},
Volume = {{79}},
