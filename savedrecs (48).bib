@article{ WOS:000088741800003,
Author = {Agostoni, C and Carratu, B and Boniglia, C and Riva, E and Sanzini, E},
Title = {{Free amino acid content in standard infant formulas: Comparison with
   human milk}},
Journal = {{JOURNAL OF THE AMERICAN COLLEGE OF NUTRITION}},
Year = {{2000}},
Volume = {{19}},
Number = {{4}},
Pages = {{434-438}},
Month = {{AUG}},
Abstract = {{Objective: To compare the concentration of non-protein nitrogen (NPN)
   and free amino acids (FAA) in powdered and liquid commercial formulas
   with that in human milk.
   Methods: The non-protein nitrogen and FAAs in pooled breast milk was
   compared with that in 11 protein-modified starting infant formulas
   (seven powdered, four liquid whey-predominant formulas) and one powdered
   soy-formula. Human milk was collected at the end of each feeding
   (hindmilk) over 24 hours in a group of 40 healthy lactating women after
   delivery of full-term infants at age one month.
   Results: In human milk glutamic acid plus glutamine and taurine were the
   prevalent amino acids, accounting for around 50\% total FAA. In the
   analysed formulas the total FAA fraction was 10\% or even less than in
   human milk, mostly represented by taurine, while methionine was high in
   soy formula. The sum of glutamic acid and glutamine in all the formulas
   was much lower than in human milk.
   Conclusions: Breastfed infants are supplied with FAA, mainly glutamic
   acid and glutamine, compared to formula-fed counterparts. The different
   FAA intake might be the origin of some functional differences at the
   enteral level between breast- and formula-fed infants.}},
