@article{ WOS:000366871100024,
Author = {Muradyan, Gevorg and Muradyan, Atom Zh},
Title = {{Generalized Bloch states of a two-level atom in the field of
   counterpropagating waves: application to the Kapitza-Dirac diffraction
   problem}},
Journal = {{PHYSICA SCRIPTA}},
Year = {{2015}},
Volume = {{90}},
Number = {{11}},
Month = {{NOV}},
Note = {{21st Nuclear Physics Workshop on Marie and Pierre Curie, Kazimierz
   Dolny, POLAND, SEP 23-28, 2014}},
Organization = {{Maria Curie Sklodowska Univ, Dept Theoret Phys}},
Abstract = {{The Bloch-state approach to atomic translational states is extended out
   of adiabatic following and Raman-Nath approximations, taking into
   account the possible transfer of population from the initially populated
   internal energy level. The presented theory suggests a notion of
   potential energy corresponding to the initially populated internal
   state, and makes a generalization in the treatment of energy spectrum
   band-gap structure. Applying the theory to off-resonant Kapitza-Dirac
   diffraction and two-pulse standing wave atom interferometer (for the
   study of non-coherent processes in the cold atomic gases) we discuss the
   ability of the present approach for large-angle split multipath atom
   interferometry.}},
