@article{ WOS:000285053700023,
Author = {Wei, Guoliang and Wang, Zidong and Shen, Bo},
Title = {{Error-Constrained Filtering for a Class of Nonlinear Time-Varying Delay
   Systems With Non-Gaussian Noises}},
Journal = {{IEEE TRANSACTIONS ON AUTOMATIC CONTROL}},
Year = {{2010}},
Volume = {{55}},
Number = {{12}},
Pages = {{2876-2882}},
Month = {{DEC}},
Abstract = {{In this technical note, the quadratic error-constrained filtering
   problem is formulated and investigated for discrete time-varying
   nonlinear systems with state delays and non-Gaussian noises. Both the
   Lipschitz-like and ellipsoid-bounded nonlinearities are considered. The
   non-Gaussian noises are assumed to be unknown, bounded, and confined to
   specified ellipsoidal sets. The aim of the addressed filtering problem
   is to develop a recursive algorithm based on the semi-definite programme
   method such that, for the admissible time-delays, nonlinear parameters
   and external bounded noise disturbances, the quadratic estimation error
   is not more than a certain optimized upper bound at every time step. The
   filter parameters are characterized in terms of the solution to a convex
   optimization problem that can be easily solved by using the
   semi-definite programme method. A simulation example is exploited to
   illustrate the effectiveness of the proposed design procedures.}},
