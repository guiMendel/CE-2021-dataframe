@article{ WOS:000232756700167,
Author = {Saharan, V and Roberts, J and Manev, V and Chia, YH and MacLean, G and
   McMullen, SR},
Title = {{Moisture-uptake by the positive active material from the casting solvent
   and the ambient environment}},
Journal = {{JOURNAL OF POWER SOURCES}},
Year = {{2005}},
Volume = {{146}},
Number = {{1-2, SI}},
Pages = {{809-812}},
Month = {{AUG 26}},
Note = {{12th International Meeting on Lithium Batteries, Nara, JAPAN, JUN 27-JUL
   02, 2004}},
Abstract = {{It is widely recognized that the presence of residual moisture has
   significant detrimental effects on the performance of lithium ion
   batteries. Studies have shown that the positive active material can be a
   major source of moisture contributors to the overall cell moisture. The
   aim of the present study is to understand the factors affecting moisture
   uptake by the doped-lithium nickel cobalt oxide material from the
   casting solvent, acetone, and the ambient environment. As-is and the
   air-dried powders under various conditions (300 degrees C for 24 h, 500
   degrees C for 8 h, and 500 degrees C for 24 h) were exposed to ambient
   and humid air for various lengths of time. Similarly, all these powders
   were exposed to extra dry (47 ppm) and wet (5789 ppm) acetone. Karl
   Fisher measurements at 160 degrees C and 290 degrees C show that
   humidity levels (80-85\% relative humidity versus 20-25\%) and the
   exposure times are the critical factors. Acetone wetness and the length
   of exposure in it do not contribute significantly to the moisture uptake
   by the active material. Cathode powder drying helps in minimizing the
   amount of moisture uptake from the environment. (c) 2005 Elsevier B.V.
   All rights reserved.}},
