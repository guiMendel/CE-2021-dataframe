@article{ WOS:000088527100008,
Author = {van der Burgt, YEM and Bergsma, J and Bleeker, IP and Mijland, PJHC and
   Kamerling, JP and Vliegenthart, JFG},
Title = {{Substituent distribution in highly branched dextrins from methylated
   starches}},
Journal = {{CARBOHYDRATE RESEARCH}},
Year = {{2000}},
Volume = {{327}},
Number = {{4}},
Pages = {{423-429}},
Month = {{AUG 7}},
Abstract = {{Granular potato starch and amylopectin potato starch were methylated to
   molar substitutions (MS) up to 0.29. Extensive alyha-amylase digestion
   gave mixtures of partially methylated oligomers. Precipitation of larger
   fragments by methanol yielded mainly alpha-limit dextrins (84-99\%).
   Methanol precipitates were extensively digested with beta-amylase
   yielding alpha,beta-limit dextrins. The average substitution level of
   branched glucose residues in the dextrins thus obtained was determined
   after per deuteriomethylation by using FAB mass spectrometry, and
   compared with that of the linearly linked glucose residues. The present
   work demonstrates that methylation does not show any preference for
   substitution at either branched or linearly linked glucose residues,
   taking into account the inherently lower amount of substitution sites at
   branched residues. The results corroborate earlier studies wherein it
   was found that substituents in branched regions are distributed almost
   randomly. In addition, the data enable the determination of the average
   degree of branching of partially methylated dextrins. (C) 2000 Elsevier
   Science Ltd. All rights reserved.}},
