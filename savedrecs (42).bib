@article{ WOS:000165549100018,
Author = {Kreisel, J and Glazer, AM},
Title = {{Estimation of the compressibility of Na0.5Bi0.5TiO3 and related
   perovskite-type titanates}},
Journal = {{JOURNAL OF PHYSICS-CONDENSED MATTER}},
Year = {{2000}},
Volume = {{12}},
Number = {{46}},
Pages = {{9689-9698}},
Month = {{NOV 20}},
Abstract = {{We report on the estimation of the compressibility of Na0.5Bi0.5TiO3 and
   its constituent polyhedra through two different approaches: the
   so-called polyhedral approach and that based on the pressure dependence
   of particular Raman-active modes. In order to investigate the
   reliability of the results obtained for Na0.5Bi0.5TiO3, the two
   approaches have also been applied to perovskite-type titanates, ATiO(3)
   (A = Ca, Ba, Sr, Pb), and the results are compared with compressibility
   data reported in the literature. The comparative study shows that (i)
   ATiO(3) compressibilities are overestimated by the polyhedral approach,
