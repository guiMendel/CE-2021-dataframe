@article{ WOS:000365747300004,
Author = {Lied, Liv Ingeborg},
Title = {{Text-Work-Manuscript: What Is an `Old Testament Pseudepigraphon'?}},
Journal = {{JOURNAL FOR THE STUDY OF THE PSEUDEPIGRAPHA}},
Year = {{2015}},
Volume = {{25}},
Number = {{2}},
Pages = {{150-165}},
Month = {{DEC}},
Abstract = {{The 2013 volume Old Testament Pseudepigrapha: More Noncanonical
   Scriptures, edited by Richard Bauckham, James R. Davila and Alexander
   Panayotov, is a highly important contribution to the field of
   Pseudepigrapha studies, making previously unpublished material available
   for further study. This review essay discusses the editorial strategies
   that have shaped this volume, focusing in particular on the
   representation of its basic building block, the pseudepigraphon.
   Exploring two entries in the volume, `The Book of Noah' and `The Story
   of Melchizedek with the Melchizedek Legend and the Chronicon Paschale',
   this article demonstrates how privileging the early `work' as the
   default mode of representation creates imaginations of pseudepigrapha
   that may not match the manuscript sources that have in fact survived.}},
