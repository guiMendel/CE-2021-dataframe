Language = {{English}},
Affiliation = {{Menjivar, M (Corresponding Author), Univ Nacl Autonoma Mexico, Dept Biol, Fac Quim, Lab 209, Edif B,2 Piso,Ciudad Univ, Mexico City 04510, DF, Mexico.
   Univ Nacl Autonoma Mexico, Dept Biol, Fac Quim, Lab 209, Mexico City 04510, DF, Mexico.
   Hosp Juarez Mexico, Dept Reprod Biol, Mexico City, DF, Mexico.}},
DOI = {{10.1095/biolreprod63.5.1549}},
ISSN = {{0006-3363}},
