@article{ WOS:000232296200032,
Author = {Zou, BD and Aodeng, GW and Shang, HS and Qi, L},
Title = {{Simultaneous determination of germanium and some major ash forming
   elements in lignites using a dual-view ICP-OES}},
Journal = {{SPECTROSCOPY AND SPECTRAL ANALYSIS}},
Year = {{2005}},
Volume = {{25}},
Number = {{9}},
Pages = {{1496-1499}},
Month = {{SEP}},
Abstract = {{In this article, an ICP-OES method of simultaneous determination
   germanium and some major ash forming elements especially for Fe, Al, Ca,
   Mg, K, Na, Ti in lignites is described. An ICP-OES instrument equipped
   with a dual-view plasma torch and a simultaneous detector was used. This
   technology allows the simultaneous determination of trace elements and
   major components in the axial view along with in the radial view. The
   moderate operation condition of the instrument and analytical wavelength
   lack of interference from elements in the sample matrix was selected,
   the spectral background was automaticly eliminated. The obtained coal
   ashes were dissolved in mixtures of nitric acid, hydrofluoric acid and
   perchloric acid after coal samples were cinerated. The analytical
   results were in good agreement with those provided by national standard
   methods. The detection limit of the instrument is ranged between 0.000
   39-0.10 mu g. center dot mL(-1), the precision of the method is in the
   range of 0.79\%-2.84\%, the average recovery of samples is 92.38\%, the
   proposed method is well suitable for the determination germanium and
   some major ash forming elements in lignites.}},
