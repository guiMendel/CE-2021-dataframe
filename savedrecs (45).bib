@article{ WOS:000089981600007,
Author = {Orosz, S and Mezes, M and Vetesi, M and Erdelyi, M and Kiss, L},
Title = {{Investigation on the effect of additional bile salts on growth and blood
   parameters in Peking duck fed with barley-based diet}},
Journal = {{ARCHIV FUR GEFLUGELKUNDE}},
Year = {{2000}},
Volume = {{64}},
Number = {{5}},
Pages = {{224-230}},
Month = {{OCT}},
Abstract = {{The effect of additive bile salts on Peking ducks, fed barley-based
   diet, were studied. During the growing period (48 days) control and 45\%
   barley-based diets were isonitrogenous and isoenergetic. Additional bile
   salts content was 0.3\% in the 45\% barley-based diet. Performance
   traits, 3 alpha -hydroxy bile acid, VLDL, triglyceride and cholesterol
   concentration of the blood plasma were determined.
   The 3 alpha -hydroxy bile acid content in the plasma increased with age
   in table ducks. The highest activity of synthesis and reabsorption of
   bile acids was detected within the last two weeks in the case of female
   ducks. The 45\% bailey-based diet decreased the plasma 3 alpha -hydroxy
   bile acid concentration on the 21(st), 35(th) and 48(th) day of age in
   both sexes compared to control. It suggests that the soluble NSPs
   enriched barley reduces the reabsorption of bile acids, exerting an
   indirect negative effect on the entrrohepatic cycle in table duck. The
   additional 0.3\% bile salts increased the 3 alpha -hydroxy bile acid
   concentration of the blood plasma on the 21(st), 35(th) and 48(th) day
   of age in both sexes acid compensated the bile acid lowering effect of
   45\% barley in the diet.}},
