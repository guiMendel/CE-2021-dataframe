@article{ WOS:000616180800003,
Author = {Gus'kov, I. A. and Muslimov, E. R. and Mel'nikov, A. N. and Gil'fanov,
   A. R.},
Title = {{Design procedure for a holographic display considering the diffraction
   efficiency of a volume phase hologram}},
Journal = {{JOURNAL OF OPTICAL TECHNOLOGY}},
Year = {{2020}},
Volume = {{87}},
Number = {{11}},
Pages = {{650-657}},
Month = {{NOV 1}},
Abstract = {{A procedure for the design of optical schemes of holographic displays is
   developed. The procedure implies the simultaneous optimization of the
   image quality factors and the root-mean-square values of the hologram
   diffraction efficiency at the control points. The procedure is
   implemented at the level of specific software tools and utilized for the
   design of an example scheme of a waveguide holographic display. The
   display operates within the 480-620 nm spectral range and has a 10
   degrees angular field of view and an exit pupil diameter of 10 mm. The
   input of radiation into the waveguide plate and the radiation output are
   achieved using diffractive elements operating in the transmitted light.
   The output element in this case is a volume phase hologram recorded by
   two point sources. The calculation results show that the implementation
   of the proposed design makes it possible to reduce the radius of the
   circle of confusion in terms of the angular measure by 0.28 `-0.97 `.
   The diffraction efficiency of the output hologram in this case is closer
   to the target value by 2.5\% to 19\%. (C) 2021 Optical Society of
   America}},
