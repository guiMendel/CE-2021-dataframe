@article{ WOS:000089712900027,
Author = {D'Archivio, AA and Galantini, L and Tettamanti, E},
Title = {{Study on intermicellar interactions and micellar size in aqueous
   solutions of sodium taurocholate by measurements of collective diffusion
   and self-diffusion coefficients}},
Journal = {{JOURNAL OF PHYSICAL CHEMISTRY B}},
Year = {{2000}},
Volume = {{104}},
Number = {{39}},
Pages = {{9255-9259}},
Month = {{OCT 5}},
Abstract = {{Aqueous micellar solutions of sodium taurocholate (NaTC) have been
   studied by using quasi-elastic lightscattering (QELS) and H-1
   pulse-gradient-stimulated-echo NMR (PGSTE-NMR). The collective and
   selfdiffusion coefficients have been obtained as a function of NaTC
   concentration (0.05-0.20 M) and the ionic strength of the solution
   (0-0.8 M NaCl added). The comparison of the two techniques' data in each
   solution has been performed in the context of micellar growth and
   intermicellar interactions. The model of monodisperse Brownian particles
   with hydrodynamic interaction and hard core repulsion gives rise to a
   satisfactory agreement with the experimental data. The translational
   diffusion coefficient of the single micellar aggregate has been
   calculated for each solution. The size of the micellar aggregates
   depends on the ionic strength and bile salt concentration.}},
