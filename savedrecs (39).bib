@article{ WOS:000233056900002,
Author = {Yagasaki, T},
Title = {{Homotopy types of the components of spaces of embeddings of compact
   polyhedra into 2-manifolds}},
Journal = {{TOPOLOGY AND ITS APPLICATIONS}},
Year = {{2005}},
Volume = {{153}},
Number = {{2-3}},
Pages = {{174-207}},
Month = {{SEP 1}},
Note = {{2nd International Conference on Geometric Topology, Dubrovnik, CROATIA,
   SEP 29-OCT 05, 2002}},
Abstract = {{Suppose M is a connected PL 2-manifold and X is a compact connected
   subpolyhedron of M (X not equal 1 pt, a closed 2-manifold). Let
   epsilon(X, M) denote the space of topological embeddings of X into M
   with the compact-open topology and let epsilon(X, M)(0) denote the
   connected component of the inclusion i(X): X subset of M in epsilon(X,
   M). In this paper we classify the homotopy type of epsilon(X, M)(0) in
   terms of the subgroup G = Im{[}i(X{*}):pi(1)(X) -> pi(1)(M)]. We show
   that if G is not a cyclic group and M not equal T-2, K-2 then epsilon(X,
   M)(0) similar or equal to {*}, if G is a nontrivial cyclic group and M
   not similar or equal to P-2, T-2, K-2 then epsilon(X, M)(0) similar or
   equal to S-1, and when G = 1, if X is an arc or M is orientable then
   epsilon(X, M)(0) similar or equal to ST(M) and if X is not an arc and M
   is nonorientable then epsilon(X, M)(0) similar or equal to ST(M). Here
   O-1 is the circle, T-2 is the torus, P-2 is the projective plane and K-2
   is the Klein bottle. The symbol ST(M) denotes the tangent unit circle
   bundle of M with respect to any Riemannian metric of M and when M is
   nonorientable. M denotes the orientable double cover of M. (c) 2004
   Elsevier B.V. All rights reserved.}},
