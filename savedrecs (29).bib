@article{ WOS:000283072400045,
Author = {Cescato, Davide and Boelcskei, Helmut},
Title = {{QR Decomposition of Laurent Polynomial Matrices Sampled on the Unit
   Circle}},
Journal = {{IEEE TRANSACTIONS ON INFORMATION THEORY}},
Year = {{2010}},
Volume = {{56}},
Number = {{9}},
Pages = {{4754-4761}},
Month = {{SEP}},
Abstract = {{We consider Laurent polynomial (LP) matrices defined on the unit circle
   of the complex plane. QR decomposition of an LP matrix A(s) yields QR
   factors Q(s) and R(s) that, in general, are neither LP nor rational
   matrices. In this paper, we present an invertible mapping that
   transforms Q(s) and R(s) into LP matrices. Furthermore, we show that,
   given QR factors of sufficiently many samples of A(s), it is possible to
   obtain QR factors of additional samples of A(s) through application of
   this mapping followed by interpolation and inversion of the mapping. The
   results of this paper find applications in the context of signal
   processing for multiple-input multiple-output (MIMO) wireless
   communication systems that employ orthogonal frequency-division
   multiplexing (OFDM).}},
