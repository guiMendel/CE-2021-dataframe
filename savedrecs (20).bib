@article{ WOS:000285735000002,
Author = {Meisinger, Christa and Heier, Margit and von Scheidt, Wolfgang and
   Kirchberger, Inge and Hoermann, Allmut and Kuch, Bernhard},
Title = {{Gender-Specific Short and Long-Term Mortality in Diabetic Versus
   Nondiabetic Patients With Incident Acute Myocardial Infarction in the
   Reperfusion Era (the MONICA/KORA Myocardial Infarction Registry)}},
Journal = {{AMERICAN JOURNAL OF CARDIOLOGY}},
Year = {{2010}},
Volume = {{106}},
Number = {{12}},
Pages = {{1680-1684}},
Month = {{DEC 15}},
Abstract = {{The aim of this study was to investigate gender-specific short- and
   long-term mortalities after a first acute myocardial infarction (AMI) in
   patients with and without diabetes mellitus (DM). The study was based on
   505 men and 196 women with DM and 1,327 men and 415 women without DM
   consecutively hospitalized with a first-ever AMI from January 1998 to
   December 2003 recruited from a population-based MI registry. Patients
   were followed until December 31, 2005 (median follow-up time 4.3 years).
   In men and women, no significantly independent association between DM
   and short-term mortality was observed. After multivariable adjustment
   odds ratios (95\% confidence intervals {[}CIs]) for 28-day case fatality
   were 1.45 (95\% CI 0.90 to 2.34) in men with DM compared to men without
   DM and 1.44 (95\% CI 0.66 to 3.15) in women with DM compared to women
   without DM. Conversely, in 28-day AMT survivors DM was significantly
   associated with long-term mortality in age-adjusted analyses, in which
   men with DM had a hazard ratio (RR) of 1.57 (95\% CI 1.18 to 2.10) for
   all-cause mortality compared to non-DM men; the corresponding HR in
   women with DM was 2.91 (95\% CI 1.82 to 4.65). After multivariable
   adjustment the strong association in women with DM remained significant
   (HR 2.56, 95\% CI 1.53 to 4.27); however, in men with DM it became
   borderline significant (HR 1.36, 95\% CI 1.00 to 1.85). In conclusion,
   short-term mortality was not significantly increased in men and women
   with DM after a first-ever AMI, although estimates were relatively high,
   indicating a possible relation. However, long-term mortality was higher
   in patients with AMI and DM, particularly in women. (C) 2010 Elsevier
   Inc. All rights reserved. (Am J Cardiol 2010;106:1680-1684)}},
