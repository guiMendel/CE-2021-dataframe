@article{ WOS:000232546500015,
Author = {Guzenko, AF and Yudin, AL and Yatsyna, NA and Kolesnikov, LV},
Title = {{Adsorption of imidacarbocyanine polymethine dyes on silver bromide
   surface}},
Journal = {{RUSSIAN JOURNAL OF PHYSICAL CHEMISTRY}},
Year = {{2005}},
Volume = {{79}},
Number = {{10}},
Pages = {{1638-1641}},
Month = {{OCT}},
Abstract = {{A variety of polymethine cyanine dyes, both free and adsorbed on AgBr
   microcrystals, are characterized by XPS. Oxaimidacarbocyanine dye
   molecules are adsorbed through interaction between their terminal alkyl
   sulfite groups and surface silver ions of the AgBr lattice. The chlorine
   substituents in the dye molecules are unstable to X-ray and UV
   irradiation. The adsorption of spectral sensitizers modifies their
   electronic state and the composition of the AgBr surface, as is
   indicated by changes in the photoelectron spectrum. It is inferred that
