@article{ WOS:000233207200014,
Author = {Yokoyama, S and Nakano, H and Nishizawa, A and Kaneko, T and Harada, K
   and Hanada, K},
Title = {{A case of photocontact urticaria induced by photodynamic therapy with
   topical 5-aminolaevulinic acid}},
Journal = {{JOURNAL OF DERMATOLOGY}},
Year = {{2005}},
Volume = {{32}},
Number = {{10}},
Pages = {{843-847}},
Month = {{OCT}},
Abstract = {{Photodynamic therapy (PDT) with topical application of 5-aminolaevulinic
   acid (ALA) is a promising new treatment option for the management of
   various cutaneous malignancies. Generally, topical ALA-based PDT has
   relatively insignificant adverse effect,; of transient character; these
   include itching, stinging or burning pain and slight to moderate
   erythema. We describe the first case of photocontact urticaria induced
   by topical ALA-based PDT for the treatment of unilesional mycosis
   fungoides. Although the first treatment session resulted merely in mild
   erythema, the second PDT caused marked urticaria corresponding to the
   PDT-applied area with an intolerable stinging sensation. A photopatch
   test demonstrated that black light and visible light irradiation after
   topical ALA Provoked an urticarial reaction in the patient's uninvolved
   skin. These observations suggested an allergic pathogenesis for the
   wheal formation induced by PDT with topical ALA in this case.
   Photocontact urticaria should be considered as a possible adverse effect
   in ALA-based PDT.}},
