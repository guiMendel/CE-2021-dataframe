@article{ WOS:000233443800008,
Author = {Barker, EN and Campbell, S and Tebb, AJ and Neiger, R and Herrtage, ME
   and Reid, SWJ and Ramsey, IK},
Title = {{A comparison of the survival times of dogs treated with mitotane or
   trilostane for pituitary-dependent hyperadrenocorticism}},
