@article{ WOS:000363821600014,
Author = {Mclaughlin, Milena M. and Skoglund, Erik and Bergman, Scott and Scheetz,
   Marc H.},
Title = {{Development of a pharmacy student research program at a large academic
   medical center}},
Journal = {{AMERICAN JOURNAL OF HEALTH-SYSTEM PHARMACY}},
