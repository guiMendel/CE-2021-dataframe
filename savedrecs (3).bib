@article{ WOS:000590387000002,
Author = {Gao, Hui and Liu, Tong},
Title = {{LOOSE CRYSTALLINE LIFTS AND OVERCONVERGENCE OF STALE (phi, tau)-MODULES}},
Journal = {{AMERICAN JOURNAL OF MATHEMATICS}},
Year = {{2020}},
Volume = {{142}},
Number = {{6}},
Pages = {{1733-1770}},
Month = {{DEC}},
Abstract = {{Let p be a prime. K a finite extension of Q(p), and let G(K) be the
   absolute Galois group of K. The category of Stale (phi, tau)-modules is
   equivalent to the category of p-adic Galois representa- tions of G(K).
   In this paper, we show that all etale (phi, tau)-modules are
   overconvergent; this answers a question of Caruso. Our result is an
   analogue of the classical overconvergence result of Cherbonnier and
   Colmez in the setting of etale (phi, Gamma)- modules. However, our
   method is completely different from theirs. Indeed, we first show that
   all p-power-torsion representations admit loose crystalline lifts; this
   allows us to construct certain Kisin models in these torsion
   representations. We study the structure of these Kisin models, and use
   them to build an overconvergence basis.}},
