@article{ WOS:000284433100002,
Author = {Whelan, Simon and Money, Daniel},
Title = {{The Prevalence of Multifurcations in Tree-space and Their Implications
   for Tree-search}},
Journal = {{MOLECULAR BIOLOGY AND EVOLUTION}},
Year = {{2010}},
Volume = {{27}},
Number = {{12}},
Pages = {{2674-2677}},
Month = {{DEC}},
Abstract = {{Phylogenetic tree-search is a major aspect of many evolutionary studies.
   Several tree rearrangement algorithms are available for tree-search, but
   it is hard to draw general conclusions about their relative performance
   because many effects are data set specific and can be highly dependent
   on individual implementations (e.g., RAxML or phyml). Using only the
   structure of the rearrangements proposed by the Nearest Neighbor
   Interchange (NNI) algorithm, we show tree-search can prematurely
   terminate if it encounters multifurcating trees. We validate the
   relevance of this result by demonstrating that in real data the majority
   of possible bifurcating trees potentially encountered during tree-search
   are actually multifurcations, which suggests NNI would be expected to
   perform poorly. We also show that the star-decomposition algorithm is a
   special case of two other popular tree-search algorithms, subtree
   pruning and regrafting (SPR) and tree bisection and reconnection (TBR),
   which means that these two algorithms can efficiently escape when they
   encounter multifurcations. We caution against the use of the NNI
   algorithm and for most applications we recommend the use of more robust
   tree-search algorithms, such as SPR and TBR.}},
