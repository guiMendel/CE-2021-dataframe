@article{ WOS:000231747600009,
Author = {Honkanen, P and Olsen, SO and Verplanken, B},
Title = {{Intention to consume seafood - the importance of habit}},
Journal = {{APPETITE}},
Year = {{2005}},
Volume = {{45}},
Number = {{2}},
Pages = {{161-168}},
Month = {{OCT}},
Abstract = {{The role of habit strength and past behaviour were studied in order to
   gain a better understanding of seafood consumption behaviour. A sample
   of Norwegian adults (N = 1579) responded to a self-administered
   questionnaire about seafood consumption habits, past frequency of
   seafood consumption, and attitude towards and intention to eat seafood.
   Structural equation modelling revealed that past behaviour and habit,
   rather than attitudes, were found to explain differences in intention,
   indicating that forming intention does not necessarily have to be
   reasoned. The results also indicated that When a strong habit is
   present, the expression of an intention might be guided by the salience
   of past behaviour rather than by attitudes. The findings of this study
   might thus have consequences for dietary interventions. (c) 2005
   Elsevier. Ltd. All rights reserved.}},
