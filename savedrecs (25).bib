@article{ WOS:000283621300124,
Author = {Zhang, Hui and Zhang, Hui and Tang, Longcheng and Liu, Gang and Zhang,
   Daijun and Zhou, Lingyun and Zhang, Zhong},
Title = {{The Effects of Alumina Nanofillers on Mechanical Properties of
   High-Performance Epoxy Resin}},
Journal = {{JOURNAL OF NANOSCIENCE AND NANOTECHNOLOGY}},
Year = {{2010}},
Volume = {{10}},
Number = {{11, SI}},
