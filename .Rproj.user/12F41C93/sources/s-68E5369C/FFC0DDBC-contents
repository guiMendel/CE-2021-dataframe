columnsToKeep <- c(
  "AUTHOR",
  "JOURNAL",
  "MONTH",
  "PUBLISHER",
  "TITLE",
  "YEAR",
  "ABSTRACT",
  "LANGUAGE",
  "AFFILIATION",
  "DOI",
  "KEYWORDS.PLUS",
  "RESEARCH.AREAS",
  "NUMBER.OF.CITED.REFERENCES",
  "TIMES.CITED",
  "USAGE.COUNT.LAST.180.DAYS",
  "USAGE.COUNT.SINCE.2013"
)

# Only keeps the columns of interest form the data frame
filterColumns <- function(df) {
  return(df[columnsToKeep])
}
