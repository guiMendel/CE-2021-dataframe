@article{ WOS:000368913900003,
Author = {Alterman, Julia F. and Hall, Lauren M. and Coles, Andrew H. and Hassler,
   Matthew R. and Didiot, Marie-Cecile and Chase, Kathryn and Abraham,
   Jasmin and Sottosanti, Emily and Johnson, Emily and Sapp, Ellen and
   Osborn, Maire F. and Difiglia, Marian and Aronin, Neil and Khvorova,
   Anastasia},
Title = {{Hydrophobically Modified siRNAs Silence Huntingtin mRNA in Primary
   Neurons and Mouse Brain}},
Journal = {{MOLECULAR THERAPY-NUCLEIC ACIDS}},
Year = {{2015}},
Volume = {{4}},
Month = {{DEC 1}},
Abstract = {{Applications of RNA interference for neuroscience research have been
   limited by a lack of simple and efficient methods to deliver
   oligonucleotides to primary neurons in culture and to the brain. Here,
   we show that primary neurons rapidly internalize hydrophobically
   modified siRNAs (hsiRNAs) added directly to the culture medium without
   lipid formulation. We identify functional hsiRNAs targeting the mRNA of
   huntingtin, the mutation of which is responsible for Huntington's
   disease, and show that direct uptake in neurons induces potent and
   specific silencing in vitro. Moreover, a single injection of
   unformulated hsiRNA into mouse brain silences Htt mRNA with minimal
   neuronal toxicity. Thus, hsiRNAs embody a class of therapeutic
   oligonucleotides that enable simple and straightforward functional
   studies of genes involved in neuronal biology and neurodegenerative
   disorders in a native biological context.}},
