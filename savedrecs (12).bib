@article{ WOS:000364519800014,
Author = {Harat, Z. Naghibi and Lakpour, N. and Sadeghipoor, H. R. and
   Kamalinejad, M. and Eshraghian, M. R. and Naghibi, B. and Akhondi, M. M.
   and Binaafar, S. and Sadeghi, M. R.},
Title = {{Immobilising effect of Ruta graveolens L. on human spermatozoa: coumarin
   compounds are involved}},
Journal = {{ANDROLOGIA}},
Year = {{2015}},
Volume = {{47}},
Number = {{10}},
Pages = {{1183-1189}},
Month = {{DEC}},
Abstract = {{This study was designed to find out Ruta graveolens L. functional
   components, which have immobilisation effect on human spermatozoa for
   contraceptive use. A five-step fractionation method was used to derive
   different components from rue aqueous extract by using hexane,
   chloroform, ethanol, acetone and ultrapure water. Gas
   chromatography-mass spectrophotometery (GC-MS) of all fractions and the
   aqueous extract were performed to determine the chemical components. The
   immobilisation assay and membrane integrity test were also performed
   with four different coumarins, which were found in GC-MS in a
   concentration of 10m. Hexane, chloroform, acetone and ethanol fractions
   could significantly decrease motility of sperms within the first and the
   second hours. Hexane fraction had also significant immediate effect. The
   aqueous fraction had no effect on sperm motility. Meanwhile, GC-MS
   revealed that aqueous extract and effective fractions had similar
   coumarin compounds. We performed the immobilisation assay on four
   different coumarins, which were found in GC-MS in a concentration of
   10m. Reduction of sperm motility was only significant for xanthotoxin.
   In the sperm viability and membrane integrity tests, hexane and
   ethanolic fractions could impair sperm vitality significantly, in
   contrast to coumarins. These results indicated that a part of
   immobilising effect of rue could be due to its coumarins. The possible
   mechanism could be blocking of spermatozoa potassium channels.}},
Publisher = {{WILEY}},
Address = {{111 RIVER ST, HOBOKEN 07030-5774, NJ USA}},
