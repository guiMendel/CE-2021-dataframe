@article{ WOS:000366177700008,
Author = {Liu, Xinnuan and Zhou, Luozeng and Xu, Qunjie},
Title = {{Preparation and Electrochemical Performance of Li-rich Cathode Material
   Li-1.17{[}Ni0.17Co0.17Mn0.50]O-2 Synthesized by Solid State Method via
