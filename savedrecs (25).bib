@article{ WOS:000282930100027,
Author = {Curiel, M. A. and Nedev, N. and Nesheva, D. and Soares, J. and Haasch,
   R. and Sardela, M. and Valdez, B. and Sankaran, B. and Manolov, E. and
   Bineva, I. and Petrov, I.},
Title = {{Microstructural characterization of thin SiOx films obtained by physical
   vapor deposition}},
Journal = {{MATERIALS SCIENCE AND ENGINEERING B-ADVANCED FUNCTIONAL SOLID-STATE
   MATERIALS}},
Year = {{2010}},
Volume = {{174}},
