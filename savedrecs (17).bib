@article{ WOS:000364770500322,
Author = {Ouyang, Yong and He, Jinliang and Hu, Jun and Zhao, Gen and Wang,
   Zhongxu and Wang, Shan X.},
Title = {{Contactless Current Sensors Based on Magnetic Tunnel Junction for Smart
   Grid Applications}},
Journal = {{IEEE TRANSACTIONS ON MAGNETICS}},
Year = {{2015}},
Volume = {{51}},
Number = {{11}},
Month = {{NOV}},
Note = {{IEEE International Magnetics Conference (Intermag), Beijing, PEOPLES R
   CHINA, MAY 11-15, 2015}},
Organization = {{IEEE; Tsinghua Univ; Inst Phys; Chinese Acad Sci; Beijing Univ; Chinese
   Phys Soc; Chinese Inst Elect; Chinese Mat Res Soc}},
Abstract = {{Advanced sensing techniques are key technologies required for the smart
   grid. Magnetic tunnel junction (MTJ) sensors hold great promises in the
   distributed linear current sensing, with small size, low price, and
   excellent performances. In this paper, MTJ Wheatstone bridge sensors
   were fabricated, attaining a typical sensitivity of 4.94 mV/V/Oe with an
   offset of -2.84 mV/V. A contactless current sensor based on the MTJ
   sensor was designed and realized, achieving a sensitivity of 7.75 mV/A
   and an offset of -8.68 mV. We can adjust the sensitivity and the linear
   range of the sensor by changing the gap of the magnetic core, and make
   the sensor much less susceptible to electrical wire's location and
   orientation. This paper provides a practical current sensing technique
   for smart grid applications.}},
