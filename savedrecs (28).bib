@article{ WOS:000282074600003,
Author = {Koester, R. and Meinertz, T.},
Title = {{I-f Channel Inhibition with Ivabradine: A New Way to Lower Heart Rate}},
Journal = {{DIABETES STOFFWECHSEL UND HERZ}},
Year = {{2010}},
Volume = {{19}},
Number = {{5}},
Pages = {{337-344}},
Month = {{SEP 20}},
Abstract = {{An increased heart rate is considered a cardiovascular risk factor. A
   reduction in heart rate to 55-60 beats/minute is recommended for the
   treatment of chronic stable angina pectoris. Ivabradine is a drug which
   reduces the heart rate by inhibiting the I-f channel in the sinus node
   and by reducing the slope of the diastolic depolarization. The blood
   pressure is not affected. During treatment with ivabradine, there is no
   negative influence on metabolism, as can often be seen in treatment with
   betablockers. Ivabradine showed anti-anginal effects in pre-clinical
   studies, which were comparable to those of betablockers or calcium
   channel blockers. In controlled studies, the addition of therapy with
   ivabradine to standard therapy reduced angina pectoris and the number of
   myocardial infarctions, particularly if heart rate was >= 70
   beats/minute. In patients with angina on exertion, the additional
   treatment with ivabradine was associated with reduced mortality. In
   summary, treatment with ivabradine bears a new potential for the therapy
   of angina pectoris. This potential is not being fully utilized as yet.}},
