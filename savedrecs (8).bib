@article{ WOS:000588522000001,
Author = {Starling, Natalie R. and Vissicchio, Carly and Grottke, Katharine},
Title = {{Opening the Educational Leadership Door: Promoting the Collaboration of
   OBM and Education}},
Journal = {{JOURNAL OF ORGANIZATIONAL BEHAVIOR MANAGEMENT}},
Year = {{2021}},
Volume = {{41}},
Number = {{1}},
Pages = {{32-63}},
Month = {{JAN 2}},
Abstract = {{What is the future of the relationship between Organizational Behavior
   Management (OBM) and education? This article presents a discussion of
   some current issues in education as opportunities for the integration of
   OBM frameworks and practices. Opportunities are examined in the context
   of systems-level supports and leadership in education. We hypothesize
   that an integration of OBM principles and applications has the potential
   to help educators address multiple critical issues in the field of
   education, specifically: (a) educator needs at the individual-level,
   such as educator burnout, engagement, and performance; (b)
   implementation fidelity of system-wide initiatives and evidence-based
   interventions; and (c) educator needs at the leadership-level,
   particularly the training of educational leaders. Recommendations for
   how OBM researchers and practitioners can expand engagement with
   educational settings are explored. Given the impact of organizational
   leadership, we emphasize the future collaboration between OBM and
   educational leadership, specifically the inclusion of OBM principles and
   applications in pre-professional curricula and professional continuing
   education training.}},
