Author = {Chang, Yen-Jung and Lee, Yu-Ting and Gao, Jhih-Rong and Wu, Pei-Ci and
   Wang, Ting-Chi},
Title = {{NTHU-Route 2.0: A Robust Global Router for Modern Designs}},
Journal = {{IEEE TRANSACTIONS ON COMPUTER-AIDED DESIGN OF INTEGRATED CIRCUITS AND
   SYSTEMS}},
