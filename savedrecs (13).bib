@article{ WOS:000370165900003,
Author = {Rezaee, Zahra and Shariat, Mamak and Valadan, Mehrnaz and Ebrahim, Bita
   and Sedighi, Bahareh and Kiumarsi, Mehrnoush and Bandegi, Pooya},
Title = {{Evaluation of Complications and Risk Factors for Umbilical Cord
