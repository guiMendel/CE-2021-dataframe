@article{ WOS:000285825600003,
Author = {Renwick, Chris},
Title = {{Patrick Geddes and the politics of evolution}},
Journal = {{ENDEAVOUR}},
Year = {{2010}},
Volume = {{34}},
Number = {{4}},
Pages = {{151-156}},
Month = {{DEC}},
Abstract = {{Ever since they began to be widely discussed during the early nineteenth
   century, evolutionary ideas have played a controversial role in debates
   about politics and social reform. Understanding the political
   commitments of those who have sought to integrate politics and evolution
   is a complex challenge, though; not least because memories of
   mid-twentieth-century eugenic policies have frequently shaped how we
   talk about biosocial science. However, as the case of the Scottish
   biologist-turned-town-planner Patrick Geddes highlights, while we need
   to be aware of the broad appeal that biosocial science has historically
   held, we also need to recognise that current political categories can be
   misleading when thinking about those of who have put evolution and
   politics together.}},
