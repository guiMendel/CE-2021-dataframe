@article{ WOS:000575197000101,
Author = {Kayathi, Aparna and Chakrabarti, Prodosh Prasad and Bonfim-Rocha, Lucas
   and Cardozo-Filho, Lucio and Jegatheesan, Veeriah},
Title = {{Selective extraction of polar lipids of mango kernel using Supercritical
   Carbon dioxide (SC-CO2) extraction: Process optimization of extract
   yield/phosphorous content and economic evaluation}},
Journal = {{CHEMOSPHERE}},
Year = {{2020}},
Volume = {{260}},
Month = {{DEC}},
Abstract = {{Lipids are biomolecules extracted from plant sources and plant residues
   and have a beneficial role in various food, nutrition and medical
   applications. Supercritical carbon-dioxide as an advanced highpressure
   technology which increases the productivity and has negligible
   environmental impact is employed for the selective extraction of polar
   lipids from the lipid matrix in mango kernel for the first time. The
