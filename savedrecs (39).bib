@article{ WOS:000232503700007,
Author = {Yang, GY and Hao, QT and Ji, WQ and Jia, WP and He, Z},
Title = {{Effects of Ca addition on the microstructures and mechanical properties
   of Mg-5Al-0.4Zn based casting alloy}},
Journal = {{ACTA METALLURGICA SINICA}},
Year = {{2005}},
Volume = {{41}},
Number = {{9}},
Pages = {{933-939}},
Month = {{SEP}},
Abstract = {{In the sand-casting magnesium alloys Mg-5Al-0.4Zn-0.2Mn-Sr-Ti-xCa,
   existed alpha-Mg, (alpha+ Al2Ca) eutectic and Mg17Al12 when x=0.37,
   0.70, 0.93, 1.69, or alpha-Mg and (alpha+(Mg, Al)(2)Ca) eutectic when
   x=2.93. With increasing x, the room temperature mechanical properties
   (Ob, 6) of the alloys decreased, meanwhile the strength at 200 degrees C
   increased first and then decreased slightly, and the elongation
   decreased rapidly. The yield strength at 200 degrees C (sigma(0.2,200
   degrees C)) of the alloys was greatly improved with the increase of x
   value. DSC analysis proved that the thermal stability of Mg17Al12 phase
   was improved by dissolution of Ca. The grain boundary strength at 200
   degrees C was improved through the formation of the Al2Ca or (Mg,
   Al)(2)Ca phases at the grain boundary. TEM analysis shows that the
   dislocations slip not only at the basal slip plane (0001), but also at
   the (10 (1) over bar0) prismatic slip plane and ((1) over bar(1) over
   bar 21) pyramidal slip plane at 200 degrees C. The dislocation can be
   piled up by Al2Ca phases at the grain boundary resulting in the higher
   yield strength of the alloy containing Ca than those without Ca at 200
   degrees C.}},
