@article{ WOS:A1995TF82700003,
Author = {MINEVA, T and RUSSO, N and SICILIA, E and TOSCANO, M},
Title = {{SPECTROSCOPIC CONSTANTS OF SIH2, GEH2, SNH2, AND THEIR CATIONS AND
