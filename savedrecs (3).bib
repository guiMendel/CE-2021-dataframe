@article{ WOS:000602690300066,
Author = {Cochran, David and Cook, Gates and Rensing, Adam and Frimberger, Dominic
   and Meenakshi-Sundaram, Bhalaajee},
Title = {{A Rare Case of T-Cell Lymphoma Presenting With Bilateral Nephrolithiasis
   and Acute Renal Failure}},
Journal = {{UROLOGY}},
Year = {{2020}},
Volume = {{146}},
Pages = {{222-224}},
Month = {{DEC}},
Abstract = {{We present the rare case of a young boy who was found to have T-cell
   lymphoma after presenting with flank pain and bilateral nephrolithiasis.
   He initially underwent bilateral ureteral stent placement but returned
   with oliguria and acute renal failure. His subsequent workup revealed
   lymphoma involving both kidneys. He was started on chemotherapy for his
   lymphoma and dialysis for his renal failure. His stones ultimately
   dissolved with aggressive hydration and correction of serum uric acid
   levels. In this report, we discuss the identification and management of
   this rare condition. (C) 2020 Elsevier Inc.}},
