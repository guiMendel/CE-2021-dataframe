@article{ WOS:000232970100001,
Author = {Huang, L and Weil, MH and Sun, SJ and Cammarata, G and Cao, L and Tang,
   WC},
Title = {{Levosimendon improves postresuscitation outcomes in a rat model of CPR}},
Journal = {{JOURNAL OF LABORATORY AND CLINICAL MEDICINE}},
Year = {{2005}},
Volume = {{146}},
Number = {{5}},
Pages = {{256-261}},
Month = {{NOV}},
Abstract = {{In this study we sought to determine whether a calcium sensitizer,
   levosimendan, would have a more favorable effect on postresuscitation
   myocardial function and, consequently, postresuscitation survival than
   beta-adrenergic dobutamine. The extreme decrease in survival before
   hospital discharge of resuscitated victims is attributed, in part, to
   postresuscitation myocardial failure, and dobutamine has been
   recommended for the management of postresuscitation myocardial failure.
   We studied a total of 15 animals. Ventricular fibrillation was induced
   in Sprague-Dawley rats weighing 450 to 550 g. Cardiopulmonary
   resuscitation (CPR), including chest compressions and mechanical
   ventilation, was begun after 8 minutes of untreated cardiac arrest.
   Electrical defibrillation was attempted after 6 minutes of CPR. Each
   animal was resuscitated. Animals were randomized to undergo treatment
   with levosimendan, dobutamine, or saline-solution placebo. These agents
   were administered 10 minutes after the return of spontaneous
   circulation. Levosimendan was administered in a loading dose of 12 mu g
   kg(-1) over a 10-minute period, followed by infusion of 0.3 mu g kg(-1)
   min(-1) over the next 230 minutes. Dobutamine was continuously infused
   of a dosage of 3 mu g kg(-1) min(-1). Saline-solution placebo was
   administered in the same volume and over the same amount of time as
   levosimendan. Levosimendan and dobutamine produced comparable increases
   in cardiac output and rate of left-ventricular pressure increase.
   However, administration of levosimendan resulted in lower heart rates
   and lesser increases in left ventricular diastolic pressure compared
   with both dobutamine and placebo. The duration of postresuscitation
   survival was significantly greater with levosimendan (16 +/- 2 hours),
   intermediate with dobutamine (11 +/- 2 hours) and least with
   saline-solution placebo (8 +/- 1 hour). Levosimendan and dobutamine both
   improved postresuscitation myocardial function. However, levosimendan
   produced more favorable postresuscifation myocardial function and
   increased the duration of postresuscitation survival.}},
