@article{ WOS:000176458800010,
Author = {Correa, ES and Euclides, K and Alves, RGD},
Title = {{Evaluation of a beef cattle production system. 2. Weight performance}},
Journal = {{REVISTA BRASILEIRA DE ZOOTECNIA-BRAZILIAN JOURNAL OF ANIMAL SCIENCE}},
Year = {{2000}},
Volume = {{29}},
Number = {{6, 1}},
Pages = {{1986-1995}},
Month = {{NOV-DEC}},
Abstract = {{The weighted performance data of animals from the ``Beef Cattle
   Production System{''} at Embrapa Beef Cattle were analyzed. The weights
   were corrected at the various age-standard and the model included the
   effects of calf's year (A) and month of birth (M), age of cow at calving
   (C), call sex (S), and A x M, A x S, M x S and C x S interactions. The
   205-day mean weight was 147,8 kg and it was affected by S, C and A x M.
   Male calves were 4.9\% heavier than females at 205-day mean weight,
   Calves from cows of C 1 (3 years old or less), 3 (5 years) and 7 (12
   years and above) showed weights around the mean, calves from cows of C 2
   (4 years) and 5 (8 and 9 years) were 4 kg lighter than the mean, while
   calves from cows of C 4 (6 and 7 years) and 6 (10 to 12 years) weighted
   2 and 6 kg above the mean, respectively. The 365-day mean weight was
   161.4 kg and it was affected by S and A x M, The male calves were 53\%
   heavier than the females at 365-day mean weight. The 550 and 730-day
   weights were 235.4 and 272.1 kg, respectively, which were affected by
   The A x M and A x S. The results showed that first calving cows wean
   calves with weaning weight similar to the mean of the herd, when
   receiving adequate management.}},
