@article{ WOS:A1995TM89900010,
Author = {Nordell, KJ and Miller, GJ},
Title = {{BaCu5Al8: A structure containing interconnected {[}Al8Cu4](2-)
   icosahedra}},
Journal = {{CROATICA CHEMICA ACTA}},
Year = {{1995}},
Volume = {{68}},
Number = {{4}},
Pages = {{825-835}},
Month = {{NOV}},
Abstract = {{BaCu5Al8 was synthesized from stoichiometric amounts of the elements by
   heating in a tube furnace or melting in an are welder. BaCu5Al8 forms a
   variation of the NaZn13 structure in which large Ba2+ cations are
   surrounded by a three-dimensional network of interconnected Cu-centered,
   {[}Al8Cu4](2-) icosahedra. The compound was characterized by both powder
   and single crystal X-ray diffraction. BaCu5Al8 crystallizes in the space
   group <Fm(3)over bar c> (no.226); a = 12.2049(38) Angstrom; Z = 8; R =
   0.018; Goof = 1.16. EuCu5Al8 was characterized by powder X-ray
   diffraction, and lattice parameters were refined from the measured
   a-theta values; a = 11.9715(19) Angstrom. Magnetic susceptibility for
   both BaCu5Al8 and EuCu5Al8, and resistivity measurements on EuCu5Al8 are
   reported. Extended Huckel calculations on stoichiometries ranging from
   BaCu13 to BaCuAl12, demonstrate the tuning of the system's stoichiometry
   to balance the Fermi energy and overlap populations.}},
