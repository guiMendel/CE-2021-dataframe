@article{ WOS:000600839700008,
Author = {Hu, Fan and Zhang, Fan and Zou, Zhentao},
Title = {{R\&D investment under time-inconsistent preferences}},
Journal = {{ECONOMICS LETTERS}},
Year = {{2020}},
Volume = {{197}},
Month = {{DEC}},
Abstract = {{We extend the real options model of R\&D by incorporating
   time-inconsistent preferences. We show that time-inconsistent
   preferences can lead to both under-investment and over-investment in
   R\&D, which depends on the magnitude of technological uncertainty. When
   the technological uncertainty is low, the sophisticated entrepreneurs
   exercise earlier than the time-consistent entrepreneur. However, the
   sophisticated entrepreneurs invest later when the technological
   uncertainty is high. (C) 2020 Elsevier B.V. All rights reserved.}},
