@article{ WOS:000234209000008,
Author = {Bagarello, F},
Title = {{A non-commutative approach to ordinary differential equations}},
Journal = {{INTERNATIONAL JOURNAL OF THEORETICAL PHYSICS}},
Year = {{2005}},
Volume = {{44}},
Number = {{8}},
Pages = {{1193-1216}},
Month = {{AUG}},
Abstract = {{We adapt ideas coming from Quantum Mechanics to develop a
   non-commutative strategy for the analysis of some systems of ordinary
   differential equations. We show that the solution of such a system can
   be described by an unbounded, self-adjoint and densely defined operator
   H which we call, in analogy with Quantum Mechanics, the Hamiltonian of
   the system.
   We discuss the role of H in the analysis of the integrals of motion of
   the system. Finally, we apply this approach to several examples.}},
