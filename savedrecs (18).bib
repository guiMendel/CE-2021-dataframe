@article{ WOS:000364153300009,
Author = {Skrzypacz, Andrzej and Toikka, Juuso},
Title = {{Mechanisms for Repeated Trade}},
Journal = {{AMERICAN ECONOMIC JOURNAL-MICROECONOMICS}},
Year = {{2015}},
Volume = {{7}},
Number = {{4}},
Pages = {{252-293}},
Month = {{NOV}},
Abstract = {{How does feasibility of efficient repeated trade depend on the features
   of the environment such as persistence of values, private information
   about their evolution, or trading frequency? We derive a necessary and
   sufficient condition for efficient, unsubsidized, and voluntary trade,
   which implies that efficient contracting requires sufficient congruence
   of expectations. This translates to bounds on persistence of values and
   on private information about their evolution, and distinguishes
   increasing patience from more frequent interaction; the latter need not
   facilitate efficiency even when the former does. We also discuss
   second-best mechanisms and extend the characterization to general
   dynamic collective choice problems.}},
