@article{ WOS:000578998600013,
Author = {Gao, Chao and Li, Ning and Xue, Chaorui and Wang, Huiqi and Liu, Hantao
   and Chang, Qing and Yang, Jinlong and Hu, Shengliang},
Title = {{Highly improved mechanical performances of polyvinyl butyral through
   fluorescent carbon dots}},
Journal = {{MATERIALS LETTERS}},
Year = {{2020}},
Volume = {{280}},
Month = {{DEC 1}},
Abstract = {{A new application of fluorescent carbon dots (CDs) with low cost and
   environmental friendliness is developed by incorporating them into
   polyvinyl butyral (PVB) polymer matrix. The introduction of CDs
   significantly enhances the mechanical properties of PVB. The yield
   strength, tensile strength, and adhesion strength are 200\%, 144\% and
   13-fold higher than those of pure PVB. Ultra-small size and abundant
   O-related groups endows CDs with superior affinity and strong binding
   force with PVB molecules, giving rise to the remarkable enhancement of
   PVB mechanical properties. Therefore, this work offers a promising
   substitute for PVB as interlayer materials of the automotive windshield.
