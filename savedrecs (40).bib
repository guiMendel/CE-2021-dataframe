@article{ WOS:000166203800004,
Author = {Cheng, JS and Lee, KC and Wang, JL and Tseng, LL and Chou, KJ and Tang,
   KY and Jan, CR},
Title = {{Histamine-induced increases in intracellular free Ca2+ levels in
   hepatoma cells}},
Journal = {{CHINESE JOURNAL OF PHYSIOLOGY}},
Year = {{2000}},
Volume = {{43}},
Number = {{4}},
Pages = {{165-169}},
Month = {{DEC 31}},
Abstract = {{The effect of histamine on intracellular free Ca2+ levels ({[}Ca2+]i) in
   HA22/VGH human hepatoma cells were evaluated using fura-2 as a
   fluorescent Ca2+ dye. Histamine (0.2-5 muM) increased {[}Ca2+](i) in a
   concentration-dependent manner with an EC50 value of about 1 muM, The
   {[}Ca2+](i) response comprised an initial rise, a slow decay, and a
   sustained phase, Extracellular Ca2+ removal inhibited 50\% of the
   {[}Ca2+](i) signal. In Ca2+-free medium, after cells were treated with 1
   muM thapsigargin (an endoplasmic reticulum Ca2+ pump inhibitor), 5 muM
   histamine failed to increase {[}Ca2+](i). After pretreatment with 5
   double up arrow M histamine in Ca2+-free medium for 4 min, addition of 3
   mM Ca2+ induced a {[}Ca2+](i) increase of a magnitude 7-fold greater
   than control. Histamine (5 muM)-induced intracellular Ca2+ release was
   abolished by inhibiting phospholipase C with 2 muM 1-(6-((17 beta
   -3-methoxyestra-1,3,5(10)-trien-17-yl)amino)hexyl)-1H-pyrrole-2,5-dione
   (U73122), and by 5 muM pyrilamine but was not altered by 50 muM
   cimetidine. Together, this study shows that histamine induced
   {[}Ca2+](i) increases in human hepatoma cells by stimulating H1, but not
   H2, histamine receptors. The {[}Ca2+](i) signal was caused by Ca2+
   release from thapsigargin-sensitive endoplasmic reticulum in an inositol
   1,4,5-trisphosphate-dependent manner, accompanied by Ca2+ entry.}},
