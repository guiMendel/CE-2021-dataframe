@article{ WOS:000283371200018,
Author = {Jung, Yong Chae and Cho, Jae Whan},
Title = {{Application of shape memory polyurethane in orthodontic}},
Journal = {{JOURNAL OF MATERIALS SCIENCE-MATERIALS IN MEDICINE}},
Year = {{2010}},
Volume = {{21}},
Number = {{10}},
Pages = {{2881-2886}},
Month = {{OCT}},
Abstract = {{A shape memory polymer wire for orthodontic application was prepared by
   melt-spinning of polyurethane block copolymer (PU) which was synthesized
   in a two-step process from a reaction of 4,4'-methylene
   bis(phenylisocyanate), poly(epsilon-caprolactone)diol (PCL), and
   1,4-butanediol. An orthodontic test using the PU wire was carried out in
   an orthodontic model with a metal bracket. High shape recovery force of
   70 gf for PU wire at 40 wt\% hard segment content could be preserved for
   even 1 month after a shape recovery force test at a constant temperature
   of 50A degrees C. The shape recovery force decreased exponentially
   during the initial 2 h, but reached an equilibrium shape recovery force
   of 50 gf after about 20 days. It was found that this shape recovery
   force was sufficient to correct misaligned teeth in the orthodontic
   test. The shape memory PU wire possesses strong potential as a novel
   orthodontic appliance with esthetically appealing appearance.}},
