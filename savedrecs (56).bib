@article{ WOS:A1995RY43800004,
Author = {TAKEO, M and TSUNEKAGE, N and TAKATA, E and MAEDA, Y},
Title = {{DETERMINATION OF TIN BY AAS USING A LANTHANUM-TREATED GRAPHITE TUBE -
   REMOVAL OF INTERFERENCE FROM AMINO-ACIDS CONTAINING SULFUR}},
Journal = {{BUNSEKI KAGAKU}},
Year = {{1995}},
Volume = {{44}},
Number = {{9}},
Pages = {{697-702}},
Month = {{SEP}},
Abstract = {{In determinations of tin in biotic samples by AAS using a
   lanthanum-treated graphite furnace (LGF-AAS), amino acids containing
   sulfur were found to interfere remarkably. A study on removal of the
   interference resulted in the following proposed method. One gram of
   biological sample was weighed and put into a Kjeldahl flask, to which 20
   ml of nitric acid was added and heated to decompose organic matter. The
   contents were then cooled and taken into a 100 ml-volumetric flask, to
   which 2 ml of nitric acid, 5 ml of potassium dichromate solution
   (2w/v\%) and 3 g of diammonium hydrogenphosphate were added as matrix
   modifiers. After diluting to the mark with water, the solution was
   subjected to LGF-AAS. The detection limit for tin was 0.05 mu g/g and
   the determination limit was 0.17 mu g/g. The method could be
   successfully applied to the analysis of tin in fish tissues.}},
