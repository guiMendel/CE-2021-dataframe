@article{ WOS:000089230900040,
Author = {Southall, BL and Schusterman, RJ and Kastak, D},
Title = {{Masking in three pinnipeds: Underwater, low-frequency critical ratios}},
Journal = {{JOURNAL OF THE ACOUSTICAL SOCIETY OF AMERICA}},
Year = {{2000}},
Volume = {{108}},
Number = {{3, 1}},
Pages = {{1322-1326}},
Month = {{SEP}},
Abstract = {{Behavioral techniques were used to determine underwater masked hearing
   thresholds for a northern elephant seal (Mirounga angustirostris), a
   harbor seal (Phoca vitulina), and a California sea lion (Zalophus
   californianus). Octave-band white noise maskers were centered at five
   test frequencies ranging from 200 to 2500 Hz; a slightly wider noise
   band was used for testing at 100 Hz. Critical ratios were calculated at
   one masking noise level for each test frequency. Above 200 Hz, critical
   ratios increased with frequency. This pattern is similar to that
   observed in most animals tested, and indicates that these pinnipeds lack
   specializations for detecting low-frequency tonal sounds in noise.
   However, the individual pinnipeds in this study, particularly the
   northern elephant seal, detected signals at relatively low
   signal-to-noise ratios. These results provide a means of estimating
   zones of auditory masking for pinnipeds exposed to anthropogenic noise
   sources. (C) 2000 Acoustical Society of America.
