@article{ WOS:A1995TT21400005,
Author = {Pettigrew, MJ and Taylor, CE and Jong, JH and Currie, IG},
Title = {{Vibration of a tube bundle in two-phase Freon cross-flow}},
Journal = {{JOURNAL OF PRESSURE VESSEL TECHNOLOGY-TRANSACTIONS OF THE ASME}},
Year = {{1995}},
Volume = {{117}},
Number = {{4}},
Pages = {{321-329}},
Month = {{NOV}},
Abstract = {{Two-phase cross-flow exists in many shell-and-tube heat exchangers. The
   U-bend region of nuclear steam generators is a prime example. Testing in
   two-phase flow simulated by air-water provides useful results
   inexpensively. However, two-phase flow parameters, in particular surface
   tension and density ratio, are considerably different in air-water than
   in steam-water. A reasonable compromise is testing in liquid-vapor
   Freon, which is much closer to steam-water while much simpler
   experimentally. This paper presents the first results of a series of
   tests on the vibration behavior of tube bundles subjected to two-phase
   Freon cross-flow. A rotated triangular tube bundle of tube-to-diameter
   ratio of 1.5 was tested over a broad range of void fractions and mass
   fluxes. Fluidelastic instability, random turbulence excitation, and
   damping were investigated. Well-defined fluidelastic instabilities were
   observed in continuous two-phase flow regimes. However, intermittent
   two-phase flow regimes had a dramatic effect on fluidelastic
   instability. Generally, random turbulence excitation forces are much
   lower in Freon than in air-water. Damping is very dependent on void
   fraction, as expected.}},
