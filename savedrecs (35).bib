@article{ WOS:000232966200005,
Author = {Drozdowski, H and Taberski, B},
Title = {{Molecular ordering in some liquid aromatic hydrocarbons}},
Journal = {{PHYSICS AND CHEMISTRY OF LIQUIDS}},
Year = {{2005}},
Volume = {{43}},
Number = {{5}},
Pages = {{449-457}},
Month = {{OCT}},
Abstract = {{This study presents a comparison of the structures and molecular
   correlations for the linear aromatic hydrocarbons: benzene, naphthalene,
   and anthracene in the liquid phase, performed for the first time by the
   method of X-ray diffraction. Also for the first time the X-ray
   diffraction results obtained for anthracene at 513 K have been reported.
   Monochromatic radiation CuK alpha was used to determine the scattered
   radiation intensity between S-min = 4 pi sin Theta(min)/lambda 0.417
   angstrom(-1) and S-max = 4 pi sin Theta(max)/lambda = 7.06 angstrom(-1).
   The mean angular distributions of X-ray scattered intensity were
   measured and the differential radial distribution functions of electron
   density (DRDFs) were calculated. The mean distances between the
   neighbouring molecules and the mean coordination numbers were found. The
   most probable models of local ordering of these molecules were
   suggested. Correlations have been found between the number of benzene
   rings in the molecules studied and their physical properties.}},
