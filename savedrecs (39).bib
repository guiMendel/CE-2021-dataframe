@article{ WOS:000232640700037,
Author = {Martin, MZ and Labbe, N and Rials, TG and Wullschleger, SD},
Title = {{Analysis of preservative-treated wood by multivariate analysis of
   laser-induced breakdown spectroscopy spectra}},
Journal = {{SPECTROCHIMICA ACTA PART B-ATOMIC SPECTROSCOPY}},
Year = {{2005}},
Volume = {{60}},
Number = {{7-8}},
