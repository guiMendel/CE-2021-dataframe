@article{ WOS:000594905600024,
Author = {Sun, Hua},
Title = {{Private Information Delivery}},
Journal = {{IEEE TRANSACTIONS ON INFORMATION THEORY}},
Year = {{2020}},
Volume = {{66}},
Number = {{12}},
Pages = {{7672-7683}},
Month = {{DEC}},
Abstract = {{We introduce the problem of private information delivery (PID),
   comprised of K messages, a user, and N servers (each holds M <= K
   messages) that wish to deliver one out of K messages to the user
   privately, i.e., without revealing the delivered message index to the
   user. The information theoretic capacity of PID, C, is defined as the
   maximum number of bits of the desired message that can be privately
   delivered per bit of total communication to the user. For the PID
   problem with K messages, N servers, M messages stored per server, and N
   >= inverted right perpendicularK/Minverted left perpendicular, we
   provide an achievable scheme of rate 1/inverted right
