@article{ WOS:000165877600081,
Author = {Scibetta, M and Chaouadi, R and Puzzolante, JL},
Title = {{Analysis of tensile and fracture toughness results on irradiated
   molybdenum alloys, TZM and Mo-5\%Re}},
Journal = {{JOURNAL OF NUCLEAR MATERIALS}},
Year = {{2000}},
Volume = {{283}},
Number = {{A}},
Pages = {{455-460}},
