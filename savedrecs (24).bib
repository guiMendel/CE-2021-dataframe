@article{ WOS:000289224300004,
Author = {Haarhuis, C. M. Klein and de Jongste, W. M.},
Title = {{Reconstructing and assessing the evaluation logic of the Dutch Closed
   Criminal Cases Evaluation Commission: report of a meta-evaluation}},
Journal = {{EVIDENCE \& POLICY}},
Year = {{2010}},
Volume = {{6}},
Number = {{4}},
Pages = {{483-503}},
Month = {{NOV}},
Abstract = {{In this paper, we offer an account of the meta-evaluation of a special
   administrative arrangement, the Dutch Closed Criminal Cases Evaluation
   Commission (Dutch acronym: CEAS).The CEAS is a provisional evaluation
   institution meant to investigate alleged missteps during criminal
   investigation and prosecution, which may have caused the conviction of
   an innocent person
   We developed a tailor-made approach to reconstruct the commission's
   `evaluation logic'; this is illustrated by reconstructing the activities
   of a selection committee and two investigative subcommittees of the
   CEAS. Subsequently, we applied a set of criteria to assess among others
   the timeliness, independence and transparency of the commission's
   work.We offer a number of conclusions and recommendations, particularly
   with regard to future (meta-)evaluation research.}},
