@article{ WOS:000377858900025,
Author = {Raman, Sheila K. and Balakrishnan, E.},
Title = {{ENDOSCOPIC RESECTION OF A RARE CASE OF SOLITARY FIBROUS TUMOUR OF THE
   NOSE AND PARANASAL SINUSES}},
Journal = {{JOURNAL OF EVOLUTION OF MEDICAL AND DENTAL SCIENCES-JEMDS}},
Year = {{2015}},
Volume = {{4}},
Number = {{91}},
Pages = {{15711-15713}},
Month = {{NOV 12}},
Abstract = {{A solitary fibrous tumour is a rare neoplasms of mesenchymal origin and
   arises mainly in the pleura. It is characterised by spindle cells,
   branching blood vessels, stromal hyalinization and CD34, 99
   immunoreactivity. Occurance in the nasal cavity is extremely rare. We
   report a case of nasal solitary fibrous tumour in a 66 year old lady who
   presented with right sided nasal obstruction, headache and profuse nasal
   bleeding of 3 months duration. Mass was seen arising from the right
   middle turbinate with extension into the right middle meatus, ethmoid
   sinus and sphenoethmoidal recess. The tumour measuring 3.2x1.5x3.5 cm
   was successfully resected by endoscopic surgery. There has been no
   evidence of tumour recurrence in the 24 months follow-up following
   surgery.}},
