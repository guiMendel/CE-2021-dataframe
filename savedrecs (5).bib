@article{ WOS:000573212200011,
Author = {Tostado-Veliz, Marcos and Kamel, Salah and Jurado, Francisco},
Title = {{An efficient and reliable power flow solution method for large scale
   Ill-conditioned cases based on the Romberg's integration scheme}},
Journal = {{INTERNATIONAL JOURNAL OF ELECTRICAL POWER \& ENERGY SYSTEMS}},
Year = {{2020}},
Volume = {{123}},
Month = {{DEC}},
Abstract = {{Recently, the Power Flow solution in large scale ill-conditioned systems
   has attracted huge attention. Despite the considerable efforts conducted
   in this line, this issue may be considered an open topic nowadays.
   Consequently, the paper aims at filling this gap by proposing a novel
   robust and efficient Power Flow solution technique inspired by the
   Romberg's Integration Scheme. The resulting algorithm is numerically
   stable and has a computational burden comparable to the Newton's
   technique. Several numerical results in different ill-conditioned
   systems from 3012-, to 13659-bus systems serve to show the performance
   of the developed method. Other available Power Flow solvers are
   considered for comparison. In all studied cases, results obtained with
   the developed method were promising, outperforming the other considered
   methodologies.}},
