@article{ WOS:000232228300007,
Author = {Turner, LD and Domen, KFEM and Scholten, RE},
Title = {{Diffraction-contrast imaging of cold atoms}},
Journal = {{PHYSICAL REVIEW A}},
Year = {{2005}},
Volume = {{72}},
Number = {{3}},
Month = {{SEP}},
Abstract = {{We consider the inverse problem of in-line holography, applied to
   minimally destructive imaging of cold atom clouds. Absorption imaging
   near resonance provides a simple, but destructive measurement of atom
   column density. Imaging off resonance greatly reduces heating, and
   sequential images may be taken. Under the conditions required for
