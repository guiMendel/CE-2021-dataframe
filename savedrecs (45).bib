@article{ WOS:000165174300006,
Author = {Rehm, W},
Title = {{Applied fluid dynamics with high-performance supercomputing}},
Journal = {{ATW-INTERNATIONALE ZEITSCHRIFT FUR KERNENERGIE}},
Year = {{2000}},
Volume = {{45}},
Number = {{10}},
Pages = {{606+}},
Month = {{OCT}},
Abstract = {{The development of increasingly more powerful computers and
   supercomputers in coupled computer systems as well as the growing
   requirements to be met by the results computed for specific problems
   continuously add to the importance of numerical simulations of flow
   mechanics.
   Special applications exist, for instance, in weather forecasting and in
   turbulent combustion processes within the framework of safety analyses.
   Numerical simulation on parallel computers, and the distribution of
   problems over several computers in a computer complex with the resultant
   high computer capacity, allow multidimensional, transient, compressible,
   turbulent, and chemically reactive flows to be computed.
   With this type of computer system it is possible, for instance, to
   handle the hydrogen problem in reactor containments in the course of
   accidents exceeding the design basis and, in this way, develop
   engineered safeguards measures against such occurrences.}},
