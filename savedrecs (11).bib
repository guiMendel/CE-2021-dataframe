@article{ WOS:000377735900007,
Author = {Chtourou, Haifa and Dahmen, Ines and Karray, Fatma and Sayadi, Sami and
   Dhouib, Abdelhafidh},
Title = {{Biodiesel Production of Amphora sp and Navicula sp by Different Cell
   Disruption and Lipid Extraction Methods}},
Journal = {{JOURNAL OF BIOBASED MATERIALS AND BIOENERGY}},
Year = {{2015}},
Volume = {{9}},
Number = {{6}},
Pages = {{588-595}},
Month = {{DEC}},
Abstract = {{Diatoms are considered as third generation biofuel. In this framework,
   three-isolated diatom from Tunisian biotopes were identified and tested
   for their ability to produce lipids, which then converted to biodiesel.
   The lipid extraction with modified Bligh and Dyer methods coupling with
   microwave cell disruption could increase the lipid yield for all the
   studied strains. On the other hand, the direct-transesterification
   allowed higher yield of crude biodiesel than
   extraction-transesterification. Chloroform-based
   direct-transesterification from microwave treated cells led to higher
   FAME yield: from 16 to 18\%, 9 to 26\% and 5 to 13\% of the dry weight,
   in Amphora subtropica CTM 20013, Amphora sp. CTM 20023 and Navicula sp.
   CTM 20025, respectively. Palmitic (C16:0) and palmitoleic (C16:1) acids
   were dominant and both accounted for 74\%, 55\% and 68\% of total FAME
   in CTM 20013, CTM 20023 and CTM 20025 strains, respectively. For Amphora
   sp. and Navicula sp. the lipid yield was ameliorated with lipid
