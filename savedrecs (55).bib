@article{ WOS:A1995RV98600009,
Author = {GASEMYR, J and NATVIG, B},
Title = {{SOME ASPECTS OF RELIABILITY-ANALYSIS IN SHOCK-MODELS}},
Journal = {{SCANDINAVIAN JOURNAL OF STATISTICS}},
Year = {{1995}},
Volume = {{22}},
Number = {{3}},
Pages = {{385-393}},
Month = {{SEP}},
Abstract = {{In this paper we consider a binary, monotone system whose component
   states are dependent through the possible occurrence of independent
   common shocks, i.e. shocks that destroy several components at once. Such
   systems can be used to model common cause failures in reliability
   analysis. We propose to consider the common shocks as components in a
   new system, thereby obtaining a system of independent components. We
   show how the minimal path and cut sets of the two systems are related,
   and use this to examine the relation between bounds on the reliability
   based on the two systems. By the new approach some bounds are improved.
   We also discuss the problem of deriving a distribution of system
   reliability from a joint prior distribution for the component
   reliabilities, as well as data at the system level. The Marshall-Olkin
   multivariate exponential distribution fits into this framework, and some
   special cases of this model are treated as an example.}},
