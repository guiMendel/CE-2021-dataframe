@article{ WOS:000284717400003,
Author = {Fingerloos, Frank and Meyer, Lars and Wiens, Udo},
Title = {{On the Necessity of Gradients at Parking Decks}},
Journal = {{BETON- UND STAHLBETONBAU}},
Year = {{2010}},
Volume = {{105}},
Number = {{11}},
Pages = {{695-702}},
Month = {{NOV}},
Abstract = {{On the Necessity of Gradients at Parking Decks
   When design and executing new car parks or when surveying existing ones
   an often discussed question is whether a gradient of parking decks is
   always necessary In this context different arguments are commonly used
   These arguments have been collected and reviewed under the aspect of
   usability as a car park on the one hand and under the aspect of
   durability of the rein forced construction on the other hand It has been
   shown why in the new DBV Code of Practice Multi Storey and Underground
   Car Parks {[}1] an incline is not required as a standard construction
   method in every case Instead of that it has been concluded that an
   appropriate owners decision on pro or contra an incline would be
   reasonable and necessary based on the above consideration of
   differentiated arguments}},
