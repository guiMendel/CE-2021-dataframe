@article{ WOS:A1995UD33500038,
Author = {Lumme, I and Arkhipov, V and Kettunen, I},
Title = {{Deposition gradients and foliar and soil leachate concentrations of air
   pollutants in Scots pine stands of SE Finland and the Karelian Isthmus,
   NW Russia}},
