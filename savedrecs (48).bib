@article{ WOS:000089000600005,
Author = {Bykov, A},
Title = {{Fibrant extensions and conditions of movability}},
Journal = {{ACTA MATHEMATICA HUNGARICA}},
Year = {{2000}},
Volume = {{88}},
Number = {{3}},
Pages = {{213-220}},
Month = {{AUG}},
Abstract = {{The concept of a fibrant extension of compacts can he used in the study
   of some properties related to different conditions of movability. For
   example, ``empty{''} strong shape components of a given compact metric
   space X correspond to those path components of a fibrant extension (X)
   over tilde of X which do not intersect X. The fibrant extension (X) over
   tilde can be constructed as a cotelescope of an ANR-sequence associated
   with X. Using this representation of (X) over tilde, we prove the
   following: If a continuum is movable and virtually pointed 1-movable,
   then it is pointed movable.
   As a corollary, we get immediately that movable continua, which do not
   have ``empty{''} strong shape components, are pointed movable. In
   particular, continua, both fibrant and movable, are of this kind. In
   fact, they are locally path connected approximate polyhedra.}},
