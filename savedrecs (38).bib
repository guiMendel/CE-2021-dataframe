@article{ WOS:000233094700043,
Author = {Sanchez-Cruz, H and Rodriguez-Dagnino, RM},
Title = {{Compressing bilevel images by means of a three-bit chain code}},
Journal = {{OPTICAL ENGINEERING}},
Year = {{2005}},
Volume = {{44}},
Number = {{9}},
Month = {{SEP}},
Abstract = {{We propose a lossless compression method based on a chain code composed
   of only three symbols. The method is applicable to compress 2-D binary
   object shapes, and it consists of representing the orthogonal direction
   changes of the discrete contour, corresponding to each object binary
   shape, by three bits of a chain code. According to our experimental
   results, we find that this method is suitable for the representation of
   bilevel images. The results are about 25\% more efficient in compression
   than the Freeman chain code method, and an average of 29\% better than
   the Joint Bilevel Image Experts Group (JBIG) compressor. (c) 2005
   society of Photo-Optical Instrumentation Engineers.}},
