@article{ WOS:000232756700165,
Author = {Clark, NH and Doughty, DH},
Title = {{Development and testing of 100 kW/1 min Li-ion battery systems for
   energy storage applications}},
Journal = {{JOURNAL OF POWER SOURCES}},
Year = {{2005}},
Volume = {{146}},
Number = {{1-2, SI}},
Pages = {{798-803}},
Month = {{AUG 26}},
Note = {{12th International Meeting on Lithium Batteries, Nara, JAPAN, JUN 27-JUL
   02, 2004}},
Abstract = {{Two 100 kW min(-1) (1.67 kW h(-1)) Li-ion battery energy storage systems
   (BESS) are described. The systems include a high-power Li-ion battery
   and a 100 kW power conditioning system (PCS). The battery consists of 12
   modules of 12 series-connected Saft Li-ion VL30P cells. The stored
   energy of the battery ranges from 1.67 to 14 kW h(-1) and has an
   operating voltage window of 515-405V (dc). Two complete systems were
   designed, built and successfully passed factory acceptance testing after
   which each was deployed in a field demonstration. The first
   demonstration used the system to supplement distributed microturbine
   generation and to provide load following capability. The system was run
   at its rated power level for 3 min, which exceeded the battery design
