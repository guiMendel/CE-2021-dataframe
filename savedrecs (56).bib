@article{ WOS:A1995TG37600008,
Author = {LELOUP, SJLE and TMANNETJE, L},
Title = {{PRIMARY PRODUCTION OF RANGELANDS IN SOUTHERN MAIL - A STUDY OF
   METHODOLOGY}},
Journal = {{TROPICAL GRASSLANDS}},
Year = {{1995}},
Volume = {{29}},
Number = {{3}},
Pages = {{177-182}},
Month = {{SEP}},
Abstract = {{The primary production of rangelands in the Sudanian zone of Mali was
   estimated using a method which aims to describe the situation in an
   average and/or a dry rainfall year. This method, proposed by Breman and
   De Ridder (1991), distinguishes 3 approaches differing in accuracy
   level. Two of them, the `'rough'' approach and the `'semi-detailed''
   approach, were applied to rangelands near 3 villages. The `'rough''
   approach derives primary production from purely theoretical functions
   and available data on rainfall and soil parameters, while the
   `'semi-detailed'' approach includes data from a year of field work.
   The `'rough'' approach produced reasonably accurate estimates in one
   village but not in 2 others. We consider that changes in the vegetation
   as a result of more intensive use of rangelands in the 2 villages meant
   that the theoretical functions were no longer applicable there. As this
   is generally the case in the Sudanian zone of Mali, the method of Breman
   and De Ridder appears unsuitable for this region. However, the `'rough''
   approach can be used as a reference to quantify vegetation changes.}},
