@article{ WOS:000603999600001,
Author = {Diarsa, Meghna and Gupte, Akshaya},
Title = {{Optimization and Extraction of Natural Dye from Tagetes Erecta and
   Dyeing of Cotton and Silk Fabric Using Banana (Musa Sp.) Pseudo Stem Sap}},
Journal = {{JOURNAL OF NATURAL FIBERS}},
Abstract = {{The extraction of natural dye using an aqueous method is a concept of
   green chemistry to avoid environmental pollution and toxicity. The
   present study was performed to improve the color strength and fastness
   properties of the fabrics by using natural dye and banana pseudo stem
   sap as a natural mordant. The natural dye was extracted from marigold
   flowers (Tagetes erecta) and optimized for dyeing of cotton and silk
   fabrics. The extraction of the dye was done in an aqueous solution
   without using any solvents. The optimum results were obtained when the
   fabric was dyed at 80 degrees C for 60 min by keeping the mass to liquid
   ratio 1:40 at pH-7 (1.19). The color strength and CIELab (L{*}, a{*},
   and b{*}) color parameters were also investigated. The effect of banana
   pseudo stem saps as a natural mordant on the color strength and fastness
   properties of the cotton and silk fabrics were evaluated and compared.
   The cotton fabrics were further characterized by FTIR spectroscopy and
   Scanning electron microscopy (SEM) to assess the physical and chemical
   changes after dyeing and mordanting.}},
