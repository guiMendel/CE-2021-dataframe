@article{ WOS:000233378200070,
Author = {Liu, K and Zhang, X and Sumanasekera, C and Lester, RL and Dickson, RC},
Title = {{Signalling functions for sphingolipid long-chain bases in Saccharomyces
   cerevisiae}},
Journal = {{BIOCHEMICAL SOCIETY TRANSACTIONS}},
