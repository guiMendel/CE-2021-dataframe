@article{ WOS:000610412700003,
Author = {Jung, Woo Hoon and Choi, Wonjae and Oh, Joo Hwan},
Title = {{Analysis and Design of the Acoustic Meta Surface Unit Structure}},
Journal = {{JOURNAL OF THE KOREAN SOCIETY FOR NONDESTRUCTIVE TESTING}},
Year = {{2020}},
Volume = {{40}},
Number = {{6}},
Pages = {{404-412}},
Month = {{DEC}},
Abstract = {{Recent advances in artificial materials, namely metamaterials and
   metasurfaces, have aided in generating new technologies for ultrasonic
   applications. However, majority of the previous research studies have
   focused on elastic waves and acoustic waves in air, whereas underwater
   acoustics (one of the most important topics in ultrasonic applications)
   has been rarely studied. In particular, very limited research has been
   conducted on underwater metasurfaces that can tailor the flow of
   acoustic waves with very thin structures. In this study, as a basic
   approach to develop underwater metasurface technologies, we present a
   novel underwater metasurface that can tailor the flow of reflected waves
   towards any desired direction, along with the corresponding numerical
   and experimental corroborations. We found that the solid-fluid
   interaction can become very active, even though such interaction has
   almost been overlooked in previous studies. Furthermore, we designed an
   underwater metasurface that can suppress undesired solid-fluid
   interactions, and the corresponding numerical and experimental
   validations were accordingly conducted. We believe that this study can
   provide a cogent premise for developing various underwater metasurfaces.}},
