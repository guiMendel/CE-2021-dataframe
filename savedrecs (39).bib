@article{ WOS:000231649200004,
Author = {Bogachev, SA and Somov, BV},
Title = {{Comparison of the fermi and betatron acceleration efficiencies in
   collapsing magnetic traps}},
Journal = {{ASTRONOMY LETTERS-A JOURNAL OF ASTRONOMY AND SPACE ASTROPHYSICS}},
Year = {{2005}},
Volume = {{31}},
Number = {{8}},
Pages = {{537-545}},
Month = {{AUG}},
Abstract = {{The acceleration of charged particles in the solar corona during flares
   is investigated in terms of a model in which the electrons and ions
   preaccelerated in the magnetic reconnection region are injected into a
   collapsing magnetic trap. Here, the particle energy increases rapidly
   simultaneously through the Fermi and betatron mechanisms. Comparison of
   the efficiencies of the two mechanisms shows that the accelerated
   electrons in such a trap produce more intense hard X-ray (HXR) bursts
   than those in a trap where only the Fermi acceleration mechanism would
   be at work. This effect explains the Yohkoh and RHESSI satellite
   observations in which HXR sources more intense than the HXR emission
   from the chromosphere were detected in the corona. (c) 2005 Pleiades
   Publishing, Inc.}},
