@article{ WOS:000594337700022,
Author = {Huang, Yi-Ping and Huang, Chih-Chieh and Lee, Ching-Sung and Hsu,
   Wei-Chou},
Title = {{High-Performance Normally-OFF AlGaN/GaN Fin-MISHEMT on Silicon With Low
   Work Function Metal-Source Contact Ledge}},
