@article{ WOS:000233789200011,
Author = {Rusop, M and Soga, T and Jimbo, T},
Title = {{X-ray photoelectron spectroscopy studies on the bonding properties of
   oxygenated amorphous carbon nitride thin films synthesized by pulsed
   laser deposition at different substrate temperatures}},
