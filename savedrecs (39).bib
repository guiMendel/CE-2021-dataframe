@article{ WOS:000230123500081,
Author = {Yang, CP and Hsiao, SH and Chen, YC},
Title = {{Soluble and light-colored polyimides from 2,3,2 `,3 `-oxydiphthalic
   anhydride and aromatic diamines}},
Journal = {{JOURNAL OF APPLIED POLYMER SCIENCE}},
Year = {{2005}},
Volume = {{97}},
Number = {{3}},
Pages = {{1352-1360}},
Month = {{AUG 5}},
Abstract = {{A series of polyimides were prepared from 2,3,2',3'-oxydiphthalic
   anhydride (3,3'-ODPA) with various aromatic diamines via three different
   synthetic procedures. The one-step and two-step methods with the thermal
   imidization of poly(amic acids) (PAAs) yielded polyimides with a
   relatively low inherent viscosity, these produced brittle films. The
   polyimides prepared by the two-step method via the chemical imidization
   of PAA precursors exhibited a higher inherent viscosity and afforded
   tough and creaseable films. All the 3,3'-ODPA based polyimides had a
   significantly higher solubility than the corresponding polyimides from
   3,4,3',4'-oxydiphthalic anhydride. The films cast from 3,3'-ODPA
   polyimides also showed high optical transparencies and less color, with
   an ultraviolet-visible absorption edge of 370-397 nm and low yellowness
   index values of 11.3-29.8. These polyimides exhibited glass-transition
   temperatures in the range 211-289 degrees C and showed no significant
   decomposition below 500 degrees C under nitrogen or air atmospheres. (c)
   2005 Wiley Periodicals, Inc.}},
