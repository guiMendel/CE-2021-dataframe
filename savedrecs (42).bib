@article{ WOS:000166137000003,
Author = {Cao, JG},
Title = {{Cheeger isoperimetric constants of Gromov-hyperbolic spaces with
   quasi-poles}},
Journal = {{COMMUNICATIONS IN CONTEMPORARY MATHEMATICS}},
Year = {{2000}},
Volume = {{2}},
Number = {{4}},
Pages = {{511-533}},
Month = {{NOV}},
Abstract = {{Let X be a non-compact complete manifold (or a graph) which admits a
   quasi-pole and has bounded local geometry. Suppose that X is
   Gromov-hyperbolic and the diameters (for a fixed Gromov metric) of the
   connected components of X(infinity) have a positive lower bound. Under
   these assumptions we show that X has positive Cheeger isoperimetric
   constant. Examples are also constructed to show that the Cheeger
   constant h(X) may be zero if any of the above assumption on X is
   removed.
   Applications of this isoperimetric estimate include the solvability of
   the Dirichlet problem at infinity for non-compact Gromov-hyperbolic
   manifolds X above. In addition, we show that the Martin boundary partial
   derivative X-Delta of such a space X is homeomorphic to the geometric
   boundary X(infinity) of X at infinity.}},
