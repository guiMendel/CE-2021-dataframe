@article{ WOS:000370214100001,
Author = {Wan Xuemei},
Title = {{Poetry, Poetics and Science Fiction Epics: An Interview with Professor
   Frederick Turner}},
Journal = {{FOREIGN LITERATURE STUDIES}},
Year = {{2015}},
Volume = {{37}},
Number = {{6}},
Pages = {{1-9}},
Month = {{DEC}},
Abstract = {{Frederick Turner is a poet, a Founders Professor of Arts and Humanities
   at the University of Texas at Dallas, one of the leading thinkers and
   spokespersons for the restoration ecology movement, a founder of and
   spokesman for two contemporary American poetic movements, the New
   Formalism and the New Narrative, and a proposer of ``Natural
   Classicism.{''} He has published 34 books, including 24 books of poetry.
   As a poet, he writes both short lyrics and long narratives. This
   interview centers on Professor Turner's poetry, poetic theories, and the
   elements influencing his creation of poems. His science epics unite the
   disparate forms of science fiction and epic in strict metrical forms. He
   believes that the two forms can offer the inevitable choice of medium
