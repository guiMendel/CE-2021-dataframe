@article{ WOS:000281779000011,
Author = {Wei, Xiaogang and Zhang, Jiepeng and Zhu, Yifu},
Title = {{All-optical switching in a coupled cavity-atom system}},
Journal = {{PHYSICAL REVIEW A}},
Year = {{2010}},
Volume = {{82}},
Number = {{3}},
Month = {{SEP 13}},
Abstract = {{We report an experimental observation of all-optical switching in a
   cavity QED system consisting of multiple three-level atoms confined in a
   cavity mode. A signal light is coupled into the normal mode of the
   cavity-atom system and its transmission through the cavity is switched
   on or off by a control laser coupled to the atoms from free space. Our
   experiment shows that the all-optical switching in the cavity QED system
   can be performed for the cavity mode tuned on resonance with or off
   resonance from the atomic transition frequency.}},
