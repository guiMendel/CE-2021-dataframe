@article{ WOS:000088580300018,
Author = {Fujii, A and Ootake, R and Fujisawa, T and Ozaki, M and Ohmori, Y and
   Laga, T and Yoshino, K and Lu, HF and Chan, HSO and Ng, SC},
Title = {{Optical properties, spectral narrowing of photoluminescence and blue
   electroluminescence of poly(phenylene pyridine) derivatives}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2000}},
Volume = {{77}},
Number = {{5}},
Pages = {{660-662}},
Month = {{JUL 31}},
Abstract = {{Optical properties of poly(phenylene pyridine) derivatives,
   poly(2,5-dialkoxy-1,4phenylene-alt-2,5-pyridine) (PHOnPY25), such as
   optical absorption, photoluminescence (PL), and electroluminescence
   (EL), have been studied. The electronic energy structures of PHOnPY25
