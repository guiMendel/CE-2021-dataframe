@article{ WOS:000600755700001,
Author = {Demeulemeester, Samuel},
Title = {{The 100\% money proposal of the 1930s: an avatar of the Currency
   School's reform ideas?}},
Journal = {{EUROPEAN JOURNAL OF THE HISTORY OF ECONOMIC THOUGHT}},
Year = {{2021}},
Volume = {{28}},
Number = {{4}},
Pages = {{577-598}},
Month = {{JUL 4}},
Abstract = {{This paper argues that the 100\% money proposal of the 1930s should not
   (as is often the case) simply be considered as an avatar, extended to
   deposit currency, of the Currency School's reform prescriptions. The
   English Bank Charter Act of 1844, indeed, conveyed a double rejection:
   it not only sought to divorce the issuing from the lending of money, but
   also to prevent all kind of monetary management, by enacting a specific
   issuing rule (the ``currency principle{''}) into law. The 100\% money
   proposal, in contrast, was designed independently of any monetary policy
   recommendations, leaving open the debate of ``rules versus
   discretion{''}.}},
