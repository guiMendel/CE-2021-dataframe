@article{ WOS:000359507900009,
Author = {Bontea, Costel-Gabriel and Nikshych, Dmitri},
Title = {{On the Brauer-Picard group of a finite symmetric tensor category}},
Journal = {{JOURNAL OF ALGEBRA}},
Year = {{2015}},
Volume = {{440}},
Pages = {{187-218}},
Month = {{OCT 15}},
Abstract = {{Let C-n denote the representation category of finite supergroup Lambda
   k(n) x Z/2Z. We compute the Brauer Picard group BrPic(C-n) of C-n. This
   is done by identifying BrPic(C-n) with the group of braided tensor
   autoequivalences of the Drinfeld center of en and studying the action of
   the latter group on the categorical Lagrangian Grassmannian of C-n. We
   show that this action corresponds to the action of a projective
   symplectic group on a classical Lagrangian Grassmannian. (C) 2015
   Elsevier Inc. All rights reserved.}},
