@article{ WOS:A1995RP41100002,
Author = {SHIKAMA, H and MIYATA, K and SAKAE, N and MITSUISHI, Y and NISHIMURA, K
   and KURODA, K and KATO, M},
Title = {{NOVEL MUTEIN OF TUMOR-NECROSIS-FACTOR-ALPHA (F4614) WITH REDUCED
   HYPOTENSIVE EFFECT}},
Journal = {{JOURNAL OF INTERFERON AND CYTOKINE RESEARCH}},
Year = {{1995}},
Volume = {{15}},
Number = {{8}},
Pages = {{677-684}},
Month = {{AUG}},
Abstract = {{To eliminate systemic toxicity, including the hypotension associated
   with human tumor necrosis factor cy (TNF-(alpha), we constructed mutant
   proteins (muteins) by mean of genetic engineering. A novel mutein,
   F4614, containing mutations of (5)Thr --> Gly and (6)Pro --> Asp, which
   resulted in the introduction of cell-adhesive Arg-Gly-Asp and (29)Arg
   --> Val, had remarkably reduced hypotensive effects and lower lethality,
   We present evidence that the Arg --> Val mutation at position 29 is
   largely responsible for the reduced hypotensive effect. This effect of
   F3614 was thought to be closely correlated with its low inducibility of
   nitric oxide and prostaglandin E(2) in vivo. In addition, the
   therapeutically effective dose of F46P4 to MethA
   fibrosarcoma-transplanted mice was increased compared with that of
   TNF-alpha. Indicating a wide therapeutic index. These results indicated
   that F4614 has several advantages as a systemic therapeutic drug in the
   treatment of cancer.}},
