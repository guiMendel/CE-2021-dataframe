@article{ WOS:A1995TN89500007,
Author = {Christodoulides, JA and Murthy, AS and Hadjipanayis, GC},
Title = {{Structural and magnetic properties of granular Cr100-xFex}},
Journal = {{MATERIALS SCIENCE AND ENGINEERING A-STRUCTURAL MATERIALS PROPERTIES
   MICROSTRUCTURE AND PROCESSING}},
Year = {{1995}},
Volume = {{204}},
