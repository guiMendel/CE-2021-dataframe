@article{ WOS:000232616600003,
Author = {Nakai, M},
Title = {{Dependance of Dirichlet integrals upon lumps of Riemann surfaces}},
Journal = {{PROCEEDINGS OF THE JAPAN ACADEMY SERIES A-MATHEMATICAL SCIENCES}},
Year = {{2005}},
Volume = {{81}},
Number = {{7}},
Pages = {{131-133}},
Month = {{SEP}},
Abstract = {{Take a simple arc gamma in an open Riemann suface R carrying a
   nonconstant harmonic function u with finite Dirichlet integral D(u; R).
   Form a Riemann surface R with lump C/gamma by pasting
   R\textbackslash{}gamma with C\textbackslash{}gamma crosswise along
   gamma, i.e. R-gamma, := (R\textbackslash{}gamma) U-gamma
   (C\textbackslash{}gamma), and the transplant u(gamma) of u on R to
   R-gamma characterized by its being harmonic on R-gamma with D(u(gamma);
   R-gamma) < + infinity and u(gamma) = u at the ideal boundary of R-, and
   hence of R in a suitable sense. We are interested in the comparison of
   D(u(gamma) R-gamma) with D(u; R) when we take a variety of choices of
   pasting arcs gamma in R, and we will prove that D(u(gamma); R-gamma) <
   D(u; R) for any u level arc gamma in R, D(u(gamma); R-gamma) > D(u; R)
   for any u conjugate level arc gamma in R, and as a consequence of these
   two facts there is a nondegenerate arc gamma (i.e. not a point arc
   gamma) in R such that D(mu(gamma); R-gamma) = D(u; R).}},
