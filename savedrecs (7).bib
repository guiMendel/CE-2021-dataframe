@article{ WOS:000571177700006,
Author = {Liu, Yu and Lan, Zhongwen and Yu, Zhong and Guo, Rongdi and Jiang,
   Xiaona and Wu, Chuanjian and Sun, Ke},
Title = {{Modulation of PSSW resonance field affected by exchange stiffness A in
   Fe/NiFe/Fe multi-layer films with different Fe film thicknesses}},
Journal = {{JOURNAL OF MAGNETISM AND MAGNETIC MATERIALS}},
Year = {{2020}},
Volume = {{514}},
Month = {{NOV 15}},
Abstract = {{Microwave devices, designed by the electromagnetic characteristics of
   spin waves, are widely used in radio frequency circuits and microwave
   systems, such as magnetic tunable fillers, frequency multiplier, and
   signal-to-noise ratio enhancers. In order to manufacture these devices,
   it is not only the saturation magnetization (4 pi M-s) and ferromagnetic
   resonance linewidth (Delta H) that need to be regulated, but also the
   exchange stiffness (A). In this study, the Fe (t nm)/Ni81Fe19 (50 nm)/Fe
   (t nm) multilayer films were proposed to regulate the 4 pi M-s, Delta H,
   and A, which were fabricated using electron beam evaporation, and the
   dynamic magnetic properties of the film were studied by ferromagnetic
   resonance (FMR) spectroscopy. Firstly, we obtain the 4 pi M-s, gamma and
   H-a by fitting the uniform FMR mode. Then we obtain the exchange
   stiffness by fitting the first (p = 1) perpendicular standing spin wave
   (PSSW) mode. With the increasing Fe film thickness, the calculated
   saturation magnetization increased from 9250 to 14060Gs, FMR linewidth
   increased from 111.5 to 140.30e and the exchange stiffness decreased
   from 22.1 x 10(-12) to 7.8 x 10(-12) J/m.}},
