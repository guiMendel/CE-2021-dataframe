@article{ WOS:000284087800001,
Author = {Ensoli, Barbara and Bellino, Stefania and Tripiciano, Antonella and
   Longo, Olimpia and Francavilla, Vittorio and Marcotullio, Simone and
   Cafaro, Aurelio and Picconi, Orietta and Paniccia, Giovanni and Scoglio,
