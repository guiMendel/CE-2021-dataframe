@article{ WOS:A1995TG13100008,
Author = {YANUSOVA, L and STIOPINA, N and FEIGIN, L and BAKLAGINA, Y and
   KHRIPUNOV, A},
Title = {{LANGMUIR-BLODGETT-FILMS OF SUBSTITUTED CELLULOSE ACETOMYRISTATE -
   FABRICATION AND X-RAY-DIFFRACTION STUDY}},
Journal = {{MATERIALS SCIENCE \& ENGINEERING C-BIOMIMETIC MATERIALS SENSORS AND
   SYSTEMS}},
Year = {{1995}},
Volume = {{2}},
Number = {{4}},
Pages = {{225-227}},
Month = {{SEP}},
Note = {{International Conference on Intelligent Materials (ICIM2), WILLIAMSBURG,
   VA, 1994}},
Abstract = {{The behaviour of the monolayer of cellulose triester of myristic and
   acetic acids on a water surface has been studied. The surface
   pressure-area (pi-A) isotherm curves of the monolayer of triester had a
   plateau region. The cyclic pi-A curve measurements, i.e. the cycles of
   compression and successive expansion of the monolayer, showed that the
   structural change in the monolayer under compression was nonreversible.
