@article{ WOS:000230880200013,
Author = {Oh-hashi, K and Nagai, T and Tanaka, T and Yu, H and Hirata, Y and
   Kiuchi, K},
Title = {{Determination of hypoxic effect on neprilysin activity in human
   neuroblastoma SH-SY5Y cells using a novel HPLC method}},
Journal = {{BIOCHEMICAL AND BIOPHYSICAL RESEARCH COMMUNICATIONS}},
Year = {{2005}},
Volume = {{334}},
Number = {{2}},
Pages = {{380-385}},
Month = {{AUG 26}},
Abstract = {{Alzheimer's disease (AD) is characterized by extracellular deposition of
   amyloid-beta-peptide (A beta), which is closely associated with the
   metabolic balance between AP production and clearance activities.
   Neprilysin is one of the important enzymes to degrade AP in the brain
   and alternation of its activity would contribute to the AD
   neuropathology. However, measurement of neprilysin activity in neuronal
   cells, especially the extracellular activity, is very difficult because
   of its weak activities. In the present study, we established a sensitive
   method enough to estimate extracellular neprilysin activity of living
   cell cultivated in a 96-well plate using HPLC-fluorometric system, and
   investigated the effect of hypoxia, a closely associated event with
   neurodegenerative diseases, on neprilysin activity of human
   neuroblastorna SH-SY5Y cells. We demonstrated that chronic but not acute
   hypoxia significantly attenuated neprilysin activity without any
   alterations of neprilysin gene expression. The present study suggests
   that chronic hypoxia may down-regulate extracellular neprilysin activity
   of neuronal cells to impair AP degradation and associate with the
   development of amyloid pathology. (c) 2005 Elsevier Inc. All rights
   reserved.}},
