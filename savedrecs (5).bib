@article{ WOS:000598311700002,
Author = {Wang, SaiWei and Lai, JianCheng and Li, ZhiJian and Yang, XiaoDan and
   Li, ZhenHua},
Title = {{Design and implementation of a linear array laser emitting optical
   system based on diffractive principles}},
Journal = {{OPTIK}},
Year = {{2020}},
Volume = {{224}},
Month = {{DEC}},
Abstract = {{This study investigated the design and implementation of a linear array
   laser emitting optical system based on diffractive optical elements. The
   system offered relatively good energy efficiency and beam uniformity, as
   well as a simpler structure than conventional line laser illumination
   systems. Based on the diffractive model, a hybrid local-global search
   algorithm was proposed to solve for the structural parameters of the
   diffractive element. This method is simple to describe and flexible upon
   implementation. It also offers high efficiency. Using this method, a
   diffractive optical element was designed to generate 13 1.06 mu m laser
   beams. The theoretical diffractive efficiency approached 80 \% and the
   inter-beam non-uniformity was less than 20 \%. The diffractive element
   was fabricated via photolithography and a corresponding optical system
   was built to verify its performance. Experimental measurements showed
   that the total diffractive efficiency of the 13 beams was 70.83 \% and
   their non-uniformity was 11.96 \%. These results approach those
   calculated theoretically, and thus verify the correctness and
   effectiveness of the system.}},
