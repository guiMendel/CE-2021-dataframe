@article{ WOS:000598315300001,
Author = {Sanematsu, Haruki and Matsushita, Yoshitaka and Takeuchi, Masayuki and
   Takai, Atsuro},
Title = {{Amino-Functionalization of Vinyl-Substituted Aromatic Diimides by
   Quantitative and Catalyst-Free Hydroamination{*}{*}}},
Journal = {{CHEMISTRY-A EUROPEAN JOURNAL}},
Year = {{2021}},
Volume = {{27}},
Number = {{3}},
Pages = {{934-938}},
Month = {{JAN 13}},
Abstract = {{Development of facile and versatile synthetic tools for decorating
   pi-conjugated molecules has attracted considerable interest because of
   their potential application in creating novel functional pi-systems.
   Reported herein are quantitative catalyst-free hydroamination reactions
   of a series of aromatic diimide compounds having vinyl groups at the
   pi-core, which have been confirmed by NMR, UV-vis absorption
   spectroscopy, mass analysis, and single-crystal X-ray structural
   analysis. Kinetic studies revealed that the hydroamination reaction of a
   vinyl-substituted naphthalenediimide with an aliphatic amine proceeded
   rapidly under benign conditions. Similarly, the two vinyl groups
   attached to aromatic diimides reacted with amines simultaneously,
   resulting in the formation of amine bisadducts and macromolecules. An
   amino group appended perylenediimide through an ethylene spacer at the
   pi-core exhibited distinct fluorescence switching in response to acid
   and base.}},
