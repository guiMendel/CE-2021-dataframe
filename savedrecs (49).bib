@article{ WOS:000088570900007,
Author = {Blanc, G and Barakat, A and Guyot, R and Cooke, R and Delseny, I},
Title = {{Extensive duplication and reshuffling in the arabidopsis genome}},
Journal = {{PLANT CELL}},
Year = {{2000}},
Volume = {{12}},
Number = {{7}},
Pages = {{1093-1101}},
Month = {{JUL}},
Abstract = {{Systematic analysis of the Arabidopsis genome provides a basis for
   detailed studies of genome structure and evolution. Members of multigene
   families were mapped, and random sequence alignment was used to identify
   regions of extended similarity in the Arabidopsis genome. Detailed
   analysis showed that the number, order, and orientation of genes were
   conserved over large regions of the genome, revealing extensive
   duplication covering the majority of the known genomic sequence. Fine
   mapping analysis showed much rearrangement, resulting in a patchwork of
   duplicated regions that indicated deletion, insertion, tandem
   duplication, inversion, and reciprocal translocation. The implications
   of these observations for evolution of the Arabidopsis genome as well as
   their usefulness for analysis and annotation of the genomic sequence and
   in comparative genomics are discussed.}},
