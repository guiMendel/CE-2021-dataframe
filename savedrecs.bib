@article{ WOS:000608875100066,
Author = {Khrenova, Maria G. and Mulashkin, Fedor D. and Bulavko, Egor S. and
   Zakharova, Tatiana M. and Nemukhin, V, Alexander},
Title = {{Dipole Moment Variation Clears Up Electronic Excitations in the
   pi-Stacked Complexes of Fluorescent Protein Chromophores}},
Journal = {{JOURNAL OF CHEMICAL INFORMATION AND MODELING}},
Year = {{2020}},
Volume = {{60}},
Number = {{12}},
Pages = {{6288-6297}},
Month = {{DEC 28}},
Abstract = {{We propose a quantitative structure-property relationship (QSPR) model
   for prediction of spectral tuning in cyan, green, orange, and red
   fluorescent proteins, which are engineered by motifs of the green
   fluorescent protein. Protein variants, in which their chromophores are
   involved in the pi-stacking interaction with amino acid residues
   tyrosine, phenylalanine, and histidine, are prospective markers useful
   in bioimaging and super-resolution microscopy. In this work, we
   constructed training sets of the pi-stacked complexes of four
   fluorescent protein chromophores (of the green, orange, red, and cyan
   series) with various substituted benzenes and imidazoles and tested the
   use of dipole moment variation upon excitation (DMV) as a descriptor to
   evaluate the vertical excitation energies in these systems. To validate
   this approach, we computed and analyzed electron density distributions
   of the pi-stacked complexes and correlated the QSPR predictions with the
   reference values of the transition energies obtained using the
   high-level ab initio quantum chemistry methods. According to our
   results, the use of the DMV descriptor allows one to predict excitation
   energies in the pi-stacked complexes with errors not exceeding 0.1 eV,
   which makes this model a practically useful tool in the development of
   efficient fluorescent markers for in vivo imaging.}},
