@article{ WOS:000234156500003,
Author = {Efremov, SV and Polyak, BT},
Title = {{Using predictive control to synchronize chaotic systems}},
Journal = {{AUTOMATION AND REMOTE CONTROL}},
Year = {{2005}},
Volume = {{66}},
Number = {{12}},
Pages = {{1905-1915}},
Month = {{DEC}},
Abstract = {{A method of synchronization of several identical chaotic systems using a
   new type of control was proposed. It is based on predicting the
   trajectories of each of the nonlinear systems and correcting them by the
   deviation of the prediction from the desired value. Three types of
   control with global interaction between the systems, local interaction
   between the systems, and one leading oscillator affecting the rest of
   them were proposed. After synchronization, all systems make identical
   chaotic motions. This method features low level of the control action;
   in principle, control may be made arbitrarily small. Numerical modeling
   bore out efficiency of this method for any number of oscillators.}},
