@article{ WOS:000360274600008,
Author = {Fuentes-Gari, Maria and Velliou, Eirini and Misener, Ruth and Pefani,
   Eleni and Rende, Maria and Panoskaltsis, Nicki and Mantalaris,
   Athanasios and Pistikopoulos, Efstratios N.},
Title = {{A systematic framework for the design, simulation and optimization of
   personalized healthcare: Making and healing blood}},
Journal = {{COMPUTERS \& CHEMICAL ENGINEERING}},
Year = {{2015}},
Volume = {{81}},
Number = {{SI}},
Pages = {{80-93}},
