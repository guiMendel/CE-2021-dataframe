@article{ WOS:000594257600001,
Author = {Andrgie, Abegaz Tizazu and Darge, Haile Fentahun and Mekonnen, Tefera
   Worku and Birhan, Yihenew Simegniew and Hanurry, Endiries Yibru and
   Chou, Hsiao-Ying and Wang, Chih-Feng and Tsai, Hsieh-Chih and Yang, Jen
   Ming and Chang, Yen-Hsiang},
Title = {{Ibuprofen-Loaded Heparin Modified Thermosensitive Hydrogel for
   Inhibiting Excessive Inflammation and Promoting Wound Healing}},
Journal = {{POLYMERS}},
Year = {{2020}},
Volume = {{12}},
Number = {{11}},
Month = {{NOV}},
Abstract = {{Hydrogels have been investigated as ideal biomaterials for wound
   treatment owing to their ability to form a highly moist environment
   which accelerates cell migration and tissue regeneration for prompt
   wound healing. They can also be used as a drug carrier for local
   delivery, and are able to activate immune cells to enhance wound
   healing. Here, we developed heparin-conjugated
   poly(N-isopropylacrylamide), an injectable, in situ gel-forming polymer,
   and evaluated its use in wound healing. Ibuprofen was encapsulated into
   the hydrogel to help reduce pain and excessive inflammation during
   healing. In addition to in vitro studies, a BALB/c mice model was used
   to evaluate its effect on would healing and the secretion of
   inflammatory mediators. The in vitro assay confirmed that the ibuprofen
   released from the hydrogel dramatically reduced
   lipopolysaccharide-induced inflammation by suppressing the production of
   NO, PGE2 and TNF-alpha in RAW264.7 macrophages. Moreover, an in vivo
   wound healing assay was conducted by applying hydrogels to wounds on the
   backs of mice. The results showed that the ibuprofen-loaded hydrogel
