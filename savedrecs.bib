@article{ WOS:000603068600003,
Author = {Cheong, Z. and Moreira, G. M. and Bettega, M. H. F. and Blanco, F. and
   Garcia, G. and Brunger, M. J. and White, R. D. and Sullivan, J. P.},
Title = {{A comparison of experimental and theoretical low energy positron
   scattering from furan}},
Journal = {{JOURNAL OF CHEMICAL PHYSICS}},
Year = {{2020}},
Volume = {{153}},
Number = {{24}},
Month = {{DEC 28}},
Abstract = {{This paper presents a joint experimental and theoretical study of
   positron scattering from furan. Experimental data were measured using
   the low energy positron beamline located at the Australian National
   University and cover an energy range from 1 eV to 30 eV. Cross sections
   were measured for total scattering, total elastic and inelastic
   scattering, positronium formation, and differential elastic scattering.
   Two theoretical approaches are presented: the Schwinger multichannel
   method and the independent atom method with screening corrected
   additivity rule. In addition, our data are compared to corresponding
   electron scattering results from the same target with a number of
   significant differences observed and discussed.}},
