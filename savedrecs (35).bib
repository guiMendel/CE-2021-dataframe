@article{ WOS:000234384000001,
Author = {Liu, XH and Gao, SQ and Song, WL},
Title = {{Prediction of core-annulus solids mass transfer coefficient in gas-solid
   fluidized bed risers}},
Journal = {{CHINESE JOURNAL OF CHEMICAL ENGINEERING}},
Year = {{2005}},
Volume = {{13}},
Number = {{5}},
Pages = {{577-582}},
Month = {{OCT}},
Abstract = {{Based on analysis of energy dissipation in the core region of gas-solid
   fluidized bed risers, a simplified model for determination of
   core-annulus solids mass transfer coefficient was developed according to
   turbulent diffusion mechanism of particles. The simulation results are
   consistent with published experimental data. Core-annulus solids mass
   transfer coefficient decreases with increasing particle size, particle
   density and solids circulation rate, but generally increases with
   increasing superficial gas velocity and riser diameter. In the upper
   dilute region of gas-solid fluidized bed risers, core-annulus solids
   mass transfer coefficient was found to change little with the axial
   coordinate in the bed.}},
