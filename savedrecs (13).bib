@article{ WOS:000367669700006,
Author = {Nyce, Charles and Dumm, Randy E. and Sirmans, G. Stacy and Smersh, Greg},
Title = {{THE CAPITALIZATION OF INSURANCE PREMIUMS IN HOUSE PRICES}},
Journal = {{JOURNAL OF RISK AND INSURANCE}},
Year = {{2015}},
Volume = {{82}},
Number = {{4}},
Pages = {{891-919}},
Month = {{DEC}},
Abstract = {{This study uses Miami-Dade County, Florida home sales, and Citizens
   Property Insurance Corporation data for the period 2004 through 2009 to
   measure the capitalization effect of increases in premiums on house
   prices. Using hedonic pricing models, spatial autocorrelation models,
   and difference-in-differences models, we find that new information was
   conveyed to homeowners in the higher risk areas by the 2004/2005 storms
   and that consumers appear to use the insurance premium as a ``risk
   signal.{''} We also find some support for the hypothesis that the risk
   of potential hurricane losses is communicated to potential homebuyers
   through windzone maps.}},
