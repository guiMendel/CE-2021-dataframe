@article{ WOS:000602684400001,
Author = {Somsen, G. Aernout and van Rijn, Cees J. M. and Kooij, Stefan and Bem,
   Reinout A. and Bonn, Daniel},
Title = {{Measurement of small droplet aerosol concentrations in public spaces
   using handheld particle counters}},
Journal = {{PHYSICS OF FLUIDS}},
Year = {{2020}},
Volume = {{32}},
Number = {{12}},
Month = {{DEC 1}},
Abstract = {{We measure aerosol persistence to assess the risk of transmission of
   severe acute respiratory syndrome coronavirus 2 (SARS-CoV-2) in public
   spaces. Direct measurement of aerosol concentrations, however, has
   proven to be technically difficult; we propose the use of handheld
   particle counters as a novel and easily applicable method to measure
   aerosol concentrations. This allows us to perform measurements in
   typical public spaces, each differing in volume, the number of people,
   and the ventilation rate. These data are used to estimate the relation
   between the aerosol persistence time and the risk of infection with
   SARS-CoV-2.}},
