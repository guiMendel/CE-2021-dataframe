@article{ WOS:A1995TK58500002,
Author = {TAKADA, K and HASEBA, T and TATSUMA, T and OYAMA, N and LI, QM and
   WHITE, HS},
Title = {{IN-SITU INTERFEROMETRIC MICROSCOPY OF TEMPERATURE-DEPENDENT AND
   POTENTIAL-DEPENDENT VOLUME CHANGES OF A REDOX GEL}},
Journal = {{ANALYTICAL CHEMISTRY}},
Year = {{1995}},
Volume = {{67}},
Number = {{24}},
Pages = {{4446-4451}},
Month = {{DEC 15}},
Abstract = {{Temperature- and potential-controlled phase transitions of a thin film
   of a redox gel prepared from copolymerization of N-isopropylacrylamide,
   vinylferrocene, and N,N'-methylenebisacrylamide at a Au electrode, were
   monitored by in situ phase measurement interferometric microscopy
   (PMIM). The thickness and width of the gel film in a NaClO4 solution
   decreased from similar to 10 and similar to 160 mu m, respectively, at
   temperatures below 20 degrees C, to similar to 4 and similar to 120 mu
   m, respectively, above 30 degrees C. Electrochemical oxidation of the
   ferrocene groups in the polymer gel (to positively charged ferrocenium)
   is shown to result in a similar to 5 degrees C increase in the
   temperature at which the volume changes occurred. A reversible
   potential-dependent volume change at constant temperature was also
   observed using PMIM. The results are discussed in terms of the
   dependency of hydrophobic and hydrophilic interactions of the polymer
   chains on the temperature and oxidation state of the gel film.}},
