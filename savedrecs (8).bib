@article{ WOS:000587511700001,
Author = {Kato, Shunsuke and Onoda, Akira and Taniguchi, Naomasa and Schwaneberg,
   Ulrich and Hayashi, Takashi},
Title = {{Directed Evolution of a Cp{*}Rh-III-Linked Biohybrid Catalyst Based on a
   Screening Platform with Affinity Purification}},
Journal = {{CHEMBIOCHEM}},
Year = {{2021}},
Volume = {{22}},
Number = {{4}},
Pages = {{679-685}},
Month = {{FEB 15}},
Abstract = {{Directed evolution of Cp{*}Rh-III-linked nitrobindin (NB), a biohybrid
   catalyst, was performed based on an in vitro screening approach. A key
   aspect of this effort was the establishment of a high-throughput
   screening (HTS) platform that involves an affinity purification step
   employing a starch-agarose resin for a maltose binding protein (MBP)
   tag. The HTS platform enables efficient preparation of the purified
   MBP-tagged biohybrid catalysts in a 96-well format and eliminates
   background influence of the host E. coli cells. Three rounds of directed
   evolution and screening of more than 4000 clones yielded a
   Cp{*}Rh-III-linked NB(T98H/L100K/K127E) variant with a 4.9-fold enhanced
   activity for the cycloaddition of acetophenone oximes with alkynes. It
   is confirmed that this HTS platform for directed evolution provides an
   efficient strategy for generating highly active biohybrid catalysts
   incorporating a synthetic metal cofactor.}},
