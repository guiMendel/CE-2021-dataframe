@article{ WOS:000231942200015,
Author = {Kirichenko, AV and Pfitzner, U and Ludwig, B and Soares, CM and
   Vygodina, TV and Konstantinov, AA},
Title = {{Cytochrome c oxidase as a calcium binding protein. Studies on the role
   of a conserved aspartate in helices XI-XII cytoplasmic loop in cation
   binding}},
Journal = {{BIOCHEMISTRY}},
Year = {{2005}},
Volume = {{44}},
Number = {{37}},
Pages = {{12391-12401}},
Month = {{SEP 20}},
Abstract = {{The aa(3)-type cytochrome c oxidases from mitochondria and bacteria
   contain a cation-binding site located in subunit I near heme a. In the
   oxidases from Paracoccus denitrificans or Rhodobacter sphaeroides, the
   site is occupied by tightly bound calcium, whereas the mitochondrial
   oxidase binds reversibly calcium or sodium that compete with each other.
   The functional role of the site has not yet been established. D477A
   mutation in subunit I of P. denitrificans oxidase converts the
   cation-binding site to a mitochondrial-type form that binds reversibly
   calcium and sodium ions {[}Pfitzner, U., Kirichenko, A., et al. (1999)
   FEBS Lett. 456, 365-369]. We have studied reversible cation binding with
   P. denitrificans D477A oxidase and compared it with that in bovine
   enzyme. In bovine oxidase, one Ca2+ competes with two Na+ for the
   binding, indicating the presence of two Na+-binding sites in the enzyme,
   Na-(1)(+) and Na-(2)(+). In contrast, the D477A mutant of COX from P.
   denitrificans reveals competition of Ca2+ (K-d = 1 mu M) with only one
   sodium ion (K-d = 4 mM). The second binding site for Na+ in bovine
   oxidase is proposed to involve D442, homologous to D477 in P.
   denitrificans oxidase. A putative place for Na+(2) in subunit I of
   bovine oxidase has been found with the aid of structure modeling located
   7.4 angstrom from the bound Na-(1)(+). Na-(2)(+) interacts with a
   cluster of residues forming an exit part of the so-called H-proton
   channel, including D51 and S441.}},
