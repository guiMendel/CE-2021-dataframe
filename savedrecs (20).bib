@article{ WOS:000285584200068,
Author = {Lee, Chao-Te and Lin, Hoang-Yan and Tsai, Chao-Hsu},
Title = {{Designs of broadband and wide-view patterned polarizers for stereoscopic
   3D displays}},
Journal = {{OPTICS EXPRESS}},
Year = {{2010}},
Volume = {{18}},
Number = {{26}},
Pages = {{27079-27094}},
Month = {{DEC 20}},
Abstract = {{This work presents a new methodology to design patterned circular
   polarizers consisting of in-cell polarizers, in-cell retarders and
   biaxial films to achieve very wide viewing freedom for stereoscopic 3D
   color LCDs. The biaxial films with least materials and simple
   fabrication concepts are employed for off-axis compensation of the
   in-cell retarders. In the best result, the crosstalk ratio is less than
   0.035 or 0.0082 respectively for over +/- 60 degrees or +/- 40 degrees
   viewing cone of the 3D display. As to the normal view of the proposed
   structure with glasses or LCD rotation, the crosstalk ratio is less than
   0.11\%, with 93.5\% improvement as compared with the +/- lambda/4
   patterned polarizer. The dispersion properties of materials have been
   considered in all simulations to mimic real situations. (C) 2010 Optical
   Society of America}},
