@article{ WOS:A1995RU34100017,
Author = {KWON, OY and JEONG, SY and SUH, JK and LEE, JM},
Title = {{HYDROTHERMAL SYNTHESES OF NA-MAGADIITE AND NA-KENYAITE IN THE PRESENCE
   OF CARBONATE}},
Journal = {{BULLETIN OF THE KOREAN CHEMICAL SOCIETY}},
Year = {{1995}},
Volume = {{16}},
Number = {{8}},
Pages = {{737-741}},
Month = {{AUG 20}},
Abstract = {{In order to synthesize well-crystallized Na-magadiite and Na-kenyaite as
   a single phase in a short reaction time, the amorphous silica was
   hydrothermally reacted with the alkali solution of NaOH-Na2CO3 in the
   temperature range of 150 to 180 degrees C. From a suspension containing
   SiO2 and NaOH-Na2CO3 with the molar ratios of SiO2/(NaOH + Na2CO3) = 5
   and H2O/(NaOH + Na2CO3)= 100, magadiite was formed after 24 hours of
   reaction at 150 degrees C. On the other hand, kenyaite was formed with
   magadiite or quartz simultaneously in 48-96 hours of reaction at 160 or
   170 degrees C. In the molar ratios of SiO2(NaOH + Na2CO3) = 5-10,
   H2O/(NaOH + Na2CO3) = 100, and SiO2/(NaOH + Na2CO3) = 3, H2O/ (NaOH +
