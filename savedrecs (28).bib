@article{ WOS:000282099200005,
Author = {Tseloni, Andromachi and Ntzoufras, Ioannis and Nicolaou, Anna and Pease,
   Ken},
Title = {{Concentration of personal and household crimes in England and Wales}},
Journal = {{EUROPEAN JOURNAL OF APPLIED MATHEMATICS}},
Year = {{2010}},
Volume = {{21}},
Number = {{4-5, SI}},
Pages = {{325-348}},
Month = {{OCT}},
Abstract = {{Crime is disproportionally concentrated in few areas. Though long
   established, there remains uncertainty about the reasons for variation
   in the concentration of similar crime (repeats) or different crime
   (multiples). Wholly neglected have been composite crimes when more than
   one crime types coincide as parts of a single event. The research
   reported here disentangles area crime concentration into repeats,
   multiple and composite crimes. The results are based on estimated
   bivariate zero-inflated Poisson regression models with covariance
   structure which explicitly account for crime rarity and crime
   concentration. The implications of the results for criminological
   theorizing and as a possible basis for more equitable police funding are
   discussed.}},
