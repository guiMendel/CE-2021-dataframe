@article{ WOS:000235144600015,
Author = {Tsuji, T and Okajima, S and Sasaki, A and Masuda, T},
Title = {{Steam reforming of oils produced from waste plastics}},
Journal = {{JOURNAL OF CHEMICAL ENGINEERING OF JAPAN}},
