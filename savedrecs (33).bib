@article{ WOS:000232930900008,
Author = {Krauss, RM},
Title = {{Dietary and genetic probes of atherogenic dyslipidemia}},
Journal = {{ARTERIOSCLEROSIS THROMBOSIS AND VASCULAR BIOLOGY}},
