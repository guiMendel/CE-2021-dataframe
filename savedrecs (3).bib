@article{ WOS:000508233400015,
Author = {Hassan, Nabil M. and Mansour, N. A. and Salah, Talaat and Gamal, Saher},
Title = {{Comparison of Thermoluminescence Dosimeter's Response to Neutron and
   Gamma in a Mixed Radiation Field}},
Journal = {{ARAB JOURNAL OF NUCLEAR SCIENCES AND APPLICATIONS}},
Year = {{2020}},
Volume = {{53}},
Number = {{1}},
Pages = {{149-155}},
Month = {{WIN}},
Abstract = {{Measuring radiation dose in a mixed radiation field cannot be performed
   by using only one dosimeter, but a pair of dosimeters, TLD600 and TLD700
   can be successfully used to estimate that radiation dose. Therefore, in
   the present study, the response of TLD600 and TLD700 to neutron/gamma
   mixed radiation field was deeply discussed. Moreover, the sensitivity
   and stability of TLD600 and TLD700 were studied. A strong correlation
   between TL output signal intensity of TLD700 to gamma dose appeared,
   with a linear coefficient of R-2 = 0.99. Likewise, a strong correlation
   between TL output signal intensity of TLD600 to gamma dose and neutron
   irradiation was also evident, with a linear coefficient of R-2 = 0.99.
   Thus, TLD700 can be applied to measure gamma dose with good sensitivity,
   while TLD600 can be applied to measure both gamma and neutron dose.
   Hence, a combination of TLD700 and TLD600 can be successfully employed
   to assess the radiation doses characterizing a mixed radiation field of
   neutron and gamma rays.}},
