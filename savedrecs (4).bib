@article{ WOS:000661543400070,
Author = {Pisipati, Sindhura and Zafar, Adnan and Zafar, Yousaf},
Title = {{Campylobacter coli bacteraemia: how common is it?}},
Journal = {{BMJ CASE REPORTS}},
Year = {{2020}},
Volume = {{13}},
Number = {{12}},
Month = {{DEC}},
Abstract = {{Campylobacter species are known to cause enteritis. However, over the
   past 40-50 years, there have been reports of varying presentations, such
   as cellulitis, spondylodiscitis and bacteraemia. Of the Campylobacter
   species, Campylobacter jejuni is the most common culprit for causing
   bacteraemia, however, Campylobacter coli bacteraemia is becoming more
   prevalent. Here, we discuss an unusual case of C. coli bacteraemia in a
