@article{ WOS:000595689700001,
Author = {Favre, Adrien and Pringle, James S. and Heckenhauer, Jacqueline and
   Kozuharova, Ekaterina and Gao, Qingbo and Lemmon, Emily Moriarty and
   Lemmon, Alan R. and Sun, Hang and Tkach, Natalia and Gebauer, Sebastian
   and Sun, Shan-Shan and Fu, Peng-Cheng},
Title = {{Phylogenetic relationships and sectional delineation within Gentiana
   (Gentianaceae)}},
