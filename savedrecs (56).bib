@article{ WOS:A1995TG98600001,
Author = {URAZAEV, AK and MAZITOV, BK and POLETAEV, GI},
Title = {{EFFECT OF AXOPLASMIC-TRANSPORT ON THE MECHANISM RESPONSIBLE FOR
   MAINTENANCE OF CELL-VOLUME IN RAT SKELETAL-MUSCLES}},
Journal = {{NEUROPHYSIOLOGY}},
Year = {{1995}},
Volume = {{26}},
Number = {{5}},
Pages = {{267-269}},
Month = {{SEP-OCT}},
Abstract = {{The effect of colchicine, a drug impairing fast axoplasmic now, on the
   ability of muscle fibers to recover their volume after dehydration in a
   hypertonic medium, was studied in experiments on rat diaphragm. It was
   found that the ability of the muscle fibers to maintain their volume in
   a hyperosmolar medium depends on the effect of `'trophic factors''
   transferred to the muscle by the axenic flow. Elimination of this effect
   by blocking the axoplasmic transport with colchicine impairs the ability
   of active chloride transport to be enhanced following an increase in the
   muscle osmotic pressure, which results in the loss of the muscle fiber's
   capability to maintain stable content of intracellular water in a
   hypertonic medium.}},
