@article{ WOS:000284243100020,
Author = {Johnson, James H. and Ross, Robert M. and McCullough, Russell D. and
   Mathers, Alastair},
Title = {{A Comparative Analysis of Double-Crested Cormorant Diets from Stomachs
   and Pellets from Two Lake Ontario Colonies}},
Journal = {{JOURNAL OF FRESHWATER ECOLOGY}},
Year = {{2010}},
Volume = {{25}},
Number = {{4}},
Pages = {{669-672}},
Month = {{DEC}},
Abstract = {{Double-crested cormorant (Phalacrocorax auritus) diets were compared
   with evidence from the stomachs of shot birds and from regurgitated
   pellets at High Bluff Island and Little Galloo Island, Lake Ontario. The
   highest similarity in diets determined by stomach and pellet analyses
   occurred when both samples were collected on the same day. Diet overlap
   dropped substantially between the two methods when collection periods
   were seven to ten days apart, which suggested differences in prey
   availability between the two periods. Since the average number of fish
   recovered in pellets was significantly higher than that in stomachs, use
   of pellets to determine fish consumption of double-crested cormorants
   may be more valid than stomach analysis because pellet content represent
   an integrated sampling of food consumed over approximately 24 hours.}},
