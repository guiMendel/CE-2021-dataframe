@article{ WOS:A1995TF05400001,
Author = {KARZEL, H and WEFELSCHEID, H},
Title = {{A GEOMETRIC CONSTRUCTION OF THE K-LOOP A HYPERBOLIC SPACE}},
Journal = {{GEOMETRIAE DEDICATA}},
Year = {{1995}},
Volume = {{58}},
Number = {{3}},
Pages = {{227-236}},
Month = {{DEC}},
Abstract = {{It is well known that the homogeneous orthochronous proper Lorentzgroup
   Gamma is isomorphic to the proper motion group of the hyperbolic space.
   To each Lorentz boost beta is an element of Gamma \textbackslash{}
   \{id\} there corresponds in the hyperbolic space exactly one line
   L(beta) such that beta fixes each of the two ends of L(beta).
   Furthermore beta has no fixed points but each plane containing L(beta)
   is fixed by beta. If we fix a point o, then to each other point a there
   is exactly one boost a(+) is an element of Gamma such that L(a+) is the
   line joining o and a and a(+)(o) = a. The set P of points of the
   hyperbolic space is turned in a K-loop (P, +) by a + b := a(+)(b). Each
   line of the hyperbolic space has the representation a + Z(b) where a, b
   is an element of P, b not equal 0 and Z(b) := \{x is an element of P
   \textbackslash{} x + b = b + x\}.}},
