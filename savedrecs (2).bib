@article{ WOS:000589637600011,
Author = {Channappagoudra, Manjunath and Ramesh, K. and Manavendra, G.},
Title = {{Effect of piston bowl geometry on diesel engine performance operated
   with dairy scum biodiesel}},
Journal = {{INTERNATIONAL JOURNAL OF AMBIENT ENERGY}},
Year = {{2020}},
Volume = {{41}},
Number = {{14}},
Pages = {{1628-1638}},
Month = {{DEC 5}},
Abstract = {{A sincere effort towards conversion and utilisation of any locally
   available bio-waste as alternative fuels for the internal combustion
   engine is most required nowadays. However, diesel engine parameters are
   more attuned with diesel fuel operation. In this regard, optimisation of
   piston bowl geometry is highly distinct to improve the substandard
   performance of the biodiesel operated diesel engine. In the present
   study, existing diesel engine hemispherical piston bowl geometry (HPBG)
   is modified into toroidal piston bowl geometry (TPBG) to evaluate the
   diesel engine performance operated with 20\% dairy scum biodiesel (B20).
   The TPBG-ME operation has shown 6.50\% of increased brake thermal
   efficiency, 17.24\% of increased heat release rate, 1.97\% of increased
   cylinder pressure, 3.92\% of increased pressure rise rate 8.75\% of
   reduced brake-specific fuel consumption, 15\% of lowered HC emission,
   8.33\% of reduced ignition delay and 11.36\% of decreased combustion
   duration when compared with the standard engine with piston HPBG run
   with B20 fuel blend (DSOME-B20-SE).}},
