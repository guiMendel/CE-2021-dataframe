@article{ WOS:000590030900002,
Author = {Cao, Xiaobing and Chen, Fan and Cen, Kehui and Zhang, Jie and Chen,
   Dengyu and Li, Yanjun},
Title = {{Effect of torrefaction on the pyrolysis behavior, kinetics, and phenolic
   products of lignin}},
Journal = {{BIOMASS CONVERSION AND BIOREFINERY}},
Abstract = {{Torrefaction is a process of mild pyrolysis at 200-300 degrees C under
   nitrogen atmosphere and has been considered to be an effective way to
   improve the properties of biomass. In this study, lignin was subjected
   to torrefaction at 200 degrees C, 230 degrees C, 260 degrees C, and 290
   degrees C. Thermogravimetric analysis of lignin samples was performed at
   a temperature ramp of 25 to 550 degrees C with the heating rates of 10,
   20, 30, or 40 degrees C min(-1). A distributed activation energy model
   was used to analyze the pyrolysis kinetics of lignin. Then, the lignin
   samples were subjected to pyrolysis at 600 degrees C for 10 s. The
   results showed that torrefaction had a significant effect on the
   physicochemical properties and pyrolysis characteristics of lignin.
   Among the torrefied samples, the sample treated at 290 degrees C
   exhibited the lowest mass yield (86.07\%), lowest energy yield
   (93.25\%), highest content of carbon (63.76\%), highest higher heating
   value (25.18 MJ kg(-1)), and lowest content of oxygen (30.25\%). The
   activation energy of torrefied lignin samples slightly changed in
   comparison with those of the raw samples. Torrefaction has a significant
   effect on pyrolysis products. The peak area \% of S-type (41.4\%) and
   P-type (5.4\%) phenols in the bio-oil obtained by pyrolysis of LN-200
   was the highest.}},
Publisher = {{SPRINGER HEIDELBERG}},
Address = {{TIERGARTENSTRASSE 17, D-69121 HEIDELBERG, GERMANY}},
