@article{ WOS:000089438700018,
Author = {Maurel, F and Leblanc, P and Knosp, B and Backhaus-Ricoult, M},
Title = {{Effect of yttrium on the corrosion of AB(5)-type alloys for nickel-metal
   hydride batteries}},
Journal = {{JOURNAL OF ALLOYS AND COMPOUNDS}},
Year = {{2000}},
Volume = {{309}},
Number = {{1-2}},
Pages = {{88-94}},
Month = {{SEP 14}},
Abstract = {{The influence of Y2O3 powder addition (as well as of rare earth oxides
   of Gd, no, Dy, Nd, Sm or Yb) to the metal hydride electrode on AB, alloy
   corrosion rates and on the chemical composition, morphology and density
   of the corrosion products was studied by X-ray, SEM, TEM and HRTEM.
   Adding Y2O3 reduces the corrosion rate. This is particularly observed in
   the long-term behavior of the batteries and is more pronounced at higher
   temperature. When yttrium is added as an oxide to the electrode or
   electrolyte, it is dissolved in the electrolyte and is further
   incorporated as hydroxide in the corrosion scale. The decrease in the
   global alloy corrosion rate can be explained by a decrease of the
   driving force for diffusion of Mm(3+) and OH-. Yb is the rare earth
