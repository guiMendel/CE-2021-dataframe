@article{ WOS:000165799100024,
Author = {Hogan, JF and Blum, JD and Siegel, DI and Glaser, PH},
Title = {{Sr-87/Sr-86 as a tracer of groundwater discharge and precipitation
   recharge in the Glacial Lake Agassiz Peatlands, northern Minnesota}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{2000}},
Volume = {{36}},
Number = {{12}},
Pages = {{3701-3710}},
Month = {{DEC}},
Abstract = {{A contrast in the Sr-87/Sr-86 ratio of atmospherically derived Sr
   (similar to0.710) and Sr from regional bedrock (>0.730) allows
   quantification of the proportions of precipitation recharge and
   groundwater discharge into peat columns of bogs and fens in the Glacial
   Lake Agassiz Peatlands. Two contrasting bog-fen pairs were studied: the
   Lost River peatland located over a regional groundwater discharge zone
   and the Red Lake peatland situated atop a hydrologic divide where
   recharge is expected to occur. Bog pore waters consist of 90-100\%
   precipitation in the upper 2 m, below which they mix with groundwater.
   Adjacent fens consist mostly of groundwater, ranging from similar to
   30\% near the surface to similar to 100\% with depth. At the Red Lake
   peatland, mixing relations suggest an additional Sr source that may be
   peat mineralization. Distinct Sr-87/Sr-86 and {[}Ca]/{[}Sr] ratios for
   groundwater discharging into bog and fen sites are indicative of
   distinct groundwater sources and are likely indicative of flow paths at
   two different length scales.}},
