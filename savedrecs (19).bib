@article{ WOS:000362667200031,
Author = {Tsukada, Kenji and Nihei, Eisuke},
Title = {{Ray tracing method in arbitrarily shaped radial graded-index waveguide}},
Journal = {{APPLIED OPTICS}},
Year = {{2015}},
Volume = {{54}},
Number = {{29}},
Pages = {{8795-8799}},
Month = {{OCT 10}},
Abstract = {{A ray tracing algorithm for an arbitrarily shaped axially symmetric
   graded index waveguide was proposed. This was achieved by considering
   the center of the waveguide (optical axis) as a set of discrete points.
   The refractive index depends on the distance of the ray position from
   the optical axis. This distance was approximated as the shortest
   distance between the ray position and a point in the set. Using this
   algorithm, ray tracing could be performed, regardless of the waveguide
   configuration. In this study, a precise explanation of the algorithm is
   given and the errors are evaluated. A technique to reduce computing time
   is also included. (c) 2015 Optical Society of America}},
