Language = {{English}},
Affiliation = {{BUNZL, K (Corresponding Author), GSF FORSCHUNGSZENTRUM UNWELT \& GESUNDHEIT,INST STRAHLENSCHUTZ,D-85758 OBERSCHLEISSHEIM,GERMANY.}},
DOI = {{10.1021/ie00047a005}},
