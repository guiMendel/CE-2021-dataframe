@article{ WOS:000234410700015,
Author = {Vinet, EV and Gomez-Maqueo, MEL},
Title = {{Applicability of Mexican and United States norms for the MMPI-A in the
   assessment of Chilean adolescents}},
Journal = {{REVISTA MEXICANA DE PSICOLOGIA}},
Year = {{2005}},
Volume = {{22}},
Number = {{2}},
Pages = {{519-528}},
Month = {{DEC}},
Abstract = {{The applicability of USA and Mexican norms for the MMPI-A to assess
   Chilean adolescents was evaluated with two strategies: (1) raw scores in
   the MMPI-A scales, obtained from 777 Chilean adolescents who came from
   an urban setting, were attending school, and had no evidence of
   emotional or behavioral problems, were compared to raw scores obtained
   by both normative samples, evaluating the magnitude of significant
   differences, and (2) the MMPI-A profile for the Chilean sample was
   plotted against USA and Mexican norms and significant deviations from
   T50 were analyzed. Clinical and cultural interpretations of the results
   indicate that Mexican norms are suitable for Chilean male adolescents,
   while for female adolescents local norms will have to be developed.}},
