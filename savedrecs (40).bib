Title = {{Abundance of Lytechinus variegatus (Echinoidea : Toxopneustidae) in the
   island of Cubagua,Venezuela}},
Journal = {{REVISTA DE BIOLOGIA TROPICAL}},
Year = {{2000}},
Volume = {{48}},
