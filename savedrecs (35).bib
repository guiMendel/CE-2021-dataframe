@article{ WOS:000232940000007,
Author = {Maddox, JD and Wiedenmann, RN},
Title = {{Nesting of birds in wetlands containing purple loosestrife (Lythrum
   salicaria) and cattail (Typha spp.).}},
Journal = {{NATURAL AREAS JOURNAL}},
Year = {{2005}},
Volume = {{25}},
Number = {{4}},
Pages = {{369-373}},
Month = {{OCT}},
Abstract = {{We examined nesting by seven native bird species in Illinois wetland
   habitats containing native cattail (Typha spp.) and non-native purple
   loosestrife, (Lythrum salicaria) as nesting substrates in 2000 and 2001.
   Over the two seasons, we found 146 nests of seven species: I 11 in
   cattail and 35 in purple loosestrife. Marsh wrens (Cistothorus
   palustris) nested in cattail (11 nests) but not in loosestrife in either
   year. Common grackles (Quiscalus quiscula) nested predominantly in
   cattail (63 in cattail vs. I in loosestrife). Only red-winged blackbirds
   (Agelaius phoeniceus) nested extensively in both cattail (37 nests) and
   loosestrife (30 nests). Individual nests of four bird species were found
   only in loosestrife. First-egg dates and peak nesting activity of
   red-winged blackbirds were almost three weeks later in loosestrife than
   in cattail. Despite the difference in nesting dates, daily survival
   rates of red-winged blackbird nests were similar in the two nesting
   substrates in both years. One wetland was burned in 2001, nearly
   eliminating cattail but not loosestrife as an early-season nesting
   substrate at the site; after the burn, red-winged blackbirds nested
   later in loosestrife in 2001, as they had done in 2000. At the burned
   site where loosestrife was the only available nesting substrate, marsh
   wrens were absent in 2001, although they had nested in cattail at that
   site in 2000. Our results suggest that wetlands dominated by purple
   loosestrife are not adequate breeding habitats for marsh wrens and
   common grackles. Habitats with a mixture of cattail and loosestrife
   supported successful nesting by red-winged blackbirds, albeit later in
   loosestrife.}},
