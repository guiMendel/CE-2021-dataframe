@article{ WOS:000165605900014,
Author = {Shelton, TG and Appel, AG},
Title = {{Cyclic CO2 release and water loss in the western drywood termite
   (Isoptera : kalotermitidae)}},
Journal = {{ANNALS OF THE ENTOMOLOGICAL SOCIETY OF AMERICA}},
Year = {{2000}},
Volume = {{93}},
Number = {{6}},
Pages = {{1300-1307}},
Month = {{NOV}},
Abstract = {{CO2 release and water loss patterns of Incisitermes minor (Hagen)
   pseudergates were investigated using now-through respirometry
   techniques. Four hours of real-time CO2 release and water loss data were
   recorded for pseudergates of I. minor at 20-40 degreesC. Cyclic release,
   but not traditional DGC, was the CO2 release pattern observed in these
   pseudergates. Variability of (V) over dot CO2, expressed as coefficient
   of variability (CV) was used to estimate the level of cycling in each
   recording. CV decreased exponentially with temperature. Mean (V) over
   dot CO2 values ranged from 0.24 +/- 0.07 ml CO2 g(-1) h(-1) at 25
   degreesC to 0.89 +/- 0.16 ml CO2 g(-1) h(-1) at 35 degreesC. (V) over
   dot CO2 increased significantly (P = 0.014) with temperature, but was
   not influenced (P = 0.063) by body mass. Q(10) for (V) over dot CO2 over
   the range of 20-40 degreesC was 1.75 for I. minor pseudergates. Water
   loss cycled coincidently with CO2 release. Water loss rates ranged from
   5.22 +/- 1.97 mg H2O lost g(-1) h(-1) at 20 degreesC to 17.96 +/- 2.55
   mg H2O lost g(-1) h(-1) at 40 degreesC. Q(10) for water loss over the
   range of 20-40 degreesC was 1.86 for I. minor pseudergates. Daily
   respiratory water loss represented from 9.23\% at 20 degreesC to 4.72\%
   at 25 degreesC of total daily water loss. Cuticular water loss
   represented from 93.5\% at 25 degreesC to 89.1\% at 30 degreesC of total
   daily water loss.}},
