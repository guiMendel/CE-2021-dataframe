@article{ WOS:000088054600025,
Author = {Deumal, M and Novoa, JJ},
Title = {{The microscopic basis of the intermolecular magnetism. An ab initio
   study on molecular crystals}},
Journal = {{JOURNAL OF MOLECULAR STRUCTURE-THEOCHEM}},
Year = {{2000}},
Volume = {{506}},
Number = {{SI}},
Pages = {{287-296}},
Month = {{JUL 14}},
Note = {{5th World Congress of Theoretically Oriented Chemists (WATOC), IMPERIAL
   COLL, LONDON, ENGLAND, AUG 01-06, 1999}},
Organization = {{World Assoc Theoretical Organ Chem}},
Abstract = {{The mechanism of the magnetic interaction in purely organic molecular
   crystals is not well known at the microscopic level. This drawback makes
   impossible the adequate rationalisation of the already known molecular
   magnets and, in the long run, stops the progress towards a rational
   design of better molecular magnets. In this work we present new facts on
   the properties of the intermolecular magnetic interactions (also known
   as through-space magnetism) on nitronyl nitroxides, obtained by
   combining the analysis of molecular crystals with high level ab initio
   computations. These results show that the magnetic properties of
   nitronyl nitroxides cannot be rationalized by looking only at the
   relative orientations of the ONCNO groups, the spin containing groups in
   nitronyl nitroxides, as assumed up to now. (C) 2000 Elsevier Science
   B.V. All rights reserved.}},
