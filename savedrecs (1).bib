@article{ WOS:000599765400001,
Author = {Dokuni, Ryota and Nagano, Tatsuya and Jimbo, Naoe and Sato, Hiroki and
   Kiriu, Tatsunori and Yasuda, Yuichiro and Yamamoto, Masatsugu and
   Tachihara, Motoko and Kobayashi, Kazuyuki and Maniwa, Yoshimasa and
   Nishimura, Yoshihiro},
Title = {{High expression level of serpin peptidase inhibitor clade E member 2 is
   associated with poor prognosis in lung adenocarcinoma}},
Journal = {{RESPIRATORY RESEARCH}},
Year = {{2020}},
Volume = {{21}},
Number = {{1}},
Month = {{DEC 14}},
Abstract = {{Background Recent studies have revealed that serpin peptidase inhibitor
   clade E member 2 (SERPINE2) is associated with tumorigenesis. However,
   SERPINE2 expression and its role in lung adenocarcinomas are still
   unknown. Methods The expression levels of SERPINE2 in 74 consecutively
   resected lung adenocarcinomas were analyzed by using immunostaining.
   Inhibition of SERPINE2 expression by small interfering RNA (siRNA) was
   detected by quantitative PCR. Cell number assays and cell apoptosis
   assays were performed to clarify the cell-autonomous function of
   SERPINE2 in A549 and PC9 lung cancer cells. Results The overall survival
   of patients with high SERPINE2 expression was significantly worse than
   that of patients with low SERPINE2 expression (P = 0.0172). Multivariate
   analysis revealed that SERPINE2 expression was an independent factor
   associated with poor prognosis (P = 0.03237). The interference of
   SERPINE2 decreased cell number and increased apoptosis in A549 and PC9
   cells Conclusion These results suggest that SERPINE2 can be used as a
   novel prognostic marker of lung adenocarcinoma.}},
