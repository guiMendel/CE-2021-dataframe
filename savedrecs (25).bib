@article{ WOS:000286714400004,
Author = {Raison-Peyron, N.},
Title = {{Implants and prostheses (excluding dentistry) and metal allergies}},
Journal = {{REVUE FRANCAISE D ALLERGOLOGIE}},
Year = {{2010}},
Volume = {{50}},
Number = {{2}},
Pages = {{S23-S28}},
Month = {{NOV}},
Abstract = {{Metals (mainly nickel, chrome and cobalt) released from metallic
   implants may become potential allergens under the effect of corrosion in
   contact with body fluids. It would appear that these reactions are very
   rare, as opposed to skin contact allergies to these same metals, and
   difficult to diagnose. A patient presenting with signs of intolerance to
   a metallic implant will often only recover once the suspected material
   is removed (whenever possible), which subsequently makes it possible to
   confirm the involvement of hypersensitivity to one of the implant's
   components (revealed though positive skin tests) during these incidents,
   insofar as it concerns the skin. Moreover, there is no consensus
   attitude in France on the action to be taken in the event of a metallic
   implant fitted in a patient with prior sensitization to one of the
   metals used. Prospective studies with tests before and after the fitting
   of metallic implants are necessary. (C) 2010 Elsevier Masson SAS. All
   rights reserved.}},
