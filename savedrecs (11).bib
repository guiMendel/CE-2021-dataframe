@article{ WOS:000363354000017,
Author = {Aktas, T. and Thy, P. and Williams, R. B. and McCaffrey, Z. and Khatami,
   R. and Jenkins, B. M.},
Title = {{Characterization of almond processing residues from the Central Valley
   of California for thermal conversion}},
