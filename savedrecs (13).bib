@article{ WOS:000368231000002,
Author = {Masaki, Satoshi and Ogawa, Takayoshi},
Title = {{Existence and uniqueness of two dimensional Euler-Poisson system and WKB
   approximation to the nonlinear Schrodinger-Poisson system}},
Journal = {{JOURNAL OF MATHEMATICAL PHYSICS}},
Year = {{2015}},
Volume = {{56}},
Number = {{12}},
Month = {{DEC}},
Abstract = {{In this paper, we study a dispersive Euler-Poisson system in two
   dimensional Euclidean space. Our aim is to show unique existence and the
   zero-dispersion limit of the time-local weak solution. Since one may not
   use dispersive structure in the zero-dispersion limit, when reducing the
   regularity, lack of critical embedding H-1 subset of L-infinity becomes
   a bottleneck. We hence employ an estimate on the best constant of the
   Gagliardo-Nirenberg inequality. By this argument, a reasonable
   convergence rate for the zero-dispersion limit is deduced with a slight
   loss. We also consider the semiclassical limit problem of the
   Schrodinger-Poisson system in two dimensions. (C) 2015 AIP Publishing
   LLC.}},
