@article{ WOS:000364437700041,
Author = {Xie, Xiao and Wang, Mingsong and Mei, Ju and Hu, Fengqing and Ding,
   Fangbao and Lv, Lei},
Title = {{Pyruvate kinase M2 interacts with DNA damage-binding protein 2 and
   reduces cell survival upon UV irradiation}},
Journal = {{BIOCHEMICAL AND BIOPHYSICAL RESEARCH COMMUNICATIONS}},
Year = {{2015}},
Volume = {{467}},
Number = {{2}},
Pages = {{427-433}},
Month = {{NOV 13}},
Abstract = {{Pyruvate Kinase M2 (PKM2) is highly expressed in many solid tumors and
   associated with metabolism reprogramming and proliferation of tumors.
   Here, we report that PKM2 can bind to DNA Damage-Binding Protein 2
   (DDB2), which is necessary for global nucleotide excision repair of UV
   induced DNA damage. The binding is promoted by UV irradiation and K433
   acetylation of PKM2. Over expression of PKM2 facilitates phosphorylation
   of DDB2 and impairs DDB2-DDB1 binding. Furthermore, knocking down of
   PKM2 increases cell survival upon UV irradiation, while over expression
   of PKM2 reduces cell survival and over expression of DDB2-DDB1 reverts
   this effect. These results reveal a previously unknown regulation of
   PKM2 on DDB2 and provide a possible mechanism for UV induced
   tumorigenesis. (C) 2015 Elsevier Inc. All rights reserved.}},
