@article{ WOS:000165642400002,
Author = {Robertson, A and Sutherland, M and Keating, JF},
Title = {{Intramedullary nailing of tibial fractures: how often are post-operative
   radiographs needed?}},
Journal = {{JOURNAL OF THE ROYAL COLLEGE OF SURGEONS OF EDINBURGH}},
Year = {{2000}},
Volume = {{45}},
Number = {{4}},
Pages = {{220-222}},
Month = {{AUG}},
Abstract = {{A series of 53 closed tibial fractures were treated with interlocking
   intramedullary nailing. Excluding the routine postoperative radiographs
   at the time of surgery, a median number of seven radiographs were taken
   per patient (range 2-13). In total, 343 radiographs were taken after the
   postoperative film. Of these 343 radiographs, only nine (3\%)
   radiographs led to a decision for clinical intervention. No radiograph
   in the first 10 weeks after surgery changed management. A reduction in
   the use of post-operative radiography after tibial nailing is
   recommended.}},
