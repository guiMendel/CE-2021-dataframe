@article{ WOS:000285485600014,
Author = {Xia, Jun and Wu, Jun},
Title = {{Electrowetting on a dielectric surface roughened with zinc oxide
   tetrapod nanocrystals}},
Journal = {{PHYSICA E-LOW-DIMENSIONAL SYSTEMS \& NANOSTRUCTURES}},
Year = {{2010}},
Volume = {{43}},
Number = {{1}},
Pages = {{81-84}},
Month = {{NOV}},
Abstract = {{Changing the contact angle between a solid substrate and a droplet with
   an external voltage is important in the field of microfluidics. On a
   flat surface, the range of the reversible contact angle is about 90
   degrees: from 30 degrees to 120 degrees. However, on a rough surface,
   such as superhydrophobic surface of carbon nanotube, carbon nanofibers,
   silicon nanowires, ZnO nanorods, etc., the reverse transition from the
   Wenzel state to Cassie state is usually prevented due to an energy
   barrier resulting in a lower contact angle range. In this paper, we
   described the electrowetting on a rough surface of ZnO tetrapods. The
   contact angle on ZnO tetrapod was as high as 155 degrees, and a wide
   range of reversible contact angles actuated by an electric potential was
   observed. Electrolysis, which was a problem in previous research of
   nano-structured superhydrophobic surface, was avoided by applying a
   uniform dielectric layer between the conductive ITO and the ZnO
   tetrapods. The proposed method has potential applications in
   microfluidics devices with a large dynamic range of capillary force. (c)
   2010 Elsevier B.V. All rights reserved.}},
