@article{ WOS:000170548900018,
Author = {Gibson, PH and Cosens, DJ},
Title = {{Studies on the local dispersal of the New Zealand flatworm, a predator
   of earthworms, in Edinburgh between 1993 and 1999}},
Journal = {{ANNALS OF APPLIED BIOLOGY}},
Year = {{2000}},
Volume = {{137}},
Number = {{3}},
Pages = {{353-359}},
Month = {{DEC}},
Abstract = {{The flatworm Artioposthia triangulata was found, from studies using
   weighted down plastic sheeting, to move predominantly through the soil
   rather than over it and to use earthworm burrows. Under compost-filled
   plastic sacks the flatworm was most active at night although its numbers
   were similar to those during the day. The transfer of specimens into an
   area covered with weighted plastic sheeting had no lasting effect on
   their numbers. The flatworm was regularly removed over 6 years from
   under paving stones, sheets of newspaper and cardboard placed on the
   ground in a garden. The rise and fall of the numbers of the flatworm
   under this debris suggest a predator-prey periodicity between the
   flatworm and earthworms of 3 years.}},
