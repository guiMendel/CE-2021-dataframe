@article{ WOS:A1995TD28000020,
Author = {MORALES, RA and ARNAL, ML and MULLER, AJ},
Title = {{THE EVALUATION OF THE STATE OF DISPERSION IN IMMISCIBLE BLENDS WHERE THE
   MINOR PHASE EXHIBITS FRACTIONATED CRYSTALLIZATION}},
Journal = {{POLYMER BULLETIN}},
Year = {{1995}},
Volume = {{35}},
Number = {{3}},
Pages = {{379-386}},
Month = {{SEP}},
Abstract = {{Differential Scanning Calorimetry (DSC) can provide a qualitative
   measure of the state of dispersion of an immiscible blend if the minor
   phase exhibits fractionated crystallization when dispersed into fine
   particles. The technique is only sensitive to the volume of the
   dispersed particle and not to its shape and can only be used when the
   exotherms of interest do not overlap with other thermal transitions
   present in the multicomponent system. Self-nucleation is a valuable tool
   to ascertain the presence of fractionated crystallization. The
   morphology induced by fractionated crystallization in immiscible blends
   could lead to enhanced plastic deformation during yielding of the
   matrix.}},
