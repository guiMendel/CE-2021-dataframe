@article{ WOS:000165365900006,
Author = {Sanchez-Puig, JM and Blasco, R},
Title = {{Puromycin resistance (pac) gene as a selectable marker in vaccinia virus}},
Journal = {{GENE}},
Year = {{2000}},
Volume = {{257}},
Number = {{1}},
Pages = {{57-65}},
Month = {{OCT 17}},
Abstract = {{The antibiotic puromycin, an inhibitor of protein synthesis, was shown
   to inhibit vaccinia virus (VV) replication. We evaluated the use of
   puromycin-resistance (pac) gene as a selectable marker in VV. A
   recombinant vaccinia virus expressing pac (VV-pac) under the control of
   a viral early/late promoter was constructed and characterized. VV-pac
   grew in the presence of puromycin at concentrations that were inhibitory
   for the parental VV and toxic for the cells. Isolation of recombinant VV
   usually relies on plaque purification under selective conditions.
   Because virus plaquing was not feasible under inhibitory puromycin
   concentration, a protocol based on serial passage of virus was devised.
   The usefulness of this procedure in selecting pac expressing viruses was
   tested by isolating a recombinant VV. (C) 2000 Elsevier Science B.V. All
   rights reserved.}},
