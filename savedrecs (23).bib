@article{ WOS:000281532200006,
Author = {Da Prato, Giuseppe and Lunardi, Alessandra},
Title = {{On the Dirichlet semigroup for Ornstein-Uhlenbeck operators in subsets
   of Hilbert spaces}},
Journal = {{JOURNAL OF FUNCTIONAL ANALYSIS}},
Year = {{2010}},
Volume = {{259}},
Number = {{10}},
Pages = {{2642-2672}},
Month = {{NOV 15}},
Abstract = {{We consider a family of self-adjoint Ornstein-Uhlenbeck operators Ea in
   an infinite dimensional Hilbert space H having the same gaussian
   invariant measure p, for all alpha is an element of {[}0, 1]. We study
   the Dirichlet problem for the equation lambda phi - L-alpha(phi) = f in
   a closed set K, with f is an element of L-2(K, mu). We first prove that
   the variational solution, trivially provided by the Lax-Milgram theorem,
   can be represented, as expected, by means of the transition semigroup
   stopped to K. Then we address two problems: 1) the regularity of the
   solution phi (which is by definition in a Sobolev space W-alpha(1,2)(K,
   mu)) of the Dirichlet problem; 2) the meaning of the 2 Dirichlet
   boundary condition. Concerning regularity, we are able to prove interior
   W-alpha(2,) (2) regularity results; concerning the boundary condition we
   consider both irregular and regular boundaries. In the first case we
   content to have a solution whose null extension outside K belongs to
   W-alpha(1,2)(H, mu). In the second case we exploit the Malliavin's
   theory of surface integrals which is recalled in Appendix A of the
   paper, then we are able to give a meaning to the trace of phi at partial
   derivative K and to show that it vanishes, as it is natural. (C) 2010
   Elsevier Inc. All rights reserved.}},
