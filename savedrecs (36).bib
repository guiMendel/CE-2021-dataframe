@article{ WOS:000232679400104,
Author = {Movahed, MR and Hashemzadeh, M and Jamal, MM},
Title = {{Increased prevalence of third-degree atrioventricular block in patients
   with type II diabetes mellitus}},
Journal = {{CHEST}},
Year = {{2005}},
Volume = {{128}},
Number = {{4}},
Pages = {{2611-2614}},
Month = {{OCT}},
Abstract = {{Background: Diabetes mellitus (DM) is a major risk for cardiovascular
   disease and mortality. There is some evidence that third-degree
   atrioventricular (AV) block occurs more commonly in patients with DM. In
   this study, we evaluated any possible association between DM and
   third-degree AV block using International Classification of Diseases,
   Ninth Revision, Clinical Modification (ICD-9-CM) codes in a very large
   inpatient database.
   Method. We used patient treatment files containing discharge diagnoses
   using ICD-9 codes of inpatient treatment from all Veterans Health
   Administration hospitals. The cohort was stratified using the ICD-9-CM
   code for DM (n = 293,124), a control group with hypertension but no DM
   (n = 552,623), and the ICD-9 code for third-degree AV block (426.0) and
   smoking (305.1, V15.82). Nile performed multivariate analysis adjusting
   for coronary artery disease, congestive heart failure, smoking,and
   hyperlipidemia. Continuous and binary variables were analyzed using
   chi(2) and Fisher exacct tests.
   Results: Third-degree AV block diagnosis was present in 3,240 of DM
   patients (1.1\%) vs 3,367 patients (0.6\%) in the control group. Using
   multivariate analysis, DM remained strongly associated with third-degree
   AV block (odds ratio, 3.1; 95\% confidential interval, 3.0 to 3.3; p <
   0.0001).
   Conclusion: Third-degree AV block occurs significantly, more in patients
   with DM. This finding may, in part, explain die high cardiovascular
   mortality in DM patients.}},
