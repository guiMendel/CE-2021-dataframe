@article{ WOS:000588798300001,
Author = {Peng, Minggang and Zhong, Xiaohui},
Title = {{When Environmental Activism Meets Local Governance: The Role of
   Government Transparency and Responsiveness in China}},
Journal = {{SOCIETY \& NATURAL RESOURCES}},
