@article{ WOS:000618698100012,
Author = {Wu, Pingfan and Zhang, Hongmei and Zhou, Ling and Zhang, Hang and Xue,
   Jiao and Wang, Yamei},
Title = {{Effects of Cotton Stalk Wood Vinegar on the Evolution of Cu and Zn Forms
   During Cattle Manure Composting}},
Journal = {{JOURNAL OF BIOBASED MATERIALS AND BIOENERGY}},
Year = {{2020}},
Volume = {{14}},
Number = {{6}},
Pages = {{760-767}},
Month = {{DEC}},
Abstract = {{In this study, wood vinegar from cotton straw pyrolysis was chosen as a
   passivating agent in order to investigate the effect of its
   physico-chemical properties on the solid products of cattle manure
   during composting. The experimental results showed that the compost
   entered a high-temperature period two days earlier than a control
   sample, with an increase of more than seven days in the duration of the
   high temperature period after cotton straw vinegar treatment. The effect
   of exchangeable passivation on the heavy metal Cu increased
   significantly with the increase in the proportion of wood vinegar
   solution. The best passivation effect on Cu was 32.28\%, at a wood
   vinegar concentration of 0.65\% (v/v). This value was 18.88\% higher
   than without wood vinegar addition. The passivation effect on the heavy
   metal Zn was inhibited at about 0.5\% (v/v) wood vinegar. At a
   concentration of 0.65\% wood vinegar, the maximum passivation reached
   26.29\%. We conclude that cotton straw wood vinegar improved the
   temperature rise, high temperature duration and heavy metal passivation
   effect on cattle manure composting.}},
