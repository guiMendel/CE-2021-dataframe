@article{ WOS:000592953200055,
Author = {Wu, Xiaowen and Gao, Shecheng and Tu, Jiajing and Shen, Lei and Hao,
   Chenglong and Zhang, Bin and Feng, Yuanhua and Zhou, Ji and Chen, Shuner
   and Liu, Weiping and Li, Zhaohui},
Title = {{Multiple orbital angular momentum mode switching at multi-wavelength in
   few-mode fibers}},
Journal = {{OPTICS EXPRESS}},
Year = {{2020}},
Volume = {{28}},
Number = {{24}},
Pages = {{36084-36094}},
Month = {{NOV 23}},
Abstract = {{Mode division multiplexing has attracted great attention because it can
   potentially overcome the limitation of single-mode fiber traffic
   capacity. However, it has been challenging to realize multiple modes
   controlling and switching due to the intrinsic overlap of the modes in
   the transmission waveguide. As a solution, we propose a cascaded
   phase-shifted long-period fiber grating (PS-LPFG) based multiple mode
   switching scheme. Using the PS-LPFGs, the multiple guided orbital
   angular momentum (OAM) modes selective controlling and switching at
   multi-wavelength can be achieved in few-mode fibers by regulating the
   grating resonance condition. In principle, a N x N mode switch matrix
   can be realized by cascading C-N(2) gratings, where each grating acts as
   a mode switch element to achieve a couple selected OAM mode switching
   and meanwhile the other modes are under nonblocking status. As a proof
   of the concept, a 2 x 2 mode switching between two OAM modes at
   different wavelengths based on one PS-LPFG element is demonstrated in
   our experiments. The switching efficiency of the two modes at two
   wavelengths 1537nm and 1558nm are similar to 98.4\% and similar to
   98.7\%, respectively. The proposed multiple OAM mode switch has
   potential applications in the future hybrid multi-dimensional
   multiplexing optical fiber communication systems. (C) 2020 Optical
   Society of America under the terms of the OSA Open Access Publishing
   Agreement}},
