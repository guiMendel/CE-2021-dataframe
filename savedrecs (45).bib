@article{ WOS:000089554500024,
Author = {Nihei, H and Ogawa, Y and Nakajima, T and Morita, S},
Title = {{Transport process of high-speed impurity pellets through curved guide
   tubes}},
Journal = {{REVIEW OF SCIENTIFIC INSTRUMENTS}},
Year = {{2000}},
Volume = {{71}},
Number = {{10}},
Pages = {{3747-3757}},
Month = {{OCT}},
Abstract = {{Curved guide tubes to transport high-speed pellets are used for pellet
   injection into plasma devices. This article describes the transport
   process of the high-speed pellets through curved guide tubes. The
   pellets pass through the curved tubes with repeating reflections. Using
   the equation of friction at each reflection point, the velocity and the
   direction of the output pellet are calculated by following the pellet
   trajectory. The analytic expressions, with respect to the velocity and
   the direction of the output pellet, are also derived. It has been
   confirmed that there is good correspondence between the results obtained
   by following the pellet trajectories, those by the analytic expressions
   and those by the experiments. The analytic expressions are particularly
   useful for the design and making of the curved guide tubes used in the
   pellet injection. (C) 2000 American Institute of Physics.
