@article{ WOS:000282217000001,
Author = {Braun, O. M. and Peyrard, M.},
Title = {{Master equation approach to friction at the mesoscale}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2010}},
Volume = {{82}},
Number = {{3, 2}},
Month = {{SEP 28}},
Abstract = {{At the mesoscale friction occurs through the breaking and formation of
   local contacts. This is often described by the earthquakelike model
   which requires numerical studies. We show that this phenomenon can also
   be described by a master equation, which can be solved analytically in
   some cases and provides an efficient numerical solution for more general
   cases. We examine the effect of temperature and aging of the contacts
   and discuss the statistical properties of the contacts for different
   situations of friction and their implications, particularly regarding
   the existence of stick-slip.}},
