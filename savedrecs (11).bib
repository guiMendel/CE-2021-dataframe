@article{ WOS:000365926300021,
Author = {Yahalom-Mack, Naama and Langgut, Dafna and Dvir, Omri and Tirosh, Ofir
   and Eliyahu-Behar, Adi and Erel, Yigal and Langford, Boaz and Frumkin,
   Amos and Ullman, Mika and Davidovich, Uri},
Title = {{The Earliest Lead Object in the Levant}},
Journal = {{PLOS ONE}},
Year = {{2015}},
Volume = {{10}},
Number = {{12}},
Month = {{DEC 2}},
Abstract = {{In the deepest section of a large complex cave in the northern Negev
   desert, Israel, a bi-conical lead object was found logged onto a wooden
   shaft. Associated material remains and radiocarbon dating of the shaft
   place the object within the Late Chalcolithic period, at the late 5th
   millennium BCE. Based on chemical and lead isotope analysis, we show
   that this unique object was made of almost pure metallic lead, likely
   smelted from lead ores originating in the Taurus range in Anatolia.
   Either the finished object, or the raw material, was brought to the
   southern Levant, adding another major component to the already-rich Late
   Chalcolithic metallurgical corpus known to-date. The paper also
   discusses possible uses of the object, suggesting that it may have been
   used as a spindle whorl, at least towards its deposition.}},
