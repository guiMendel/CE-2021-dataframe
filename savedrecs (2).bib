@article{ WOS:000592315100013,
Author = {Li, Jingxian and Shan, Xueling and Jiang, Ding and Wang, Yuru and Wang,
   Wenchang and Chen, Zhidong},
Title = {{A novel electrochemiluminescence sensor based on resonance energy
   transfer from MoS(2)QDs@g-C3N4 to NH2-SiO2@PTCA for glutathione assay}},
Journal = {{ANALYST}},
Year = {{2020}},
Volume = {{145}},
Number = {{23}},
Pages = {{7616-7622}},
Month = {{DEC 7}},
Abstract = {{In this work, a solid-state electrochemiluminescence (ECL) sensor based
   on resonance energy transfer (RET) was proposed using MoS(2)QDs@g-C3N4
   as a donor and NH2-SiO2@PTCA as an acceptor. Herein, MoS(2)QDs could
   significantly facilitate the stability and efficiency of the ECL of
   g-C3N4. PTCA provided a large platform to anchor NH2-SiO2 nanoparticles.
   The prepared MoS(2)QDs@g-C3N4 exhibited good spectral overlap with the
   UV-vis absorption spectrum of NH2-SiO2@PTCA. Based on this, we designed
   an ``off-on{''} ECL sensing strategy for sensitive and selective
   detection of glutathione (GSH). Under the best conditions, the linear
   range of the sensor for GSH detection was from 0.001 to 100 mu M with a
   detection limit of 0.63 nM (S/N = 3). More importantly, GSH in
   commercial samples can be detected using the proposed sensor, which
   indicated its superior detection capabilities and potential application
   value in commercial medicines.}},
