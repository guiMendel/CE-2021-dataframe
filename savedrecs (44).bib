@article{ WOS:000165080200005,
Author = {Cerchez, M and Boroica, L and Hulsenberg, D},
Title = {{Glasses and crystallised glasses in the BaO-B2O3-TiO2 system}},
Journal = {{PHYSICS AND CHEMISTRY OF GLASSES}},
Year = {{2000}},
Volume = {{41}},
Number = {{5}},
Pages = {{233-235}},
Month = {{OCT}},
Note = {{3rd International Conference on Borate Glasses, Crystals and Melts,
   SOFIA, BULGARIA, JUL 04-09, 2000}},
Abstract = {{Several compositions in the BaO-B2O3-TiO2 system have been studied the
   most important being BaTi4O9 with an added amount of (mol\%)
   5-50B(2)O(3) and BaTiO3 together with 25-50B(2)O(3). The melting
   temperatures were between 1100 and 1450 degreesC depending on the amount
   of B2O3. For the compositions with less than 40 mol\% TiO2 and more than
   20 mol\% B2O3 the materials obtained were vitreous. If the amount of
   TiO2 was greater than 50 mol\% they were crystalline even after
   ultrarapid quenching. The main physical properties were determined for
