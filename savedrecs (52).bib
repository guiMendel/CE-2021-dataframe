@article{ WOS:A1995TY52900015,
Author = {Gerstl, P and Pribbenow, S},
Title = {{Midwinters, end games, and body parts: A classification of part-whole
   relations}},
Journal = {{INTERNATIONAL JOURNAL OF HUMAN-COMPUTER STUDIES}},
Year = {{1995}},
Volume = {{43}},
Number = {{5-6}},
