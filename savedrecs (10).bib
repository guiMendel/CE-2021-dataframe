@article{ WOS:000367451400030,
Author = {Moon, Jongho and Choi, Younsung and Jung, Jaewook and Won, Dongho},
Title = {{An Improvement of Robust Biometrics-Based Authentication and Key
   Agreement Scheme for Multi-Server Environments Using Smart Cards}},
Journal = {{PLOS ONE}},
Year = {{2015}},
Volume = {{10}},
Number = {{12}},
Month = {{DEC 28}},
Abstract = {{In multi-server environments, user authentication is a very important
   issue because it provides the authorization that enables users to access
   their data and services; furthermore, remote user authentication schemes
   for multi-server environments have solved the problem that has arisen
   from user's management of different identities and passwords. For this
   reason, numerous user authentication schemes that are designed for
   multi-server environments have been proposed over recent years. In 2015,
   Lu et al. improved upon Mishra et al.'s scheme, claiming that their
   remote user authentication scheme is more secure and practical; however,
   we found that Lu et al.' s scheme is still insecure and incorrect. In
   this paper, we demonstrate that Lu et al.'s scheme is vulnerable to
   outsider attack and user impersonation attack, and we propose a new
   biometrics-based scheme for authentication and key agreement that can be
   used in multi-server environments; then, we show that our proposed
   scheme is more secure and supports the required security properties.}},
