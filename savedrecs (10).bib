@article{ WOS:000366622800001,
Author = {Yang, Jinfu and Zhang, Jizhao and Wang, Guanghui and Li, Mingai},
Title = {{Semantic Map Building Based on Object Detection for Indoor Navigation}},
Journal = {{INTERNATIONAL JOURNAL OF ADVANCED ROBOTIC SYSTEMS}},
Year = {{2015}},
Volume = {{12}},
Month = {{DEC 15}},
Abstract = {{Building a map of the environment is a prerequisite for mobile robot
   navigation. In this paper, we present a semantic map building method for
   indoor navigation of a robot using only the image sequence acquired by a
   monocular camera installed on the robot. First, a topological map of the
   environment is created, where each key frame forms a node of the map
   represented as visual words (VWs). The edges between two adjacent nodes
   are built from relative poses obtained by performing a novel pose
   estimation approach, called one-point RANSAC camera pose estimation
   (ORPE). Then, taking advantage of an improved deformable part model
   (iDPM) for object detection, the topological map is extended by
   assigning semantic attributes to the nodes. Extensive experimental
   evaluations demonstrate the effectiveness of the proposed monocular SLAM
   method.}},
