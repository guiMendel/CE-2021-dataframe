@article{ WOS:000366962700005,
Author = {Zhang, Bing-Jie and Teng, Xi-Feng and Bao, Mei-Fen and Zhong, Xiu-Hong
   and Ni, Ling and Cai, Xiang-Hai},
Title = {{Cytotoxic indole alkaloids from Tabemaemontana officinalis}},
Journal = {{PHYTOCHEMISTRY}},
Year = {{2015}},
Volume = {{120}},
Pages = {{46-52}},
Month = {{DEC}},
Abstract = {{Continued interest in cytotoxic alkaloids resulted in the isolation of
   37 alkaloids including 29 known monoterpenoid indole alkaloids from the
   aerial parts of Tabernaemontana officinalis. Of the remaining 8
   alkaloids, six were bisindole alkaloids named taberdivarines A-F (1-6)
   and the two were monomers named taberdivarines G and H (7-8). Alkaloids
   1 and 2 are voaphylline-vobasinyl type bisindole alkaloids, a structural
