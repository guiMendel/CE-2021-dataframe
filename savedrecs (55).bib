@article{ WOS:A1995RU85200031,
Author = {OCHIENG, J and WARFIELD, P and GREEN, B},
Title = {{INTERACTIONS OF GELATINASES WITH SOLUBLE AND IMMOBILIZED FETUIN AND
   ASIALOFETUIN}},
