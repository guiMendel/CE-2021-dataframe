@article{ WOS:000612487500002,
Author = {Xie Yilin},
Title = {{Christ and Caesar: On the Relationship between Theocracy and Kingship
   according to the Gospel of Matthew}},
Journal = {{SINO-CHRISTIAN STUDIES}},
Year = {{2020}},
Number = {{30}},
Pages = {{33-64}},
Month = {{DEC}},
Abstract = {{It has been a hot topic in the academic circle that how we understand
   the issue on Christ versus Caesar determines how we deal with the
   conflict between theocracy and kingship. This article holds the view
   that the prototype of the issue on Christ versus Caesar derives from the
   specific narrative in the Gospel of Matthew, which not only forms the
   basis but also determines the direction for the development of
   politico-religious relationship in Christian history. Moreover, this
   article argues that the transcendental connotation of (sic) in the
   literary structure of Matthew 16:13-20 biblically demonstrates an idea
   that the heavenly theocracy exceeds over the earthly kingship by the
   suffering of Christ. In addition, using the method of detailed reading
   of the biblical text, the article indicates how the religious chosen
   people not only comply with the governance from the earthly kingship but
   also fulfill the duties as citizens, which is revealed by an elaborate
   interpretation of the connotations of both (sic) and (sic). It is the
   perpetual tension between the theocracy and kingship that shapes an
   opposite even cooperative relationship for the era of the need for the
   Christian history.}},
