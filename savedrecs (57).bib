@article{ WOS:A1995RU00300003,
Author = {BIRCHALL, HM},
Title = {{REPORTING EXPERIENCES OF PANIC - SEX-DIFFERENCES IN A COMMUNITY SURVEY}},
Journal = {{JOURNAL OF COMMUNITY \& APPLIED SOCIAL PSYCHOLOGY}},
Year = {{1995}},
Volume = {{5}},
Number = {{3}},
Pages = {{167-172}},
Month = {{AUG}},
Abstract = {{This report aims to look at whether the results of a community survey of
   the prevalence of panic attacks and panic disorder could be influenced
   by a possible sex difference in the willingness to return screening
   questionnaires and to acknowledge psychiatric symptoms. The results
   suggest that men are less likely to return questionnaires, and that this
