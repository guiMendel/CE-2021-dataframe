@article{ WOS:000592474400001,
Author = {Dabitao, Djeneba and Dembele, Mamadou and Urbanowski, Michael and Kone,
   Bourahima and Wague, Mamadou and Coulibaly, Nadie and Sarro, Yeya Dit
   Sadio and Baya, Bocar and Goita, Drissa and Dao, Sounkalo and Belson,
   Michael and Klein, Sabra L. and Achenbach, Chad and Holl, Jane L. and
   Diakite, Mahamadou and Doumbia, Seydou and Bream, Jay H. and Bishai,
   William R. and Diallo, Souleymane and Murphy, Robert L.},
Title = {{Genetic Variation in Human IL10 Proximal Promoter and Susceptibility to
   HIV-1 Infection in Mali, West Africa}},
Journal = {{AIDS RESEARCH AND HUMAN RETROVIRUSES}},
Year = {{2021}},
Volume = {{37}},
Number = {{1}},
Pages = {{57-61}},
Month = {{JAN 1}},
Abstract = {{It is now recognized that to fully understand the role of host genetic
   variation on susceptibility to HIV-1 infection, investigations must be
   extended to African populations. We sought to determine if genetic
   variation in IL10 are associated with HIV-1 infection in a West African
   cohort in Mali. HIV-infected and -uninfected individuals were genotyped
   for three common single nucleotide polymorphisms (SNPs) located at
   positions -592 (C/A), -819 (C/T), and -1082 (G/A) of the IL10 promoter.
   We found that the ATA haplotype, which has been previously associated
   with low IL-10 expression, was the most represented in the cohort.
   Although we observed a trend toward an increased frequency of ATA/ATA
   carriage in HIV-infected compared with -uninfected individuals, the
   difference was not statistically significant. Similarly, individual IL10
   SNPs were not significantly enriched in the HIV-infected group,
   suggesting that IL10 genetic variants are not associated with HIV-1 in
   this West African cohort from Mali.}},
