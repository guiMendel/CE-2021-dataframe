@article{ WOS:000281095700019,
Author = {Maia, Filipa M. and Rodriguez, Oscar and Macedo, Eugenia A.},
Title = {{LLE for (water plus ionic liquid) binary systems using
   {[}C(x)mim]{[}BF4] (x=6, 8) ionic liquids}},
