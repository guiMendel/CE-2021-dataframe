@article{ WOS:A1995TD46600006,
Author = {SAWYER, JS and BACH, NJ and BAKER, SR and BALDWIN, RF and BORROMEO, PS
   and COCKERHAM, SL and FLEISCH, JH and FLOREANCIG, P and FROELICH, LL and
