@article{ WOS:000282159500004,
Author = {Lind, Joan and Marshall, Donald E. and Rohde, Steffen},
Title = {{COLLISIONS AND SPIRALS OF LOEWNER TRACES}},
Journal = {{DUKE MATHEMATICAL JOURNAL}},
Year = {{2010}},
Volume = {{154}},
Number = {{3}},
Pages = {{527-573}},
Month = {{SEP 15}},
Abstract = {{We analyze Loewner traces driven by functions asymptotic to kappa root 1
   - t. We prove a stability result when kappa not equal 4, and we show
   that kappa = 4 can lead to nonlocally connected hulls. As a consequence,
   we obtain a driving term lambda(t) so that the hulls driven by K
   lambda(t) are generated by a continuous curve for all kappa > 0 with
   kappa not equal 4, but not when K = 4, so that the space of driving
   terms with continuous traces is not convex. As a byproduct, we obtain an
   explicit construction of the traces driven by kappa root 1 - t and a
   conceptual proof of the corresponding results of Kaget; Nienhuis, and
   Kadanoff}},
