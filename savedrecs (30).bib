@article{ WOS:000233947100010,
Author = {Conrad, B and Conrad, K and Helfgott, H},
Title = {{Root numbers and ranks in positive characteristic}},
Journal = {{ADVANCES IN MATHEMATICS}},
Year = {{2005}},
Volume = {{198}},
Number = {{2}},
Pages = {{684-731}},
Month = {{DEC 20}},
Abstract = {{For a global field K and an elliptic curve E-n over K(T), Silverman's
   specialization theorem implies rank(E-n(K(T))) <= rank(E-t (K)) for all
   but finitely many t is an element of P-1 (K). If this inequality is
   strict for all but finitely many t, the elliptic curve E-n is said to
   have elevated rank. All known examples of elevated rank for K = Q rest
   on the parity conjecture for elliptic curves over Q, and the examples
   are all isotrivial.
   Some additional standard conjectures over Q imply that there does not
   exist a non-isotrivial elliptic curve over Q(T) with elevated rank. In
   positive characteristic, an analogue of one of these additional
   conjectures is false. Inspired by this, for the rational function field
   K = K(U) over any finite field K with characteristic A 2, we construct
   an explicit 2-parameter family E-c,E-d of non-isotrivial elliptic curves
   over K(T) (depending on arbitrary c, d E 0) such that, under the parity
   conjecture, each E-c,E-d has elevated rank. (c) 2005 Elsevier Inc. All
   rights reserved.}},
