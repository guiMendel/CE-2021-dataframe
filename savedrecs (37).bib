@article{ WOS:000232228800067,
Author = {Castillo, IP and Sherrington, D},
Title = {{Exact mappings between fermionic Ising spin-glass and classical
   spin-glass models}},
Journal = {{PHYSICAL REVIEW B}},
Year = {{2005}},
Volume = {{72}},
Number = {{10}},
Month = {{SEP}},
Abstract = {{We present in this paper exact analytical expressions for the
   thermodynamical properties and Green's functions of a certain family of
   fermionic Ising spin-glass models with Hubbard interaction by noticing
   that their Hamiltonian is a function of the number operator only. The
   thermodynamical properties are mapped to the classical
   Ghatak-Sherrington spin-glass model, while the the density of states
   (DOS) is related to its joint spin-field distribution. We discuss the
   presence of the pseudogap in the DOS with the help of this mapping.}},
