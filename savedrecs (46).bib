@article{ WOS:000089324200038,
Author = {Angelini, L and Nardocci, N and Estienne, M and Conti, C and Dones, I
   and Broggi, G},
Title = {{Life-threatening dystonia-dyskinesias in a child: Successful treatment
   with bilateral pallidal stimulation}},
Journal = {{MOVEMENT DISORDERS}},
Year = {{2000}},
Volume = {{15}},
Number = {{5}},
Pages = {{1010-1012}},
Month = {{SEP}},
Abstract = {{We report a 13-year-old boy who developed severe, refractory
   dystonia-dyskinesias as an abrupt worsening of a previously
   nonprogressive movement disorder. The movements became continuous,
   requiring artificial respiration and continuous sedation in the
   intensive-care unit. Various drugs and drug combinations failed to
   achieve control. The child was then treated successfully with bilateral
   pallidal (GPi) stimulation as shown in the videotape. Four months later
   and without medication, the boy regained autonomous gait and audible
   speech; his neurologic condition continues to improve.}},
