@article{ WOS:A1995UN64700004,
Author = {Glasauer, S and Amorim, MA and Bloomberg, JJ and Reschke, MF and Peters,
   BT and Smith, SL and Berthoz, A},
Title = {{Spatial orientation during locomation following space flight}},
Journal = {{ACTA ASTRONAUTICA}},
Year = {{1995}},
Volume = {{36}},
Number = {{8-12}},
Pages = {{423-431}},
Month = {{OCT-DEC}},
Note = {{11th Symposium on Man in Space, TOULOUSE LABEGE, FRANCE, MAR 27-31, 1995}},
Organization = {{Int Acad Astronaut; Ctr Natl Etudes Spatiales}},
Abstract = {{To investigate changes in spatial orientation ability and walking
   performance following space flight, 7 astronaut subjects were asked pre-
   and post-flight to perform a goal directed locomotion paradigm which
   consisted of walking a triangular path with and without vision. This new
   paradigm, involving inputs from different sensory systems, allows
   quantification of several critical parameters, like orientation
   performance, walking velocities and postural stability, in a natural
   walking task, The paper presented here mainly focusses on spatial
   orientation performance quantified by the errors in walking the
   previously seen path without vision, Errors in length and reaching the
   corners did not change significantly from pre- to post-flight, while
   absolute angular errors slightly increased post-flight, The significant
   decrease in walking velocity and a change in head-trunk coordination
   while walking around the corners of the path observed post-flight may
   suggest that during re-adaptation to gravity the mechanisms which are
   necessary to perform the task have to be re-accomplished.}},
