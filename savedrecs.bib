@article{ WOS:000610083100001,
Author = {You, Rui and Lu, Wenshuai and Ruan, Yong and Hao, Yilong},
Title = {{Micromachined inertial switch for sub-g monitoring using gravity-based
   threshold compensation}},
Journal = {{ELECTRONICS LETTERS}},
Year = {{2021}},
Volume = {{57}},
Number = {{1}},
Pages = {{22-24}},
Month = {{JAN}},
Abstract = {{This letter presents the original design of a microelectromechanical
   systems inertial switch with ultra-low threshold (similar to 0.6 g) and
   great potential for batch manufacturing. The gravity is utilized to
   narrow the gap between electrodes during operation, defined here as
   threshold compensation. This lightens the fabrication burden, enabling
   the device to be made by standard silicon-on-glass micromachining
   without excessive optimization. Finite element analysis has been
   conducted to demonstrate positive effects of gravity involvement (9.4 mu
   m reduction of the gap) and compliance design of the movable electrodes
   (multiplied contact time and less than half coefficient of restitution).
   Fabricated inertial switches have been installed into a vibration system
   to test fundamental characteristics. Under vibration conditions with
   amplitude of 0.6 g and frequencies from 25 to 40 Hz, the devices achieve
   reliable switching with triggering time ranging from 81.2 to 119.3 mu s.}},
