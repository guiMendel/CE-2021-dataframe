@article{ WOS:A1995RR91100034,
Author = {ANTOSZEWSKI, J and SEYMOUR, DJ and FARAONE, L and MEYER, JR and HOFFMAN,
   CA},
Title = {{MAGNETOTRANSPORT CHARACTERIZATION USING QUANTITATIVE MOBILITY-SPECTRUM
   ANALYSIS}},
