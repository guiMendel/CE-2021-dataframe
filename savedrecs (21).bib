@article{ WOS:000284192000009,
Author = {Li, Xiaona and Gilhooly, III, William P. and Zerkle, Aubrey L. and
   Lyons, Timothy W. and Farquhar, James and Werne, Josef P. and Varela,
   Ramon and Scranton, Mary I.},
Title = {{Stable sulfur isotopes in the water column of the Cariaco Basin}},
Journal = {{GEOCHIMICA ET COSMOCHIMICA ACTA}},
Year = {{2010}},
Volume = {{74}},
Number = {{23}},
Pages = {{6764-6778}},
Month = {{DEC 1}},
Abstract = {{Previous geochemical and microbiological studies in the Cariaco Basin
   indicate intense elemental cycling and a dynamic microbial loop near the
   oxic-anoxic interface. We obtained detailed distributions of sulfur
   isotopes of total dissolved sulfide and sulfate as part of the on-going
   CARIACO time series project to explore the critical pathways at the
   level of individual sulfur species. Isotopic patterns of sulfate
   (delta(34)S(SO4)) and sulfide (delta(34)S(H2S)) were similar to trends
   observed in the Black Sea water column: delta(34)S(H2S) and
   delta(34)S(SO4) were constant in the deep anoxic water (varying within
   0.6 parts per thousand for sulfide and 0.3 parts per thousand for
   sulfate), with sulfide roughly 54 parts per thousand depleted in (34)S
   relative to sulfate. Near the oxic anoxic interface, however, the
   delta(34)S(H2S) value was similar to 3 parts per thousand heavier than
   that in the deep water, which may reflect sulfide oxidation and/or a
   change in fractionation during in situ sulfide production through
   sulfate reduction (SR). delta(34)S(H2S) and Delta(33)S(H2S) data near
   the oxic anoxic interface did not provide unequivocal evidence to
   support the important role of sulfur-intermediate disproportionation
   suggested by previous studies. Repeated observation of minimum
   delta(34)S(SO4) values near the interface suggests `readdition' of
   (34)S-depleted sulfate during sulfide oxidation. A slight increase in
   delta(34)S(SO4) values with depth extended over the water column may
   indicate a reservoir effect associated with removal of (34)S-depleted
   sulfur during sulfide production through SR. Our delta(34)S(H2S) and
   Delta(33)S(H2S) data also do not show a clear role for
   sulfur-intermediate disproportionation in the deep anoxic water column.
   We interpret the large difference in delta(34)S between sulfate and
   sulfide as reflecting fractionations during SR in the Cariaco deep
   waters that are larger than those generally observed in culturing
   studies. Published by Elsevier Ltd.}},
Publisher = {{PERGAMON-ELSEVIER SCIENCE LTD}},
Address = {{THE BOULEVARD, LANGFORD LANE, KIDLINGTON, OXFORD OX5 1GB, ENGLAND}},
Type = {{Article}},
