@article{ WOS:000280625200005,
Author = {Lu, Di and Realff, Matthew J.},
Title = {{Point-based standard optimization with life cycle assessment for product
   design}},
Journal = {{COMPUTERS \& CHEMICAL ENGINEERING}},
Year = {{2010}},
Volume = {{34}},
Number = {{9, SI}},
Pages = {{1356-1364}},
