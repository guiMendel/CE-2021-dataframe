@article{ WOS:000285334500003,
Author = {Ploss, Martin and Kuhn, Andreas},
Title = {{Kinetics of filamentous phage assembly}},
Journal = {{PHYSICAL BIOLOGY}},
Year = {{2010}},
Volume = {{7}},
Number = {{4}},
Month = {{DEC}},
Abstract = {{Filamentous phages release their progeny particles by a secretory
   process without lysing the bacterial cell. By this process about 6 viral
   particles per min are secreted from each cell. We show here that when
   the major coat protein (gp8) is provided from a plasmid we observe a
   phage progeny production rate depending on the induction of gp8 by IPTG.
   We also show that a transfection of Escherichia coli lacking F-pili is
   observed using a mutant of M13 that carries an ampicillin resistance
   gene, and phage particles are secreted in the absence of an F-plasmid.
   Extruding phage was visualized by atomic force microscopy (AFM) and by
   transmission electron microscopy (TEM) using gold-labeled antibodies to
   the major coat protein.}},
