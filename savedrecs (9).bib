@article{ WOS:000579424200057,
Author = {Chen, Changdong and Zhao, Lina},
Title = {{The effect of thermal-induced noise on doubly-coupled-ring optical
   gyroscope sensor around exceptional point}},
Journal = {{OPTICS COMMUNICATIONS}},
Year = {{2020}},
Volume = {{474}},
Month = {{NOV 1}},
Abstract = {{We theoretically analyze the performance of gyroscope sensor near
   exceptional point (EP), consisting of two coupled micro-ring resonators.
   Distinct thermal behaviors, due to different gains in these two rings,
   could bring about an extra resonance shift between them, which places a
   great impact on EP sensor including its sensitivity, visibility and
   stability. Our simulations demonstrate that, such thermal-induced noise
   can be amplified around EP simultaneously as well as the frequency
   splitting generated from Sagnac effect we should detect, giving rise to
   the difficulty of sensing comparatively weak rotation-induced
   perturbations, and a limited detection range while utilizing the
   superiority of EP in such a gyroscope system. Our tolerance analysis in
   this paper may have a profound referential meaning to the future
   experimental works.}},
