@article{ WOS:A1995RR70800014,
Author = {CHANG, Z},
Title = {{GENERALIZED HOLSTEIN-PRIMAKOFF TRANSFORMATION, SPIN-CHARGE SEPARATION
   AND QUANTUM NONLINEAR SIGMA-MODEL IN A 2D LATTICE}},
