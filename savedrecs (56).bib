@article{ WOS:A1995TL63200008,
Author = {Rickman, DS},
Title = {{A Bayesian analysis of the use of pooled coefficients in a structural
   regional economic model}},
Journal = {{INTERNATIONAL JOURNAL OF FORECASTING}},
Year = {{1995}},
Volume = {{11}},
Number = {{3}},
Pages = {{477-490}},
Month = {{SEP}},
Abstract = {{This paper examines the performance of the model produced by Regional
   Economic Models, Inc. (REMI), which utilizes pooled econometric
   coefficients, in forecasting and simulating the Las Vegas, Nevada
   economy. Forecasts and simulations are performed using the REMI model
   and alternative sets of parameter estimates: REMI pooled estimates;
   estimates based on local data; and two sets of estimates that are
   calculated using Bayesian shrinkage techniques. The results reveal that
   the economic multipliers produced by the REMI model are sensitive to the
   set of estimates used to parameterize the model. Regarding forecasting
   performance, the REMI coefficients produce the most accurate long-run
   forecasts while one of the Bayesian shrinkage estimators produces the
   most accurate short-run forecasts.}},
