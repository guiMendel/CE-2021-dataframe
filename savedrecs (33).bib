@article{ WOS:000231455400007,
Author = {Maleki, N and Nayebi, AM and Garjani, A},
Title = {{Effects of central and peripheral depletion of serotonergic system on
   carrageenan-induced paw oedema}},
Journal = {{INTERNATIONAL IMMUNOPHARMACOLOGY}},
Year = {{2005}},
Volume = {{5}},
Number = {{12}},
Pages = {{1723-1730}},
Month = {{NOV}},
Abstract = {{The role of serotonergic system was investigated on peripheral
   inflammation induced by intraplantary injection of carrageenan.
   Para-chlorophenylalanine (PCPA) was administered
   intracerebroventriculary (50, 100 mu g/rat) or intraperitoneally (150
   rng/kg, 3 days) and 2 or 24 h later, respectively, inflammation was
   induced by injection of carrageenan. Paw oedema was decreased
   significantly in PCPA-treated (100 mu g/rat, i.c.v.) rats compared to
   control groups. Injection of exogenous serotonin (i.c.v.) by dose of
   0.70 nmol/10 mu l/rat, but not the dose of 0.35 nmol/10 mu l/rat, 15 min
   after induction of inflammation completely reversed the
   anti-inflammatory effects of PCPA. Myeloperoxidase activity in inflamed
   paws was reduced significantly in groups received PCPA (either i.c.v. or
   i.p.) compared to controls. Exogenous serotonin (0.70 nmol/10 mu l/rat)
   reduced inflammatory response when injected (i.c.v.) 30 min before or 30
   min after the induction of inflammation. Injection of serotonin at the
   time of induction of inflammation had no inflammatory/anti-inflammatory
   effect. These results suggest that serotonin, as a neurotransmitter in
   central nervous system, may be involved in modulating peripheral
   inflammation. (c) 2005 Elsevier B.V. All rights reserved.}},
