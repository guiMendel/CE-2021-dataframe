@article{ WOS:000363238300010,
Author = {Dwivedi, Satyam and De Angelis, Alessio and Zachariah, Dave and Handel,
   Peter},
Title = {{Joint Ranging and Clock Parameter Estimation by Wireless Round Trip Time
   Measurements}},
Journal = {{IEEE JOURNAL ON SELECTED AREAS IN COMMUNICATIONS}},
Year = {{2015}},
Volume = {{33}},
Number = {{11}},
Pages = {{2379-2390}},
Month = {{NOV}},
Abstract = {{In this paper, we develop a new technique for estimating fine clock
   errors and range between two nodes simultaneously by two-way
   time-of-arrival measurements using impulse-radio ultrawideband signals.
   Estimators for clock parameters and the range are proposed, which are
   robust with respect to outliers. They are analyzed numerically and by
   means of experimental measurement campaigns. The technique and derived
   estimators achieve accuracies below 1 Hz for frequency estimation, below
   1 ns for phase estimation, and 20 cm for range estimation, at a 4-m
   distance using 100-MHz clocks at both nodes. Therefore, we show that the
   proposed joint approach is practical and can simultaneously provide
   clock synchronization and positioning in an experimental system.}},
