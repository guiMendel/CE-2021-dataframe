@article{ WOS:000232351000009,
Author = {Bradley, R},
Title = {{A semi-parametric model for asymmetric information in health plan
   markets}},
Journal = {{ECONOMIC INQUIRY}},
Year = {{2005}},
Volume = {{43}},
Number = {{4}},
Pages = {{812-822}},
Month = {{OCT}},
Abstract = {{Two sources of asymmetric information in health markets are adverse
   selection where only individuals know their illness probability and
   their preventive effort to reduce illness probabilities. This effort is
   not observed by insurers, and thus individuals cannot be adequately
   compensated for their effort. I investigate whether asymmetric
   information plays a role in health care markets through a
   semi-parametric model that allows both adverse selection and preventive
   effort. I find that preventive effort plays a role in illness
   probabilities. If the United States implements universal health
   coverage, the currently uninsured population will exert less preventive
   effort, and their illness probabilities will increase.}},
