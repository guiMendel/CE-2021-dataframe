@article{ WOS:000283091700005,
Author = {Frybort, Stephan and Mauritz, Raimund and Teischinger, Alfred and
   Mueller, Ulrich},
Title = {{Determination of the bond strength of treated wood strands embedded in
   aaEuro-aEuro-cement matrix by means of aaEuro-aEuro-pull-out test}},
Journal = {{EUROPEAN JOURNAL OF WOOD AND WOOD PRODUCTS}},
Year = {{2010}},
Volume = {{68}},
Number = {{4}},
Pages = {{407-414}},
Month = {{NOV}},
Abstract = {{Effects of chemical treatment of wood on the bonding strength of Norway
   spruce strands (Picea abies Karst.) embedded in aaEuro-aEuro-cement
   matrix were investigated by means of aaEuro-aEuro-pull-out test. Strands
   of varying thickness were used whereas aaEuro-aEuro-strong influence of
   thickness on bonding strength could be observed. Using thick strands
   (800aEuro-mu m) showed negative effect on bonding strength which is due
   to the swelling effect. The treatment of thin strands (400aEuro-mu m)
   showed that using wet strands (similar to aEuro-90\% moisture content)
   leads to the best results compared to untreated dry (12\%) strands. By
   addition of setting accelerators (ammonium chloride) bonding strength of
   wet strands was even improved. Furthermore, effects of hot water and
   sodium hydroxide extraction as well as sodium silicate treatment were
   examined.}},
