@article{ WOS:000367150700022,
Author = {Manuel Herrera-Perez, Guillermo},
Title = {{Iron Doped-ZrSiO4: Structural, Microstructural and Vibrational
   Characterization}},
Journal = {{MATERIALS RESEARCH-IBERO-AMERICAN JOURNAL OF MATERIALS}},
Year = {{2015}},
Volume = {{18}},
Number = {{6}},
Pages = {{1313-1321}},
Month = {{NOV-DEC}},
Abstract = {{Fe-x-ZrSiO4 is known for the applications in the ceramic industry such
   as ceramic pigment. In this article, we focus our attention to the
   structural, microstructural and vibrational changes of Fe-x-ZrSiO4 from
   free-mineralizer precursors, treated at different temperatures in the
   range of 1100-1600 degrees C. The refinements of X-ray diffraction
   patterns show that Fe3+ cations were distributed into tetrahedral sites
   replacing Si4+. The evolution of the shape distribution analyzed by
   transmission electron microscopy, reveal a polyhedral morphology at 1100
   degrees C during 3h. In comparison, well-rounded and homogeneous
   particle size was determined in the sample heated at 1600 degrees C
   during 24 h. On the other hand, it was observed that increase the
   content of iron and the increased heat treatment (temperature and time)
   both plays an important effect on the observed Raman results. The
   profile line deconvolution applied through the nu(3) vibration of the
   SiO4 group shows a spectral change similar to that seen in
   radiation-damaged zircon: a decrease in frequency and increase in
   bandwidth.}},
