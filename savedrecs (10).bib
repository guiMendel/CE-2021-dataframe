@article{ WOS:000364891700185,
Author = {Zhu, Zhenfeng and Cheng, Sha and Liu, Hui},
Title = {{Size-controlled synthesis of alumina nanoparticles through an
   additive-free reverse cation-anion double hydrolysis method}},
Journal = {{MATERIALS LETTERS}},
Year = {{2015}},
Volume = {{161}},
Pages = {{720-723}},
Month = {{DEC 15}},
Abstract = {{Size-controlled alumina (Al2O3) nanoparticles have been successfully
   synthesized for the first time via a reverse cation-anion double
   hydrolysis (CADH) method without any additive, employing the starting
   material of NaAlO2 and Al(NO3)(3) center dot 9H(2)O, AlCl3 center dot
   6H(2)O, Al-2(SO4)(3) center dot 18H(2)O. The structures and properties
   of as-obtained samples were well characterized by X-ray powder
   diffractometry (XRD), scanning electron microscopy (SEM) and
   transmission electron microscope (TEM). The Al2O3 samples prepared by
   CADH method using different aluminum salts have three different
   nanostructures. The as-synthesized clew-like Al2O3 was self-assembled by
   uniform nanosized cubic platelets with the side-length of 100-150 nm,
   thickness of about 20 nm. The length of these as-synthesized Al2O3
   nanorods is about 150 nm. The sidelength of as-prepared irregular Al2O3
   nanosheets is less than 50 nm. Crown Copyright (C) 2015 Published by
   Elsevier B.V. All rights reserved.}},
