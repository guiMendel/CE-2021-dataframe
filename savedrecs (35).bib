@article{ WOS:000237133300025,
Author = {Liu, G and Xu, CC and Zhu, GX and Ruan, YL and Wu, WM},
Title = {{Dynamic connection-level call admission control in multi-services
   cellular networks}},
Journal = {{CHINESE JOURNAL OF ELECTRONICS}},
Year = {{2005}},
Volume = {{14}},
Number = {{4}},
Pages = {{671-675}},
Month = {{OCT}},
Abstract = {{Recently, the dynamic call admission mechanism using stochastic control
   was found to be more stable and precise. However, it is still hard to
   get an effective dynamic call admission mechanism for multi-services
   with various requirements of Quality of service (QoS) guarantee,
   bandwidth and handoff rate. Meanwhile, the computational complexity is
   another challenge. Most of the studies on this field cope with this
   problem by setting up a multiple dimensions' stochastic model with high
   cornputational complexity. This paper buffer the data type traffic to
   regulate the occupation of channel and use the proposed single dimension
   stochastic model to compute the key parameters, such as call dropping
   probability and admission probability. The result is decreased
   computational complexity. Numerical results of simulation show that the
   proposed scheme steadily satisfies the hard constraints on call dropping
   probability for multi-services while maintaining a very high channel
   throughput.}},
