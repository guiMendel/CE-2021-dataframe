@article{ WOS:A1995RP68200004,
Author = {ARNE, CN and NORDIN, GL},
Title = {{ENHANCEMENT OF INDEXES OF VIRAL-INFECTION BY SIMULTANEOUSLY
   ADMINISTERING HELICOVERPA-ZEA AND AUTOGRAPHA-CALIFORNICA NUCLEAR
   POLYHEDROSIS VIRUSES TO LARVAL HELICOVERPA-ZEA (BODDIE)}},
Journal = {{JOURNAL OF INVERTEBRATE PATHOLOGY}},
Year = {{1995}},
Volume = {{66}},
Number = {{1}},
Pages = {{18-24}},
Month = {{JUL}},
Abstract = {{Corn earworm, Helicoverpa zea (Boddie), were simultaneously administered
   Heliothis zea and Autographa californica nuclear polyhedrosis viruses
   (HzSNPV and AcMNPV, respectively). These viruses are classified within
   the Baculoviridae (subfamily Eubaculovirinae) and are known to be
   pathogenic to H. zea. When viruses were simultaneously introduced,
   significant reductions of median lethal dose (LD(50)) estimates occurred
   for second- and fourth-instars. However, statistical analysis to
   determine the virus ratio which optimized LD(50) estimates found that
   second-instars were most susceptible to treatments containing HzSNPV
   alone. Analysis of fourth-instars indicated that a viral mixture of 65\%
   HzSNPV and 35\% AcMNPV resulted in significantly lower LD(50) estimates
   than either of the viruses as single infections. Similar studies
   involving median survival time (ST50) analysis indicated that viral
   mixtures significantly reduced the time required between infection and
   mortality. (C) 1995 Academic Press, Inc.}},
