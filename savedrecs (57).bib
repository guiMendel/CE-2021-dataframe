@article{ WOS:A1995TA53500006,
Author = {HOY, S},
Title = {{INVESTIGATIONS ON CORRELATION BETWEEN CONCENTRATION OF AMMONIA IN THE
   AIR OF STABLE AND THE ACTIVITY OF FATTENING PIGS KEPT ON DEEP LITTER}},
Journal = {{DEUTSCHE TIERARZTLICHE WOCHENSCHRIFT}},
Year = {{1995}},
Volume = {{102}},
Number = {{8}},
Pages = {{323-326}},
Month = {{AUG}},
Abstract = {{Results of continuous measurements of ammonia during three rounds with a
   total number of 297 days (appr. 7000 hour-values) and of behavioural
   investigations over 576 hours in two rounds at the begin, in the middle
   and at the end of fattening period have shown a high congruence of both
   variables (concentrations of ammonia; percentage of using self-feeders
   as indicator for animal activity) over 24 hour-cycle with a
   characteristic biphasic dynamics and two periods of increased values
   (after morning feeding between 7 and 9 a. m, and in the afternoon
   between 1 and 3 p, m.). The hour-averages of both parameters
   demonstrated a high significant positive correlation (r = 0.83; p <
   0.01): concentration of ammonia is increasing with increasing animal
   activity. To evaluate concentration or emission of ammonia under the
   view point of animal or environmental hygiene it is necessary to
   consider the dynamics of NH3-values in choosing representative
   measurement points or periods.}},
