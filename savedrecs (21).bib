@article{ WOS:000284161300008,
Author = {Itoh, Takahito and Hirai, Keita and Tamura, Masashi and Uno, Takahiro
   and Kubo, Masataka and Aihara, Yuichi},
Title = {{Synthesis and characteristics of hyperbranched polymer with phosphonic
   acid groups for high-temperature fuel cells}},
Journal = {{JOURNAL OF SOLID STATE ELECTROCHEMISTRY}},
Year = {{2010}},
Volume = {{14}},
Number = {{12}},
Pages = {{2179-2189}},
Month = {{DEC}},
Note = {{10th Annual Meeting on Advanced Batteries and Accumulators (ABA-10),
   Brno, CZECH REPUBLIC, 2009}},
Organization = {{Brno Tech Univ, Fac Elect Engn \& Commun Technolog}},
Abstract = {{Two different molecular weight hyperbranched polymers (HBP(L)-(PA)(2)
   and HBP(H)-(PA)(2)) with two phosphonic acid groups as a functional
   group at the periphery and a low molecular weight hyperbranched polymer
   (HBP(L)-(PA)(2)-Ac) with both two phosphonic acid groups and an acryloyl
   group as a cross-linker at the periphery were successfully synthesized
   as thermally stable proton-conducting electrolytes. A cross-linked
   electrolyte membrane (CL-HBP(L)-(PA)(2)) was prepared by thermal
   polymerization of the HBP(L)-(PA)(2)-Ac using benzoyl peroxide. Ionic
   conductivities of the HBP(L)-(PA)(2), the HBP(H)-(PA)(2), and the
   CL-HBP(L)-(PA)(2) under dry condition and their thermal properties were
   investigated, and also, the effect of the phosphonic acid group number
   on them was discussed. Ionic conductivities of the HBP(L)-(PA)(2) and
   the HBP(H)-(PA)(2) were found to be 1.5 x 10(-5) S cm(-1) at 150 A
   degrees C and 3.6 x 10(-6) S cm(-1) at 143 A degrees C, respectively,
   under dry condition, and showed the Vogel-Tamman-Fulcher type
   temperature dependence. The hyperbranched polymers and the cross-linked
   electrolyte membrane were thermally stable up to 300 A degrees C, and
   the cross-linked electrolyte membrane (CL-HBP-(PA)(2)) had suitable
   thermal stability as an electrolyte membrane for the high-temperature
   fuel cells under dry condition. Fuel cell measurement using a single
   membrane electrode assembly cell with the cross-linked membrane was
   performed.}},
