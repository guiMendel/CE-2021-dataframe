@article{ WOS:000286733100004,
Author = {Nasim, Md. and Kumar, Lokesh and Netrakanti, Pawan Kumar and Mohanty,
   Bedangadas},
Title = {{Energy dependence of elliptic flow from heavy-ion collision models}},
Journal = {{PHYSICAL REVIEW C}},
Year = {{2010}},
Volume = {{82}},
Number = {{5}},
Month = {{NOV 23}},
Abstract = {{We have compared the experimental data on charged-particle elliptic flow
   parameter (v(2)) in Au+Au collisions at midrapidity for root s(NN) =
   9.2, 19.6, 62.4, and 200 GeV with results from various models in
   heavy-ion collisions such as the ultrarelativistic quantum molecular
   dynamics (UrQMD) model, a multiphase transport model (AMPT), and
   heavy-ion jet interaction generator (HIJING). We observe that the
   average v(2) (< v(2)>) from the transport model UrQMD agrees well with
   the measurements at root s(NN) = 9.2 GeV but increasingly falls short of
   the experimental < v(2)> values as the beam energy increases. The
   difference in < v(2)> is of the order of 60\% at root s(NN) = 200 GeV.
   The < v(2)> results from HIJING are consistent with zero, while those
   from AMPT with default settings, a model based on HIJING with additional
   initial-and final-state rescattering effects included, give a < v(2)>
   value of about 4\% for all the beam energies studied. This is in
   contrast to an increase in < v(2)> with beam energy for the experimental
   data. A different version of the AMPT model, which includes partonic
   effects and quark coalescence as a mechanism of hadronization, gives
   higher values of < v(2)> among the models studied and is in agreement
   with the measured < v(2)> values at root s(NN) = 200 GeV. These studies
   show that the experimental < v(2)> has substantial contribution from
   partonic interactions at root s(NN) = 200 GeV, whose magnitude reduces
   with decrease in beam energy. We also compare the available data on the
   transverse momentum and pseudorapidity dependence of v(2) to those from
   these models.}},
