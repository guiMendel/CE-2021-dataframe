@article{ WOS:000618511000001,
Author = {Gill, Kristina S. and Antigua, Abigail D. and Barnett, A. Kacee and
   Hall, Aubrey J. and Klodell, Charles T.},
Title = {{Evaluation of Erythropoietin Stimulating Agents (ESA) and Their Effect
   on Blood Optimization for Cardiac Surgery}},
Journal = {{JOURNAL OF PHARMACY PRACTICE}},
Abstract = {{Background: Cardiovascular surgeries increase the risk of receiving
   blood transfusions. Erythropoietin stimulating agents (ESAs) have been
   used to decrease the transfusion rate. The objective of this study was
   to evaluate the administration of blood products post-cardiothoracic
   surgery after receiving ESAs. Methods: This is a single-center,
   retrospective cohort study. Results: Between May 2017 to May 2018, 52
   adult patients underwent cardiac surgery and received ESAs
   pre-operatively and/or post-operatively. A total of 35 patients were
   included in the study and 21 (60\%) patients did not require a blood
   transfusion while 14 (40\%) patients required a blood transfusion (p =
   0.597). The change in hemoglobin (Hgb = 0.773 g/dL, 1.7 g/dL; p = 0.002)
   and hematocrit (Hct = 2.31\%, 4.3\%; p = 0.04) was significantly
   different in patients who received ESAs alone versus ESAs with blood
   transfusion. Adverse drug reactions showed no significant difference
   between groups. Conclusions: In patients undergoing cardiac surgery,
   ESAs did not significantly reduce the need for blood transfusion. Future
   and larger studies are necessary to evaluate the effect of ESAs on blood
   transfusion.}},
