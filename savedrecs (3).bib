@article{ WOS:000595767500003,
Author = {Hartin, Samantha N. and Means, John C. and Alaimo, Joseph T. and
   Younger, Scott T.},
Title = {{Expediting rare disease diagnosis: a call to bridge the gap between
   clinical and functional genomics}},
Journal = {{MOLECULAR MEDICINE}},
Year = {{2020}},
Volume = {{26}},
Number = {{1}},
Month = {{DEC}},
Abstract = {{Approximately 400 million people throughout the world suffer from a rare
   disease. Although advances in whole exome and whole genome sequencing
   have greatly facilitated rare disease diagnosis, overall diagnostic
   rates remain below 50\%. Furthermore, in cases where accurate diagnosis
   is achieved the process requires an average of 4.8 years. Reducing the
   time required for disease diagnosis is among the most critical needs of
   patients impacted by a rare disease. In this perspective we describe
   current challenges associated with rare disease diagnosis and discuss
   several cutting-edge functional genomic screening technologies that have
   the potential to rapidly accelerate the process of distinguishing
   pathogenic variants that lead to disease.}},
