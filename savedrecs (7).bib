@article{ WOS:000590317500001,
Author = {Weatherall, James Owen},
Title = {{Two dogmas of dynamicism}},
Journal = {{SYNTHESE}},
Abstract = {{I critically discuss two dogmas of the ``dynamical approach{''} to
   spacetime in general relativity, as advanced by Harvey Brown {[}Physical
   Relativity (2005) Oxford:Oxford University Press] and collaborators. The
   first dogma is that positing a ``spacetime geometry{''} has no
   implications for the behavior of matter. The second dogma is that
   postulating the ``Strong Equivalence Principle{''} suffices to ensure
   that matter is ``adapted{''} to spacetime geometry. I conclude by
   discussing ``spacetime functionalism{''}. The discussion is presented in
   reaction to and sympathy with recent work by James Read
   {[}{''}Explanation, geometry, and conspiracy in relativity theory{''}
   (2020) Thinking about Spacetime Boston: Birkauser].}},
