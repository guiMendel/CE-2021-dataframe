@article{ WOS:000564727300002,
Author = {Wang, Weide and Yao, Dongxu and Liang, Hanqin and Xia, Yongfeng and Zuo,
   Kaihui and Yin, Jinwei and Zeng, Yu-Ping},
Title = {{Effect of in-situ formed Y2O3 by metal hydride reduction reaction on
   thermal conductivity of beta-Si3N4 ceramics}},
Journal = {{JOURNAL OF THE EUROPEAN CERAMIC SOCIETY}},
Year = {{2020}},
Volume = {{40}},
Number = {{15}},
Pages = {{5316-5323}},
Month = {{DEC}},
Abstract = {{The effect of YH2 on densification, microstructure, and thermal
   conductivity of Si3N4 ceramics were investigated by adjusting the amount
   of YH2 in the range of 0-4 wt\% using a two-step sintering method.
   Native SiO2 was eliminated, and Y2O3 was in situ formed by a metal
   hydride reduction reaction, resulting in various Y2O3/SiO2 ratios. Full
   densification of YH2-doped samples could be achieved after sintering at
   1900 degrees C for 4 h. The Y2O3/SiO2 ratio had a significant influence
   on the composition of crystalline secondary phases. Besides, the
   increased Y2O3/SiO2 ratio is conducive not only to the grain growth but
   also to the reduction of activity of SiO2 in the liquid phase, resulting
   in enlarged purified grains, reduced volume fraction of intergranular
   phases and increased Si3N4 -Si3N4 contiguity. Ultimately, the thermal
   conductivity increased by 29 \% from 95.3 to 123.0 W m(-1 )K(-1) after
   sintering at 1900 degrees C for 12 h by the substitution of Y2O3 with
   YH2.}},
