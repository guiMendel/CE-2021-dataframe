@article{ WOS:000285208500007,
Author = {Cai, Jianguo and Feng, Jian},
Title = {{EFFECT OF SUPPORT STIFFNESS ON STABILITY OF SHALLOW ARCHES}},
Journal = {{INTERNATIONAL JOURNAL OF STRUCTURAL STABILITY AND DYNAMICS}},
Year = {{2010}},
Volume = {{10}},
Number = {{5}},
Pages = {{1099-1110}},
Month = {{DEC}},
Abstract = {{The nonlinear behavior and in-plane stability of parabolic shallow
   arches with elastic rotational supports are investigated. A central
   concentrated load is applied to create the compression in the supports.
   Nonlinear buckling analysis based on the virtual work formulation is
   carried out to obtain the critical load for both symmetric snap-through
   buckling and anti-symmetric bifurcation buckling. It is found that the
   effect of rotational stiffness of the elastic supports on the critical
   loads is significant. The critical load increases as either the initial
   stiffness coefficient alpha or the stiffening rate beta increases. The
   effect of the stiffening rate beta on the critical loads decreases, as
   the initial stiffness coefficient alpha increases. In addition, the
   influence of rotational stiffness on the bifurcation buckling is larger
   than that on the snap-through buckling. The limit of the geometric
   parameter lambda between the bifurcation and snap-through buckling modes
   also increases with the increase in the stiffening rate beta. In
   addition, the snap-through buckling mode governs the buckling of the
   arch with larger stiffening rates.}},
