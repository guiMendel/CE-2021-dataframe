@article{ WOS:000363817800001,
Author = {Pascual, R. L. and Whiten, W. J.},
Title = {{The determination of floatability distribution from laboratory batch
   cell tests}},
Journal = {{MINERALS ENGINEERING}},
Year = {{2015}},
Volume = {{83}},
Pages = {{1-12}},
Month = {{NOV}},
Abstract = {{The determination of the floatability distribution for froth flotation
   from laboratory batch cell tests is ill conditioned. Tikhonov
   regularisation with accuracy estimates, such as repeat tests for the
   recovery data, provides a way to produce solutions. Two approaches are
   examined: a continuous approximation of the distribution using Laguerre
   polynomials, and using finely spaced discrete components. It is shown
   that for data generated with a known floatability distribution, the
   distribution can be recovered. For experimental data from the literature
   both methods give similar floatability distributions. The accuracy of
   the calculated floatability distributions is estimated. Repeat data is
   important in determining the regularisation parameter and thus insuring
   the data is not over fitted or over smoothed. (C) 2015 Elsevier Ltd. All
   rights reserved.}},
