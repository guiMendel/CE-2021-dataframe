@article{ WOS:000366177700019,
Author = {Li, Mei-Ming and Xu, Qun-Jie and Han, Jie and Yun, Hong and Min, YuLin},
Title = {{Inhibition Action and Adsorption Behavior of Green Inhibitor Sodium
   Carboxymethyl Cellulose on Copper}},
Journal = {{INTERNATIONAL JOURNAL OF ELECTROCHEMICAL SCIENCE}},
Year = {{2015}},
Volume = {{10}},
Number = {{11}},
Pages = {{9028-9041}},
Month = {{NOV}},
Abstract = {{In this paper, Sodium carboxymethyl cellulose (Na-CMC), as a new green
   corrosion inhibitor, has been investigated. The inhibition behavior of
   Na-CMC on copper in simulated cooling water was examined by EIS and
   polarization measurements. The results showed that the best inhibition
   efficiency could reach to 83.34\% when the concentration of Na-CMC is
   5mg/L at 20 degrees C. The computing result of thermodynamic parameters
   (such as Delta G and Delta H) indicated that the adsorption of Na-CMC on
   the copper surface obeyed Langmuir's adsorption isotherm and was a
   typical of chemisorption. Quantum chemical calculations revealed that
   Na-CMC could be adsorbed on copper by Carboxyl group, which may be the
   suitable adsorption site, and its average adsorption energy was
   -60.823KJ/ mol.}},
