@article{ WOS:A1995RN04600014,
Author = {SUDOL, M and CHEN, HI and BOUGERET, C and EINBOND, A and BORK, P},
Title = {{CHARACTERIZATION OF A NOVEL PROTEIN-BINDING MODULE - THE WW DOMAIN}},
Journal = {{FEBS LETTERS}},
Year = {{1995}},
Volume = {{369}},
