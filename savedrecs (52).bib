@article{ WOS:A1995TF47800039,
Author = {NIVATVONGS, S},
Title = {{STRICTUREPLASTY FOR CROHNS-DISEASE OF SMALL-INTESTINE - PRESENT STATUS
   IN WESTERN COUNTRIES}},
