@article{ WOS:000566891300006,
Author = {Argaman, Uri and Makov, Guy},
Title = {{First-principles study of the temperature dependence of the elastic
   constants of titanium}},
Journal = {{COMPUTATIONAL MATERIALS SCIENCE}},
Year = {{2020}},
Volume = {{184}},
Month = {{NOV}},
Abstract = {{For the hcp metal titanium, the elastic constants show pronounced change
   with temperature. In particular, the elastic constant C-12 exhibits an
   increase with temperature and C-13 is approximately independent of
   temperature. In this study we calculate the elastic constants for hcp
   titanium in a first principles anisotropic quasi-harmonic approach which
   includes calculations of the phonon spectra as a function of strain. The
   temperature dependence of the elastic constants is obtained in agreement
   with the experiment with the exception of C-13.}},
