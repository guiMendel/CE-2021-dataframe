@article{ WOS:000608772800001,
Author = {Mulcahy, Candace A. and Wertz, Jeanette A.},
Title = {{Using Project-Based Learning to Build College and Career Readiness Among
   Diverse Learners}},
Journal = {{TEACHING EXCEPTIONAL CHILDREN}},
Year = {{2021}},
Volume = {{53}},
Number = {{5}},
Pages = {{341-349}},
Month = {{MAY}},
Abstract = {{Mr. Reese is a high school business mathematics teacher, responsible for
   teaching a group of diverse learners in 11th and 12th grades. Students
   in Mr. Reese's course completed the basic high school mathematics
   requirements, and his course is designed to provide additional
   mathematics instruction in personal and business finance. Many of his
   students struggled in previous mathematics coursework; therefore, their
   interest and enthusiasm toward mathematics was low. Early in the school
   year, Mr. Reese noticed his students often asked questions like, ``Why
   do we need to know this?{''} and ``When are we ever going to use this in
   real life?{''} That's when Mr. Reese started doing some research and
   found a teaching method called project-based learning (PjBL). Mr. Reese
   used the PjBL format to create the ``Car Project{''} for his students to
   work on over the course of 5 weeks. He started the unit with a visit to
   a local car dealership, where students sat in new and used cars,
   discussed the car-buying process with a salesperson, toured the repair
   and body shop, and roamed the lot for their ``dream car.{''} On the bus
   ride back to school, the students excitedly discussed the experience,
   searching the internet on their phones for cars that caught their eye.}},
Publisher = {{SAGE PUBLICATIONS INC}},
Address = {{2455 TELLER RD, THOUSAND OAKS, CA 91320 USA}},
Type = {{Article}},
