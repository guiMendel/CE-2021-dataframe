@article{ WOS:000282242700054,
Author = {Ashrafizadeh, Azadeh and Ghasemi, Ali and Paesano, Jr., Andrea and
   Cerqueira Machado, Carla Fabiana and Liu, Xiaoxi and Morisako, Akimitsu},
Title = {{Structural and magnetic properties of CuxMg0.5-xZn0.5Fe2O4 (x=0-0.5)
   particles}},
Journal = {{JOURNAL OF ALLOYS AND COMPOUNDS}},
Year = {{2010}},
