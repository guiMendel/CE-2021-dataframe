@article{ WOS:000602916800001,
Author = {Jin, Shanxia and Liu, Liping and Fan, Mengying and Jia, Yaru and Zhou,
   Ping},
Title = {{A Facile Strategy for Immobilizing GOD and HRP onto Pollen Grain and Its
   Application to Visual Detection of Glucose}},
Journal = {{INTERNATIONAL JOURNAL OF MOLECULAR SCIENCES}},
Year = {{2020}},
Volume = {{21}},
Number = {{24}},
Month = {{DEC}},
Abstract = {{Pollen grain was explored as a new carrier for enzyme immobilization.
   After being modified with boric acid-functionalized titania, the pollen
   grain was able to covalently immobilize glycosylated enzymes by boronate
   affinity interaction under very mild experimental conditions (e.g., pH
   7.0, ambient temperature and free of organic solvent). The glucose
   oxidase and horse radish peroxidase-immobilized pollen grain became a
   bienzyme system. The pollen grain also worked as an indicator of the
   cascade reaction by changing its color. A rapid, simple and
   cost-effective approach for the visual detection of glucose was then
   developed. When the glucose concentration exceeded 0.5 mM, the color
   change was observable by the naked eye. The assay of glucose in body
   fluid samples exhibited its great potential for practical application.}},
