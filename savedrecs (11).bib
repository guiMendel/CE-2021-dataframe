@article{ WOS:000367535300040,
Author = {Fachri, Boy Arief and Rasrendra, Carolus Borromeus and Heeres, Hero Jan},
Title = {{Experimental and Modeling Studies on the Conversion of Inulin to
   5-Hydroxymethylfurfural Using Metal Salts in Water}},
Journal = {{CATALYSTS}},
Year = {{2015}},
Volume = {{5}},
Number = {{4}},
Pages = {{2287-2308}},
Month = {{DEC}},
Abstract = {{Inulin, a plant polysaccharide consisting of mainly d-fructose units, is
   considered an interesting feed for 5-hydroxymethylfurfural (HMF), a top
   12 bio-based chemical. We here report an exploratory experimental study
   on the use of a wide range of homogeneous metal salts as catalysts for
   the conversion of inulin to HMF in water. Best results were obtained
   using CuCl2. Activity-pH relations indicate that the catalyst activity
   of CuCl2 is likely related to Lewis acidity and not to Bronsted acidity.
   The effects of process conditions on HMF yield for CuCl2 were
   systematically investigated and quantified using a central composite
