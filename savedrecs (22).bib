@article{ WOS:000285246500012,
Author = {Soliman, Yasser and Mason, Ralph},
Title = {{Application of Subharmonic Injection Locking of LC Oscillators to
   LO-Based Phase-Shifting Phased-Array Architectures}},
Journal = {{IEEE TRANSACTIONS ON MICROWAVE THEORY AND TECHNIQUES}},
Year = {{2010}},
Volume = {{58}},
Number = {{12, 1}},
Pages = {{3475-3484}},
Month = {{DEC}},
Note = {{IEEE International Symposium on Radio-Frequency Integration Technology,
   Singapore, SINGAPORE, DEC 09-11, 2009}},
Organization = {{IEEE Microwave Theory \& Tech Soc; IEEE Solid State Circuits Soc}},
Abstract = {{A ninth-subharmonic injection-locking scheme is proposed for local
   oscillator (LO) phase-shifting-based phased-array architectures
   operating in the ISM band at 24 GHz. The presented architecture employs
   a phase-shifter at the ninth subharmonic, a high-speed regenerative
   comparator, and a negative-g(m) oscillator to synthesize multiple phases
   of ninth-harmonic LO signals from a 2.41-GHz reference at the
   destination and in close proximity to the RF/millimeter-wave front-end
   mixers with first LO at 21.69 GHz. A digital phase-shift calibration
   technique is discussed. The reported LO phase-shifting and synthesis
   scheme is implemented in IBM's 130-nm CMOS technology and consumes a
   total of 32 mA per LO element from a 1.2-V supply. The injection-locked
   oscillators have a phase-noise performance of -107.17 dBc/Hz at 1-MHz
   offset from 21.51 GHz. The maximum phase-shift error at 21.69 GHz is
   9.86 degrees and 2.1 degrees before and after calibration, respectively.}},
