@article{ WOS:000231794400008,
Author = {Ayci, F and Aydinli, M and Bozdemir, OA and Tutas, M},
Title = {{Gas chromatographic investigation of rose concrete, absolute and solid
   residue}},
Journal = {{FLAVOUR AND FRAGRANCE JOURNAL}},
