@article{ WOS:000366902700150,
Author = {Mahmoodian, Reza and Yahya, Rosiyah and Dabbagh, Ali and Hamdi, Mohd and
   Hassan, Mohsen A.},
Title = {{Investigation of Embedded Si/C System Exposed to a Hybrid Reaction of
   Centrifugal-Assisted Thermite Method}},
Journal = {{PLOS ONE}},
Year = {{2015}},
Volume = {{10}},
Number = {{12}},
Month = {{DEC 7}},
Abstract = {{A novel method is proposed to study the behavior and phase formation of
   a Si+C compacted pellet under centrifugal acceleration in a hybrid
   reaction. Si+C as elemental mixture in the form of a pellet is embedded
   in a centrifugal tube. The pellet assembly and tube are exposed to the
   sudden thermal energy of a thermite reaction resulted in a hybrid
   reaction. The hybrid reaction of thermite and Si+C produced unique
   phases. X-ray diffraction pattern (XRD) as well as microstructural and
   elemental analyses are then investigated. XRD pattern showed formation
   of materials with possible electronic and magnetic properties. The
   cooling rate and the molten particle viscosity mathematical model of the
   process are meant to assist in understanding the physical and chemical
   phenomena took place during and after reaction. The results analysis
   revealed that up to 85\% of materials converted into secondary products
   as ceramics-matrix composite.}},
