@article{ WOS:000365080900005,
Author = {Li, Zi-Min and Batchelor, Murray T.},
Title = {{Algebraic equations for the exceptional eigenspectrum of the generalized
   Rabi model}},
Journal = {{JOURNAL OF PHYSICS A-MATHEMATICAL AND THEORETICAL}},
Year = {{2015}},
Volume = {{48}},
Number = {{45}},
Month = {{NOV 13}},
Abstract = {{We obtain the exceptional part of the eigenspectrum of the generalized
   Rabi model, also known as the driven Rabi model, in terms of the roots
   of a set of algebraic equations. This approach provides a product form
   for the wavefunction components and allows an explicit connection with
   recent results obtained for the wavefunction in terms of truncated
   confluent Heun functions. Other approaches are also compared. For
   particular parameter values the exceptional part of the eigenspectrum
   consists of doubly degenerate crossing points. We give a proof for the
   number of roots of the constraint polynomials and discuss the number of
   crossing points.}},
