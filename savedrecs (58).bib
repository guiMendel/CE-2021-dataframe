@article{ WOS:A1995RM57000005,
Author = {YAMAGUCHI, M and OISHI, K},
Title = {{EFFECT OF NUCLEAR CA2+ UPTAKE INHIBITORS ON CA2+-ACTIVATED DNA
   FRAGMENTATION IN RAT-LIVER NUCLEI}},
Journal = {{MOLECULAR AND CELLULAR BIOCHEMISTRY}},
Year = {{1995}},
Volume = {{148}},
Number = {{1}},
Pages = {{33-37}},
Month = {{JUL 5}},
Abstract = {{The effect of nuclear Ca2+ uptake inhibitors on the Ca2+-activated DNA
   fragmentation in rat liver nuclei was investigated. The addition of Ca2+
   (40 mu M) into the reaction mixture containing liver nuclei in the
   presence of 2.0 mMATP caused a remarkable increase in nuclear DNA
   fragmentation. This Ca2+-activated DNA fragmentation was not seen in the
   absence of ATP, because nuclear Ca2+ uptake is not initiated without ATP
   addition. Moreover, the presence of various reagents (10 mu M
   arachidonic acid, 2.0 mM NAD(+), 10 mu M zinc sulfate and 0.2 mM
   N-ethylmaleimide), which could inhibit Ca2+-APase activity and Ca2+
   uptake in the nuclei, produced a significant inhibition of the
   Ca2+-activated DNA fragmentation in the nuclei. The results show that
   the Ca2+-activated DNA fragmentation is involved in the uptake of Ca2+
   by the nuclei, suggesting a role of Ca2+ transport system in the
   regulation of liver nuclear functions.}},
