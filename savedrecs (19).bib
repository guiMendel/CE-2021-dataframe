@article{ WOS:000362176000002,
Author = {Vaughn, Allison A. and Teeters, Stacy A.},
Title = {{Development and psychometric properties of the attitudes towards
   heterosexuals scale (ATHS): a useful tool alone or in combination}},
Journal = {{PSYCHOLOGY \& SEXUALITY}},
Year = {{2015}},
Volume = {{6}},
Number = {{4}},
Pages = {{311-324}},
Month = {{OCT 2}},
Abstract = {{While a number of scales exist for studying explicit attitudes towards
   sexual minorities (i.e., lesbians, gay men and bisexuals), there is
   currently no heterosexual parallel. The utility of such a scale,
   however, would be a boon for researchers interested in comparative,
   longitudinal and/or experimental studies of attitudes based on sexual
   orientation. With this goal in mind, we created the attitudes towards
   heterosexuals scale (ATHS) and tested its psychometric properties across
   three studies. Exploratory and confirmatory factor analysis revealed a
   uni-dimensional factor structure for the 15-item ATHS. Multiple groups'
   analysis suggested a similar factor structure for both heterosexuals and
   sexual minorities. We also investigated relationships between ATHS and
   other attitudinal, affective and demographic measures for evidence of
   construct validity. More positive attitudes on ATHS were related to more
   negative attitudes towards sexual minorities, less awareness of
   heterosexual privilege and greater positive affect towards
   heterosexuals. Lastly, we examined ATHS for temporal stability, finding
   high test-retest reliability. We offer ATHS as a measure to be used on
   its own, or in combination with other attitudinal measures, in research
   in a number of different fields across a number of different
   methodologies.}},
