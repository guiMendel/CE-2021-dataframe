@article{ WOS:000231646900039,
Author = {Zhao, W and Hunt, DC and Tanioka, K and Rowlands, JA},
Title = {{Amorphous selenium flat panel detectors for medical applications}},
Journal = {{NUCLEAR INSTRUMENTS \& METHODS IN PHYSICS RESEARCH SECTION
   A-ACCELERATORS SPECTROMETERS DETECTORS AND ASSOCIATED EQUIPMENT}},
Year = {{2005}},
Volume = {{549}},
