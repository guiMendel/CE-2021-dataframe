@article{ WOS:A1995TG52300018,
Author = {DIRSCH, B and RICHTLER, T},
Title = {{CONSTRAINTS FROM POPULATION SYNTHESIS ON THE MASS-SPECTRA OF
   POPULATION-II SYSTEMS}},
Journal = {{ASTRONOMY \& ASTROPHYSICS}},
Year = {{1995}},
Volume = {{303}},
Number = {{3}},
Pages = {{742-746}},
Month = {{NOV}},
Abstract = {{We calculate M/L(V) ratios of old stellar populations in dependence on
   the mass spectral index and the low mass cut-off, and the metallicity.
   The results are compared with mass spectra and M/L(V) values found in
   the literature for some globular clusters. It comes out that the steep
   mass spectral indices quoted by Richer et al. (1991) for M 13, NGC 6752
   and omega Centauri cannot be brought in agreement with dynamically
   derived M/L(V)-values. Only small mass spectral indices can explain the
   actually low M/L(V) ratios, in which case there is no obvious difference
   between Population I and Population II mass spectra.
   On the other hand, these small mass spectral indices cannot explain the
   high M/L(V) ratios in halos of galaxies. If Dark Matter is baryonic, it
   probably is not connected with `'normal'' star formation.}},
