@article{ WOS:A1995RR78000009,
Author = {TIGER, G and LUNDIN, D and FOWLER, CJ},
Title = {{VERATRINE-STIMULATED PHOSPHOINOSITIDE BREAKDOWN AS AN ASSAY FOR
   LOCAL-ANESTHETIC ACTIONS AT NA+ CHANNELS}},
Journal = {{ANESTHESIA AND ANALGESIA}},
Year = {{1995}},
Volume = {{81}},
Number = {{3}},
Pages = {{480-485}},
Month = {{SEP}},
Abstract = {{The blockade of veratrine-stimulated phosphoinositide breakdown in rat
   cerebral cortical miniprisms as a model of local anesthetic actions on
   voltage-dependent sodium channels was assessed. Veratrine stimulated
   phosphoinositide breakdown with an EC50 value of 5 mu M. The stimulation
   produced by 20 mu M veratrine was blocked completely by (+)-bupivacaine
   (IC50 7.6 mu M {[}mean of three separate experimental series]),
   (-)-bupivacaine (IC50 7.3 mu M), lidocaine (IC50 34 mu M), etidocaine
   (IC50 3.4 mu M), tetracaine (IC50 similar to 2 mu M), and prilocaine
   (IC50 110 mu M). Phosphoinositide breakdown responses to ouabain
   (100-1000 mu M) and K+ (50 mM) were only partially blocked by
   (+)-bupivacaine, and the responses to monensin (100 and 1000 mu M) and
   noradrenaline (30 mu M) were not blocked at all by this drug. Nifedipine
   produced no significant effects on the phosphoinositide response to 10
   mu M veratrine. It is concluded that in pulse label experiments using
   rat cerebral cortical miniprisms, local anesthetics in general, and
   (+)-bupivacaine in particular, block the phosphoinositide response to
   veratrine with a high degree of specificity. This system may be useful
   as a relatively simple and quantitative assay for drug effects on
   Na+-channels.}},
