Type = {{Article}},
Language = {{English}},
Affiliation = {{Luca, FC (Corresponding Author), Univ Penn, Sch Vet Med, Dept Biol Anim, Philadelphia, PA 19104 USA.
   Univ Penn, Sch Vet Med, Dept Biol Anim, Philadelphia, PA 19104 USA.
   Univ British Columbia, Michael Smith Labs, Vancouver, BC V6T 1Z4, Canada.}},
DOI = {{10.1091/mbc.E05-04-0337}},
