@article{ WOS:000284028900114,
Author = {Espinosa, D. and Algar, F. J. and Moreno, P. and Illana, J. and Alvarez,
   A. and Cerezo, F. and Baamonde, C. and Santos, F. and Vaquero, J. M. and
   Redel, J. and Salvatierra, A.},
Title = {{Experience of the Reina Sofia Hospital in Lobar Lung Transplantation}},
