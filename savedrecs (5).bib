@article{ WOS:000615044600009,
Author = {Salina, Jonathan},
Title = {{The Philosophical Foundation of Intersubjectivity. Models and
   Difficulties of Three Classical Perspectives in XX Century}},
Journal = {{CULTURA-RIVISTA DI FILOSOFIA LETTERATURA STORIA}},
Year = {{2020}},
Volume = {{58}},
Number = {{2-3}},
Pages = {{367-395}},
Month = {{DEC}},
Abstract = {{This article wants to offer a critical discussion about some
   ``classical{''} foundations of intersubjectivity offered by important
   philosophers in the first half of XX century. In particular, we will
   consider some important works of Giovanni Gentile (Genesi e struttura
   della societa), Edmund Husserl (Cartesianische Meditationen) and Martin
   Heidegger (Sein und Zeit). We will examine some aspects of these works
   trying to shed light on the theoretical difficulties of the
   intersubjective foundation. The specifical point of view we will adopt
   has many relations with some currents of contemporary italian philosophy
   and configures itself as a general examination about the classic
   conceptual theme of ``unity and plurality{''}.}},
