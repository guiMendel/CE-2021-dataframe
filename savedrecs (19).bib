@article{ WOS:000353476700001,
Author = {Alseda, Lluis and Ruette, Sylvie},
Title = {{ON THE SET OF PERIODS OF SIGMA MAPS OF DEGREE 1}},
Journal = {{DISCRETE AND CONTINUOUS DYNAMICAL SYSTEMS}},
Year = {{2015}},
Volume = {{35}},
Number = {{10}},
Pages = {{4683-4734}},
Month = {{OCT}},
Abstract = {{We study the set of periods of degree 1 continuous maps from a into
   itself, where sigma denotes the space shaped like the letter a (i.e., a
   segment attached to a circle by one of its endpoints). Since the maps
   under consideration have degree 1, the rotation theory can be used. We
   show that, when the interior of the rotation interval contains an
   integer, then the set of periods (of periodic points of any rotation
   number) is the set of all integers except maybe 1 or 2. We exhibit
   degree 1 sigma-maps f whose set of periods is a combination of the set
   of periods of a degree 1 circle map and the set of periods of a 3-star
   (that is, a space shaped like the letter Y). Moreover, we study the set
   of periods forced by periodic orbits that do not intersect the circuit
   of sigma; in particular, when there exists such a periodic orbit whose
   diameter (in the covering space) is at least 1, then there exist
   periodic points of all periods.}},
