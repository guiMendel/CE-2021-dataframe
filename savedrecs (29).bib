@article{ WOS:000281779600078,
Author = {Doering, Sven and Richter, Soeren and Nolte, Stefan and Tuennermann,
   Andreas},
Title = {{In situ imaging of hole shape evolution in ultrashort pulse laser
   drilling}},
Journal = {{OPTICS EXPRESS}},
Year = {{2010}},
Volume = {{18}},
Number = {{19}},
Pages = {{20395-20400}},
Month = {{SEP 13}},
Abstract = {{For the first time, in situ the hole shape evolution during ultrashort
   pulse laser drilling in semiconductor material is imaged. The
   trans-illumination of the sample at a wavelength of 1.06 mu m is
   projected onto a standard CCD camera during the ablation, providing an
   image of the contour of the ablated structure perpendicular to the
   irradiation for drilling. This demonstrated technique enables a direct,
   high resolution investigation of the temporal evolution of the drilling
   process in the depth of the material without complex sample preparation
   or post processing. (C) 2010 Optical Society of America}},
