@article{ WOS:A1995TX94700008,
Author = {Darjushkin, AE and Kondratev, NL and Orlov, AN and Prokhorov, AM and
   Pustovoi, VI},
Title = {{Dissociation of SiF4 molecules in the gas phase under the action of
   continuous-wave CO2-laser radiation: Frequency dependence}},
Journal = {{LASER PHYSICS}},
Year = {{1995}},
Volume = {{5}},
Number = {{4}},
Pages = {{747-750}},
Month = {{JUL-AUG}},
Abstract = {{We experimentally investigated the dependence of the laserchemical decay
   reaction of silicon tetrafluoride on the radiation frequency and the gas
   pressure. The reaction was observed only when the gas was subjected to
   the influence of radiation in the frequency range from 974.6 to 982.1
   cm(-1). Within the frequency ranges of 935 - 953, 1035 - 1055, and 1071
   - 1085 cm(-1), the reaction did not occur. The results of investigations
   make it possible to infer that direct laser heating cannot initiate the
   reaction. The reignition of the reaction after a short-time (from 2 to
   10 s) interruption of irradiation occurs at pressures that are
   noticeably lower than the initial ignition pressure. This effect may be
   accounted for by the appearance of clusters, which can be produced in
   the course of the reaction.}},
