@article{ WOS:000165198400016,
Author = {Tzou, GY and Huang, MN},
Title = {{Study on the minimum thickness for the asymmetrical PV cold rolling of
   sheet}},
Journal = {{JOURNAL OF MATERIALS PROCESSING TECHNOLOGY}},
Year = {{2000}},
Volume = {{105}},
Number = {{3}},
Pages = {{344-351}},
Month = {{SEP 29}},
Abstract = {{The study mainly aims at the asymmetrical PV1 cold rolling of sheet to
   investigate the minimum thickness. For the purpose of deriving the
   minimum thickness, the complete equation for asymmetrical cold rolling
   of sheet is derived by an analytical approach based on the slab method.
   Two analytical models for the minimum thickness of asymmetrical PV cold
   rolling are established, Model I considering the same frictional
   coefficients for the upper and lower rolls, whilst Model II considers
   different frictional coefficients for the upper and lower rolls. The
   relationships between the ratio of effective radius to initial thickness
   and bite angle for the two models are proposed. The comparisons of front
   tension, rolling force per unit width, reduction, and limiting roll
   speed ratio for the two models are examined. From an analytical
   approach, the values of minimum thickness can be obtained. The study
   proposes a new analytical technique to explore the minimum thickness.
