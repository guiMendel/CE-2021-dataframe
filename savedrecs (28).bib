Title = {{Crystal Structure and Characterization of
   {[}Ni(RPOEt)(2)(C2H5OH)(2)](ClO4)(2) {[}RPOEt =
   bis\{(diphenylphosphinyl)methyl\}ethyl phosphinate]}},
Journal = {{JOURNAL OF CHEMICAL CRYSTALLOGRAPHY}},
