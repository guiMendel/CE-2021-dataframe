@article{ WOS:000601294800001,
Author = {Taleghani, Pejman Rezaei and Valefi, Zia and Ehsani, Naser},
Title = {{Characterization and oxidation behaviour of nanostructured
   La-2(Zr0.7Ce0.3)(2)O-7/YSZ coatings prepared by calcined precursor
   precipitate plasma spraying}},
Journal = {{SURFACE ENGINEERING}},
Year = {{2021}},
