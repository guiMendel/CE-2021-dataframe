@article{ WOS:000231462900006,
Author = {Riou, L and Ghezzi, C and Wouessidjewe, D and Law, H and Mathieu, JP and
   Defaye, J and Bontron, R and Pasqualini, R and Fagret, D},
Title = {{Differential effects of cyclodextrins and derivatives on the biological
   behavior of the myocardial perfusion imaging agent (TcN)-Tc-99m-NOET}},
Journal = {{EUROPEAN JOURNAL OF PHARMACEUTICS AND BIOPHARMACEUTICS}},
Year = {{2005}},
Volume = {{61}},
Number = {{1-2}},
Pages = {{40-49}},
Month = {{SEP}},
Abstract = {{In addition to improving drug solubilization, cyclodextrins (CDs) also
   affect the biological behavior of the included compound. We evaluated
   the effects of two natural CDs beta-CD and gamma-CD, and six beta-CD
   derivatives, Dimeb, Trimeb, SBb, 2-HP, 6AD, and 6 MTU on the biological
   behavior of (TcN)-Tc-99m-NOET, a technetium-99m-labeled, lipophilic
   compound readily detectable through radioactivity assessment.
   Determination of CDs' affinities for (TcN)-Tc-99m-NOET indicated that
   the cavity size of gamma-CD was not suitable for (TcN)-Tc-99m-NOET
   inclusion, and that beta-CD derivatization mostly resulted in decreased
   CDs affinities for (TcN)-Tc-99m-NOET to various extents compared with
   the natural beta-CD. In vitro and ex vivo experiments performed on
   newborn rat cardiomyocytes and isolated perfused rat hearts,
   respectively, showed 1.7- and 2.3-fold maximal differences in
   (TcN)-Tc-99m-NOET cellular and tissue activities. Regression analyzes
   indicated no significant correlation between these observed biological
   differences and the affinities of the eight CDs tested for
   (TcN)-Tc-99m-NOET or for cellular membranes. In conclusion, CD
   derivatization often resulted in impaired affinity of the derivatives
   for the lipophilic compound (TcN)-Tc-99m-NOET. Moreover, the in vitro
   and ex vivo biological behavior of (TcN)-Tc-99m-NOET was greatly
   affected depending on the CD used for inclusion of the tracer. (c) 2005
   Elsevier B.V. All rights reserved.}},
