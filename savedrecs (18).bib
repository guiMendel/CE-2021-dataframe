@article{ WOS:000365931900021,
Author = {Szewczynska, Malgorzata and Pagowska, Emilia and Pyrzynska, Krystyna},
Title = {{Emissions of fluorides from welding processes}},
Journal = {{JOURNAL OF ENVIRONMENTAL SCIENCES}},
Year = {{2015}},
Volume = {{37}},
Pages = {{179-183}},
Month = {{NOV 1}},
Abstract = {{The levels of fluoride airborne particulates emitted from welding
   processes were investigated. They were sampled with the patented IOM
   Sampler, developed by J. H. Vincent and D. Mark at the Institute of
   Occupational Medicine (IOM), personal inhalable sampler for simultaneous
   collection of the inhalable and respirable size fractions. Ion
   chromatography with conductometric detection was used for quantitative
   analysis. The efficiency of fluoride extraction from the cellulose
   filter of the IOM sampler was examined using the standard sample of
   urban air particle matter SRM-1648a. The best results for extraction
   were obtained when water and the anionic surfactant
   N-Cetyl-N-N-N-trimethylammonium bromide (CTAB) were used in an
   ultrasonic bath. The limits of detection and quantification for the
   whole procedure were 8 mu g/L and 24 mu g/L, respectively. The linear
   range of calibration was 0.01-10 mg/L, which corresponds to 0.0001-0.1
   mg of fluorides per m(3) in collection of a 20 L air sample. The
   concentration of fluorides in the respirable fraction of collected air
   samples was in the range of 0.20-1.82 mg/m(3), while the inhalable
   fraction contained 0.23-1.96 mg/m(3) of fluorides during an eight-hour
   working day in the welding room. (C) 2015 The Research Center for
   Eco-Environmental Sciences, Chinese Academy of Sciences. Published by
   Elsevier B.V.}},
