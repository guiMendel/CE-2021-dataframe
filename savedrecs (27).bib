@article{ WOS:000282288100009,
Author = {Li, W. and Wang, Y. and Chen, T.},
Title = {{Investigation on the Thevenin equivalent parameters for online
   estimation of maximum power transfer limits}},
Journal = {{IET GENERATION TRANSMISSION \& DISTRIBUTION}},
Year = {{2010}},
Volume = {{4}},
Number = {{10}},
Pages = {{1180-1187}},
Month = {{OCT}},
Abstract = {{Online estimation of maximum power transfer limits using the Thevenin
   equivalent has attracted lots of attention because of its simplicity.
   This study presents a general analytical model for investigating the
   Thevenin equivalent parameters. An approximate approach is studied for
   the determination of the Thevenin equivalent parameters. The
   applicability of this approach is analysed and tested through a simple
   linear network and two complex large power systems. A
   predictor-corrector framework is proposed for higher accuracy.
   Simulation results show that the proposed techniques are effective for
   online estimation of maximum power transfer limits of weak load buses.}},
