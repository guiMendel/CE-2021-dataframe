@article{ WOS:000587138100032,
Author = {Fernandez-Ruiz, Juan and Schwerdtfeger, Rebecca M. Hakvoort and
   Alahyane, Nadia and Brien, Donald C. and Coe, Brian C. and Munoz,
   Douglas P.},
Title = {{Dorsolateral prefrontal cortex hyperactivity during inhibitory control
   in children with ADHD in the antisaccade task}},
Journal = {{BRAIN IMAGING AND BEHAVIOR}},
Year = {{2020}},
Volume = {{14}},
Number = {{6}},
Pages = {{2450-2463}},
Month = {{DEC}},
Abstract = {{Children with ADHD show significant deficits in response inhibition. A
   leading hypothesis suggests prefrontal hypoactivation as a possible
   cause, though, there is conflicting evidence. We tested the
   hypoactivation hypothesis by analyzing the response inhibition process
   within the oculomotor system. Twenty-two children diagnosed with ADHD
   and twenty control (CTRL) children performed the antisaccade task while
   undergoing an fMRI study with concurrent eye tracking. This task
   included a preparatory stage that cued a prosaccade (toward a stimuli)
   or an antisaccade (away from a stimuli) without an actual presentation
   of a peripheral target. This allowed testing inhibitory control without
   the confounding activation from an actual response. The ADHD group
   showed longer reaction times and more antisaccade direction errors.
   While both groups showed activations in saccade network areas, the ADHD
   showed significant hyperactivation in the dorsolateral prefrontal cortex
   during the preparatory stage. No other areas in the saccade network had
   significant activation differences between groups. Further ADHD group
   analysis OFF and ON stimulant medication did not show drug-related
   activation differences. However, they showed a significant correlation
   between the difference in OFF/ON preparatory activation in the
   precuneus, and a decrease in the number of antisaccade errors. These
   results do not support the hypoactivity hypothesis as an inhibitory
   control deficit general explanation, but instead suggest less efficiency
   during the inhibitory period of the antisaccade task in children. Our
   findings contrast with previous results in ADHD adults showing decreased
   preparatory antisaccade activity, suggesting a significant age-dependent
   maturation effect associated to the inhibitory response in the
   oculomotor system.}},
Publisher = {{SPRINGER}},
Address = {{ONE NEW YORK PLAZA, SUITE 4600, NEW YORK, NY, UNITED STATES}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Fernandez-Ruiz, J (Corresponding Author), Univ Nacl Autonoma Mexico, Fac Med, Dept Fisiol, Av Univ 3000, Ciudad De Mexico 04510, Mexico.
