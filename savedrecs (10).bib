@article{ WOS:000367601400008,
Author = {Lee, Yong-Ki and Kim, Sally and Son, Moon-Hyun and Kim, Min-Seong},
Title = {{Linking Organizational Justice to Job Performance: Evidence from the
   Restaurant Industry in East Asia}},
Journal = {{ASIA PACIFIC JOURNAL OF TOURISM RESEARCH}},
Year = {{2015}},
Volume = {{20}},
Number = {{1}},
Pages = {{1527-1544}},
Month = {{DEC 18}},
Abstract = {{This study examines the role of justice in a leader-subordinate dyadic
   relationship and the effect of the relationship quality on job
   performance in the restaurant industry. Based on the well-established
   framework of justice, the social exchange theory, and Confucian cultural
   background (collectivism) in East Asia, the study proposes and tests a
   model that attempts to understand a dynamic relationship among four
   dimensions of justice, two types of trust, and job performance. Using
   the data collected from restaurant employees in South Korea, the study
   finds that distributive justice and interactional justice are critical
   for nurturing an affect-based trust in East Asia.}},
