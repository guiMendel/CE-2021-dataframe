@article{ WOS:000364891700169,
Author = {Tian, Yong and Zhong, Suni and Zhu, Xiaojing and Huang, Ailin and Chen,
   Yanzhong and Wang, Xiufang},
Title = {{Mesoporous carbon spheres: Synthesis, surface modification and neutral
   red adsorption}},
Journal = {{MATERIALS LETTERS}},
Year = {{2015}},
Volume = {{161}},
Pages = {{656-660}},
Month = {{DEC 15}},
Abstract = {{A simple ultrasonic method of preparing porous carbon spheres without
   using a solvent was presented. Furfuryl alcohol acted dual functions as
   carbon sources and solvent, silica nanoparticles as template precursor,
   and oxalic acid as homogeneous catalyst. Effects of different oxidants
   (H2O2 and HNO3), on structural parameters, morphologies, and dye
   adsorption properties of carbon samples were studied. Results showed
   that specific surface areas and total pore volumes of carbon spheres
   increased with H2O2 treatment, whereas surface area and pore volume of
   carbon spheres decreased after HNO3 oxidation. Oxidized mesoporous
