@article{ WOS:000595149400019,
Author = {Qi, Liang and Cawood, Peter A. and Yang, Jianghai and Xu, Yajun and Du,
   Yuansheng},
Title = {{Quantifying temperature variation between Neoproterozoic cryochron -
   nonglacial interlude, Nanhua Basin, South China}},
Journal = {{PRECAMBRIAN RESEARCH}},
Year = {{2020}},
Volume = {{351}},
Month = {{DEC}},
Abstract = {{Three major late Neoproterozoic global cryochrons mark the coldest
   climate state in Earth history, with glaciers covering most, if not all,
   continents. However, the terrestrial paleosurface temperature
   differential between cryochmn and nonglacial interlude is unknown.
   Time-equivalent Cryogenian and Ediacaran sedimentary successions from
   South China and Oman show multiple negative excursions of the corrected
   values of their chemical index of alteration (CIA(corr)). These
   excursions reflect climate cooling and correlate with the Sturtian,
   Marinoan and Gaskiers cryochrons. Based on the tau(Na)-MAT transfer
   function (sodium chemical depletion index-mean annual temperature), we
   estimate the temperature differential between Cryogenian cryochron and
   nonglacial interlude reached 20 +/- 5.4 degrees C at around 30 degrees
   paleolatitude, and 12 +/- 5.4 degrees C temperature difference between
   the Ediacaran cryochmn and nonglacial interlude near the equator. The
   fluctuation of CIA(corr) and MAT trends within individual cryochmns
   likely indicate cold-warm climate cycling during the Cryogenian
   cryochron.}},
