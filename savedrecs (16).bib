@article{ WOS:000367754700019,
Author = {DerSimonian, Rebecca and Laird, Nan},
Title = {{Meta-analysis in clinical trials revisited}},
Journal = {{CONTEMPORARY CLINICAL TRIALS}},
Year = {{2015}},
Volume = {{45}},
Number = {{A, SI}},
Pages = {{139-145}},
Month = {{NOV}},
Abstract = {{In this paper, we revisit a 1986 article we published in this Journal,
   Meta-Analysis in Clinical Trials, where we introduced a random-effects
   model to summarize the evidence about treatment efficacy from a number
   of related clinical trials. Because of its simplicity and ease of
   implementation, our approach has been widely used (with more than 12,000
   citations to date) and the ``DerSimonian and Laird method{''} is now
   often referred to as the `standard approach' or a `popular' method for
   meta-analysis in medical and clinical research. The method is especially
   useful for providing an overall effect estimate and for characterizing
   the heterogeneity of effects across a series of studies. Here, we review
   the background that led to the original 1986 article, briefly describe
   the random-effects approach for meta-analysis, explore its use in
   various settings and trends over time and recommend a refinement to the
   method using a robust variance estimator for testing overall effect. We
   conclude with a discussion of repurposing the method for Big Data
   meta-analysis and Genome Wide Association Studies for studying the
   importance of genetic variants in complex diseases. Published by
   Elsevier Inc.}},
