@article{ WOS:000575821300005,
Author = {Figallo, V, Aldo and Pascual, Ines and Pelaitay, Gustavo},
Title = {{A new topological duality for n x m-valued Lukasiewicz-Moisil algebras}},
Journal = {{ASIAN-EUROPEAN JOURNAL OF MATHEMATICS}},
Year = {{2020}},
Volume = {{13}},
Number = {{8}},
Month = {{DEC}},
Abstract = {{In 2000, Figallo and Sanza introduced n x m-valued Lukasiewicz-Moisil
   algebras which are both a particular case of matrix Lukasiewicz algebras
   {[}W. Suchon, Matrix Lukasiewicz Algebras, Rep. Math. Logic 4 (1975)
   91-104] and a generalization of n-valued Lukasiewicz-Moisil algebras. It
   is worth noting that unlike what happens in n-valued Lukasiewicz-Moisil
   algebras, generally the De Morgan reducts of n x m-valued
   Lukasiewicz-Moisil algebras are not Kleene algebras. Furthermore, in
   {[}C. Sanza, n x m-valued Lukasiewicz algebras with negation, Rep. Math.
   Logic 40 (2006) 83-106] an important example which legitimated the study
   of this class of algebras is provided.
   In this paper, we continue the study of n x m-valued Lukasiewicz-Moisil
   algebras (or LMnxm-algebras). More precisely, we determine a new
   topological duality for these algebras. By means of this duality we
   characterize the congruences and specially the maximal congruences on
   LMnxm-algebras. Then these characterizations allow us to assert that
   LMnxm-algebras are semisimples and obtain a new description of
   subdirectly irreducible LMnxm-algebras.}},
