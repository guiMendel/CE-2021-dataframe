@article{ WOS:A1995RY19600004,
Author = {KETTER, TA and JENKINS, JB and SCHROEDER, DH and PAZZAGLIA, PJ and
   MARANGELL, LB and GEORGE, MS and CALLAHAN, AM and HINTON, ML and CHAO, J
   and POST, RM},
Title = {{CARBAMAZEPINE BUT NOT VALPROATE INDUCES BUPROPION METABOLISM}},
Journal = {{JOURNAL OF CLINICAL PSYCHOPHARMACOLOGY}},
Year = {{1995}},
Volume = {{15}},
Number = {{5}},
Pages = {{327-333}},
Month = {{OCT}},
Abstract = {{Bupropion (BUP) may be less likely than other antidepressants to cause
   switches into mania and rapid cycling, suggesting utility in bipolar
   disorder. The combination of BW with the mood-stabilizing
   anticonvulsants carbamazepine (CBZ) or valproate (VPA) is a strategy
   that might further lessen the risk of mania. CBZ induces, and to a
   lesser extent WA inhibits the hepatic metabolism of various medications,
   but their effects on BUP have not been previously studied. Inpatients
   with mood disorders had pharmacokinetic profiles of BUP and metabolites
   assessed after single, oral, 150-mg doses of BUP while receiving placebo
   (N = 17) or during chronic blind CBZ (N = 12) or VPA (N = 5)
   monotherapy. CBZ but not VPA therapy decreased BUP peak concentrations
   (C-max) by 87\% (p < 0.0001) and 24-h area under the curve (AUG) by 90\%
   (p < 0.0001), threohydrobupropion C-max by 81\% (p < 0.0009) and AUC by
   86\% (p < 0.002), and erythropydrobupropion C-max by 86\% (p < 0.05) and
   AUC by 96\% (p < 0.05). CBZ increased hydroxybupropion (H-BUP) C-max by
   71\% (p < 0.007) and AUC by 50\% (p < 0.05). VPA increased H-BW C-max by
   56\% (p < 0.09) and H-BUP AUC by 94\% (p < 0.02). Thus, CBZ markedly
   decreased BUP and increased H-BUP concentrations, whereas VPA did not
