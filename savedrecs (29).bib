@article{ WOS:000281757900007,
Author = {Aoki, T. and Shimizu, S. and Urano, E. and Futahashi, Y. and Hamatake,
   M. and Tamamura, H. and Terashima, K. and Murakami, T. and Yamamoto, N.
   and Komano, J.},
Title = {{Improvement of lentiviral vector-mediated gene transduction by genetic
   engineering of the structural protein Pr55(Gag)}},
Journal = {{GENE THERAPY}},
Year = {{2010}},
Volume = {{17}},
Number = {{9}},
Pages = {{1124-1133}},
Month = {{SEP}},
Abstract = {{The lentiviral vector is a promising tool for human gene therapy because
   of its ability to transduce genes into many cell types. However, one of
   the technical problems associated with the lentiviral vector is that
   lentiviral titers in current production systems are relatively low
   compared with the other viral vectors. In this study, we provide genetic
   evidence that the attachment of heterologous myristoylation (myr)
   signals on the amino-terminus of human immunodeficiency virus type 1
   Pr55(Gag) (Gag) can increase the viral yield up to 10-fold, leading to
   the enhancement of gene transduction in many cell lines. The myr signal
   Gag constructs behaved similarly to the wild-type Gag in targeting to
   detergent-resistant membrane compartments, Vps4-dependence for viral
   budding, and virion morphology. However, the myr signal Gag constructs
   showed improved oligomerization efficiency as measured by
   bioluminescence resonance energy transfer in living cells, contributing
   to increased viral production and efficient activation of the viral
   protease responsible for virion maturation. The genetically modified Gag
   represents the next generation lentiviral vector, and should contribute
   to the success of many lentiviral vector applications. Gene Therapy
   (2010) 17, 1124-1133; doi:10.1038/gt.2010.61; published online 22 April
   2010}},
