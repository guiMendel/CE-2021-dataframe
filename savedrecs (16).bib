@article{ WOS:000362310900006,
Author = {Bastos Pinho, Luis Gustavo and Cordeiro, Gauss Moutinho and Nobre,
   Juvencio Santos},
Title = {{On the Harris-G class of distributions: General results and application}},
Journal = {{BRAZILIAN JOURNAL OF PROBABILITY AND STATISTICS}},
Year = {{2015}},
Volume = {{29}},
Number = {{4}},
Pages = {{813-832}},
Month = {{NOV}},
Abstract = {{We investigate some properties of the Harris-G class of distributions
   (Sankhya B 73 (2011) 70-80). We demonstrate that the density function of
   the Harris-G class can be expressed as a linear combination of density
   functions of the exponentiated baseline distribution. We provide general
   formulas for moments (raw, centered, incomplete and factorial), quantile
   function, generating functions and entropies. Two numerical examples are
   presented to demonstrate the potentiality of the models in this class.
   The first one applies the Harris Burr XII distribution to model bimodal
   data. The second example uses the Harris-exponential distribution to
   model SAR image data. The results of the fitted models look promising.}},
