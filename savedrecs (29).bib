@article{ WOS:000282766100022,
Author = {Chernov, V. and Piters, T. and May, P. W. and Melendrez, R. and
   Pedroza-Montero, M. and Barboza-Flores, M.},
Title = {{Comparative study of TL created in undoped CVD diamond by beta rays, UV
   and visible light}},
Journal = {{PHYSICA STATUS SOLIDI A-APPLICATIONS AND MATERIALS SCIENCE}},
Year = {{2010}},
Volume = {{207}},
Number = {{9}},
Pages = {{2119-2124}},
Month = {{SEP}},
Note = {{SBDD XV Cultuurcentrum Hasselt Diamond Workshop, Hasselt, BELGIUM, FEB
   22-24, 2010}},
Abstract = {{The thermoluminescence (TL) properties of CVD diamond film irradiated
   with beta rays and illuminated with UV and visible light (200-450 nm)
   have been studied. The film (23 mm) was grown for 47 h on
   monocrystalline silicon. The TL glow curves are similar for beta
   irradiation and short wavelength UV light. The beta dose response curves
   and UV light time response curves exhibit linear-supralinear-sublinear
   behaviour. The structure of the glow curve was determined by a
   simultaneous deconvolution of a set of curves recorded after preliminary
   heating to various temperatures. One set of the TL peak parameters was
   found for all the curves during the deconvolution. The TL glow curves
   were found to consist of five strongly overlapped peaks. The peaks at
   about 350 and 400 K obey first-order kinetics. The next three peaks at
   about 480, 540 and 580 K are described well by general-order kinetics.
   The TL curves created with light of wavelength between 250 and 450 nm
   are different from those created by beta radiation and short wavelength
   UV light. The former have low sensitivity and consist of several
   strongly overlapped peaks, two of them are observed at about 330 and 600
   K. (C) 2010 WILEY-VCH Verlag GmbH \& Co. KGaA, Weinheim}},
