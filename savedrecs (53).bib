@article{ WOS:A1995TC16300005,
Author = {ROCHOI, TS},
Title = {{EFFECTS OF NUCLEOLAR U3 RNA AND CAP ANALOGS ON NUCLEOLAR
   RNA-TRANSCRIPTION}},
Journal = {{MOLECULES AND CELLS}},
Year = {{1995}},
Volume = {{5}},
Number = {{5}},
Pages = {{425-428}},
Month = {{OCT 31}},
Abstract = {{Nucleolar U3 RNA (8 mu M) from rat Novikoff hepatoma cells, m(3)(2,2,7)G
   (20 mM), and m(7)Gppp-Am (120 mu M) increased rat liver nucleolar RNA
   transcription in vitro by 640\%, 233\%, and 218\% respectively during 15
   min of incubation at 37 degrees C, Kinetic anaysis of effects of U3 RNA
   at a concentration of 4 mu M showed marked increase in Fate (200\%) and
   net transcription (400\%) of nucleolar RNA in isolated nucleoli of rat
   liver, These results indicate that U3 RNA may function in the initiation
   as well as in the elongation reaction of nucleolar RNA transcription.
   However, kinetic analysis of effects of m(3)(2,2,7)G nucleoside
   increased the initial transcription rate (150\%) but not the net
   transcrption of nucleolar RNA. Also, m(3)(2,2,7)G prevented RNA
   polymerase inactivation in preincubation period. In agreement with
   theses results, 5' terminal labeling with {[}gamma-P-32]ATP or
   {[}gamma-P-32]GTP did not change in the presence of m(3)(2,2,7)G
   nucleoside, These results show that m(3)(2,2,7)G does not increase new
   initiation of RNA transcription but stabilizes the existing
   transcription complex and increases the rate of transcription as well as
   preventing loss of complex during preincubation. These results suggest
   that trimethylguanosine capped U3 RNA appears to be essential for
   optimal nucleolar RNA transcription.}},
