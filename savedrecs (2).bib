@article{ WOS:000598738800015,
Author = {Shiri, Fereshteh and Shahraki, Somaye and Bazzi-Alahri, Mohammadreza},
Title = {{Assessing the in vitro and in silico interactions of two Palladium(II)
   dithiocarbamate complexes with human serum albumin}},
Journal = {{JOURNAL OF MOLECULAR STRUCTURE}},
Year = {{2020}},
Volume = {{1221}},
Month = {{DEC 5}},
Abstract = {{The use of metallodrugs in the treatment of various diseases is common.
   Free radicals' scavenging can prevent diseases such as cancer.
   Therefore, the design and synthesis of antioxidant metallodrugs can be
   very important. In this study, the antioxidant activity and HSA
   interaction of two Pd(II) complexes formulated as {[}(NeN)Pd(
   m-pr-dtc)Pd(NeN)](NO3) 2 (where pr-dtc is propylenbisdithiocarbamate,
   NeN are 2,2'-bipyridine for complex a, and 1,10 phenanthroline for
   complex b) were reported. Antioxidant activity of the Pd complexes
   against 1,1-diphenyl-2-picrylhydrazyl radicals (DPPH) revealed that
   complexes have moderate antioxidant activity (IC50 = 790 mg L-1 for
   complex a and 1030 mg L-1 for complex b). The fluorescence spectra
   demonstrated that the quenching mechanism of HSA in the presence of the
   complexes was static quenching. The complex b (K-b = 1.23 x 10(4) M-1)
   had a tendency to HSA more than the complex a (K-b = 0.257 x 10(4) M-1).
   Thermodynamic parameters showed that hydrophobic interactions
   contributed to dominantly in the interaction process. The biophysical
   approaches, including UV-Vis spectra, CD spectroscopy, and FT-IR spectra
   confirmed the conformational changes in the structure of HSA in the
   presence of Pd complexes. Molecular dynamic and docking simulation
   disclosed that both complexes could interact with different residues on
   the surface of HSA. Theoretical results were supported by experimental
   evidence. (C) 2020 Published by Elsevier B.V.}},
