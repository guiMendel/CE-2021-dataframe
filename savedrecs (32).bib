@article{ WOS:000233077500004,
Author = {Ballini, R and Fiorini, D and Palmieri, A},
Title = {{1-Nitroicosane as the key building block for the first synthesis of
   triacontan-11-ol, a new fatty alcohol isolated from Argemone mexicana}},
Journal = {{SYNTHESIS-STUTTGART}},
Year = {{2005}},
Number = {{17}},
Pages = {{2835-2837}},
Month = {{NOV 3}},
Abstract = {{The first synthesis of triacontan-11-ol, a new fatty alcohol isolated
   from Argetnone mexicana, has been realized starting from the nitroaldol
   reaction of 1-nitroicosane with decanal. Dehydration of the obtained
   nitrcalkanol gives a long-chain nitroalkene which is then reduced to
