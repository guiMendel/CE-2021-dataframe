@article{ WOS:000285988900035,
Author = {McIntosh, William Alex and Kubena, Karen S. and Tolle, Glen and Dean,
   Wesley R. and Jan, Jie-sheng and Anding, Jenna},
Title = {{Mothers and meals. The effects of mothers' meal planning and shopping
   motivations on children's participation in family meals}},
Journal = {{APPETITE}},
Year = {{2010}},
Volume = {{55}},
Number = {{3}},
Pages = {{623-628}},
Month = {{DEC}},
Abstract = {{Participation in family meals has been associated with benefits for
   health and social development of children. The objective of the study
   was to identify the impact of mothers' work of caring through planning
   regularly scheduled meals, shopping and cooking, on children's
   participation in family meals. Parents of children aged 9-11 or 13-15
   years from 300 Houston families were surveyed about parents' work, meal
   planning for and scheduling of meals, motivations for food purchases,
   importance of family meals, and children's frequency of eating dinner
