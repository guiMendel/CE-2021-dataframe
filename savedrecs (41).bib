Title = {{Spin diffusion measurements in spin-polarised liquid He-3-He-4 mixtures}},
Journal = {{JOURNAL OF LOW TEMPERATURE PHYSICS}},
Year = {{2000}},
Volume = {{121}},
Number = {{5-6}},
Pages = {{785-790}},
