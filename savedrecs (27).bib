@article{ WOS:000284161100005,
Author = {Godzikovskaya, A. A.},
Title = {{Macroseismic descriptions and parameters of Kamchatka earthquakes that
   occurred during the preinstrumental period of observation}},
Journal = {{JOURNAL OF VOLCANOLOGY AND SEISMOLOGY}},
Year = {{2010}},
Volume = {{4}},
Number = {{5}},
Pages = {{354-366}},
Month = {{OCT}},
Abstract = {{This paper presents an analysis of the newly acquired macroseismic
   descriptions of several Kamchatka earthquakes that occurred in the
   18th-19th centuries. The available data for two large earthquakes that
   occurred in 1737 were analyzed to show that even the very numerous
   descriptions of the October 17, 1737 earthquake are inadequate to
   determine the basic parameters (epicentral coordinates, magnitude, and
   epicentral intensity of shaking). The only sources of information on
   seismic events in Kamchatka for the 18th-19th centuries were reports of
   explorers, but their presence in the region was episodic and irregular.
   We therefore believe that the M=5-8.4 earthquakes were much more
   numerous during this period than indicated in all the published
   catalogs.}},
