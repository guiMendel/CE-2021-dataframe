@article{ WOS:000231636200002,
Author = {Goldstick, D},
Title = {{On what there is}},
Journal = {{PACIFIC PHILOSOPHICAL QUARTERLY}},
Year = {{2005}},
Volume = {{86}},
Number = {{3}},
Pages = {{313-320}},
Month = {{SEP}},
Abstract = {{This paper programmatically outlines a case for ``nominalism{''}. If
   brown and colored are so related logically that being colored is nothing
   over and above being brown, then whatever ``exists{''} other than
   concreta is nothing over and above concreta. Possibilities of rain and
   ``universals{''} like the shape, circularity (= the logical possibility
   of being so shaped), lack ``existence{''} in another sense. ({''}There
   are things that don't exist{''} shows that `there are' is used even more
   broadly than `there exist'.) The univocity of,exist' is disproved by ``A
   prime number between 6 and 10 exists{''} being analytic despite the
   invalidity of ontological arguments.}},
