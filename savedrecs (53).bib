@article{ WOS:A1995RU25700032,
Author = {COLFORD, N and GIORGI, PL and GAIA, E and COTRONEI, V},
Title = {{HUMAN POSTURE IN MICROGRAVITY - AN EXPERIMENT ON EUROMIR-95 TO VERIFY
   AND IMPROVE A SIMULATION TOOL}},
Journal = {{ACTA ASTRONAUTICA}},
Year = {{1995}},
Volume = {{37}},
Number = {{S}},
Pages = {{379-383}},
