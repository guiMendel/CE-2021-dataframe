@article{ WOS:000088911800009,
Author = {Hooper, WP and James, JE},
Title = {{Lidar observations of ship spray plumes}},
Journal = {{JOURNAL OF THE ATMOSPHERIC SCIENCES}},
Year = {{2000}},
Volume = {{57}},
Number = {{16}},
Pages = {{2649-2655}},
Month = {{AUG 15}},
Abstract = {{As part of the Monterey Area Ship Track experiment, which was designed
   to study ship-generated cloud tracks, ship-based measurements were made
   by a gyroscopically stabilized scanning lidar system. This paper focuses
   on the spray plume observed by lidar behind the USS Truxton, a
