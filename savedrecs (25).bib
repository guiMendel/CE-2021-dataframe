@article{ WOS:000284154400019,
Author = {Smirnov, P. V. and Rusanov, A. A.},
Title = {{Stability of the crystal-melt system in the growing of large lithium
   niobate crystals by the low melt level Czochralski technique}},
Journal = {{TECHNICAL PHYSICS}},
Year = {{2010}},
Volume = {{55}},
Number = {{11}},
Pages = {{1669-1679}},
Month = {{NOV}},
Abstract = {{The Lyapunov stability of the crystal-melt system and the solidification
   rate is studied for the growing of lithium niobate crystals by the low
   melt level Czochralski technique. The relaxation time is calculated as a
   function of growth conditions. The curves of transient processes are
   plotted at various variable perturbations. It is shown that, for
   optically homogeneous crystals to be produced, the stability of the
   solidification rate rather than the crystal-melt system as a whole
   should be ensured. The stability of the solidification rate is found to
   be maximal at the maximum temperature gradient at the solidification
   interface and the minimum melt level in a crucible.}},
