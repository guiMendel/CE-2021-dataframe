@article{ WOS:000088360600005,
Author = {Zheng, J and Peneva, V and Brown, DJF},
Title = {{Longidorus camelliae n. sp (Nematoda : Longidoridae) associated with
   ornamental cultivars of Camellia japonica L. growing in a nursery at
   Fuyang, Zhejiang Province, eastern China}},
Journal = {{SYSTEMATIC PARASITOLOGY}},
Year = {{2000}},
Volume = {{47}},
Number = {{2}},
Pages = {{119-125}},
Month = {{OCT}},
Abstract = {{A description is provided of Longidorus camelliae n. sp., a bisexual
   species associated with ornamental cultivars of Camellia japonica L.
   growing in Fuyang, Zhejiang Province, eastern China. The species is
   characterised by its small body length (1.83-3.54 mm), narrow (6-10 mu
   m), anteriorly flattened, continuous head region, non-bilobed amphidial
