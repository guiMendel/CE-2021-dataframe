@article{ WOS:A1995RZ07900006,
Author = {SUN, W and STEPHEN, JT and POTTER, LD and WU, Y},
Title = {{ROTATION-INDUCED RESONANCE AND SECOND-ORDER QUADRUPOLAR EFFECTS ON SPIN
   LOCKING OF HALF-INTEGER QUADRUPOLAR NUCLEI}},
Journal = {{JOURNAL OF MAGNETIC RESONANCE SERIES A}},
Year = {{1995}},
Volume = {{116}},
Number = {{2}},
Pages = {{181-188}},
Month = {{OCT}},
Abstract = {{The dynamics of spin locking for half-integer quadrupolar nuclei under
   sample rotation is investigated. It is shown both theoretically and
   experimentally that the spin-locking efficiency decreases significantly
   when the MAS rotation frequency is equal to the amplitude of the
   spin-locking RF field. This can be explained as a rotation-induced
   resonance effect. It is demonstrated that the second-order quadrupole
   interaction plays an important role in reducing the spin-locking
   efficiency and causes a significant lineshape distortion. The behavior
   of spin locking under DOR is shown to be similar to that under MAS.
   Based on this understanding of the spin-locking dynamics, an Al-27 CPDOR
   spectrum for the molecular sieve AlPO4-11 was obtained with a good
   signal-to-noise ratio. (C) 1995 Academic Press, Inc.}},
