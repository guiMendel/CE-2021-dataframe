@article{ WOS:000367345400001,
Author = {Ruiz Jimenez, Antonia Maria and Tomas Gonzalez-Fernandez, Manuel and
   Jimenez Sanchez, Manuel},
Title = {{Identifying with the Nation: Spain's Left-Wing Citizens in an Age of
   Crisis}},
Journal = {{SOUTH EUROPEAN SOCIETY AND POLITICS}},
Year = {{2015}},
Volume = {{20}},
Number = {{4}},
Pages = {{487-508}},
Month = {{OCT 2}},
Abstract = {{This study analyses the extent, sense, and strategies employed by
   sympathisers with Spanish state-wide left-wing parties to reconstruct'
   their affective ties with the Spanish nation after the monopolisation of
   patriotism' by Franco's regime. Such an undertaking is further
   complicated within the context of economic crisis and intensified
   peripheral nationalism found in Catalonia and the Basque Country. This
   article applies qualitative analysis to the discourse of left-wing
   participants from 11 focus groups held in March 2012 amidst the economic
   (and political) crisis. As expected, this context of crisis favoured the
   emergence of explicit discourse on the Spanish nation, providing an
   opportunity to gain a better understanding of its nature. Findings show
   that welfare nationalism and social patriotism define the kind of
   attachment developed by the interviewees towards Spain better than the
