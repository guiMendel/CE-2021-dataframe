@article{ WOS:000353847900008,
Author = {Chassaing, Irene},
Title = {{NOSTALGIA AND UTOPIA IN THE WORK OF USE TREMBLAY: FROM LA PECHE BLANCHE
   TO LA HERONNIERE)}},
Journal = {{VOIX \& IMAGES}},
Year = {{2015}},
Volume = {{40}},
Number = {{2}},
Pages = {{107+}},
Month = {{WIN}},
Abstract = {{Nostalgia, a theme that runs through Quebec literature, is generally
   associated with a certain conservatism. In the contemporary context of
   Marc Auge's ``surmodernite{''} (excessive modernity), however, this
   feeling takes on a new form that is particularly apparent in the work of
   Lise Tremblay. A detailed study of the novels La peche blanche and La
   danse juive, as well as the short story collection La heronniere, shows
   that nostalgia is not necessarily ``a sadness without an object{''}
   (Susan Stewart), an individual's sterile impetus toward a past that is
   both idealized and inaccessible. In Lise Tremblay's world, nostalgia is
   rather a desire for a transformed present, an impetus toward an ideal
   that would be supported by the community as a whole: it opens the doors
   of utopia.}},
