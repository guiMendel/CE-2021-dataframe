@article{ WOS:000606141800057,
Author = {Syltern, Julie and Jorgensen, Geir and Norstein, Jarle and Augestad,
   Knut Magne},
Title = {{Fracture and embolization of an implantable venous access device in
   patient with atrial septal defect}},
Journal = {{JOURNAL OF SURGICAL CASE REPORTS}},
Year = {{2020}},
Number = {{12}},
Month = {{DEC}},
Abstract = {{Totally implantable venous access devices (TIVADs) are frequently used
   for the administration of chemotherapy or parenteral nutrition and have
   proved to be safe and improve patient quality of life during treatment.
   Catheter-related infections are the most common complication, whereas
   catheter fracture and embolization is rarely seen. We present a case of
   a 61-year-old cancer patient in which the TIVAD fractured and embolized
   to the patient's left knee 18 months after its initial placement. An
   endovascular procedure with intraoperative imaging revealed that the
   catheter had embolized to the popliteal artery and was successfully
   removed, the only explanation being the presence of an atrial septal
   defect. A bubble echocardiogram was unsuccessful in confirming the
   diagnosis. The catheter fracture could be related to an intravascular
   procedure that was performed 18 months prior to remove fibrin sheaths as
   a cause of port malfunction, or it could be a case of pinch-off
   syndrome.}},
