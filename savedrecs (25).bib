@article{ WOS:000283546400009,
Author = {Branch, D. Ware and Gibson, Mark and Silver, Robert M.},
Title = {{Recurrent Miscarriage}},
Journal = {{NEW ENGLAND JOURNAL OF MEDICINE}},
Year = {{2010}},
Volume = {{363}},
Number = {{18}},
Pages = {{1740-1747}},
Month = {{OCT 28}},
Abstract = {{A 32-year-old woman presents for evaluation after three consecutive
