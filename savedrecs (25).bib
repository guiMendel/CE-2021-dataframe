@article{ WOS:000285081900010,
Author = {Perry, Brandon C. and Wang, Shouye and Basson, Marc D.},
Title = {{Extracellular pressure stimulates adhesion of sarcoma cells via
   activation of focal adhesion kinase and Akt}},
Journal = {{AMERICAN JOURNAL OF SURGERY}},
Year = {{2010}},
Volume = {{200}},
