@article{ WOS:A1995TD08900008,
Author = {HELANDER, ML},
Title = {{RESPONSES OF PINE NEEDLE ENDOPHYTES TO AIR-POLLUTION}},
Journal = {{NEW PHYTOLOGIST}},
