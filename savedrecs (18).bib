@article{ WOS:000362690900015,
Author = {Sayyahfar, Shirin and Nassiri, Seyed Javad and Mehrazma, Mitra and
   Sadeghian, Mahnaz},
Title = {{Pancreatic neuroendocrine tumor in a child of 3.5 years old}},
Journal = {{WORLD JOURNAL OF PEDIATRICS}},
Year = {{2015}},
Volume = {{11}},
Number = {{4}},
Pages = {{386-388}},
Month = {{NOV}},
Abstract = {{Pancreatic neuroendocrine tumor is rarely seen, particularly in
   children. One of the symptoms of this tumor is jaundice that may be
   misdiagnosed as the sign of hepatitis A, especially in countries with a
   high prevalence of this infectious disease.
   We present a 3.5-year-old girl with four weeks of icterus, who was
   misdiagnosed with hepatitis A.
   The patient was finally diagnosed as having a low grade pancreatic
   neuroendocrine carcinoma.
   Pediatricians should be aware of or consider a tumor in differential
   diagnosis of jaundice, especially prolonged one in children.}},
