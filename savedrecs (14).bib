Journal = {{JOURNAL OF WUHAN UNIVERSITY OF TECHNOLOGY-MATERIALS SCIENCE EDITION}},
Year = {{2015}},
Volume = {{30}},
Number = {{6}},
Pages = {{1225-1228}},
Month = {{DEC}},
Abstract = {{We investigated the effect of structural factor and amide grafted
   multi-walled carbon nanotubes (MWNTs-NH2) on crushing characteristics of
   filament wound CFRP tube under quasi-static compression conditon. It was
   found that CFRP tubes sequentially showed the brittle fracturing mode,
