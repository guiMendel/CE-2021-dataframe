@article{ WOS:000234205400009,
Author = {Venkitasubramaniam, P and Adireddy, S and Tong, L},
Title = {{Sensitivity and coding of opportunistic ALOHA in sensor networks with
   mobile access}},
Journal = {{JOURNAL OF VLSI SIGNAL PROCESSING SYSTEMS FOR SIGNAL IMAGE AND VIDEO
   TECHNOLOGY}},
Year = {{2005}},
Volume = {{41}},
Number = {{3}},
Pages = {{329-344}},
