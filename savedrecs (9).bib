@article{ WOS:000617653800001,
Author = {Ershov, V. A. and Tarasova, N. P. and Ershov, B. G.},
Title = {{Electronic State of Silver Nanoparticles during Their Photochemical
   Formation in a Deaerated Aqueous Solution}},
Journal = {{DOKLADY CHEMISTRY}},
Year = {{2020}},
Volume = {{495}},
Number = {{1}},
Pages = {{171-174}},
Month = {{NOV}},
Abstract = {{The effect of an increase in the electron density in silver
   nanoparticles during their formation through photochemical reduction of
   Ag+ ions in the presence of oxalate has been discovered and
   substantiated. The effect causes a shift of the of localized surface
   plasmon resonance (SPR) band from 402 to 383 nm and a decrease in its
   width. The quantitative relationship between the band shift value and
   electron density has been revealed. The observed shift of 19 nm
   corresponds to an approximately 10\% increase in the electron
   concentration in nanoparticles. The observed is possibly common for
   other metals and affects the performance of various catalytic reactions
   involving metals in the nanoscale state.}},
