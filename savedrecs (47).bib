@article{ WOS:000089030700009,
Author = {Aronovich, EL and Carmichael, KP and Morizono, H and Koutlas, IG and
   Deanching, M and Hoganson, G and Fischer, A and Whitley, CB},
Title = {{Canine heparan sulfate sulfamidase and the molecular pathology
