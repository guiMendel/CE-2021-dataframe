@article{ WOS:A1995TQ90800002,
Author = {Makarova, OL},
Title = {{Mesostigmatic mites (Parasitiformes, Mesostigmata) on the forest dung
   beetle Geotrupes stercorosus}},
Journal = {{ZOOLOGICHESKY ZHURNAL}},
Year = {{1995}},
Volume = {{74}},
Number = {{12}},
Pages = {{16-23}},
Month = {{DEC}},
Abstract = {{The community structure of phoretic miles from the dung beetle Geotrupes
   stercorosus scriba was studied basing on the data from four localities
   in eastern Europa. 18 species were recorded including 12 mesostigmatic
   ones. 8 species were noted on these beetles for the first time. Three
   species groups were distinguished as follows: 1) inhabiting dung and
   compost, 2) entomophilous, occasional on Geotrupes and 3) associated
   with the beetles of the genus Geotrupes. The absence of eucoprobiotic
   species was the characteristic of the first group due to some
   peculiarities of the biology of G. stercorosus (dwelling in forests, the
   possibility of larval development on plant diet). For the mite
   Neopodocinum meridionalis (Sellnick, 1931), G. stercorosus was proved to
   be the secondary host while G. vernalis - the primary one.}},
