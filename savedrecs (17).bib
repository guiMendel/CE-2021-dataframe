@article{ WOS:000366524100003,
Author = {Kanpanon, Nicha and Kasemsap, Poonpipope and Thaler, Philippe and
   Kositsup, Boonthida and Gay, Frederic and Lacote, Regis and Epron,
   Daniel},
Title = {{Carbon isotope composition of latex does not reflect temporal variations
   of photosynthetic carbon isotope discrimination in rubber trees (Hevea
   brasiliensis)}},
Journal = {{TREE PHYSIOLOGY}},
Year = {{2015}},
Volume = {{35}},
Number = {{11}},
Pages = {{1166-1175}},
Month = {{NOV}},
Abstract = {{Latex, the cytoplasm of laticiferous cells localized in the inner bark
   of rubber trees (Hevea brasiliensis Mull. Arg.), is collected by tapping
   the bark. Following tapping, latex flows out of the trunk and is
   regenerated, whereas in untapped trees, there is no natural exudation.
   It is still unknown whether the carbohydrates used for latex
   regeneration in tapped trees is coming from recent photosynthates or
   from stored carbohydrates, and in the former case, it is expected that
   latex carbon isotope composition of tapped trees will vary seasonally,
   whereas latex isotope composition of untapped trees will be more stable.
   Temporal variations of carbon isotope composition of trunk latex (delta
   C-13-L), leaf soluble compounds (delta C-13-S) and bulk leaf material
   (delta C-13-B) collected from tapped and untapped 20-year-old trees were
   compared. A marked difference in delta C-13-L was observed between
   tapped and untapped trees whatever the season. Trunk latex from tapped
   trees was more depleted (1.6\% on average) with more variable delta C-13
   values than those of untapped trees. delta C-13-L was higher and more
   stable across seasons than delta C-13-S and delta C-13-B, with a maximum
   seasonal difference of 0.7\% for tapped trees and 0.3\% for untapped
   trees. delta C-13-B was lower in tapped than in untapped trees,
   increasing from August (middle of the rainy season) to April (end of the
   dry season). Differences in delta C-13-L and delta C-13-B between tapped
   and untapped trees indicated that tapping affects the metabolism of both
   laticiferous cells and leaves. The lack of correlation between delta
   C-13-L and delta C-13-S suggests that recent photosynthates are mixed in
   the large pool of stored carbohydrates that are involved in latex
   regeneration after tapping.}},
