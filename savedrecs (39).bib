@article{ WOS:000231503700016,
Author = {Malis, O and Pfeiffer, LN and West, KW and Sergent, AM and Gmachl, C},
Title = {{Bound-to-bound midinfrared intersubband absorption in carbon-doped
   GaAs/AlGaAs quantum wells}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2005}},
Volume = {{87}},
Number = {{9}},
Month = {{AUG 29}},
Abstract = {{Bound-to-bound intersubband absorption in the valence band of
   modulation-doped GaAs quantum wells with digitally alloyed AlGaAs
   barriers was studied in the midinfrared wavelength range. A high-purity
   solid carbon source was used for the p-type doping. Strong narrow
