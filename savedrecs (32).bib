@article{ WOS:000232970600015,
Author = {Uslu, B and Dogan, B and Ozkan, SA and Aboul-Enein, HY},
Title = {{Electrochemical behavior of vardenafil on glassy carbon electrode:
   Determination in tablets and human serum}},
Journal = {{ANALYTICA CHIMICA ACTA}},
Year = {{2005}},
Volume = {{552}},
Number = {{1-2}},
Pages = {{127-134}},
Month = {{NOV 3}},
Abstract = {{The voltammetric behavior of vardenafil was studied on glassy carbon
   electrode using cyclic, differential pulse (DPV) and Osteryoung
   square-wave (OSW) voltammetric techniques. Vardenafil exhibited
   irreversible anodic waves over the pH range 1.00-12.00 in different
   supporting electrolytes. The current-concentration plot was rectilinear
   over the range from 4 x 10(-7) to 2 x 10(-5) M with correlation
   coefficient of 0.999. The wave was characterized as being irreversible
   and diffusion-controlled. The developed procedure was successfully
   applied to the determination of vardenafil in tablets and in spiked
   human serum. Furthermore, results obtained by the proposed method have
   been compared with high-performance liquid chromatographic method. (c)
   2005 Elsevier B.V. All rights reserved.}},
