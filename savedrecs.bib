@article{ WOS:000600670000035,
Author = {Attur, Mukundan and Lu, Cuijie and Zhang, Xiaodong and Han, Tianzhen and
   Alexandre, Cassidy and Valacca, Cristina and Zheng, Shuai and Meikle,
   Sarina and Dabovic, Branka Brukner and Tassone, Evelyne and Yang, Qing
   and Kolupaeva, Victoria and Yakar, Shoshana and Abramson, Steven and
   Mignatti, Paolo},
Title = {{Membrane-type 1 Matrix Metalloproteinase Modulates Tissue Homeostasis by
   a Non-proteolytic Mechanism}},
Journal = {{ISCIENCE}},
