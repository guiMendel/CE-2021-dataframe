@article{ WOS:000600425600003,
Author = {Lewis, Jasper R. and Campbell, James R. and Stewart, Sebastian A. and
   Tan, Ivy and Welton, Ellsworth J. and Lolli, Simone},
Title = {{Determining cloud thermodynamic phase from the polarized Micro Pulse
   Lidar}},
Journal = {{ATMOSPHERIC MEASUREMENT TECHNIQUES}},
Year = {{2020}},
Volume = {{13}},
Number = {{12}},
Pages = {{6901-6913}},
Month = {{DEC 18}},
Abstract = {{A method to distinguish cloud thermodynamic phase from polarized Micro
   Pulse Lidar (MPL) measurements is described. The method employs a simple
   enumerative approach to classify cloud layers as either liquid water,
   ice water, or mixed-phase clouds based on the linear volume
   depolarization ratio and cloud top temperatures derived from Goddard
   Earth Observing System, version 5 (GEOS5), assimilated data. Two years
   of cloud retrievals from the Micro Pulse Lidar Network (MPLNET) site in
   Greenbelt, MD, are used to evaluate the performance of the algorithm.
   The fraction of supercooled liquid water in the mixed-phase temperature
   regime (-37-0 degrees C) calculated using MPLNET data is compared to
   similar calculations made using the spaceborne Cloud-Aerosol Lidar with
   Orthogonal Polarization (CALIOP) instrument onboard the Cloud-Aerosol
   Lidar and Infrared Pathfinder Satellite Observations (CALIPSO)
   satellite, with reasonable consistency.}},
