@article{ WOS:A1995RW47400009,
Author = {BYINGTON, RP and FURBERG, CD and CROUSE, JR and ESPELAND, MA and BOND,
   MG},
Title = {{PRAVASTATIN, LIPIDS, AND ATHEROSCLEROSIS IN THE CAROTID ARTERIES
   (PLAC-II)}},
