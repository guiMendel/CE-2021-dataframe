@article{ WOS:000233930600015,
Author = {Xu, LC and Li, ZY and Tan, W and He, TJ and Liu, FC and Chen, DM},
Title = {{Density functional theory studies on the Raman and IR spectra of
   meso-tetraphenylporphyrin diacid}},
