@article{ WOS:000233174300023,
Author = {Vaccher, E and di Gennaro, G and Simonelli, C and Schioppa, O and
   Tirelli, U and GICAT},
Title = {{Evidence of activity of irinotecan in patients with advanced
   AIDS-related Kaposi's sarcoma}},
Journal = {{AIDS}},
Year = {{2005}},
Volume = {{19}},
Number = {{16}},
Pages = {{1915-1916}},
Month = {{NOV 4}},
Abstract = {{Fourteen HIV-infected patients with advanced Kaposi's sarcoma (KS)
   received Irinotecan 150 mg/m(2) intravenously on days 1 and 10. All
   patients were relapsed/progressed during highly active antiretroviral
   therapy, administered as primary antineoplastic therapy. An objective
   response, all partial remissions, occurred in 75\% of patients.
   Irinotecan was well. tolerated, severe leukopenia occurred in only 33\%
   of patients. In HIV-infected patients with advanced KS, irinotecan is
   active and well tolerated.}},
