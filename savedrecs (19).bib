@article{ WOS:000366365900007,
Author = {Amiri-Jami, Mitra and Abdelhamid, Ahmed Ghamry and Hazaa, Mahmoud and
   Kakuda, Yukio and Griffths, Mansel W.},
Title = {{Recombinant production of omega-3 fatty acids by probiotic Escherichia
   coli Nissle 1917}},
Journal = {{FEMS MICROBIOLOGY LETTERS}},
Year = {{2015}},
Volume = {{362}},
Number = {{20}},
Month = {{OCT}},
Abstract = {{Omega-3 fatty acids, including eicosapentaenoic acid (EPA) and
   docosahexaenoic acid (DHA), have beneficial effects on human health. The
   probiotic bacterium Escherichia coli Nissle is unable to produce either
   EPA or DHA. Escherichia coil Nissle was transformed with the pfBS-PS
   plasmid carrying the EPA/DHA gene cluster, previously isolated from a
   marine bacterium. The transgenic E. coil Nissle produced EPA when grown
   at 10 degrees C (16.52 +/- 1.4 mg g(-1) cell dry weight), 15 degrees C
   (31.36 +/- 0.25 mg g(-1) cell dry weight), 20 degrees C (13.71 2.8 mg g'
   cell dry weight), 25 degrees C (11.33 +/- 0.44 mg g(-1) cell dry weight)
   or 30 degrees C (0.668 +/- 0.073 mg g(-1) cell dry weight). Although DHA
   was also produced at all these temperatures, it comprised less than
   0.2\% of total extracted fatty acids. Transcriptomic analysis using
   Reverse Transcription qPCR showed upregulation of the entire gene
   cluster in E. coil Nissle. Among EPA/DHA genes, pfaB, pfaC and pfaD were
   overexpressed (expression ratio of 181.9, 39.86 and 131.61,
   respectively) as compared to pfaA (expression ratio of 3.40) and pfaE
   (expression ratio of 4.05). The EPA/DHA-producing probiotic E. coil
   Nissle may be used as a safe, alternative and economic source for the
   industrial and pharmaceutical production of EPA and DHA.}},
