@article{ WOS:000365813400002,
Author = {Omachi, Yoshie and Ito, Kimiteru and Arima, Kunimasa and Matsuda,
   Hiroshi and Nakata, Yasuhiro and Sakata, Masuhiro and Sato, Noriko and
   Nakagome, Kazuyuki and Motohashi, Nobutaka},
Title = {{Clinical impact of C-11-Pittsburgh compound-B positron emission
   tomography carried out in addition to magnetic resonance imaging and
   single-photon emission computed tomography on the diagnosis of
   Alzheimer's disease in patients with dementia and mild cognitive
   impairment}},
Journal = {{PSYCHIATRY AND CLINICAL NEUROSCIENCES}},
Year = {{2015}},
Volume = {{69}},
Number = {{12}},
Pages = {{741-751}},
Month = {{DEC}},
Abstract = {{AimsThe purpose of this study was to evaluate the clinical impact of
   addition of {[}C-11]Pittsburgh compound-B positron emission tomography
   (C-11-PiB PET) on routine clinical diagnosis of Alzheimer's disease (AD)
   dementia and mild cognitive impairment (MCI), and to assess diagnostic
   agreement between clinical criteria and research criteria of the
   National Institute on Aging-Alzheimer's Association.
   MethodsThe diagnosis in 85 patients was made according to clinical
   criteria. Imaging examinations, including both magnetic resonance
   imaging and single-photon emission computed tomography/computed
   tomography to identify neuronal injury (NI), and C-11-PiB PET to
   identify amyloid were performed, and all subjects were re-categorized
   according to the research criteria.
   ResultsAmong 40 patients with probable AD dementia (ProAD), 37 were
   NI-positive, 29 were C-11-PiB-positive, and 27 who were both NI- and
   C-11-PiB-positive were categorized as having ProAD dementia with a high
   level of evidence of the AD pathophysiological process'. Among 20
   patients with possible AD dementia (PosAD), 17 were NI-positive, and six
   who were both NI- and C-11-PiB-positive were categorized as having PosAD
   with evidence of the AD pathophysiological process'. Among 25 patients
   with MCI, 18 were NI-positive, 13 were C-11-PiB-positive, and 10 who
   were both NI- and C-11-PiB-positive were categorized as having MCI due
   to AD-high likelihood'.
   ConclusionsDiagnostic concordance between clinical criteria and research
   criteria may not be high in this study. C-11-PiB PET may be of value in
   making the diagnosis of dementia and MCI in cases with high diagnostic
   uncertainty.}},
