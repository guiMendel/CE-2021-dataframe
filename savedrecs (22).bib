@article{ WOS:000287141500009,
Author = {Menolli Junior, Nelson and Paccola-Meirelles, Luzia Doretto},
Title = {{Obtention of Lentinula edodes strains resistant to high temperatures and
   selection of strains resistant to Trichoderma sp.}},
Journal = {{CIENCIA E AGROTECNOLOGIA}},
Year = {{2010}},
Volume = {{34}},
Number = {{SI}},
Pages = {{1640-1646}},
Month = {{DEC}},
Abstract = {{Lentinula edodes (Berk.) Pegler (shiitake) is the second most consumed
   mushroom in the world and its production can be made in natural logs or
   in axenic culture using agroindustrial residues as substrate. The
   cultivation of shiitake in Brazil has been seriously harmed by the high
   incidence of contaminant fungi such as Trichoderma sp. and by the
   absence of strains adapted to tropical temperatures. Two strains
   resistant to Trichoderma sp. were obtained by selection when grown in
   contact with the contaminant fungus and, by mycelial fragmentation,
   strains were obtained that can resist high temperatures and grow at 30
   degrees C in a culture medium.}},
