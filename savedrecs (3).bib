@article{ WOS:000596317000007,
Author = {Han, Xiao and Guo, Xin and Shi, Yongli and Li, Wenhao and Zhang, Guoquan},
Title = {{Novel amino-beta-Cyclodextrins containing polymers: Fabrication,
   characterization, and biological evaluation}},
Journal = {{COLLOIDS AND SURFACES B-BIOINTERFACES}},
Year = {{2020}},
Volume = {{196}},
Month = {{DEC}},
Abstract = {{In this study, poly (mPEGMA-co-MAA) (PA) based on monomers of mPEGMA and
   MAA were synthesized, and different amino-beta-Cyclodextrins with
   various alkyl chains were conjugated to PA through carbodiimide-mediated
   coupling reactions. The obtained poly (mPEGMA-co-MAA-g-amino-beta-CD)
   (PA-g-amino-beta-CD) was characterized by FTIR, NMR and TGA. The
   fluorescence technique was used to investigate the micellization of
   PA-g-amino-beta-CDs. The results indicated that these polymers could
   self-assemble into nano-micelles in water, and PA-g-HDA-beta-CD
   possessed the lowest CMC value due to its long alkyl chains. In
   addition, the PA-g-HDA-beta-CD micelles were in spherical shapes with
   the diameter of 78.5 +/- 0.6 nm. The release of the model drug from
   PA-g-HDA-beta-CD micelles was increased as the pH reduced from 7.4-5.5
   at 37 degrees C. Cytotoxicity and cellular uptake experiments were
   performed in HepG2, which showed that the cargo-free PA-g-HDA-beta-CD
   micelles did not have obvious cytotoxicity and were mainly distributed
   in the cytoplasm of HepG2 cells by endocytosis. Moreover, the study
   about in vivo distribution of the experimental rats indicated that the
   accumulation of PA-g-HDA-beta-CD micelles mainly happened in the liver.
   Therefore, the novel amino-beta-CD containing polymers exhibit good
   potential applications in drug delivery system.}},
