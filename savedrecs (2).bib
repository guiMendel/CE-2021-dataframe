@article{ WOS:000599922100011,
Author = {Al-Harere, Manal N. and Omran, Ahmed A. and Breesam, Athraa T.},
Title = {{Captive domination in graphs}},
Journal = {{DISCRETE MATHEMATICS ALGORITHMS AND APPLICATIONS}},
Year = {{2020}},
Volume = {{12}},
Number = {{6}},
Month = {{DEC}},
Abstract = {{In this paper, a new definition of graph domination called ``Captive
   Domination{''} is introduced. The proper subset of the vertices of a
   graph G is a captive dominating set if it is a total dominating set and
   each vertex in this set dominates at least one vertex which doesn't
   belong to the dominating set. The inverse captive domination is also
   introduced. The lower and upper bounds for the number of edges of the
   graph are presented by using the captive domination number. Moreover,
   the lower and upper bounds for the captive domination number are found
   by using the number of vertices. The condition when the total domination
   and captive domination number are equal to two is discussed and obtained
   results. The captive domination in complement graphs is discussed.
   Finally, the captive dominating set of paths and cycles are determined.}},
