@article{ WOS:000282807000011,
Author = {Liu, Yong-Lu and Huang, Ming-Qiu},
Title = {{A light-cone QCD sum rule approach for the Xi baryon electromagnetic
   form factors and the semileptonic decay Xi(c) -> Xi e(+)nu(e)}},
Journal = {{JOURNAL OF PHYSICS G-NUCLEAR AND PARTICLE PHYSICS}},
Year = {{2010}},
Volume = {{37}},
Number = {{11}},
Month = {{NOV}},
Abstract = {{The electromagnetic form factors of the Xi baryons and the semileptonic
   decay process Xi(c) -> Xi e(+)nu(e) are investigated in the frame work
   of the light-cone QCD sum rule method with Ioffe-type interpolating
   currents. Our estimates on the magnetic moments are mu(Xi)0 = -(1.75 +/-
   0.21)mu(N) and mu(Xi)- = -(1.01 +/- 0.11)mu(N). The decay width of the
   semileptonic process is expected to be Gamma(Xi(c) -> Xi e(+)nu(e)) =
   (6.17(-2.48)(+2.24)) x 10(-14) GeV. The results show that the adoption
   of this type of interpolating current improves the calculations of the
   magnetic form factors and gives more reliable prediction for the
   analysis of the semileptonic decay process.}},
