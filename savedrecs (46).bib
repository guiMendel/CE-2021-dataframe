@article{ WOS:000166102000029,
Author = {Baptista-Giacomelli, FR and Pagliarini, MS and de Almeida, JL},
Title = {{Elimination of micronuclei from microspores in a Brazilian oat (Avena
   sativa L.) variety}},
Journal = {{GENETICS AND MOLECULAR BIOLOGY}},
Year = {{2000}},
Volume = {{23}},
Number = {{3}},
Pages = {{681-684}},
Month = {{SEP}},
Abstract = {{Chromosome elimination in interspecific hybrids is a powerful tool in
   breeding programs. Of 12 Brazilian oat varieties recommended for
   cultivation in southern Brazil, one variety, UFRGS 15, presented a new
   type of chromosome elimination never described in any other species.
   Chromosomes that produced micronuclei during meiosis were eliminated
   from microspores as microcytes. The micronucleus reached the microspore
   wall and formed a kind of bud, separated from the microspore. The
   eliminated microcytes gave origin to small and sterile pollen grains.
   The possible applications of this finding for oat breeding are
   discussed.}},
