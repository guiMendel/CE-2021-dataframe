@article{ WOS:000618698100010,
Author = {Yang, Fangyuan and Feng, Lei and Zhen, Xiaofei},
Title = {{Study on the Biogas Production Kinetic Models for Anaerobic Digestion of
   Cattle Manure at Medium and Low Temperatures}},
Journal = {{JOURNAL OF BIOBASED MATERIALS AND BIOENERGY}},
Year = {{2020}},
Volume = {{14}},
Number = {{6}},
Pages = {{747-752}},
Month = {{DEC}},
Abstract = {{Fresh cow manure was chosen as a material to carry out anaerobic
   digestion experiments at different temperatures (15, 20, 25, and 37
   degrees C). First-order hydrolysis and anaerobic digestion kinetic
   models were separately fitted to the experimental data. The results
   showed that the linear fitting R2 values for the first-order hydrolysis
   model were greater than 0.90, indicating that the first-order hydrolysis
   model agreed well with the cow manure anaerobic digestion processes at
   different temperatures. The low-temperature hydrolysis rate constants k
   were smaller than zero, and smaller k values appeared at lower
   temperatures, indicating that the activity of hydrolytic microorganisms
   was inhibited at low temperatures, and the hydrolysis rates decreased.
   The (lnym+ lnk) values at 15, 20, and 25 degrees C were 0.7062, 1.0922,
   and 1.5705, respectively, indicating that the elevation of temperature
   gradually improved methane production performance. At medium
   temperatures, this value reached 2.5341 and was ca. 68.10\% higher than
   that at 25 degrees C. The Gompertz and logistic equations were adopted
   for the nonlinear fitting of the relationship between cumulative methane
   output and time at different temperatures, and all the R-2 values
   exceeded 0.99. Except the group at 15 degrees C agreeing with the
   logistic equation, the other three groups followed the Gompertz
   equation.}},
