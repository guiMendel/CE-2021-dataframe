@article{ WOS:000284304600010,
Author = {Nascetti, A. and Valerio, P. and Caputo, D. and de Cesare, G.},
Title = {{Detection system based on a novel large area hybrid detector}},
Journal = {{MICROELECTRONICS JOURNAL}},
Year = {{2010}},
Volume = {{41}},
Number = {{11, SI}},
Pages = {{752-757}},
Month = {{NOV}},
Note = {{3rd International Workshop on Advances in Sensors and Interfaces, Trani,
   ITALY, JUN 25-26, 2009}},
Organization = {{IEEE}},
Abstract = {{A system level implementation of a large area hybrid detector is
   presented. The detector used in this system consists of an array of
   hydrogenated amorphous silicon photodiodes directly connected to a CMOS
   readout chip, which is vertically integrated over the sensor array using
   flip-chip bonding. In particular, the proposed solution relies on a
   stack of interconnection layers, deposited on top of the photodiode
   array, to route each individual pixel output to a separate pre-amplifier
   channel. This avoids the need for a geometrical matching between the
   sensor array and the chip contact pads. As a consequence, conventional
