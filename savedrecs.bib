@article{ WOS:000603896800001,
Author = {Das, Tarun Kumar and Chatterjee, Sayan},
Title = {{Compact hairpin line bandpass filter with improved spurious passbands
   suppression}},
Journal = {{INTERNATIONAL JOURNAL OF ELECTRONICS}},
Year = {{2021}},
Volume = {{108}},
Number = {{8}},
Pages = {{1309-1325}},
Month = {{AUG 3}},
Abstract = {{The present paper describes the design of a folded hairpin-line bandpass
   filter centred at 2.5 GHz applicable for Wireless Local Area Network
   (WLAN) with improved spurious suppression. The size of the conventional
   filter has been reduced by 30\% by folding the open-end arms towards the
   inward direction. Thus, improvements over the skirt characteristics and
   shifting of the second harmonic to higher frequency region have been
   obtained. However, the attenuation level at the harmonic frequency
   remains almost unaffected. Subsequently, the inner and outer edges of
   the coupled-lines have been modified with Minkowski fractals to
   compensate for the modal phase velocities. As a result, more than 36 dB
   of harmonic suppression up to three times the centre frequency along
   with size reduction of 30\% has been achieved.}},
