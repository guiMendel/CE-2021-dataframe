@article{ WOS:000373311100016,
Author = {de Araujo, Laudiceia Loureno},
Title = {{GEOGRAPHY AND GENDER ISSUES AT WORK CONTEXT: INTEGRATION OF CONTEMPORARY
   FORMS OF WOMEN IN FORMAL LABOUR MARKET}},
