@article{ WOS:000596928500004,
Author = {Guan, Xian and Ge, Liang-Quan and Zeng, Guo-Qiang and Deng, Xiao-Qin and
   Xu, Li-Peng and Guo, Sheng-Liang},
Title = {{Determination of gross alpha and beta activities in Zouma River based on
   online HPGe gamma measurement system}},
Journal = {{NUCLEAR SCIENCE AND TECHNIQUES}},
Year = {{2020}},
Volume = {{31}},
Number = {{12}},
Month = {{DEC 4}},
Abstract = {{This paper describes a low-cost and fast method to evaluate gross alpha
   and beta (-) radioactivities in natural water based on an online
   high-purity germanium detector gamma measurement system. The major gamma
   activities in natural water are provided by natural and artificial
   radionuclides such as (40) K, Cs-137, and radionuclides belonging to
   U-238 and Th-232 series. The main alpha emitters related to gamma
   emissions in natural water are Ra-224 (240.98 keV) and Ra-226 (186.21
   keV), and the beta (-) emitters are (40) K (1460.85 keV), Bi-214 (609.31
   keV), Tl-208 (583.19 keV), and Pb-214 (351.93 keV). The formula for
   gross alpha and beta (-) activity concentration is based on these
   radionuclides, and the short half-life decay products are considered in
   the calculation. The detection efficiency of the device across energy
   region (0-3 MeV) is obtained through Monte Carlo simulation, and a
   calibration experiment is conducted to verify the simulation results.
   Gamma radioactivity is measured continuously for 114 d in Pixian County
   and Dongfeng Canal located in the Zouma River, Chengdu, Sichuan
   Province, China. A comparison of the calculation results and monitoring
   data from the Sichuan Management and Monitoring Center Station of
   Radioactive Environment indicates that the percentage and absolute error
   of alpha activity concentration is lower than 53\% and 0.02 Bq/L,
   respectively, and that of beta -activity concentration is lower than
   33.2\% and 0.016 Bq/L, respectively. The method can rapidly determine
   gross alpha and beta (-) activity concentrations in natural water
   online.}},
