@article{ WOS:000361923100033,
Author = {Hoang Nguyen},
Title = {{A new knowledge-based measure for intuitionistic fuzzy sets and its
   application in multiple attribute group decision making}},
Journal = {{EXPERT SYSTEMS WITH APPLICATIONS}},
