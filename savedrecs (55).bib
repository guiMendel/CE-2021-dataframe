@article{ WOS:A1995TB45100008,
Author = {HAN, EK and COTTY, F and SOTTAS, C and JIANG, H and MICHELS, CA},
Title = {{CHARACTERIZATION OF AGT1 ENCODING A GENERAL ALPHA-GLUCOSIDE TRANSPORTER
   FROM SACCHAROMYCES}},
Journal = {{MOLECULAR MICROBIOLOGY}},
Year = {{1995}},
