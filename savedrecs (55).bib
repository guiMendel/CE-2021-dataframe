@article{ WOS:A1995RV81800036,
Author = {KRAKOVSKY, A and PERCUS, JK},
Title = {{HYDRODYNAMIC MODEL OF AN INHOMOGENEOUS ELECTRON-GAS}},
Journal = {{PHYSICAL REVIEW B}},
Year = {{1995}},
Volume = {{52}},
Number = {{11}},
Pages = {{7901-7906}},
Month = {{SEP 15}},
Abstract = {{We present a simple hydrodynamic model for the ground state of an
   inhomogeneous electron gas. The profile relation and response are
   obtained explicitly. In the case of a uniform reference system the
   profile relation reduces to that of the Sjolander-Stott theory. We
   confirm their results using the most recent numerical data. We compare
   numerical results for a heavy impurity problem in two dimensions with
   the corresponding results in three dimensions. A density modulation is
   conjectured to develop in the region just around a negative impurity at
   sufficiently low electron densities, as a signature of the Wigner
   transition.}},
