@article{ WOS:000089159100004,
Author = {Neitzke, M and Therburg, A},
Title = {{Changes in the concentration of ultraviolet-B radiation absorbing
   compounds in leaves of beech (Fagus sylvatica L.) along an elevational
   gradient}},
Journal = {{FLORA}},
Year = {{2000}},
Volume = {{195}},
Number = {{3}},
Pages = {{236-244}},
Month = {{AUG}},
Abstract = {{Sun and shade leaves of beech were collected along an elevational
   gradient in the Hunsruck (Rhineland-Palatinate). Leaf area, specific
   leaf weight and the flavonoid contents (measured as absorbance of
   methanolic extracts) were determined. Sun leaves exhibited higher
   absorbances at 280 and 320 nm per unit leaf area and per gram dry mass
   than shade leaves. UV-B absorbance of the sun leaves increased with
   increasing elevation. A positive correlation was found between the
   absorbance of sun leaves at 280 nm and 320 nm and specific leaf weight.
   A change in mineral nutrition (measured as mineral contents of the
   leaves) along the elevational gradient was not determined. It is
   supposed that the increase in absorbance of ultraviolet-B-radiation is
   caused by an increase in ultraviolet-B radiation in the Hunsruck.}},
