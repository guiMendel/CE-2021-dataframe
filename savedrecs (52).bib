@article{ WOS:A1995TG85500005,
Author = {MEYER, G and BOTTA, G and FEDELE, G and CREMASCHI, D},
Title = {{REGULATION OF L-VALINE ABSORPTION BY OPIOIDS INTERACTING WITH
   MU-RECEPTORS IN RABBIT ILEUM}},
Journal = {{EXPERIENTIA}},
Year = {{1995}},
Volume = {{51}},
Number = {{11}},
Pages = {{1045-1051}},
Month = {{NOV}},
Abstract = {{In intact tissue, {[}D-Ala(2),MePhe(4),Gly-ol(5)] enkephalin (10(-5) M;
   mu-ligand), diminished short-circuit current (I-sc) and increased water,
   Na+ and Cl- net fluxes in vitro under open circuit conditions; it also
   inhibited L-valine absorption and L-valine-dependent variations of
   short-circuit current (Delta I-sc,I-val). Naloxone (10(-6) M)
   antagonized these effects. In the absence of the muscularis and
   myenteric plexus this enkephalin or morphine (mu-ligand) reduced I-sc
   and Delta I-sc,I-val. These enkephalin effects occurred at different
   times. Different concentrations of enkephalin were tested for their
   effects on Delta I-sc,I-val. {[}D-Ala(2),D-Leu(5)] enkephalin (mainly a
   delta-ligand) significantly decreased I-sc but not Delta I-sc,I-val. The
   reduction of L-valine absorption does not depend on the effects on basal
   ion transport. Interaction of opioids with mu-receptors located in the
   submucosal plexus and/or in the epithelial cell accounts for this
   reduction. This enkephalin effect seems to be at least partially under
   the control of the myenteric plexus.}},
