@article{ WOS:A1995RT32700007,
Author = {ROSSET, N and ANDREOLI, A},
Title = {{CRISIS-INTERVENTION AND AFFECTIVE-DISORDERS - A COMPARATIVE
   COST-EFFECTIVENESS STUDY}},
