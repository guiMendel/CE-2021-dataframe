@article{ WOS:000232419200010,
Author = {Gulbrandsen, T and Engelstad, F},
Title = {{Elite consensus on the Norwegian welfare state model}},
Journal = {{WEST EUROPEAN POLITICS}},
Year = {{2005}},
Volume = {{28}},
Number = {{4}},
Pages = {{898-918}},
Month = {{SEP}},
Abstract = {{Is the Norwegian welfare state model under pressure? In this article we
   argue that this model will persist to the extent that there is a
   relatively strong consensus among the majority of the national elites to
   support it. The empirical results presented indicate that the majority
   of the Norwegian elites at the time of the study (2000) supported the
   basic institutions and policies in the model and the political
   compromises upon which they are based. They express strong support for
   the system of centralised wage settlements, a main element in the
   industrial relations system. They rally behind continued economic
   transfers to the rural areas. A majority (albeit small) of the elite
   groups prefers to uphold the present state model of production and
   distribution of welfare services. There are, however, differences
   between the various elite groups as to how they view the welfare state
   model. While the members of the private business elite express
   preference for basic changes, the mass media elite, the academic elite
   and the church leaders are clearly in favour of the present model. The
   opinions of the individual leaders are particularly related to the elite
   groups to which they belong. Social and educational background has much
   less effect upon their attitudes.}},
