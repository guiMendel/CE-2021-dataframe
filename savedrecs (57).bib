@article{ WOS:A1995RR13900013,
Author = {PHAM, CK and IKEGAMI, M and TANAKA, M},
Title = {{DISCRETE-TIME CELLULAR NEURAL NETWORKS WITH 2 TYPES OF NEURON CIRCUITS
   FOR IMAGE-CODING AND THEIR VLSI IMPLEMENTATIONS}},
Journal = {{IEICE TRANSACTIONS ON FUNDAMENTALS OF ELECTRONICS COMMUNICATIONS AND
   COMPUTER SCIENCES}},
