@article{ WOS:000234059500011,
Author = {Savost'yanov, GA and Grefner, NM and Golubeva, TB and Savost'yanova, EG},
Title = {{Three-dimensional organization of epithelia of the AB(2) composition by
   the example of the cochlea sensory epithelium of birds}},
Journal = {{JOURNAL OF EVOLUTIONARY BIOCHEMISTRY AND PHYSIOLOGY}},
Year = {{2005}},
Volume = {{41}},
Number = {{5}},
Pages = {{570-579}},
Month = {{SEP-OCT}},
Abstract = {{Using a new approach developed by the authors for analysis of
   histoarchitectonics of cellular layers, theoretical and experimental
   study is carried out of the three-dimensional structure of sensory
   epithelium of the avian cochlea with the 1 : 2 ratio of sensory and
   supporting cells. A basic three-dimensional model of this composition
   was constructed to describe the shape and arrangement of the cells at
   different levels of the layer and its variants with partial and complete
   isolation of sensory cells. The presence of translational symmetry in
   the sensory epithelium is shown. Comparison of theoretical models and
   results of morphological study of the sensory epithelium has allowed
   concluding that they well reflect the real tissue structure. The models
   facilitate essentially the three-dimensional reconstruction of the
   sensory epithelium and allow using the minimal number of sections as
   well as predicting its changes in development.}},
