@article{ WOS:000609003400028,
Author = {Hamagami, Hiroki and Yamaguchi, Yoshiki and Tanaka, Hiroshi},
Title = {{Chemical Synthesis of Residue-Selectively C-13 and H-2
   Double-Isotope-Labeled Oligosaccharides as Chemical Probes for the
   NMR-Based Conformational Analysis of Oligosaccharides}},
Journal = {{JOURNAL OF ORGANIC CHEMISTRY}},
Year = {{2020}},
Volume = {{85}},
Number = {{24}},
Pages = {{16115-16127}},
Month = {{DEC 18}},
Abstract = {{The conformational analysis of oligosaccharide is a fundamental issue in
   glycobiology. NMR measurements of atom-selectively C-13-labeled
   oligosaccharides have provided valuable information concerning their
   conformation, which would not be possible using nonlabeled
   oligosaccharides. The amount of accessible information from an
   atom-selectively labeled molecule, however, is limited. In this work, we
   report on the chemical synthesis of residue-selectively C-13- and
   H-2-labeled oligosaccharides and their use in conformational analysis.
   H-1 NMR measurements of such double isotope-labeled compounds can
   provide a great deal of information on the dihedral angles across
   glycosidic linkages. We demonstrated this method in the conformational
   analyses of some linear and branched beta(1,3)-glucan oligosaccharides.}},
