@article{ WOS:000233783000008,
Author = {Kostochka, A and Nakprasit, K},
Title = {{On equitable Delta-coloring of graphs with low average degree}},
Journal = {{THEORETICAL COMPUTER SCIENCE}},
Year = {{2005}},
Volume = {{349}},
Number = {{1}},
Pages = {{82-91}},
Month = {{DEC 12}},
Note = {{Workshop on Graph Colorings, Schloss Dagstuhl, GERMANY, SEP 21-26, 2003}},
Abstract = {{An equitable coloring of a graph is a proper vertex coloring such that
   the sizes of any two color classes differ by at most 1. Hajnal and
   Szemeredi proved that every graph with maximum degree Delta is equitably
   k-colorable for every k >= Delta + 1. Chen, Lih, and Wu conjectured that
   every connected graph with maximum degree Delta >= 3 distinct from K
   Delta+1 and K-Delta,K-Delta is equitably Delta-colorable. This
   conjecture has been proved for graphs in some classes such as bipartite
   graphs, outerplanar graphs, graphs with maximum degree 3, interval
   graphs. We prove that this conjecture holds for graphs with average
   degree at most Delta/5. (c) 2005 Elsevier B.V. All rights reserved.}},
