Title = {{ROLE OF RADIATIVE-TRANSFER IN MAINTENANCE AND DESTRUCTION OF
   STRATOCUMULUS CLOUDS}},
Journal = {{ATMOSPHERIC ENVIRONMENT}},
Year = {{1995}},
Volume = {{29}},
