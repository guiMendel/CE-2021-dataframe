@article{ WOS:000165766100005,
Author = {Li, C},
Title = {{On well posed generalized best approximation problems}},
Journal = {{JOURNAL OF APPROXIMATION THEORY}},
Year = {{2000}},
Volume = {{107}},
Number = {{1}},
Pages = {{96-108}},
Month = {{NOV}},
Abstract = {{Let C be a closed bounded convex subset of X with 0 being an interior
   point of C and p(C) be the Minkowski functional with respect to C. Let G
   be a nonempty closed, boundedly relatively weakly compact subset of a
   Banach space X. For a point x epsilon X, we say the minimization problem
   min(C)(x, G) is well posed if there exists a unique point (z) over bar
   such that p(C)((z) over bar -x) = lambda (C)(x, G) and every sequence
   \{z(n)\} subset of G satisfying lim(n-->infinity) p(C)(z(n) - x) =
   lambda (C)(x, G) converges strongly to the point (z) over bar where
   lambdaC(x, G) = inf(z epsilonG) p(C)(z - x). Under the assumption that C
   is both strictly convex and Kadec, we prove that the set X-o(G) of all x
   epsilon X such that the problem min(C)(x, G) is well posed is a residual
   subset of X extending the results in the case that the modulus of
   convexity of C is strictly positive due to Blasi and Myjak. In addition,
   we also prove these conditions are necessary. (C) 2000 Academic Press.}},
