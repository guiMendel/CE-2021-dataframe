@article{ WOS:000436887400004,
Author = {Horta, Paulo Lemos and Roth, Nadine},
Title = {{Whose Cosmopolitanism Is It Anyway? Musings from the Global Academy}},
Journal = {{NEW GLOBAL STUDIES}},
