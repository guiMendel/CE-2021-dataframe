@article{ WOS:000597338000001,
Author = {Dimitrov, Dimitre D. and Lafleur, Peter M.},
Title = {{Revisiting water retention curves for simple hydrological modelling of
   peat}},
Journal = {{HYDROLOGICAL SCIENCES JOURNAL-JOURNAL DES SCIENCES HYDROLOGIQUES}},
Year = {{2021}},
Volume = {{66}},
Number = {{2}},
Pages = {{252-267}},
Month = {{JAN 25}},
Abstract = {{Accurate modelling of peat water contents (theta) is critical for
   wetland studies. We modified the Campbell and Van Genuchten soil water
   retention curves (SWRCs) by replacing their empirical parameters with
   measurable properties. Combining the water table depth (d (WT)) into
   SWRCs, we derived formulae for calculating volumetric theta from d (WT),
   coded in a simple model to test our hypotheses that d (WT) is a reliable
   predictor of theta for peat of low and high water holding capacity at
   near-saturation. We compared our simulations with time-domain
   reflectometry theta measurements at Mer Bleue bog (Ontario, Canada) and
   the Western Peatland fen (Alberta, Canada). Constraining Campbell SWRC
   at extreme drying and waterlogging rather than wilting point and field
   capacity produced superior results. The Van Genuchten SWRC was
   approximated by hyperbolic and inverse hyperbolic segments. When
   simplified, its performance reconciles with that of the modified
   Campbell SWRC. Overall, our formulae performed well with generalized
