@article{ WOS:A1995TQ73400005,
Author = {Oubaid, V and Amelang, M},
Title = {{The attractiveness of a person as a potential mate as a function of that
   persons past reproductive record - Test of a sociobiological assumption}},
Journal = {{GRUPPENDYNAMIK-ZEITSCHRIFT FUR ANGEWANDTE SOZIALPSYCHOLOGIE}},
Year = {{1995}},
Volume = {{26}},
Number = {{4}},
Pages = {{445-456}},
Month = {{DEC}},
Abstract = {{The focus of the present study lies on a sociobiological conclusion that
   a person's value on the `'partner market'' is influenced by an already
   proved reproductive ability. The influence should depend on the amount
   of possible future investments, which results `'for a new partner'' from
   a past reproduction of the target-person. A higher expected investment
   should lower the person's value on the partner market. To test these
   assumptions, scenarios were generated in which an imagined partner
   already has a child (vs. not) and whether if this child is living
   together with the imagined partner (vs. not). The dependent variable was
   the judged attractivity under different conditions of this person as a
   mating partner (of having a family with this imagined partner). The
   results of N=203 students support the assumptions in general with partly
   different effects for men and women. tow-cost-conditions (e.g. child is
   dead) produced neutral judgements, whereas high-cost-conditions produced
   lower attractivity judgements.}},
