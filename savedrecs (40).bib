@article{ WOS:000166229800016,
Author = {Axelrod, EG and Martiouchev, LM and Lyovkina, YV},
Title = {{Kinetics of free sidebranch dendrite growth from a solution}},
Journal = {{PHYSICA STATUS SOLIDI A-APPLIED RESEARCH}},
Year = {{2000}},
Volume = {{182}},
Number = {{2}},
Pages = {{687-696}},
Month = {{DEC}},
Abstract = {{The transition rate (mass change) in a free quasi-two-dimensional growth
   of individual NH4Cl dendrites from a supersaturated aqueous solution is
   investigated. It is found that the integral experimental kinetic curves
   can be fitted by an exponential function corresponding to a Weibull
   model. It is shown that the Kolmogorov-Avrami theory can be applied to
   the description of growth kinetics of the individual dendrite. In
   addition we find small amplitude relaxation oscillations of the dendrite
   mass with base frequency of about 0.1 Hz. The origin of these
   oscillations is connected with the interaction of the diffusion fields
   of an existing and arising secondary branches. This con elusion agrees
   qualitatively with our computer simulation results.}},
