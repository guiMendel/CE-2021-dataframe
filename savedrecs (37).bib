@article{ WOS:000232077000009,
Author = {Wei, GC},
Title = {{Transparent ceramic lamp envelope materials}},
Journal = {{JOURNAL OF PHYSICS D-APPLIED PHYSICS}},
Year = {{2005}},
Volume = {{38}},
Number = {{17}},
