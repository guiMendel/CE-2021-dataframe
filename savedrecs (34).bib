@article{ WOS:000232168100023,
Author = {Maruta, Daizo},
Title = {{Charge neutrality condition of black holes and its application to the
   disc-jet system}},
Journal = {{MONTHLY NOTICES OF THE ROYAL ASTRONOMICAL SOCIETY}},
Year = {{2005}},
Volume = {{363}},
Number = {{1}},
Pages = {{236-240}},
Month = {{OCT 11}},
Abstract = {{In this paper, we derive the stationary Maxwell equations near the event
   horizon of a Kerr black hole in Boyer-Lindquist coordinates. Using the
   obtained radial component of the electric current density vector, we
   obtain the charge neutrality condition of the black hole. This condition
   depends only on the toroidal magnetic field and is independent of the
   spin of the black hole. Moreover, we apply it to the electric current
   structure of the disc jet system around the black hole. In particular,
   we refer to the case of the jet structure having a one-layer and
   two-layer structure in the electric current.}},
