@article{ WOS:A1995TL47700006,
Author = {Hazlett, BA},
Title = {{Behavioral plasticity in crustacea: Why not more?}},
Journal = {{JOURNAL OF EXPERIMENTAL MARINE BIOLOGY AND ECOLOGY}},
Year = {{1995}},
Volume = {{193}},
Number = {{1-2}},
Pages = {{57-66}},
Month = {{NOV 29}},
Abstract = {{The extent of plasticity in the behavior of hermit crabs is reviewed.
   Data are presented demonstrating that in the hermit crab Calcinus
   tibicen, the diversity of experience with shell types affects the ease
   with which crabs enter novel shells. Hermit crabs that had more diverse
   experience with shell types made fewer mistakes and entered novel shells
   more quickly than individuals with less diverse experience. Examples of
   the limitations on the extent of plasticity are reviewed and some of the
   mechanisms which could lead to limitations are discussed. These
   mechanisms include (a) sensory capabilities, (b) morphological
   constraints, (c) limits on learning abilities, and (d) constraints by
   cognitive structure, i.e. the nature of the connections between
   categories of behavior. As an example of the latter, data are presented
   for the hermit crab Clibanarius vittatus showing that crabs that
   regularly detected blood of conspecifics (a presumed indicator of a high
   predation level) did not select different shells than controls not
   exposed to such input.}},
