@article{ WOS:000359316600002,
Author = {Teo, Yik R. and Fleming, Andrew J.},
Title = {{Optimal integral force feedback for active vibration control}},
Journal = {{JOURNAL OF SOUND AND VIBRATION}},
Year = {{2015}},
Volume = {{356}},
Pages = {{20-33}},
Month = {{NOV 10}},
Abstract = {{This paper proposes an improvement to Integral Force Feedback UFO, which
   is a popular method for active vibration control of structures and
   mechanical systems. Benefits of IFF include robustness, guaranteed
   stability and simplicity. However, the maximum damping performance is
   dependent on the stiffness of the system; hence, some systems cannot be
   adequately controlled. In this paper, an improvement to the classical
   force feedback control scheme is proposed. The improved method achieves
   arbitrary damping for any mechanical system by introducing a
   feed-through term. The proposed improvement is experimentally
   demonstrated by actively damping an objective lens assembly for a
   highspeed confocal microscope. (C) 2015 Elsevier Ltd. All rights
