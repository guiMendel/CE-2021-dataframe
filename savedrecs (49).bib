@article{ WOS:000088527600008,
Author = {Vettas, N},
Title = {{On entry, exit, and coordination with mixed strategies}},
Journal = {{EUROPEAN ECONOMIC REVIEW}},
Year = {{2000}},
Volume = {{44}},
Number = {{8}},
Pages = {{1557-1576}},
Month = {{AUG}},
Abstract = {{A dynamic entry and exit game is studied where, following Dixit and
   Shapiro (1986, The economics of strategic planning, Lexington Books, pp.
   63-79), firms play symmetric mixed strategies. Coordination among
   entrants improves and creates a tendency for the value of incumbency to
   increase as the number of incumbent firms increases (up to the point of
   excess capacity). This feature implies that, when considering mixed
   strategies, the relevant payoffs are not monotonic: as the probability
   of entry increases, expected profit for a given firm, conditional on
   entry, increases over some initial range. It is shown that the payoffs
   have an interesting dynamic structure which allows the complete
   characterization of the equilibrium, (C) 2000 Elsevier Science B.V. All
   rights reserved. JEL classification: C72; C73; D43; L13.}},
