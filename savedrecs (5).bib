@article{ WOS:000600392500001,
Author = {Schmidt, Charlotte Vinther and Poojary, Mahesha M. and Mouritsen, Ole G.
   and Olsen, Karsten},
Title = {{Umami potential of Nordic squid (Loligo forbesii)}},
Journal = {{INTERNATIONAL JOURNAL OF GASTRONOMY AND FOOD SCIENCE}},
Year = {{2020}},
Volume = {{22}},
Month = {{DEC}},
Abstract = {{Nordic squid (Loligo forbesii) cuts of mantle, arms, fins, and liver
   were investigated for their umami taste potential. The cuts were
   analysed raw, and additional samples of cooked mantle (sous vide, 30 min
   at 55 degrees C) and a corresponding cooking liquid were included in the
   study. The samples were prepared in analytical as well as biological
   triplicate, analysed for content of free amino acids (n = 17) and free
   5'-mononucleotides (n = 5), wherefrom the equivalent umami concentration
   (EUC) was calculated. It was found that the squid liver contains the
   highest amount of umami-specific compounds (Glu = 462 mg/100 g, XMP = 22
   mg/100 g; EUC = 2.3 x 10(4) mg/100 g) followed by cooked mantle (EUC =
   3.5 x 10(3) mg/100 g) > raw mantle (EUC = 3.3 x 10(3) mg/100 g) > fins
   (EUC = 2.3 x 10(3) mg/100 g) > arms (EUC = 1.7 x 10(3) mg/100 g) >
   cooking liquid (EUC = 82 mg/100 g), with all values referring to wet
   weight. In addition, high contents of sweet-tasting free amino acids
   Pro, Ala, and Gly were found but in significantly lower amounts after
   cooking. Thus, from the perspective of taste, a variety of cuts from
   squid may advantageously be used for cooking, the liver constituting a
   particularly great potential in terms of umami taste. However, special
   attention should be given to the method of preparation of the cuts of
   squid (mantle, arms, and fins) due to the high content of collagen.}},
