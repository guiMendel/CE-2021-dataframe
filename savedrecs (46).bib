@article{ WOS:000089660800005,
Author = {Buller, G and Hoth, S and Suchandt, S},
Title = {{Expert system for aiding the diagnosis in hearing screening}},
Journal = {{BIOMEDIZINISCHE TECHNIK}},
Year = {{2000}},
Volume = {{45}},
