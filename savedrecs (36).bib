@article{ WOS:000232321800065,
Author = {Hermans, B and Puers, R},
Title = {{A portable multi-sensor data-logger for medical surveillance in harsh
   environments}},
Journal = {{SENSORS AND ACTUATORS A-PHYSICAL}},
