@article{ WOS:000233469400020,
Author = {Morton, AC and Arnold, ND and Gunn, J and Varcoe, R and Francis, SE and
   Dower, SK and Crossman, DC},
Title = {{Interleukin-1 receptor antagonist alters the response to vessel wall
   injury in a porcine coronary artery model}},
Journal = {{CARDIOVASCULAR RESEARCH}},
Year = {{2005}},
Volume = {{68}},
Number = {{3}},
Pages = {{493-501}},
Month = {{DEC 1}},
Abstract = {{Objective: To determine the influence of IL-1 on the arterial response
   to experimental injury in porcine models of percutancous coronary
   intervention (PCI).
   Methods: An intravenous (i.v.) bolus of 0.5 mg/kg followed by a
   subcutaneous (s.c.) infitision of 2 mg/kg/24 h of human IL-1 receptor
   antagonist (IL-1ra) inhibited neutrophil recruitment in response to
   intradermal IL-1. Using this dose regimen, five groups of pigs were
   studied: Group 1, oversized balloon angioplasty of 2 coronary vessels
   (14-day infusion, 28th day sacrifice and analysis); Groups 2, 3, 4, and
   5, oversized stenting of 2 coronary vessels (Group 2: 14-day infitision,
   28th day analysis; Group 3: 14-day infusion, 14th day analysis; Group 4:
   28-day infusion, 28th day analysis; Group 5: 28-day infusion, 90th day
   analysis). Neointimal area was quantified by standard means.
   Results: In Group 1, IL-1ra resulted in a 23\% decrease in neointimial
   area (p=0.04); in Group 2, a 34\% increase (p=0.001); in Group 3, a 38\%
