@article{ WOS:000218579600005,
Author = {Fang, Fuquan and Lei, Fengchun and Wu, Jie},
Title = {{The Symmetric Commutator Homology of Link Towers and Homotopy Groups of
   3-Manifolds}},
Journal = {{COMMUNICATIONS IN MATHEMATICS AND STATISTICS}},
Year = {{2015}},
Volume = {{3}},
Number = {{4}},
Pages = {{497-526}},
Month = {{DEC}},
Abstract = {{A link tower is a sequence of links with the structure given by removing
   the last components. Given a link tower, we prove that there is a chain
   complex consisting of (non-abelian) groups given by the symmetric
   commutator subgroup of the normal closures in the link group of the
   meridians excluding the meridian of the last component with the
   differential induced by removing the last component. Moreover, the
   homology groups of these naturally constructed chain complexes are
   isomorphic to the homotopy groups of the manifold M under certain
   hypothesis. These chain complexes have canonical quotient abelian chain
   complexes in Minor's homotopy link groups with their homologies
   detecting certain differences of the homotopy link groups in the towers.}},
