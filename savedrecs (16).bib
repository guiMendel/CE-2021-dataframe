@article{ WOS:000375132300003,
Author = {Jaria-Chacon, Natalia and Guardia-Olmos, Joan},
Title = {{A HIGHER EDUCATION EXPENDITURE PREDICTION MODEL: EVIDENCE FROM EUROPEAN
   COUNTRIES}},
Journal = {{ADVANCES AND APPLICATIONS IN STATISTICS}},
Year = {{2015}},
Volume = {{47}},
Number = {{2}},
Pages = {{153-174}},
Month = {{NOV}},
Abstract = {{The implementation of the European Higher Education Area (EHEA) has
   resulted in inevitable changes in the educational structure of many
   countries. In fact, nowadays public expenditure on higher education is a
   current and vital issue in a complex economic environment with increased
   demands for economic information. This article captures the need to
   establish the impact of some economic and academic variables (observed
   and latent) which determine public expenditure on higher education.
   Through structural equation models applied in a general model over
   thirty European countries along a time series of fourteen years, an
   invariant and stable structure factor can be assumed. From the
   perspective of prediction, statistically significant results indicate
   that it is possible to confirm a general longitudinal model proposed and
   which can be extrapolated to other countries to analyze this level of
   expenditure with the aim of developing improvement proposals.}},
