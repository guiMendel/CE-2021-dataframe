@article{ WOS:000368939700005,
Author = {Saenko, Oleg A.},
Title = {{Strong eddy compensation for the Gulf Stream heat transport}},
Journal = {{GEOPHYSICAL RESEARCH LETTERS}},
Year = {{2015}},
Volume = {{42}},
Number = {{24}},
Pages = {{10739-10744}},
Month = {{DEC 28}},
Abstract = {{Using a high-resolution ocean model forced with high-resolution
   atmospheric fields, a 5year mean heat budget of the upper ocean in the
   Gulf Stream (GS) region is analyzed. The heat brought to the region with
   the mean flows along the GS path is 2-3 times larger than the heat loss
   to the atmosphere, with the difference being balanced by a strong
   cooling effect due to lateral eddy heat fluxes. However, over a broad
   area off the Grand Banks, the eddies warm the uppermost ocean layers,
   partly compensating for the loss of heat to the atmosphere. The upward
   eddy heat flux, which brings heat from the deeper ocean to the upper
   layers, is 30-80\% of the surface heat loss.}},
