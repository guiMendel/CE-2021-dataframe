Title = {{Mossbauer studies of mixed-valence spin-crossover iron complexes with a
   hexadentate tripod ligand}},
Journal = {{HYPERFINE INTERACTIONS}},
Year = {{2005}},
Volume = {{166}},
Number = {{1-4}},
Pages = {{397-402}},
Month = {{NOV}},
Abstract = {{The spin-crossover behaviors of mixed-valence iron compounds
   {[}Fe(II)H(3)L]{[}Fe(III)L](NO(3))(2) (1) and
   {[}Fe(II)H(3)L(Me)]{[}Fe(III)L(Me)](NO(3))(2) (2) have been investigated
   by (57)Fe Mossbauer spectroscopy, where H(3)L is a hexadentate N(6)
