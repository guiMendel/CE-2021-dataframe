@article{ WOS:000358459300025,
Author = {Campos, Victor A. and Lima, Carlos V. and Martins, Nicolas A. and
   Sampaio, Leonardo and Santos, Marcio C. and Silva, Ana},
Title = {{The b-chromatic index of graphs}},
Journal = {{DISCRETE MATHEMATICS}},
Year = {{2015}},
Volume = {{338}},
Number = {{11}},
Pages = {{2072-2079}},
Month = {{NOV 6}},
Abstract = {{A b-coloring of the vertices of a graph is a proper coloring where each
   color class contains a vertex which is adjacent to at least one vertex
   in each other color class. The b-chromatic number of G is the maximum
   integer b(G) for which G has a b-coloring with b(G) colors. This problem
   was introduced by Irving and Manlove (1999), where they showed that
   computing b(G) is N P-hard in general and polynomial-time solvable for
   trees. A natural question that arises is whether the edge version of
   this problem is also NIP-hard or not. Here, we prove that computing the
   b-chromatic index of a graph G is N P-hard, even if G is either a
   comparability graph or a C-k-free graph, and give partial results on the
   complexity of the problem restricted to trees, more specifically, we
   solve the problem for caterpillars graphs. Although solving problems on
   caterpillar graphs is usually quite simple, this problem revealed itself
   to be unusually hard. The presented algorithm uses a dynamic programming
   approach that combines partial solutions which are proved to exist if,
   and only if, a particular polyhedron is non-empty. (C) 2015 Elsevier
   B.V. All rights reserved.}},
