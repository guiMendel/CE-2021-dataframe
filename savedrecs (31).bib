@article{ WOS:000233682200022,
Author = {Mizuno, T and Sugiyama, N and Tezuka, T and Moriyama, Y and Nakaharai, S
   and Maeda, T and Takagi, S},
Title = {{High-speed source-heterojunction-MOS-transistor (SHOT) utilizing
   high-velocity electron injection}},
Journal = {{IEEE TRANSACTIONS ON ELECTRON DEVICES}},
Year = {{2005}},
Volume = {{52}},
Number = {{12}},
Pages = {{2690-2696}},
Month = {{DEC}},
Abstract = {{We have developed the source-heterojunction-MOS-transistor (SHOT), a
   novel high-speed MOSFET with relaxed-SiGe/strained-Si heterojunction
   source structures for quasi-ballistic or full-ballistic transistors.
   Using the band-offset energy at the source SiGe/strained-Si
   heterojunction, high velocity electrons can be injected into the
   strained-Si channel from the SiGe source region. For the first time, we
   have experimentally demonstrated that the transconductance is enhanced
   in SHOT for high applied drain voltage, compared to that of strained-
   and conventional silicon-on-insultor MOSFETs. We have also shown that
   the transconductance enhancement in SHOT depends on both the gate drive
   and the drain bias.}},
