@article{ WOS:000089030800038,
Author = {Pivin, JC and Dimova-Malinovska, D and Sendova-Vassileva, M and
   Nikolaeva, M and Martucci, A},
Title = {{Optical applications of ion beam irradiation}},
Journal = {{VACUUM}},
