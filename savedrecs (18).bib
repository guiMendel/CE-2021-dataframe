@article{ WOS:000365356900007,
Author = {Vergara, Jean-Claude Seguin},
Title = {{SUCCESSIVE SLIDINGS OF PLEASURE, FROM ``S{''} TO ``X{''} IN THE SPANISH
   CINEMA}},
