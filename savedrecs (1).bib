@article{ WOS:000582504100006,
Author = {Ilatovskaia, M. and Baertel, F. and Fabrichnaya, O.},
Title = {{Phase relations in the Al2O3-TiO2-SiO2 system}},
Journal = {{CERAMICS INTERNATIONAL}},
Year = {{2020}},
Volume = {{46}},
Number = {{18, B, SI}},
Pages = {{29402-29412}},
Month = {{DEC 15}},
Abstract = {{Phase relations in the Al2O3-TiO2-SiO2 system have been investigated
   experimentally using X-ray diffraction, scanning electron microscopy
   combined with energy-dispersive X-ray spectrometry, and differential
   thermal analysis. The solid-state invariant reaction SiO2+Al2TiO5 <->
   Al6Si2O13+TiO2 has been found at the temperature of 1743 +/- 5 K. On the
   liquidus surface, the eutectic invariant reaction has been established
   at 1750 K and two transitional-type reactions at 1779 and 2007 K,
   respectively. Based on the experimental results obtained, isothermal
   sections of the ternary system within the range of 1486-1747 K have been
   predicted. A vertical SiO2-Al2TiO5 section has been constructed.}},
