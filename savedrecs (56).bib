@article{ WOS:000208243000004,
Author = {Wright, Alden H. and Vose, Michael D.},
Title = {{Finiteness of the Fixed Point Set for the Simple Genetic Algorithm}},
Journal = {{EVOLUTIONARY COMPUTATION}},
Year = {{1995}},
Volume = {{3}},
Number = {{3}},
Pages = {{299-309}},
Month = {{FAL}},
Abstract = {{The infinite population simple genetic algorithm is a discrete dynamical
   system model of a genetic algorithm. It is conjectured that trajectories
   in the model always converge to fixed points. This paper shows that an
   arbitrarily small perturbation of the fitness will result in a model
   with a finite number of fixed points. Moreover, every sufficiently small
   perturbation of fitness preserves the finiteness of the fixed point set.
   These results allow proofs and constructions that require finiteness of
   the fixed point set. For example, applying the stable manifold theorem
   to a fixed point requires the hyperbolicity of the differential of the
   transition map of the genetic algorithm, which requires ( among other
   things) that the fixed point be isolated.}},
