@article{ WOS:000359029900014,
Author = {Lian, Jie and Wang, Xiaona},
Title = {{Exponential stabilization of singularly perturbed switched systems
   subject to actuator saturation}},
Journal = {{INFORMATION SCIENCES}},
Year = {{2015}},
Volume = {{320}},
Pages = {{235-243}},
Month = {{NOV 1}},
Abstract = {{This paper considers the problem of exponential stabilization for a
   class of singularly perturbed switched systems subject to actuator
   saturation. A time-dependent switching law is designed to obtain the
   exponential stabilization conditions of the switched systems with
   unstabilizable subsystems. Based on the multiple Lyapunov functions
   method depending on singular perturbation parameters, a domain of
   attraction is constructed. Furthermore, in order to maximize an estimate
   of the domain of attraction, convex optimization problems are
   formulated. Finally, a numerical example is presented in order to
   illustrate the effectiveness of the established results. (C) 2015
   Elsevier Inc. All rights reserved.}},
