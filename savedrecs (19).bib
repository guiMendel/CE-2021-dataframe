@article{ WOS:000359867600005,
Author = {Stapleton, Laura M. and Pituch, Keenan A. and Dion, Eric},
Title = {{Standardized Effect Size Measures for Mediation Analysis in
   Cluster-Randomized Trials}},
Journal = {{JOURNAL OF EXPERIMENTAL EDUCATION}},
Year = {{2015}},
Volume = {{83}},
Number = {{4}},
Pages = {{547-582}},
Month = {{OCT 2}},
Abstract = {{This article presents 3 standardized effect size measures to use when
   sharing results of an analysis of mediation of treatment effects for
   cluster-randomized trials. The authors discuss 3 examples of mediation
   analysis (upper-level mediation, cross-level mediation, and cross-level
   mediation with a contextual effect) with demonstration of the
   calculation and interpretation of the effect size measures using a
   simulated dataset and an empirical dataset from a cluster-randomized
