@article{ WOS:A1995TE84000003,
Author = {CRONAN, TA and WALEN, HR},
Title = {{THE DEVELOPMENT OF PROJECT PRIMER - A COMMUNITY-BASED LITERACY PROGRAM}},
Journal = {{READING RESEARCH AND INSTRUCTION}},
Year = {{1995}},
Volume = {{35}},
Number = {{1}},
Pages = {{37-47}},
Month = {{FAL}},
Abstract = {{Disillusionment with the current educational system has led to new
   attempts to solve the growing problem of low literacy. This paper
   outlines the development and implementation of Project PRIMER (PRoducing
   Infant/Mother Ethnic Readers), one attempt to combat low literacy.
   Descriptions of the recruitment strategies, roles and training of
   participating college students, and barriers to implementation are
   included. The procedures employed to develop the intervention programs
   are included. One hundred forty-seven families participated The results
   indicated that parents engaged in the targeted behaviors and that
   children in the high intervention program showed improvements in their
