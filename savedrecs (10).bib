@article{ WOS:000367373900006,
Author = {Stenberg, Markku P. V. and Pack, Kevin and Wilhelm, Frank K.},
Title = {{Adaptive identification of coherent states}},
Journal = {{PHYSICAL REVIEW A}},
Year = {{2015}},
Volume = {{92}},
Number = {{6}},
Month = {{DEC 31}},
Abstract = {{We present methods for efficient characterization of an optical coherent
   state vertical bar alpha >. We choose measurement settings adaptively
   and stochastically, based on data while it is collected. Our algorithm
   divides the estimation into two distinct steps: (i) before the first
   detection of a vacuum state, the probability of choosing a measurement
   setting is proportional to detecting vacuum with the setting, which
   makes using too similar measurement settings twice unlikely; and (ii)
   after the first detection of vacuum, we focus measurements in the region
   where vacuum is most likely to be detected. In step (i) {[}(ii)] the
   detection of vacuum (a photon) has a significantly larger effect on the
   shape of the posterior probability distribution of a. Compared to
   nonadaptive schemes, our method makes the number of measurement shots
   required to achieve a certain level of accuracy smaller approximately by
   a factor proportional to the area describing the initial uncertainty of
   a in phase space. While this algorithm is not directly robust against
   readout errors, we make it such by introducing repeated measurements in
   step (i).}},
