@article{ WOS:000089716000001,
Author = {de Saint Victor, X and Houdeville, R},
Title = {{Influence of periodic wakes on the development of a boundary layer}},
Journal = {{AEROSPACE SCIENCE AND TECHNOLOGY}},
Year = {{2000}},
Volume = {{4}},
Number = {{6}},
Pages = {{371-381}},
Month = {{SEP}},
Abstract = {{The flow in turbomachinery is strongly influenced by the rotor-stator
   interactions. It is known that the unsteadiness of the flow has for
   effect re advance the laminar-turbulent transition and to alter the heat
   transfer. The problem of the wake-boundary layer interaction has been
   studied experimentally by using a simplified geometrical configuration.
   The stator blade is simulated by a flat plate on which the development
   of the boundary layer disturbed by periodic wakes is observed. The wakes
   are generated by moving bars having a circular section, put upstream of
   the flat plate and running with a constant velocity in the wind of a
   low-speed tunnel. Numerical studies relating to the phenomenon of
   interaction are limited. Part of the experimental study has been carried
   out in a fully turbulent flow in order to avoid the problem of
   transition. These results are compared with those from a code solving
   the fully turbulent unsteady boundary layer equations. (C) 2000 Editions
   scientifiques et medicales Elsevier SAS.}},
