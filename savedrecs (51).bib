@article{ WOS:A1995TE61100003,
Author = {XIANG, ZX and HUANG, FH and SHI, YY and XU, YW},
Title = {{INCORPORATING THE PROTEIN-DIPOLE LANGEVIN-DIPOLE MODEL INTO
   TANFORD-KIRKWOOD THEORY}},
Journal = {{JOURNAL OF COMPUTATIONAL CHEMISTRY}},
Year = {{1995}},
Volume = {{16}},
Number = {{12}},
Pages = {{1468-1473}},
Month = {{DEC}},
Abstract = {{A new technique incorporating the protein-dipole Langevin-dipole (PDLD)
   model into the Tanford-Kirkwood (TK) formula has been proposed which
   provides a rather detailed description of solvent and ionic strength
   effects on the electrostatic energies. Applications of this method to
   realistic problems have been performed and concern the solvation
   energies of four residues of bovine pancreatic trypsin inhibitor (BPTI)
   and the pK shift of His-64 of mutant subtilisin BPN'. We focus our
   calculation on the back-field effects of bulk solvent.dagger Our
   calculations indicate that the back-field effects of bulk solvent on
   protein dipoles can simply be ignored, introducing a relative error less
   than 3\%; whereas such back field-effects on protein net charges are
   relatively important and cannot simply be ignored, especially when
