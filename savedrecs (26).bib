@article{ WOS:000282765700045,
Author = {Gunnlaugsson, H. P. and Molholt, T. E. and Mantovan, R. and Masenda, H.
   and Naidoo, D. and Dlamini, W. B. and Sielemann, R. and Bharuth-Ram, K.
   and Weyer, G. and Johnston, K. and Langouche, G. and Olafsson, S. and
   Gislason, H. P. and Kobayashi, Y. and Yoshida, Y. and Fanciulli, M. and
   ISOLDE Collaboration},
Title = {{Paramagnetism in Mn/Fe implanted ZnO}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2010}},
Volume = {{97}},
Number = {{14}},
Month = {{OCT 4}},
Abstract = {{Prompted by the generally poor understanding of the nature of magnetic
   phenomena in 3d-metal doped ZnO, we have undertaken on-line (57)Fe
   Mossbauer spectroscopy on ZnO single crystals in an external magnetic
   field of 0.6 T, following the implantation of radioactive (57)Mn ions at
   room temperature. The Mossbauer spectra of the dilute Fe impurities are
   dominated by sextets whose angular dependence rules out an ordered
   magnetic state (which had been previously proposed) but are well
   accounted for on the basis of Fe(3+) paramagnetic centers on
   substitutional Zn sites with unusually long relaxation times (> 20 ns).
   (C) 2010 American Institute of Physics. {[}doi:10.1063/1.3490708]}},
