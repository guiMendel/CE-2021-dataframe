@article{ WOS:000285732400001,
Author = {Chen, Hsiao-Hwa and Huang, Yen-Han},
Title = {{Virtual Space-Time Coding for Achieving Transmit Diversity without
   Antenna Array}},
Journal = {{JOURNAL OF INTERNET TECHNOLOGY}},
Year = {{2010}},
Volume = {{11}},
Number = {{7, SI}},
Pages = {{887-900}},
Month = {{DEC}},
Abstract = {{In this paper, we propose a virtual transmit diversity scheme which is
   able to achieve a full transmit diversity gain with only single transmit
   antenna. We use locally generated random sequences at a transmitter to
   emulate independent space-time coded channel coefficients to achieve
   full transmit diversity gain at the receiver. The performance of such a
   virtual transmit diversity scheme is analyzed and compared with
   traditional Alamouti transmit diversity scheme. The properties of
   locally generated fading channel coefficients to achieve 2 x 1 virtual
   transmit diversity gain are studied and it is revealed that either
   Gaussian or evenly distributed random sequences can be applied to the
   virtual transmit diversity schemes. The 2 x 1 virtual transmit diversity
   scheme will also be generalized to N-t x 1 virtual transmit diversity
   scheme in this paper. The work is significant to introduce a new
   sub-optimal approach to achieve N-t x 1 transmit diversity at a very low
   complexity.}},
