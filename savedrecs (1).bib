@article{ WOS:000573514000003,
Author = {Fu, Yike and Cen, Dong and Zhang, Tian and Jiang, Shi and Wang, Yifan
   and Cai, Xiujun and Li, Xiang and Han, Gaorong},
Title = {{Implantable fibrous scaffold with hierarchical microstructure for the
   `on-site' synergistic cancer therapy}},
Journal = {{CHEMICAL ENGINEERING JOURNAL}},
Year = {{2020}},
Volume = {{402}},
Month = {{DEC 15}},
Abstract = {{The tissue-targeting delivery and intracellular transportation of
   therapeutics have been highly demanded for effective cancer therapy, but
   remains challenging. We hereby report an implantable scaffold consisting
   of composite fibers with unique particle-on-fiber microstructure for
   localized synergistic chemo-/starvation therapy. In this system, silica
   nanoparticles are loaded with glucose oxidase (GOx) and protected by a
   poly dopamine (PDA) coating. A hypoxia-activated prodrug (banoxantrone,
   AQ4N) is absorbed at the particle surface (MGPA). Meanwhile,
   biodegradable fibrous scaffold, composed of polycaprolactone (PCL) and
   gelatin, are incorporated with hyaluronidase (HAase), and self-assembled
   with MGPA nanoparticles. When the composite scaffold is implanted at the
   tumor site, particle liberation occurs in a pH-dependent manner. HAase
   released effectively decomposes extracellular matrix (ECM), and thereby
   promote the cellular uptake of MGPA nano particles. Subsequently, MGPA
   exhausts intracellular glucose and O-2, and in turn activates AQ4N to
   toxic AQ4. The findings indicate that this scaffold can significantly
   enhance accumulation of therapeutics at tumor site for a prolonged
   period and resulted in promoted tumor inhibition. This study has
   therefore proposed an alternative design of therapeutic device,
   potentially combining advantages of nanoparticle-form and implantable
   drug delivery systems, for cancer treatment with high efficacy.}},
