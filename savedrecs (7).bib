@article{ WOS:000581077900017,
Author = {Hirai, Hiroshi and Shimada, Hiroshi},
Title = {{Current Distributions in the Vicinity of Electrical Contacts with a Two
   Dimensional Electron Gas under High Magnetic Fields}},
Journal = {{JOURNAL OF THE PHYSICAL SOCIETY OF JAPAN}},
Year = {{2020}},
Volume = {{89}},
Number = {{11}},
Month = {{NOV 15}},
Abstract = {{Current distributions near electrodes with a two-dimensional electron
   gas were investigated by low temperature measurement of the longitudinal
   resistivity rho(xx), and the Hall resistivity rho(xy), of a Hall-bar
   type sample at a GaAs/AlGaAs heterojunction interface. Current
   distributions were calculated based on the experimental results using
   the Drude theory and the conformal mapping. As a result, the current
   distributions were determined by only one parameter, gamma. When
   gamma(B) was obtained from the experimental results, it was determined
   that gamma approximate to 1 at B > 0.3 T in the high mobility sample.
   This implies that, the current distribution near the electrodes does not
   change significantly in this magnetic field range. The response-current
   density J(R) and the Fermi-surface current density J(F) are presented to
   visually show the current distributions near the electrodes.}},
