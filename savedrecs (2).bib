@article{ WOS:000588340500001,
Author = {Chang, H. W. and Jen, S. U. and Tseng, D. H. and Liao, Y. H. and Hsieh,
   H. Y. and Wang, C. R. and Chang, W. C.},
Title = {{Comparison on the Delta E effect in Fe87Ga13 alloy by doping Dy and Tb}},
Journal = {{MATERIALS SCIENCE AND ENGINEERING B-ADVANCED FUNCTIONAL SOLID-STATE
   MATERIALS}},
Year = {{2020}},
Volume = {{262}},
Month = {{DEC}},
Abstract = {{R-doping effects on the magneto-mechanical properties, including Young's
   modulus (E) at magnetic field (H), that is Delta E effect, and
   magnetostriction (lambda), of Fe86.8Ga13R0.2 alloys (R = Tb and Dy) are
   studied. Delta E-s/E-0 of Fe87Ga13 alloy is 6\%, and it is significantly
   enhanced to 12\% by doping Dy, but reduced to 4.5\% for Tb-doped alloy.
   The trend of magnetostriction sensitivity (d lambda/dH) of
   Fe86.8Ga13R0.2 alloys with R is similar to that of Delta E-s/E-0 with R.
   Most importantly, strong Delta E effect due to high d lambda/dH for
   Fe-86.8Ga13Dy0.2 alloy favors magnetic applications. Large Delta E
   effect is considered to correlate to magnetic softening and lattice
   softening by doping Dy. For Tb-doped alloy, the increase of lambda(s)
   might be related to the decrease of Fe content in the matrix due to the
   appearance of minor Tb2Fe17 phase. However, the decrease of d lambda/dH
   might result from increased coercivity and decreased permeability
   related to the introduction of Tb2Fe17 phase and result in reduced Delta
   E effect. The findings of this work provide an effective pathway of
   enhancing the magneto-mechanical properties for Fe-rich Galfenol alloys.}},
