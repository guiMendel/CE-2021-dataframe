@article{ WOS:A1995QM20500005,
Author = {RAE, DW},
Title = {{USING DISTRICT MAGNITUDE TO REGULATE POLITICAL-PARTY COMPETITION}},
Journal = {{JOURNAL OF ECONOMIC PERSPECTIVES}},
Year = {{1995}},
Volume = {{9}},
Number = {{1}},
Pages = {{65-75}},
Month = {{WIN}},
Abstract = {{This is an essay about the use of districting systems to regulate
   electoral outcomes and the evolution of party systems. By changing the
   number of seats from each district, a balance can be struck between the
   often conflicting goals of minority representation and
   defractionalization. Defractionalization is the process of forming
   broad-based governing majorities. Special attention is given to the
   electoral systems of Italy, Spain, and the United States. In the case of
   the United States, the author recommends a proportional representation
   system that mixes high-magnitude districts that encourage minority
   representation and low-magnitude districts that promote
   defractionalization.}},
