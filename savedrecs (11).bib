@article{ WOS:000368126300031,
Author = {Falade, Kolawole O. and Oyeyinka, Samson A.},
Title = {{COLOR, CHEMICAL AND FUNCTIONAL PROPERTIES OF PLANTAIN CULTIVARS AND
