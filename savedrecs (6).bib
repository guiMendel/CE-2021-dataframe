@article{ WOS:000595176900021,
Author = {Zahida, Khalil and Joseph, Prakash and Balaraju, Girisha and Al Kaabi,
   Saad Rashid Mohammad and Naushad, Vamanjore A.},
Title = {{Hidden Nematode Causing Recurrent Pancreatitis: A Case Report}},
Journal = {{CUREUS}},
Year = {{2020}},
Volume = {{12}},
Number = {{12}},
Month = {{DEC 1}},
Abstract = {{Even though hepatobiliary ascariasis has been found to cause
   pancreatitis, it is rare in Qatar and other countries in the Middle
   East. In this report, we present a case of biliary duct ascariasis
   causing recurrent pancreatitis. A 46-year-old woman from the Philippines
   presented with recurrent clinical and biochemical features of acute
   pancreatitis and was found to have hepatobiliary ascariasis. She was
   successfully treated with endoscopic retrograde cholangiopancreatography
   (ERCP) and antihelminthic medication. Although hepatobiliary ascariasis
   as a cause of pancreatitis is rare, it should be considered in patients
   with recurrent pancreatitis without an obvious cause, especially in
   those from endemic areas or those who have visited endemic areas.}},
