@article{ WOS:000364355700016,
Author = {Ding, Wanjing and Hu, Zhijuan and Zhang, Zhewen and Ma, Qiaoqiao and
   Tang, Huifang and Ma, Zhongjun},
Title = {{Physapubescin B Exhibits Potent Activity against Human Prostate Cancer
   In Vitro and In Vivo}},
Journal = {{JOURNAL OF AGRICULTURAL AND FOOD CHEMISTRY}},
Year = {{2015}},
Volume = {{63}},
Number = {{43}},
Pages = {{9504-9512}},
Month = {{NOV 4}},
Note = {{1st ACS-AGFD and ACSThailand Chapter Joint Symposium, Bangkok, THAILAND,
   MAR 04-05, 2014}},
Abstract = {{The present data showed that a natural compound isolated from the plant
   Physalis pubescens L. (Solanaceae), physapubescin B, exhibited antitumor
   activity against prostate cancer in vitro and in vivo. Treating prostate
   cancer cells with physapubescin B resulted in the accumulation of cells
   in the G2/M phase, which was associated with reduced Cdc25C levels and
   increased levels of CyclinB1, P21 as well as p-Cdk1 (Tyr15).
   Additionally, reactive oxygen species (ROS) generation was increased in
   physapubescin B-treated PC-3 cells. Furthermore, the physapubescin
   B-induced decrease of Cdc25C protein expression together with the G2/M
   phase cell cycle arrest were significantly abrogated by antioxidant NAC
   and GSH. Our data also demonstrated that physapubescin B exhibited
   strong in vivo antitumor efficacy in human prostate cancer PC3
   xenograft. In conclusion, our results provide clear evidence that
   physapubescin B exhibits antitumor activity both in vitro and in vivo
   and deserves further development as an anticancer agent.}},
