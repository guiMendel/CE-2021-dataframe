@article{ WOS:000238586400009,
Author = {Reinhartz, O and Hill, JD and Al-Khaldi, A and Pelletier, MP and
   Robbins, RC and Farrar, DJ},
Title = {{Thoratec ventricular assist devices in pediatric patients: Update on
   clinical results}},
