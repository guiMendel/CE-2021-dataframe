@article{ WOS:000166598500005,
Author = {Gundersen, AC and Nedreaas, KH and Kjesbu, OS and Albert, OT},
Title = {{Fecundity and recruitment variability of Northeast Arctic Greenland
   halibut during 1980-1998, with emphasis on 1996-1998}},
