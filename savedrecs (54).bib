@article{ WOS:A1995TB89200012,
Author = {VANDERVELDEN, MC and SIEGEL, LS},
Title = {{PHONOLOGICAL RECODING AND PHONEME AWARENESS IN EARLY LITERACY - A
   DEVELOPMENTAL-APPROACH}},
Journal = {{READING RESEARCH QUARTERLY}},
Year = {{1995}},
Volume = {{30}},
Number = {{4}},
Pages = {{854-875}},
Month = {{OCT-DEC}},
Abstract = {{THIS ARTICLE reports on two studies. In the first study, we investigated
   the development and role of phonological recoding in beginning reading,
   phonological recoding was measured as a set of skills that involves
   using systematic relationships between letters and phonemes to recognize
   or pronounce unknown printed strings (words and pseudowords), or to
   spell. One hundred and twenty children, age 5 to 8 were given a
   speech-to-print matching, a pseudoword reading, and a spelling task. The
   results indicated regular phases in phonological recoding prior to
   accuracy in reading simple pseudowords and a strong but changing
   relationship between these rudimentary skills and several reading tasks.
   In the second study, we investigated interrelationships with phoneme
   awareness that we measured as a hierarchy of skill in recognizing and
   segmenting phonemes in spoken words. A strong but changing relationship
   was found for the six measures of phoneme awareness with developmental
   phases in phonological recoding, with reading, and with spelling. The
   finding that phonological recoding and phoneme awareness are complex
   skills that develop gradually and reciprocally with reading and spelling
   indicates a need for multiple measures in research.}},
