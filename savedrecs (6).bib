@article{ WOS:000591378200001,
Author = {Lu, Zhenyong and Wang, Yue and Hasebe, Yasushi and Zhang, Zhiqiang},
Title = {{Electrochemical Sensing Platform Based on Lotus Stem-derived Porous
   Carbon for the Simultaneous Determination of Hydroquinone, Catechol and
   Nitrite}},
Journal = {{ELECTROANALYSIS}},
Year = {{2021}},
Volume = {{33}},
Number = {{4}},
Pages = {{956-963}},
Month = {{APR}},
Abstract = {{A simple and highly selective electrochemical sensor based on carbonized
   lotus stem (CLS) was developed for the simultaneous determination of
   hydroquinone (HQ), catechol (CC), and nitrite (NT) by using cyclic
   voltammetry (CV) and amperometry (AMP) methods. The CLS was
   characterized by the methods including field emission scanning electron
   microscopy (FE-SEM), Raman spectrum, FT-IR spectrum and X-ray
   diffraction (XRD). Brunauer-Emmett-Teller (BET) method was used to
   evaluate the pore structure and surface area of CLS. The oxidation peaks
   for HQ (116.2 mV), CC (220.1 mV), and NT (818.9 mV) were well separated
   under optimized conditions, which improved their simultaneous
   determination. The CLS modified electrode showed a good linear range
   between 1.0x10 (-6) to 7.0x10 (-4) M for HQ, and the detection limit was
   calculated as 0.15 mu M. For CC the linear relationship was 1.0x10 (-6)
   to 3.0x10 (-3) M with the detection limit of 0.11 mu M. For NT the
   linear relationship was 5.0x10 (-7) to 4.0x10 (-3) M with the detection
   limit of 0.09 mu M. The results indicated that the intrinsic structure
   of natural biomass can be expected to design porous carbon for
   electrochemical sensors.}},
