@article{ WOS:000282646400039,
Author = {Wang, Jianghua and He, Duanwei and Duffy, Thomas S.},
Title = {{Stress state of diamond and gold under nonhydrostatic compression to 360
   GPa}},
Journal = {{JOURNAL OF APPLIED PHYSICS}},
Year = {{2010}},
Volume = {{108}},
Number = {{6}},
Month = {{SEP 15}},
Abstract = {{Diamond and gold powders were compressed nonhydrostatically in a diamond
   anvil cell and examined by x-ray diffraction using a radial geometry to
   evaluate the evolution of stresses and strains in these materials to
   ultrahigh pressure. We found that near isostrain continuity developed
   across diamond and gold grains under uniaxial compression. The observed
   mean pressure of diamond powder reached to 360(40) GPa while it was only
   31(1) GPa for the polycrystalline gold under the highest load.
   Polycrystalline diamond can support a microscopic deviatoric stress of
   160(18) GPa at about 360 GPa. Due to the deformation of the diamond
   anvil culet, the macroscopic differential stress of the diamond sample
   was limited to about 43(8) GPa. There is no evidence that the diamond
   grains have yielded in our experimental pressure range. (c) 2010
   American Institute of Physics. {[}doi:10.1063/1.3485828]}},
