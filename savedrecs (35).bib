@article{ WOS:000233439100023,
Author = {Brion, E and Akulin, VM and Dumer, I and Harel, G and Kurizki, G},
Title = {{Coherence protection by random coding}},
Journal = {{JOURNAL OF OPTICS B-QUANTUM AND SEMICLASSICAL OPTICS}},
Year = {{2005}},
Volume = {{7}},
Number = {{10, SI}},
Pages = {{S353-S355}},
Month = {{OCT}},
Abstract = {{We show that the multidimensional Zeno effect combined with
   non-holonomic control allows one to efficiently protect quantum systems
   from decoherence by a method similar to classical random coding. The
