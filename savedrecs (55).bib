@article{ WOS:A1995RX30000012,
Author = {SINGH, A and SINGH, SP and BAMEZAI, R},
Title = {{POSTNATAL MODULATION OF HEPATIC BIOTRANSFORMATION SYSTEM ENZYMES VIA
   TRANSLACTATIONAL EXPOSURE OF F1 MOUSE PUPS TO TURMERIC AND CURCUMIN}},
Journal = {{CANCER LETTERS}},
Year = {{1995}},
Volume = {{96}},
Number = {{1}},
Pages = {{87-93}},
Month = {{SEP 4}},
Abstract = {{The potential for the transfer of active principle(s) of turmeric
   (Curcuma longa L.) and curcumin (major pigment in turmeric) via
   translactational route and its modulatory influence on the hepatic
   biotransformation system enzymes in the lactating darns and their
   suckling offspring was assessed. Turmeric (4 g/kg b.w. per day) and
   curcumin (0.4 g/kg b.w. per day) induced significant (P < 0.01) increase
   in the hepatic levels of glutathione S-transferase (GST) and acid
   soluble sulfhydryl (-SH) after 14 or 21 days treatment in lactating dams
   and translactationally exposed F-1 pups. However the lower dose of
   curcumin (0.2 g/kg b.w. per day) could modulate hepatic GST activity (P
   < 0.05) and -SH (P < 0.01) only after 21 days of treatment in darns and
   pups. Cytochrome b(5) and cytochrome P450 levels were significantly
   elevated (P < 0.05) in the dams as well as their suckling pups of both
   14 and 21 days age groups at the selected dose levels of turmeric (4
   g/kg b.w.) and curcumin (0.4 g/kg b.w.). The induction in hepatic
   biotransformation system enzymes in lactating dams and F-1 progeny
   suggests the passage of active constituents and/or metabolites of
   turmeric and curcumin via the translactational route.}},
