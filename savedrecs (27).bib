@article{ WOS:000282231100011,
Author = {Ergas, Sarina J. and Sengupta, Sukalyan and Siege, Ryan and Pandit, Arka
   and Yao, Yifu and Yuan, Xin},
Title = {{Performance of Nitrogen-Removing Bioretention Systems for Control of
   Agricultural Runoff}},
Journal = {{JOURNAL OF ENVIRONMENTAL ENGINEERING-ASCE}},
Year = {{2010}},
Volume = {{136}},
Number = {{10}},
Pages = {{1105-1112}},
Month = {{OCT}},
Abstract = {{This research evaluated nitrogen-removing bioretention systems for
   control of nutrients, organics, and solids in agricultural runoff.
   Pilot-scale experiments were conducted with bioretention systems
   incorporating aerobic nitrification and anoxic denitrification zones
   with sulfur or wood chips as denitrification substrates. Varying
   hydraulic loading rates (HLRs), influent concentrations, and wetting and
   drying periods were applied to the units during laboratory and two
   seasons of field tests with dairy farm runoff. Total N removal
   efficiencies greater than 88\% were observed in both units with
   synthetic storm water. In first-season field tests, moderate removal
   efficiencies were observed for chemical oxygen demand (46\%), suspended
   solids (69\%), total phosphorous (TP) (66\%), and total N (65\%). During
   the second season, operational changes in the farm resulted in lower
   organic, solids, and nutrient loadings resulting in improved effluent
   quality, especially for suspended solids (81\% removal) and total N
   (82\% removal). The systems were not hydraulically overloaded even at 20
   times the normal HLR.}},
