@article{ WOS:000368573900040,
Author = {Hue, Ryan J. and Tonks, Ian A.},
Title = {{Ethylene Polymerizations Using Parallel Pressure Reactors and a Kinetic
   Analysis of Chain Transfer Polymerization}},
Journal = {{JOVE-JOURNAL OF VISUALIZED EXPERIMENTS}},
Year = {{2015}},
Number = {{105}},
Month = {{NOV}},
Abstract = {{We demonstrate a method for high-throughput catalyst screening using a
   parallel pressure reactor starting from the initial synthesis of a
   nickel alpha-diimine ethylene polymerization catalyst. Initial
   polymerizations with the catalyst lead to optimized reaction conditions,
   including catalyst concentration, ethylene pressure and reaction time.
   Using gas-uptake data for these reactions, a procedure to calculate the
   initial rate of propagation (k(p)) is presented. Using the optimized
   conditions, the ability of the nickel a-diimine polymerization catalyst
   to undergo chain transfer with diethylzinc (ZnEt2) during ethylene
   polymerization was investigated. A procedure to assess the ability of
   the catalyst to undergo chain transfer (from molecular weight and C-13
   NMR data), calculate the degree of chain transfer, and calculate chain
   transfer rates (k(e)) is presented.}},
