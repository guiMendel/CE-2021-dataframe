@article{ WOS:000598170200006,
Author = {Li, Yanan and Wang, Jiang and Tong, Wangshu and Zhang, Shaojie and Wang,
   Zhihao and An, Qi and Zhang, Yihe},
Title = {{Enhanced dielectric properties of halloysite/PVDF-HFP modified by Li-ion
   realizing superior energy conversion ability}},
Journal = {{CHEMICAL PHYSICS LETTERS}},
Year = {{2020}},
Volume = {{761}},
Month = {{DEC 16}},
Abstract = {{High dielectric material has attracted interest in applications of
   triboelectric nanogenerators for its high charge-trapping capability. We
   reported the enhancement of both dielectric and piezoelectric properties
   by adding modified halloysite (HAL) into PVDF-HFP. Due to the combined
   effect of triboelectric generation by enhanced dielectric constant and
   piezoelectric generation by increase of beta-phase, the HAL/PVDF-HFP
   composites film modified by Li+ and water exhibited superior generated
   current of 9.23 x 10(7) A compared to the pure PVDF-HFP with 1.65 x
   10(-7) A and unmodified HAL/PVDF-HFP with 2.68 x 10(-7) A. This work is
   expected to inspire the design and fabrication of high dielectric and
   piezoelectric material for nanogenerators.}},
