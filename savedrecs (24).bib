@article{ WOS:000286570100005,
Author = {Kohn, Karsten and Ullrich, Katrin},
Title = {{Do Women Start Smaller Businesses? Opening Up the Black Box of Start-up
   Size}},
Journal = {{BETRIEBSWIRTSCHAFTLICHE FORSCHUNG UND PRAXIS}},
Year = {{2010}},
Volume = {{62}},
Number = {{6}},
Pages = {{654-681}},
Month = {{NOV-DEC}},
Abstract = {{Women not only start fewer, but also different businesses than men. Our
   paper scrutinizes gender differences in start-up size. The decision
   about start-up size is modeled as a sequential process along four
   dimensions: (1) extent of self-employment, (2) industry choice, (3) the
   number of hired workers and team partners, and (4) financing demand.
   Using individual-level data from the KfW Start-up Monitor, a large-scale
   population survey on start-up activity in Germany, we estimate the
   different stages of the sequential process. When controlling for a broad
   set of socio-demographic characteristics of the entrepreneur, gender
   differences prevail with respect to the extent of self-employment, the
   choice of industry, and the decision to hire employees. Yet there is no
   direct gender effect with respect to start-up financing. Financing
   demand is driven by the predetermined dimensioning of the start-up
   project rather than by personal characteristics.}},
