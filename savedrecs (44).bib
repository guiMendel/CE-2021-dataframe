@article{ WOS:000089766200009,
Author = {Gill, AM and Leigh, DE},
Title = {{Community college enrollment, college major, and the gender wage gap}},
Journal = {{INDUSTRIAL \& LABOR RELATIONS REVIEW}},
Year = {{2000}},
Volume = {{54}},
Number = {{1}},
Pages = {{163-181}},
Month = {{OCT}},
Abstract = {{The literature on the narrowing of the gender wage gap during the 1980s
   considers, among other factors, the closing of the male-female
   differential in post-secondary education. This paper looks specifically
   at the role played by the dramatic relative increase in women's
   enrollment in two-year colleges. With independent cross-sections
   developed using NLSY data, the authors find that the gender wage gap
   narrowed by 0.0469 log points between 1985 and 1990 and by 0.0932 log
   points between 1989 and 1994. The more pronounced decrease observed for
   1989-94 is largely explained by erosion of male-female differences in
   weeks worked, job tenure, and full-time employment. A more novel finding
   is evidence that while change in the quantity of education provides
   essentially no explanatory power, disaggregating education by two-year
   and four-year providers and by major field of study accounts for
   8.5-11\% of the closing of the wage gap over the 1989-94 period.}},
