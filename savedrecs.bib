@article{ WOS:000603569300005,
Author = {Zhang, Zhimo and Cai, Min and Li, Rui and Meng, Fanqi and Zhang, Qinghua
   and Gu, Lin and Ye, Zijin and Xu, Gang and Fu, Ying-Shuang and Zhang,
   Wenhao},
Title = {{Controllable synthesis and electronic structure characterization of
   multiple phases of iron telluride thin films}},
Journal = {{PHYSICAL REVIEW MATERIALS}},
Year = {{2020}},
Volume = {{4}},
Number = {{12}},
Month = {{DEC 29}},
Abstract = {{We present an investigation on the controlled growth of epitaxial iron
   telluride films of multiple phases by molecular beam epitaxy. By
   optimizing the substrate temperature, we fabricate different phases of
   alpha-FeTe, beta-FeTe, and FeTe2, respectively, whose crystalline
   morphologies are determined by means of in situ scanning tunneling
   microscopy/spectroscopy and ex situ scanning transmission electron
   microscopy. While both alpha- and beta-FeTe films are metallic, we
   uncover a similar to 185- and 65-mV semiconducting band gap for the
   (100) and (011) facets of FeTe2 film, respectively, with the former one
   being compatible with the first principles calculations. Moreover, for
   FeTe2, we observe reduced gaps with enhanced conductance in the vicinity
   of edge boundaries, which are dependent on the geometries of step
   orientation and possibly in correlation to the magnetic anisotropy with
   structural distortion. Our study provides insight into the controllable
   synthesis of Fe-Te compounds with variation of stoichiometries and
   surface terminations, which may generalize to other epitaxial Fe
   chalcogenide films.}},
