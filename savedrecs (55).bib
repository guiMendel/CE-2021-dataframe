@article{ WOS:A1995RW03400024,
Author = {JOHNSON, MA and TYACK, SG},
Title = {{MOLECULAR EVOLUTION OF INFECTIOUS LARYNGOTRACHEITIS VIRUS (ILTV,
   GALLID-HERPESVIRUS-1) - AN ANCIENT EXAMPLE OF THE ALPHAHERPESVIRIDAE}},
