@article{ WOS:000373842700001,
Author = {Wu, Chi-Fang and Chiou, Jian-Geng and Minakova, Maria and Woods,
   Benjamin and Tsygankov, Denis and Zyla, Trevin R. and Savage, Natasha S.
   and Elston, Timothy C. and Lew, Daniel J.},
Title = {{Role of competition between polarity sites in establishing a unique
   front}},
Journal = {{ELIFE}},
Year = {{2015}},
Volume = {{4}},
Month = {{NOV 2}},
Abstract = {{Polarity establishment in many cells is thought to occur via positive
   feedback that reinforces even tiny asymmetries in polarity protein
   distribution. Cdc42 and related GTPases are activated and accumulate in
   a patch of the cortex that defines the front of the cell. Positive
   feedback enables spontaneous polarization triggered by stochastic
   fluctuations, but as such fluctuations can occur at multiple locations,
   how do cells ensure that they make only one front? In polarizing cells
   of the model yeast Saccharomyces cerevisiae, positive feedback can
   trigger growth of several Cdc42 clusters at the same time, but this
   multi-cluster stage rapidly evolves to a single-cluster state, which
   then promotes bud emergence. By manipulating polarity protein dynamics,
   we show that resolution of multi-cluster intermediates occurs through a
   greedy competition between clusters to recruit and retain polarity
   proteins from a shared intracellular pool.}},
