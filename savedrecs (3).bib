@article{ WOS:000600753400008,
Author = {Jackowicz, Krzysztof and Kozlowski, Lukasz and Podgorski, Blazej and
   Winkler-Drews, Tadeusz},
Title = {{Do political connections shield from negative shocks? Evidence from
   rating changes in advanced emerging economies}},
Journal = {{JOURNAL OF FINANCIAL STABILITY}},
Year = {{2020}},
Volume = {{51}},
Month = {{DEC}},
Abstract = {{In this study, we examine whether political connections affect market
   reactions to rating changes. Using a new and comprehensive dataset on
   ten advanced emerging markets, we find that political connections
   attenuate negative reactions linked to rating downgrades. This effect is
   shaped by political ties involving influential politicians and concerns
   all sample firms and a subsample of nonfinancial companies. Moreover, we
   establish that state-owned companies or companies with significant state
   ownership do not benefit from the mitigation effect of political
   connections. (C) 2020 Elsevier B.V. All rights reserved.}},
