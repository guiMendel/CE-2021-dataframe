@article{ WOS:A1995TB46700098,
Author = {LEON, J and SHULAEV, V and YALPANI, N and LAWTON, MA and RASKIN, I},
Title = {{BENZOIC-ACID 2-HYDROXYLASE, A SOLUBLE OXYGENASE FROM TOBACCO, CATALYZES
