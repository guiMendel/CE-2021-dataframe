@article{ WOS:000599934000016,
Author = {Li, Ning and Lv, Xin and Xu, Shoukun and Li, Bo and Gu, Yuwan},
Title = {{An Improved Water Surface Images Segmentation Algorithm Based on the
   Otsu Method}},
Journal = {{JOURNAL OF CIRCUITS SYSTEMS AND COMPUTERS}},
Year = {{2020}},
Volume = {{29}},
Number = {{15}},
Month = {{DEC 15}},
Abstract = {{The one-dimensional Otsu method is an adaptive threshold method. It
   obtains the optimal threshold for image segmentation by the maximum
   between-class variance, without considering the minimum within-class
   variance. As the background of water surface image is mostly uniform,
   using this feature, the threshold selection tactics adopt the
   combination of the one-dimensional Otsu method and the uniformity
   measurement, proposes the threshold segmentation method based on
   uniformity measurement, and adopts the performance evaluation method
   based on GT image to compare the segmentation result. Experimental
   results demonstrate that effectiveness of the improved Otsu method is
   generally better than the traditional Otsu method, and the other four
   commonly used threshold segmentation methods for the water surface
   image, which improves the segmentation accuracy of such images and
   reduces the segmentation error rate. At the same time, as the water
   surface image is usually affected by light intensity, water ripple and
   other factors, this paper also adopts the relevant correction algorithm
   to further improve the segmentation accuracy.}},
