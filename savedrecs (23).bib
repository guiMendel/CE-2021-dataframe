@article{ WOS:000286612100029,
Author = {Barros, Fabio S. M. and Honorio, Nildimar A. and Arruda, Mercia E.},
Title = {{Mosquito Anthropophily: Implications on Malaria Transmission in the
   Northern Brazilian Amazon}},
Journal = {{NEOTROPICAL ENTOMOLOGY}},
Year = {{2010}},
Volume = {{39}},
Number = {{6}},
Pages = {{1039-1043}},
Month = {{NOV-DEC}},
Abstract = {{Seasonal variation of adults and larvae of anophelines was studied
   during 2003 and 2004 in Roraima State, located in the Northern region of
   Brazilian Amazon. Species diversity increased with distance of capture
   to human dwellings. Greater diversity was found in extradomiciliary
   collections than in peridomiciliary or intradomiciliary. A significant
   association between Anopheles darlingi Root and An. albitarsis (s.1.)
   Arribalzaga (Diptera: Culicidae) breeding sites and the proximity to
   human dwellings was observed. Malaria Sporozoite Antigen Panel Assay
   (Vectest (TM) Malaria) indicated An. albitarsis s.1. as one of the local
   vectors in the studied area. In this study, an index to describe the
   anthropophilic behavior of each anopheline species is proposed.}},
