@article{ WOS:000233957600007,
Author = {Wenzel, A and Haugen, EN and Goyette, M},
Title = {{Sexual adjustment in postpartum women with generalized anxiety disorder}},
Journal = {{JOURNAL OF REPRODUCTIVE AND INFANT PSYCHOLOGY}},
Year = {{2005}},
Volume = {{23}},
Number = {{4}},
Pages = {{365-366}},
Month = {{NOV}},
Abstract = {{In a sample of 80 community women who were approximately 10 weeks
   postpartum, 10 (12.5\%) were diagnosed with Generalized Anxiety Disorder
   (GAD). Women with GAD reported similar levels of sexual desire, arousal,
   activity, and orgasm in the context of intercourse as women without GAD,
   although they reported higher levels of body image self-consciousness
   and sexual fear and avoidance. Results suggest that postpartum GAD is
   associated with distress, but not dysfunction, in women's sexual
   relationships.}},
