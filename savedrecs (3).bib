@article{ WOS:000591780400013,
Author = {Jin, N. and Zhu, Y. and Geng, Z. and Fedkiw, R.},
Title = {{A Pixel-Based Framework for Data-Driven Clothing}},
Journal = {{COMPUTER GRAPHICS FORUM}},
Year = {{2020}},
Volume = {{39}},
Number = {{8}},
Pages = {{135-144}},
Month = {{DEC}},
Abstract = {{We propose a novel approach to learning cloth deformation as a function
   of body pose, recasting the graph-like triangle mesh data structure into
   image-based data in order to leverage popular and well-developed
   convolutional neural networks (CNNs) in a two-dimensional Euclidean
   domain. Then, a three-dimensional animation of clothing is equivalent to
   a sequence of two-dimensional RGB images driven/choreographed by time
   dependent joint angles. In order to reduce nonlinearity demands on the
   neural network, we utilize procedural skinning of the body surface to
   capture much of the rotation/deformation so that the RGB images only
   contain textures of displacement offsets from skin to clothing. Notably,
   we illustrate that our approach does not require accurate unclothed body
   shapes or robust skinning techniques. Additionally, we discuss how
   standard image based techniques such as image partitioning for higher
   resolution can readily be incorporated into our framework.}},
