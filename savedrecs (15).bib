@article{ WOS:000364629300007,
Author = {Ashcroft, Helen O. and Wood, Bernard J.},
Title = {{An experimental study of partial melting and fractional crystallization
   on the HED parent body}},
Journal = {{METEORITICS \& PLANETARY SCIENCE}},
Year = {{2015}},
Volume = {{50}},
Number = {{11}},
Pages = {{1912-1924}},
Month = {{NOV}},
Abstract = {{We have performed an experimental and modeling study of the partial
   melting behavior of the HED parent body and of the fractional
   crystallization of liquids derived from its mantle. We estimated the
   mantle composition by assuming chondritic ratios of refractory
   lithophile elements, adjusting the Mg\# and core size to match the
   density and moment of inertia of Vesta, and the compositions of Mg-rich
   olivines found in diogenites. The liquidus of a mantle with Mg\#
   (=100{*}{[}Mg/(Mg+Fe)]) 80 is similar to 1625 degrees C and, under
   equilibrium conditions, the melt crystallizes olivine alone until it is
   joined by orthopyroxene at 1350 degrees C. We synthesized the melt from
   our 1350 degrees C experiment and simulated its fractional
   crystallization path. Orthopyroxene crystallizes until it is replaced by
   pigeonite at 1200 degrees C. Liquids become eucritic and crystal
   assemblages resemble diogenites below 1250 degrees C. MELTS correctly
   predicts the olivine liquidus but overestimates the orthopyroxene
   liquidus by similar to 70 degrees C. Predicted melt compositions are in
   reasonable agreement with those generated experimentally. We used MELTS
   to determine that the range of mantle compositions that can produce
   eucritic liquids and diogenitic solids in a magma oceanmodel is Mg\#
   75-80 (with chondritic ratios of refractory elements). A mantle with
   Mg\# similar to 70 can produce eucrites and diogenites through
   sequential partial melting.}},
