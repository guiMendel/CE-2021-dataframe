@article{ WOS:000363530100001,
Author = {Petrovici, A. and Andrei, O.},
Title = {{Stellar weak interaction rates and shape coexistence for Se-68 and Kr-72
   waiting points}},
Journal = {{EUROPEAN PHYSICAL JOURNAL A}},
Year = {{2015}},
Volume = {{51}},
Number = {{10}},
Month = {{OCT 28}},
Abstract = {{Beyond-mean-field results on stellar beta-decay and continuum electron
   capture rates for the rp-process waiting point nuclei Se-68 and Kr-72 at
   densities rho Y-e = 10(4)-10(7) mol/cm(3) and temperatures T =
   10(8)-10(10) K are presented. The structure of the even-even parent and
   odd-odd daughter nuclei dominated by shape coexistence and mixing is
   self-consistently described within the complex Excited Vampir model
   using realistic effective interactions in rather large model spaces. The
   influence of the shape mixing in the structure of the low-lying parent
   states as well as in the independently calculated daughter states on the
   stellar weak interaction rates is discussed.}},
