@article{ WOS:A1995TV03700006,
Author = {Zatsepina, OV and Zhelev, NZ and Jordan, EG},
Title = {{Immunolocation of nucleolar phosphoprotein B23 within mitotic
   centrosomes in cultured mammalian cells}},
Journal = {{MOLECULAR BIOLOGY}},
Year = {{1995}},
Volume = {{29}},
Number = {{6, 2}},
Pages = {{813-818}},
Month = {{NOV-DEC}},
Abstract = {{Indirect immunofluorescence assays with antibodies against the nucleolar
   protein B23 under conditions used to discern cytoskeletal elements
   revealed specific staining of mitotic centrosomes in various cultured
   mammalian cells. B23 appeared in the centrosomes after the breakdown of
   the nuclear envelope in prometaphase, and persisted up to the beginning
   of telophase. Morphologically the immunolabeled zone corresponded to the
   pericentriolar material. Interphase, prophase, or telophase centrosomes
   as well as the residual bodies were not stained with anti-B23
   antibodies. Upon cell exposure to nocodazole (0.1 mu g/ml, 2 h)
   destroying the mitotic spindle, B23 was uniformly distributed over the
   cytoplasm. after removal of the nocodazole block B23 was again found in
   the centrosomes. The possible significance of the centrosomal
   association of B23 in mitosis is discussed.}},
