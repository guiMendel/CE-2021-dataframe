@article{ WOS:A1995RW32600008,
Author = {HELDER, MN and OZKAYNAK, E and SAMPATH, KT and LUYTEN, FP and LATIN, V
   and OPPERMANN, H and VUKICEVIC, S},
Title = {{EXPRESSION PATTERN OF OSTEOGENIC PROTEIN-1 (BONE MORPHOGENETIC
   PROTEIN-7) IN HUMAN AND MOUSE DEVELOPMENT}},
Journal = {{JOURNAL OF HISTOCHEMISTRY \& CYTOCHEMISTRY}},
