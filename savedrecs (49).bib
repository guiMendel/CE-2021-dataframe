@article{ WOS:000088431300001,
Author = {Porter, NA and Zhang, GR and Reed, AD},
Title = {{Free radical addition to branched allylsilanes: stereoselective
   formation and elimination of beta bromosilanes}},
Journal = {{TETRAHEDRON LETTERS}},
Year = {{2000}},
Volume = {{41}},
Number = {{31}},
Pages = {{5773-5777}},
Month = {{JUL 29}},
Abstract = {{Lewis acid-promoted free radical addition to 3-silyl-1-butenes occurs in
   good yield to give predominantly the cis-substituted alkene. The
   transfer reaction occurs by atom-transfer addition of the alkyl bromide
