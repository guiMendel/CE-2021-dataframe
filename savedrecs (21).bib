@article{ WOS:000285325100013,
Author = {He, Dong-Mei and Kaleem, Imdad and Qin, Si-Ying and Dai, Da-Zhang and
   Liu, Gui-Yan and Li, Chun},
Title = {{Biosynthesis of glycyrrhetic acid 3-O-mono-beta-D-glucuronide catalyzed
   by beta-D-glucuronidase with enhanced bond selectivity in an ionic
   liquid/buffer biphasic system}},
Journal = {{PROCESS BIOCHEMISTRY}},
Year = {{2010}},
Volume = {{45}},
Number = {{12}},
Pages = {{1916-1922}},
Month = {{DEC}},
Abstract = {{The bond selective hydrolysis of glycyrrhizin (GL) to glycyrrhetic acid
   3-O-mono-beta-D-glucuronide (GAMG) catalyzed by recombinant
   beta-D-glucuronidase from Escherichia coli BL21 (PGUS-E) was
   successfully performed in an ionic liquid (IL)/buffer biphasic system.
   Five ILs were analyzed, however, a hydrophobic IL
   1-butyl-3-methylimidazolium hexafluorophosphate ({[}BMIM]PF6) showed the
   best bio-compatibility with PGUS-E. An obvious enhancement in the
   initial reaction rate, substrate conversion, GAMG yield and chemical
   bond selectivity (S-cb) was observed using 40\% (v/v) {[}BMIM]PF6/buffer
   as the reaction medium when compared to the acetate buffer medium. Under
   the optimized conditions (pH 6.0, temperature 50 degrees C, substrate
   concentration 6 mM and shaking speed 200 rpm), the initial reaction
   rate, the GAMG yield and the S-cb reached 3.15 mM h(-1), 74.36\% and
   98.12\%, respectively. The recyclability of {[}BMIM]PF6 was also studied
   and found to be reusable for five batches with high recovery percentage
   (>= 92\%). Furthermore, the desired product and byproduct were easily
   separated since they were distributed in different phases. Additionally,
   higher V-max (3.14 versus 2.24 mM h(-1)), lower apparent K-m (1.21
   versus 1.80 mM) and E-d (25.97 versus 32.60 kJ mol(-1)) were achieved in
   {[}BMIM]PF6/buffer biphasic system than that in monophasic buffer
   system. (C) 2010 Elsevier Ltd. All rights reserved.}},
