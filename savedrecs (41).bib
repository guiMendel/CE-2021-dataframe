@article{ WOS:000165883800013,
Author = {Shaikhaidarov, R and Volkov, AF and Takayanagi, H and Petrashov, VT and
   Delsing, P},
Title = {{Josephson effects in a superconductor-normal-metal mesoscopic structure
   with a dangling superconducting arm}},
Journal = {{PHYSICAL REVIEW B}},
Year = {{2000}},
Volume = {{62}},
Number = {{22}},
Pages = {{14649-14652}},
Month = {{DEC 1}},
Abstract = {{We studied a mesoscopic crosslike normal-metal structure connected to
   two superconducting (S) and two normal (N) reservoirs. We observed the
   Josephson effect under unusual conditions when there is no current
   through one of the two S/N interfaces. The potential difference between
