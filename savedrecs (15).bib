@article{ WOS:000367675600030,
Author = {Ahmadzadeh-Shooshtari, Babak and Golshan, Mohamad Esmail Hamedani and
   Sadeghkhani, Iman},
Title = {{A combined method to efficiently adjust frequency-based anti-islanding
   relays of synchronous distributed generation}},
Journal = {{INTERNATIONAL TRANSACTIONS ON ELECTRICAL ENERGY SYSTEMS}},
Year = {{2015}},
Volume = {{25}},
Number = {{11}},
Pages = {{3042-3059}},
Month = {{NOV}},
Abstract = {{An important requirement for interconnecting distributed generation is
   anti-islanding protection. This paper uses the capabilities of two
   methods, application region (AR) and power imbalance AR, to efficiently
   adjust frequency-based relays. The proposed method gives a proper simple
   and nonexpensive solution to the synchronous distributed generation
   islanding detection problem. It employs the common protection devices:
   frequency or rate of change of frequency relays. These relays are
   adjusted in order to satisfy the anti-islanding protection and safe and
   abnormal frequency variation requirements and also to discriminate
   between islanding and nonislanding disturbances. The procedure is
   applied to a real system, which is a portion of the Isfahan Regional
   Electric Network in Iran, to show the effectiveness of the proposed
   approach. Copyright (c) 2014 John Wiley \& Sons, Ltd.}},
