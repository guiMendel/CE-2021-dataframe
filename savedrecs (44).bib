@article{ WOS:000088841800024,
Author = {Zhu, SJ and Peng, LM and Moriya, T and Mutoh, Y},
Title = {{Effect of stress ratio on fatigue crack growth in TiAl intermetallics at
   room and elevated temperatures}},
Journal = {{MATERIALS SCIENCE AND ENGINEERING A-STRUCTURAL MATERIALS PROPERTIES
   MICROSTRUCTURE AND PROCESSING}},
Year = {{2000}},
Volume = {{290}},
Number = {{1-2}},
Pages = {{198-206}},
Month = {{OCT 15}},
Abstract = {{The fatigue crack growth behavior of gamma-based titanium aluminides
   (TiAl) with a fine duplex structure and lamellar structure has been
   investigated by scanning electron microscope (SEM) in situ observation
   in vacuum at 750 degrees C and room temperature. For the duplex
   structured material the fatigue crack growth rates are dominated by the
   maximum stress intensity, particularly at 750 degrees C. The threshold
   stress intensity range for fatigue crack growth at 750 degrees C is
   lower than that at room temperature for any corresponding stress ratio.
   The fatigue crack growth rate at 750 degrees C is affected by creep
   deformation in front of the crack tip. The severe crack blunting occurs
   when the stress ratio is 0.5. For the lamellar structured material the
   scatter of fatigue crack growth data is very large. Small cracks
   propagate at the stress intensity range below the threshold for long
   fatigue crack growth. The effects of microstructure on fatigue crack
   growth are discussed. (C) 2000 Elsevier Science S.A. All rights
   reserved.}},
