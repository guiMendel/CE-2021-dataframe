@article{ WOS:000089844800013,
Author = {Kohler, K and Reinacher, M},
Title = {{Epidemiology of tumors and tumor like lesions of the endocrine system
   and the gonads of necropsied dogs}},
Journal = {{TIERAERZTLICHE PRAXIS AUSGABE KLEINTIERE HEIMTIERE}},
Year = {{2000}},
Volume = {{28}},
Number = {{5}},
Pages = {{305-310}},
Month = {{SEP}},
Abstract = {{Prevalence of tumors and tumor-like lesions of the gonads and the
   endocrine system in 1500 dogs necropsied in the years 1997 to 1999 is
   estimated. Such tumors form the second most frequent type of neoplasias
   after lymphomas, even if nodular hyperplasias are not included. Nodular
   hyperplasia of the adrenal glands is the most often occurring
   proliferative lesion followed by interstitial cell tumors in the testis.
