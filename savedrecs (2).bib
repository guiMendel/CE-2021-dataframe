@article{ WOS:000583921700009,
Author = {Chen, Qiuxia and Hall, Justin and Foley, Timothy L. and Wan, Jinqiao and
   Li, You and Israel, David I.},
Title = {{A method for estimating binding affinity from primary DEL selection data}},
Journal = {{BIOCHEMICAL AND BIOPHYSICAL RESEARCH COMMUNICATIONS}},
Year = {{2020}},
Volume = {{533}},
Number = {{2}},
Pages = {{249-255}},
Month = {{DEC 3}},
Abstract = {{DEL selections are binding assays conducted with mixtures of chemically
   diverse DNA-tagged ligands and a screening target. DEL selections use
   DNA sequence counts to measure target binding, where ideally higher
   affinity ligands will have higher counts than weaker affinity ligands.
   However, there is not always a clear relationship between DNA sequence
   count (assay signal) and binding affinity. This disconnect may be due to
   the fidelity of library chemistry, where reactions often do not go to
   completion, and also to repetitive rounds of binding and elution that
   are standard practice in most DEL selection experiments. We describe
   here a strategy that addresses both of these issues and provides a means
   to calculate ligand affinity from primary selection data. The reaction
   yields of selected compounds during DEL library synthesis can also be
   predicted with this method. (C) 2020 Published by Elsevier Inc.}},
