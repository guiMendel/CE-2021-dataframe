@article{ WOS:A1995RP37000001,
Author = {INOUE, H and SONODA, T and MORITA, K and MAKISHIMA, A},
Title = {{DISPERSION AND SOLUBILITY OF 1,4-DIHYDROXYANTHRAQUINONE (C14H8O4) IN A
   SILICA-GEL}},
Journal = {{PHYSICS AND CHEMISTRY OF GLASSES}},
Year = {{1995}},
Volume = {{36}},
Number = {{4}},
Pages = {{155-159}},
Month = {{AUG}},
Abstract = {{SiO2 gels doped with 1,4-dihydroxyanthraquinone (DAQ) were prepared by
   the sol-gel process at 20, 40 and 60 degrees C. The amounts of remaining
   chemical groups, such as ethanol, water and ethoxy groups, were
   dependent upon the preparation temperature. The gel prepared at 20
   degrees C contained large amounts of residual ethanol and water and
   those prepared at 60 degrees C contained a large amount of ethoxy
   groups. Gels prepared at 40 degrees C had the minimum residual chemicals
   and chemical groups in the present system. A clear and transparent gel
   doped with the DAQ molecule could be made from a particular
   concentration. Concentration as an apparent solubility of the DAQ
   molecule in the gels was dependent on preparation temperature. The
   apparent solubility was strongly related to the solubility in the
   residual solution around the gelation point. The lifetime of the excited
   state of the DAQ molecule in the gels was also dependent on the
   preparation temperature It was considered that the decrease in the
   lifetime was due to quenching by the interaction between the DAQ
   molecules and the limit of the monomeric dispersion was strongly related
   to the interaction of the remaining chemical groups.}},
