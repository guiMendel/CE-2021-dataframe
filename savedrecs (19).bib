@article{ WOS:000358468700014,
Author = {Andrews, Matt},
Title = {{Explaining Positive Deviance in Public Sector Reforms in Development}},
Journal = {{WORLD DEVELOPMENT}},
Year = {{2015}},
Volume = {{74}},
Pages = {{197-208}},
Month = {{OCT}},
Abstract = {{Public sector reforms are commonplace in developing countries. Much of
   the literature about these reforms reflects on their failures. This
   paper asks about the successes and investigates which of two competing
   theories best explain why some reforms are positive deviants:
   ``solution- and leader-driven change{''} (SLDC) and ``problem-driven
   iterative adaptation{''} (PDIA). The theories are used to analyze data
   emerging from a case survey involving thirty cases from Princeton
   University's Innovations for Successful Society (ISS) program. The bulk
   of evidence from this study supports a PDIA explanation, but there is
   reason to believe that SLDC hypotheses also have value. (C) 2015
   Published by Elsevier Ltd.}},
Publisher = {{PERGAMON-ELSEVIER SCIENCE LTD}},
Address = {{THE BOULEVARD, LANGFORD LANE, KIDLINGTON, OXFORD OX5 1GB, ENGLAND}},
Type = {{Article}},
