@article{ WOS:000284420900004,
Author = {Hasegawa, Tomoyuki and Oda, Keiichi and Wada, Yasuhiro and Sato, Yasushi
   and Yamada, Takahiro and Yoshida, Eiji and Murayama, Hideo and Saito,
   Kyoko and Takeda, Tohoru and Kikuchi, Kei},
Title = {{A practical method of determining cross-calibration factors of PET
   scanners by moving a point-like Na-22 radioactive source}},
Journal = {{ANNALS OF NUCLEAR MEDICINE}},
Year = {{2010}},
Volume = {{24}},
Number = {{9}},
Pages = {{655-661}},
Month = {{NOV}},
Abstract = {{Thus far, cylindrical phantoms with F-18 or Ge-68/Ga-68 have been used
   in the standard techniques for determining the cross-calibration factors
   (CCFs) of PET scanners. This paper describes a new practical method that
   uses a point-like Na-22 radioactive source for determining CCFs.
   A point-like Na-22 radioactive source (about 700 kBq) was equipped with
   a spherical aluminium absorber capsule to ensure the symmetry of the
   emitted photons. During measurements, the source was moved in the axial
   direction to cover the whole axial field of view (FOV) of a clinical PET
   scanner, SET-2400 W. The region-of-interest (ROI) values obtained in
   reconstructed images without scatter and attenuation corrections were
   used to calculate the CCFs of the PET scanner.
   The CCFs obtained by the proposed method agreed with those obtained by
   the standard cross-calibration (CC) method within a precision of 1.5\%
   (sigma). The temporal variations in the CCFs by the standard CC method
   were reproduced by the proposed method with a precision better than 1\%.
   The CC method with a moving Na-22 point-like radioactive source is
   practically useful for determining the CCFs of PET scanners and
   monitoring their variations.}},
