@article{ WOS:000233180600030,
Author = {Gaillard, C and Billard, I and Chaumont, A and Mekki, S and Ouadi, A and
   Denecke, MA and Moutiers, G and Wipff, G},
Title = {{Europium(III) and its halides in anhydrous room-temperature
   imidazolium-based ionic liquids: A combined TRES, EXAFS, and molecular
   dynamics study}},
Journal = {{INORGANIC CHEMISTRY}},
Year = {{2005}},
Volume = {{44}},
Number = {{23}},
Pages = {{8355-8367}},
Month = {{NOV 14}},
Abstract = {{Combining spectroscopic techniques (TRES and EXAFS) and molecular
   dynamics simulations, we have investigated the state of trivalent
   europium dissolved in room-temperature ionic liquids (RTILs), as a
   function of the RTIL anion and in the presence of added chloride anions.
   The studied RTILs are based on the 1-butyl-3-methyl-imidazolium
   (Bumi(+)) cation and differ by their anionic counterparts: BF4- PF6- Tf-
   (triflate, CF3SO3-), and Tf2N- {[}(CF3SO2)(2)N (-)]. The results show
   the strong influence of the RTIL nature on the first solvation shell Of
   Europium and on its complexation with chloride. Depending on the RTIL,
   europium(III), which was introduced in solution as a triflate salt, is
   found to be solvated either by RTIL anions only or as neutral
