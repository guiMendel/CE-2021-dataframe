@article{ WOS:000420223800001,
Author = {Engelen, Adnan and Sugiyono and Budijanto, Slamet},
Title = {{Process and Formula Optimizations on Dried Sago (Metroxylon sagu) Noodle
   Processing}},
Journal = {{AGRITECH}},
Year = {{2015}},
Volume = {{35}},
Number = {{4}},
Pages = {{359-367}},
Month = {{NOV}},
Abstract = {{The aim of this research was to optimize the processing of dried sago
   noodle. Sago noodle was made through a process using twin screw extruder
   with the addition of GMS (Glycerol Monostearate) and ISP (Isolated
   Soybean Protein). Process optimation was performed with RSM (response
   surface methodology) using three process variables, i.e. extruder
   temperature (65-80 degrees C), GMS concentration (0-5\%), and ISP
   concentration (0-10\%). The optimum process condition was temperature of
   80 degrees C. GMS of 4.5\%, and ISP of 3.7\%. The optimum condition
   produced dried noodle having a hardness of 2499.62 gf, stickiness of
   235.12 gf, elongation of 168.96\% and cooking loss of 6.23\%.}},
