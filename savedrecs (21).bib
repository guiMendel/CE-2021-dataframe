@article{ WOS:000288974000018,
Author = {Liao, Isaac H. and Corbett, Blythe A. and Gilbert, Donald L. and Bunge,
   Silvia A. and Sharp, Frank R.},
Title = {{Blood gene expression correlated with tic severity in medicated and
   unmedicated patients with Tourette Syndrome}},
Journal = {{PHARMACOGENOMICS}},
Year = {{2010}},
Volume = {{11}},
Number = {{12, SI}},
Pages = {{1733-1741}},
Month = {{DEC}},
Abstract = {{Background: Tourette Syndrome (TS) has been linked to both genetic and
   environmental factors. Gene-expression studies provide valuable insight
   into the causes of TS; however, many studies of gene expression in IS do
   not account for the effects of medication. Materials \& methods: To
   investigate the effects of medication on gene expression in TS patients,
   RNA was isolated from the peripheral blood of 20 medicated TS subjects
   (MED) and 23 unmedicated TS subjects (UNMED), and quantified using
   whole-genome Affymetrix microarrays. Results: D2 dopamine receptor
   expression correlated positively with tic severity in MED but not UNMED.
   GABA(A) receptor epsilon subunit expression negatively correlated with
   tic severity in UNMED but not MED. Phenylethanolamine
   N-methyltransferase expression positively correlated with tic severity
