@article{ WOS:000286075700354,
Author = {Oya, G. and Matsumoto, Y. and Hironaga, R. and Irie, A.},
Title = {{The influence of inclination of magnetic field on Josephson-vortex flow
   in intrinsic Josephson junctions in (Bi1-xPbx)(2)Sr2CaCu2Oy
   single-crystal mesas}},
Journal = {{PHYSICA C-SUPERCONDUCTIVITY AND ITS APPLICATIONS}},
Year = {{2010}},
Volume = {{470}},
Number = {{1, SI}},
Pages = {{S827-S829}},
Month = {{DEC}},
Note = {{9th International Conference on Materials and Mechanisms of
   Superconductivity, Tokyo, JAPAN, SEP 07-12, 2009}},
Abstract = {{We study the influence of inclination of magnetic field on
   Josephson-vortex flow in intrinsic Josephson junctions in
   (Bi1-xPbx)(2)Sr2CaCu2Oy (x = 0.15) single-crystals between 30 and 55 K.
   Three kinds of behavior of Josephson vortices is observed on the I-V
   characteristics, due to the intersection of stacks of pancake vortices
   depending on the inclination angle theta of magnetic field: (a) At theta
   approximate to 0 degrees, the Josephson vortices collectively flow, (b)
   at 0 degrees <vertical bar theta vertical bar< 1.5 degrees, they are
   increasingly restrained with vertical bar theta vertical bar by stacks
   of pancake vortices, but noncollectively released from these by the bias
   current to the junctions, and (c) at vertical bar theta vertical bar >
   1.5 degrees, they are strongly restrained by the stacks of pancake
   vortices without released. (C) 2010 Elsevier B.V. All rights reserved.}},
