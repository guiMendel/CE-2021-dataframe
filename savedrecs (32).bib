Author = {Johnson, PV and Malone, CP and Kanik, I and Tran, K and Khakoo, MA},
Title = {{Integral cross sections for the direct excitation of the A
   (3)Sigma(+)(u), B (3)Pi(g), W (3)Delta(u), B ` (3)Sigma(-)(u), a ` 1
