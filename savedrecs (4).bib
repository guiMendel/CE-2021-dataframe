@article{ WOS:000596520700007,
Author = {Liu, Xiao and Gao, Libo and Zhao, Qian and Wang, Xiangmin and Yang, Chao
   and Bi, Junlong and Yang, Runhuan and Jin, Xiuli and Lan, Rui and Cui,
   Rongjun and Wang, Xiaochun and Li, Wenying and Wang, Xuesong and Yang,
   Ying and Yu, Xin and Lin, Yingbo and Liu, Jianping and Yin, Gefen},
Title = {{Inhibition of porcine reproductive and respiratory syndrome virus by PKC
   inhibitor dequalinium chloride in vitro}},
Journal = {{VETERINARY MICROBIOLOGY}},
Year = {{2020}},
Volume = {{251}},
Month = {{DEC}},
Abstract = {{As a severe disease characterized by reproductive failure and
   respiratory distress, porcine reproductive and respiratory syndrome
   (PRRS) is one of the most leading threats to the swine industry
   worldwide. Highly evolving porcine reproductive and respiratory syndrome
   virus (PRRSV) strains with distinct genetic diversity make the current
   vaccination strategy much less cost-effective and thus urge alternative
   protective host directed therapeutic approaches. RACK1-PKC-NF-kappa B
   signalling axis was suggested as a potential therapeutic target for PRRS
   control, therefore we tested the inhibitory effect of PKC inhibitor
   dequalinium chloride (DECA) on the PRRSV infection in vitro. RT-qPCR,
   western blot, Co-IP and cytopathic effect (CPE) observations revealed
   that DECA suppressed PRRSV infection and protected Marc-145 cells and
   porcine alveolar macrophages (PAMs) from severe cytopathic effects, by
   repressing the PKC alpha expression, the interaction between RACK1 and
   PKC alpha, and subsequently the NF-kappa B activation. In conclusion,
   the data presented in this study shed more light on deeper understanding
   of the molecular pathogenesis upon PRRSV infection and more importantly
   suggested DECA as a potential promising drug candidate for PRRS control.}},
