@article{ WOS:000281766300006,
Author = {Bhattacharya, Debojit and Shil, Suranjan and Misra, Anirban and Klein,
   D. J.},
Title = {{Intramolecular ferromagnetic coupling in bis-oxoverdazyl and
   bis-thioxoverdazyl diradicals with polyacene spacers}},
Journal = {{THEORETICAL CHEMISTRY ACCOUNTS}},
Year = {{2010}},
Volume = {{127}},
Number = {{1-2}},
Pages = {{57-67}},
Month = {{SEP}},
Abstract = {{We predict the intramolecular exchange coupling constant (J) for 10
   different oxo- and thioxo-verdazyl-based hi-spin ground-state diradicals
   with linear polyacene couplers of varying length using the broken
   symmetry approach in an unrestricted DFT framework. The magnetic
   characteristics of these systems are explained using the spin-density
   distribution, and an analysis is made by ``magnetic{''} orbitals. The
   nuclear independent chemical shift (NICS) values have been calculated
   for the diradicals. The average NICS(1) (1 above the ring surface) value
   per benzenoid ring increases as the size of the coupler increases.
   So-called Delta NICS(1) values {[}the difference among average NICS(1)
   per benzenoid ring in the coupler and the NICS(1) of the linear
   polyacene molecule] are correlated with J values. Bond orders and
   hyperfine coupling constants have also been evaluated and analyzed for
   the diradicals.}},
