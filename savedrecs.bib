@article{ WOS:000599671000001,
Author = {Lin, Wei-Rong and Chen, Hsin-Tong and Luo, Szu-Ting},
Title = {{Relationships among emotional blackmail, job frustration, turnover
   intention for tour leaders}},
Journal = {{ANATOLIA-INTERNATIONAL JOURNAL OF TOURISM AND HOSPITALITY RESEARCH}},
Year = {{2021}},
Volume = {{32}},
Number = {{2}},
Pages = {{262-273}},
Month = {{APR 3}},
Abstract = {{Due to the nature of their work, tour leaders need to interact closely
   with the tour members; thus, the former are often subjected to emotional
   blackmail by the latter. Therefore, this study investigated the
   relationship among tour leaders' perceived emotional blackmail, job
   frustration, and turnover intention. Licenced tour leaders were chosen
   as participants of this study and were asked to answer a
   self-administered survey questionnaire. A total of 355 valid
   questionnaires were collected at the end of the study period. The
   results showed that the higher the tour leaders' perceived blackmail,
   the higher the job frustration and the turnover intention are.
   Furthermore, based on the results, job frustration has a positive effect
   on turnover intention.}},
