@article{ WOS:000420940300007,
Author = {Schneider, Hartmut},
Title = {{Preliminary considerations on a reform of 211, 212 and 213 of the Penal
   Code (StGB)}},
Journal = {{FORENSISCHE PSYCHIATRIE PSYCHOLOGIE KRIMINOLOGIE}},
Year = {{2015}},
Volume = {{9}},
Number = {{4}},
Pages = {{245-250}},
Month = {{NOV}},
Abstract = {{Against the background of years of experience as a prosecutor for
   capital offences and as a Federal public prosecutor, the author
   justifies the necessity of murder criteria and a clearly differentiated
   maximum sentence in the form of condensed propositions. He argues
   against a solution using defined legal examples, which will slowly lead
   to abolition of the sentence of life imprisonment. Simultaneously, as a
   problem of the current regulations he names the inescapability of the
   absolute penalty in the presence of a murder criterion even then, when
   substantial factors reducing culpability are obvious and in this case
   pleads for a solution on the side of the legal consequences. He finishes
   with his own suggestion for revision of 211, 212 of the Penal Code
   (Strafgesetzbuch, StGB).}},
