@article{ WOS:000088542900003,
Author = {Sanders, NW and Mann, NH},
Title = {{Automated scoring of patient pain drawings using artificial neural
   networks: efforts toward a low back pain triage application}},
Journal = {{COMPUTERS IN BIOLOGY AND MEDICINE}},
Year = {{2000}},
Volume = {{30}},
Number = {{5}},
Pages = {{287-298}},
Month = {{SEP}},
Abstract = {{The goal of this research was to examine methods of automatically
   scoring patient pain drawings. Two hundred and fifty pain drawings were
   selected from the files of an orthopaedic surgeon who specializes in the
   treatment of low back pain patients. An artificial neural network was
   designed to score these drawings. The drawings were segmented into 85
   regions following dermatomal mappings and from these regions the percent
   area in pain in each was computed and used as the neural network input
   variables, With five outcome categories (scores) we obtained a
   classification sensitivity of 49\%, which is approximately as well as
   physician experts and discriminant analysis achieved using a subset of
   the same data. We conclude that an artificial neural network is well
   suited to automatically score patient pain drawings. (C) 2000 Elsevier
   Science Ltd. All rights reserved.}},
