@article{ WOS:A1995TF57600048,
Author = {RUSEK, K and CLARKE, NM and TUNGATE, G and WARD, RP},
Title = {{NONRESONANT BREAKUP EFFECTS IN (LI-6)OVER-RIGHT-ARROW+NI-58
   ELASTIC-SCATTERING AT 70.5 MEV}},
Journal = {{PHYSICAL REVIEW C}},
Year = {{1995}},
Volume = {{52}},
Number = {{5}},
Pages = {{2614-2619}},
Month = {{NOV}},
Abstract = {{Recently published data for the elastic scattering of 70.5 MeV polarized
   Li-6 by Ni-58 have been compared with the results of coupled-channels
   calculations. The diagonal and off-diagonal potentials used in the
   analysis were generated from phenomenological alpha + Ni-58 and (d) over
   right arrow + Ni-58 optical potentials via the cluster-folding method.
   Examined were the effects of projectile excitation to the three
   low-lying T=0 resonant states and to nonresonant continuum states in the
   energy range 0.63 MeV-10.03 MeV, relative to the (6Li) --> alpha + d
   breakup threshold at 1.47 MeV. In common with earlier analyses,
   calculations limited to the resonant excited states reproduced the data
   only when the interaction and coupling potentials were renormalized.
   However, the inclusion of the nonresonant excited states removed the
   need for this renormalization. Calculations including both the resonant
   and nonresonant states yielded a description of the differential cross
   section data superior to that resulting from calculations limited to the
   resonant states even when the latter were performed with renormalized
   potentials. Calculations of the tensor analyzing powers reproduced the
   data well and favored a near zero value for the spectroscopic amplitude
   of the D-state admixture in the Li-6 ground state cluster wave function.}},
