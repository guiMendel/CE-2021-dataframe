@article{ WOS:000363922900001,
Author = {Virtanen, Helena and Autio, Anu and Siitonen, Riikka and Liljenback,
   Heidi and Saanijoki, Tiina and Lankinen, Petteri and Makila, Jussi and
   Kakela, Meeri and Teuho, Jarmo and Savisto, Nina and Jaakkola, Kimmo and
   Jalkanen, Sirpa and Roivainen, Anne},
Title = {{Ga-68-DOTA-Siglec-9-a new imaging tool to detect synovitis}},
Journal = {{ARTHRITIS RESEARCH \& THERAPY}},
Year = {{2015}},
Volume = {{17}},
Month = {{NOV 3}},
Abstract = {{Introduction: Vascular adhesion protein-1 (VAP-1) is an adhesion
   molecule, which upon inflammation is rapidly translocated from
   intracellular sources to the endothelial cell surface. We have recently
   discovered that sialic acid-binding immunoglobulin-like lectin 9
   (Siglec-9) is a leukocyte ligand of VAP-1 and that Ga-68-labeled
   Siglec-9 motif peptide facilitates in vivo imaging of inflammation. This
   study evaluated the feasibility of Ga-68-DOTA-Siglec-9 positron emission
   tomography (PET) for the assessment of synovitis.
   Methods: Rabbits with synovial inflammation were injected with F-18-FDG
   or Ga-68-DOTA-Siglec-9 and studied by gamma counting and
   autoradiography. Certain rabbits were also examined with magnetic
   resonance imaging (MRI). After PET imaging, rabbits were intravenously
   administered with anti-VAP-1 antibody to evaluate luminal expression of
   VAP-1 by immunohistochemistry. Finally, binding of Siglec-9 peptide and
   VAP-1 positive vessels were evaluated by double staining of rheumatoid
   arthritis synovium.
   Results: Intra-articular injection of hemagglutinin induced mild
   synovial inflammation in rabbit knee with luminal expression of VAP-1.
   Synovitis was clearly visualized by Ga-68-DOTA-Siglec-9 PET in addition
   to F-18-FDG-PET and MRI. Compared with the F-18-FDG, the ex vivo
   inflamed-to-control synovium ratio of Ga-68-DOTA-Siglec-9 was similar
   (1.7 +/- 0.4 vs. 1.5 +/- 0.2, P = 0.32). Double staining revealed that
   Siglec-9 peptide binds to VAP-1 positive vessels in human rheumatoid
   synovium.
   Conclusion: Ga-DOTA-Siglec-9 PET tracer detected VAP-1 positive
   vasculature in the mild synovitis of rabbits comparable with F-18-FDG,
   suggesting its potential for in vivo imaging of synovial inflammation in
   patients with rheumatic diseases.}},
