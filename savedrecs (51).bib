@article{ WOS:A1995TH90400003,
Author = {MILLINGTON, JJ and CLARK, JD},
Title = {{THE CHARO ARRO CANYON-MOUTH SHEET SYSTEM, SOUTH-CENTRAL PYRENEES, SPAIN
   - A STRUCTURALLY INFLUENCED ZONE OF SEDIMENT DISPERSAL}},
