@article{ WOS:000166030300006,
Author = {Mghazli, S and Villemin, D and Mansour, M and Jaouad, A and Cherqaoui, D},
Title = {{Application of neural networks to quantitative structure-antifungal
   activity relationships of 1-{[}1-(substituted phenyl)vinyl]imidazoles}},
Journal = {{FRESENIUS ENVIRONMENTAL BULLETIN}},
Year = {{2000}},
Volume = {{9}},
Number = {{11-12}},
Pages = {{734-745}},
Month = {{NOV-DEC}},
Abstract = {{Structure-antifungal activity relationships were established for a
   sample of sixty-one 1-{[}1-(substituted phenyl)vinyl]imidazoles using a
   three-layer back-propagation neural network. The molecular structure was
   described using a common skeleton with four possible substitutions. The
   relationships between structure and activity were examined
   quantitatively using physicochemical and structural parameters. Neural
   networks proved to give better results than other models in the
   literature. The contribution of each descriptor to the
   structure-activity relationships was evaluated. Hydrophobicity of the
   molecule was thus found to take the most relevant part in the molecular
   description.}},
