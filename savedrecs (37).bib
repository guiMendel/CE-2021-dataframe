@article{ WOS:000231249200004,
Author = {Buunk, BP and Kuyper, H and van der Zee, YG},
Title = {{Affective response to social comparison in the classroom}},
Journal = {{BASIC AND APPLIED SOCIAL PSYCHOLOGY}},
Year = {{2005}},
Volume = {{27}},
Number = {{3}},
Pages = {{229-237}},
Month = {{SEP}},
Abstract = {{In a study among 609 secondary school students, the affective reactions
   to social comparisons of grades were examined. Overall, the students
   reported more frequent responses to upward than to downward comparison,
   more identification than contrast in their comparisons, and more
   frequent responses implying a self-focus than an other-focus. The most
   frequent response was self-focused identification with an upward
   comparison target, that is, the hope that one might in the future
   receive a good grade similar to that of the target. In general, girls
   showed more altruistic and empathic responses, and boys more egocentric
   and hostile responses to social comparison. Those with a low performance
   level responded more often to downward comparison with the fear that
   they would receive a similar low grade on a next test.}},
