@article{ WOS:000233737700004,
Author = {Kumar, V and Rani, A and Chauhan, GS},
Title = {{Quantitative and qualitative analysis of trypsin inhibitor during seed
   development in Indian soybean genotypes}},
Journal = {{JOURNAL OF FOOD SCIENCE AND TECHNOLOGY-MYSORE}},
Year = {{2005}},
Volume = {{42}},
Number = {{6}},
Pages = {{477-480}},
Month = {{NOV-DEC}},
Abstract = {{Four Indian soybean genotypes were investigated for the pattern of
   accumulation of trypsin inhibitor (TI) content during seed development
   and to determine the TI content at green pod consumption stage.. All the
   Genotypes exhibited varying levels of TI content at very early stage of
   seed development. This was substantiated by the presence of Kunitz
   inhibitor band, the polypeptide component which constitutes about 80\%
   of TI activity, at 30 days after flowering when native PAGE was run for
   different stages of bean development for different genotypes. Genotypes
   also differed in accumulation pattern of TI content during seed
   development. TI content, on dry weight basis, increased continuously
   from 30 days of flowering (DAF) till maturity for `Pb1' and `NRC37'
   while in `Shilajeet' the same pattern was observed except there was
   decline just before harvest maturity. Entirely different accumulation
   pattern was observed in `JS335' where TI content at 30 DAF was as high
   as observed at maturity and thereafter it fluctuated. Significant levels
   of TI were observed at suitable picking stage for green pod consumption
   in all four genotypes. It may be inferred that because of high moisture
   content TI content is mistakenly considered low in immature soybean.}},
