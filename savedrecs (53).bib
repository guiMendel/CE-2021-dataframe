@article{ WOS:A1995RY30800039,
Author = {TRAPP, T and HOLSBOER, F},
Title = {{LIGAND-INDUCED CONFORMATIONAL-CHANGES IN THE MINERALOCORTICOID RECEPTOR
   ANALYZED BY PROTEASE MAPPING}},
Journal = {{BIOCHEMICAL AND BIOPHYSICAL RESEARCH COMMUNICATIONS}},
Year = {{1995}},
Volume = {{215}},
Number = {{1}},
Pages = {{286-291}},
Month = {{OCT 4}},
Abstract = {{The human mineralocorticoid receptor (MR) binds the agonists aldosterone
   and cortisol and the antagonist progesterone with a comparably high
   affinity. We used limited proteolysis of human MR synthesized by in
   vitro translation to detect structural alterations induced by these
   different endogenous ligands, Steroid binding induces a conformational
   change within the receptor protein. This structural alteration renders a
   fragment of MR resistent to proteolysis. Agonists and antagonist vary in
   how well they protect the MR fragment against proteolysis. But the two
   agonists also differ in their ability to protect, indicating that
   agonists and antagonists, but also different agonists, may induce
   distinct conformational changes. Ligand-independent removal of
   MR-associated heat-shock proteins induces no detectable structural
   change but completely prevents ligand binding of MR. (C) 1995 Academic
   Press. Inc.}},
