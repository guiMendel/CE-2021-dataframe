@article{ WOS:000587913600068,
Author = {Ma, Ling and Wang, Yongqin and Luo, Yuanxin},
Title = {{A method for assessing lifetime of an extrusion container subjected to
   fatigue-creep interaction}},
Journal = {{ENGINEERING FAILURE ANALYSIS}},
Year = {{2020}},
Volume = {{118}},
Month = {{DEC}},
Abstract = {{Extrusion container (EC) is the most expensive piece of press tooling in
   an extrusion machine. This work is dedicated to proposing a method to
   model the damage evolution of EC and assessing its serving lifetime. To
   describe the mechanical properties and deterioration, a constitutive
   model with Armstrong-Fredrick/Ohno-Wang (AF-OW) kinematic hardening rule
   and an improved isotropic hardening rule is built and Lemaitre's damage
   law is employed. Due to high temperature, creep deformation has also
   been taken into consideration and been incorporated in inelastic strain.
   Simulation of EC's lifetime assessment under its real service condition
   has been implemented and the results are convincing and meaningful.
   Therefore, this work can be used to optimize the parameters of EC design
   and extrusion process, and guide EC's maintenance and layer replacement
   in order to enhance its service lifetime and reduce the extrusion cost.}},
