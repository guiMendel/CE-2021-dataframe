@article{ WOS:000602724900001,
Author = {Golubkov, Mikhail S. and Nikulina, Vera N. and Tiunov, V, Alexei and
   Golubkov, Sergey M.},
Title = {{Stable C and N Isotope Composition of Suspended Particulate Organic
   Matter in the Neva Estuary: The Role of Abiotic Factors, Productivity,
   and Phytoplankton Taxonomic Composition}},
Journal = {{JOURNAL OF MARINE SCIENCE AND ENGINEERING}},
Year = {{2020}},
Volume = {{8}},
Number = {{12}},
Month = {{DEC}},
Abstract = {{Knowledge of carbon and nitrogen isotopic ratios in organic matter and
   their changes is important when studying nutrient cycles in aquatic
   ecosystems. Relationships between delta C-13 and delta N-15 values of
   suspended particulate organic matter (POM), water temperature, salinity,
   pH, redox potential, chlorophyll a concentration, primary production,
   and biomasses of different taxonomic groups of phytoplankton in the Neva
   Estuary were statistically analyzed. We tested the hypothesis that the
   studied physicochemical and biogeochemical characteristics, as well as
   the species composition of phytoplankton and its productivity, can be
   significant predictors of changes in the isotopic ratios of suspended
   particulate organic matter in estuaries. In the Neva Estuary, delta
   C-13(POM) (-16.8--27.6 parts per thousand) and delta N-15(POM) (2.3-7.3
   parts per thousand) changed synchronously. Statistical analysis showed
   that for both isotopes, the photosynthetic activity and taxonomic
   composition of phytoplankton are important. For C-13(POM), the second
   most important factor was water salinity, which was apparently
   associated with the transition of algae from CO2 to HCO3 consumption
   during photosynthesis in estuarine waters. For N-15(POM) changes, the
   most important abiotic factor was pH. The study showed that the
   dependences of POM isotopic ratios on environmental variables obtained
   for continental and oceanic waters are also valid in transitional zones
   such as the Neva Estuary.}},
