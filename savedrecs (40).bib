@article{ WOS:000166039900056,
Author = {Tomaras, TN and Tsamis, NC and Woodard, RP},
Title = {{Back reaction in light cone QED}},
Journal = {{PHYSICAL REVIEW D}},
Year = {{2000}},
Volume = {{62}},
Number = {{12}},
Month = {{DEC 15}},
Abstract = {{We consider the back reaction of quantum electrodynamics upon an
   electric field E(x(+)) = - A(-)'(x(+)) which is parallel to x(3) and
   depends only on the light cone coordinate x(+) = (x(0) + x(3))/root2.
   Novel features are that the mode functions have simple expressions for
   arbitrary A(-)(x(+)) and that one cannot ignore the usual light cone
   ambiguity at zero + momentum. Each mode of definite canonical momentum
   k(+) experiences pair creation at the instant when its kinetic momentum
   p(+) = k(+) - eA(-)(x(+)) vanishes, at which point operators from the
   surface at x(-) = - infinity play a crucial role. Our formalism permits
   a more explicit and complete derivation of the rate of particle
   production than is usually given. We show that the system can be
   understood as the infinite boost limit of the analogous problem of an
   electric field which is homogeneous on surfaces of constant x(0).}},
