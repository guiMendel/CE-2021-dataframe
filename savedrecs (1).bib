@article{ WOS:000599506900034,
Author = {Guo, Hui-Juan and Wang, Huai-Xiang and Guo, Yu-Jie and Liu, Gui-Xian and
   Wan, Jing and Song, Yue-Xian and Yang, Xin-An and Jia, Fei-Fei and Wang,
   Fu-Yi and Guo, Yu-Guo and Wen, Rui and Wan, Li-Jun},
Title = {{Dynamic Evolution of a Cathode Interphase Layer at the Surface of
   LiNi0.5Co0.2Mn0.3O2 in Quasi-Solid-State Lithium Batteries}},
Journal = {{JOURNAL OF THE AMERICAN CHEMICAL SOCIETY}},
Year = {{2020}},
Volume = {{142}},
Number = {{49}},
Pages = {{20752-20762}},
Month = {{DEC 9}},
Abstract = {{Intensive understanding of the surface mechanism of cathode materials,
   such as structural evolution and chemical and mechanical stability upon
   charging/discharging, is crucial to design advanced solid-state lithium
   batteries (SSLBs) of tomorrow. Here, via in situ atomic force microscopy
   monitoring, we explore the dynamic evolution process at the surface of
   LiNi0.3Co0.2Mn0.3O2 cathode particles inside a working SSLB. The dynamic
   formation process of the cathode interphase layer, with an
   inorganic-organic hybrid structure, was real-time imaged, as well as the
   evolution of its mechanical property by in situ scanning of the
   Deijaguin-Muller-Toporov modulus. Moreover, different components of the
   cathode interphase layer, such as LiF, Li2CO3, and specific organic
   species, were identified in detailat different stages of cycling, which
   can be directly correlated with the impedance buildup of the battery. In
   addition, the transition metal migration and the formation of new phases
   can further exacerbate the degradation of the SSLB. A relatively stable
   cathode interphase is key to improving the performance of SSLBs. Our
   findings provide deep insights into the dynamic evolution of surface
   morphology, chemical components and mechanical properties of the cathode
   interphase layer, which are pivotal for the performance optimization of
   SSLBs.}},
