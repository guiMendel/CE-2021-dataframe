@article{ WOS:000604224200010,
Author = {Xue, Suqin and Wang, Jianrong and Wu, Qiao and Zhang, Lei and Dai, Rong
   and Tian, Bonian and Wang, Wei and Zhang, Weibin and Zhang, Fuchun},
Title = {{The electronic and optical properties of Ni-doped Bi4O5I2:
   First-principles calculations}},
Journal = {{RESULTS IN PHYSICS}},
Year = {{2020}},
Volume = {{19}},
Month = {{DEC}},
Abstract = {{The electronic and optical properties of Ni-doped Bi4O5I2 systems were
   investigated via first-principle calculations. For the pristine Bi4O5I2,
   the band gap was calculated to be 2.17 eV, which was close to the
   experimental report. When the concentration of Ni increased, the band
   gap obviously decreased, which was ascribed to the introduction of Ni 3d
   doping states in the forbidden band gap. Based on the dielectric
   function analysis, the variations of the optical properties mainly took
   place in the visible light range, and the additional peak in the visible
   light range originated from the electron transition between the Ni 3d
   and Bi 6p states. Accordingly, the light absorption in the visible light
   range obviously increased, which was favorable for photocatalytic
   applications.}},
