@article{ WOS:000283121100008,
Author = {Kazerani, T. and Zhao, G. F. and Zhao, J.},
Title = {{Dynamic Fracturing Simulation of Brittle Material using the Distinct
   Lattice Spring Method with a Full Rate-Dependent Cohesive Law}},
Journal = {{ROCK MECHANICS AND ROCK ENGINEERING}},
Year = {{2010}},
Volume = {{43}},
Number = {{6, SI}},
Pages = {{717-726}},
Month = {{NOV}},
Abstract = {{A full rate-dependent cohesive law is implemented in the distinct
   lattice spring method (DLSM) to investigate the dynamic fracturing
   behavior of brittle materials. Both the spring ultimate deformation and
   spring strength are dependent on the spring deformation rate. From the
   simulation results, it is found that the dynamic crack propagation
   velocity can be well predicted by the DLSM through the implemented full
   rate-dependent cohesive law. Furthermore, a numerical investigation on
