Title = {{Polyvalent DNA prime and envelope protein boost HIV-1 vaccine elicits
   humoral and cellular responses and controls plasma viremia in rhesus
   macaques following rectal challenge with an R5 SHIV isolate}},
Journal = {{JOURNAL OF MEDICAL PRIMATOLOGY}},
Year = {{2005}},
Volume = {{34}},
