@article{ WOS:000232192500004,
Author = {Inaba, K and Koga, A and Suga, SI and Kawakami, N},
Title = {{Phase diagram of orbital-selective Mott transitions at finite
   temperatures}},
Journal = {{JOURNAL OF THE PHYSICAL SOCIETY OF JAPAN}},
Year = {{2005}},
Volume = {{74}},
Number = {{9}},
Pages = {{2393-2396}},
Month = {{SEP}},
Abstract = {{Mott transitions in the two-orbital Hubbard model with different
   bandwidths are investigated at finite temperatures. By means of the
   self-energy functional approach, we discuss the stability of the
   intermediate phase that has one orbital localized and the other
   itinerant; this phase is caused by the orbital-selective Mott transition
   (OSMT). It is shown that the OSMT realizes two different coexistence
   regions at finite temperatures in accordance with the recent results of
   Liebsch. We further find that a particularly interesting behavior
   emerges around the special conditions U = U' and J = 0, which includes a
   new type of coexistence region that has three distinct states. By
   systematically changing the Hund coupling, we establish the global phase
   diagram to elucidate the key role played by the Hund coupling on the
   Mott transitions.}},
