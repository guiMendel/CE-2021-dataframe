@article{ WOS:A1995RL31500016,
Author = {RUFFIN, DC and SCHUMACHER, J and COMER, JS},
Title = {{UTERINE TORSION ASSOCIATED WITH SMALL-INTESTINAL INCARCERATION IN A MARE
   AT 126 DAYS OF GESTATION}},
Journal = {{JOURNAL OF THE AMERICAN VETERINARY MEDICAL ASSOCIATION}},
Year = {{1995}},
Volume = {{207}},
Number = {{3}},
Pages = {{329-330}},
Month = {{AUG 1}},
Abstract = {{A mare at 126 days of gestation was examined because of signs of
   abdominal pain. The cause of abdominal pain could not be determined by
   physical examination. lack of response to analgesics and small
   intestinal distention on palpation per rectum prompted exploratory
   celiotomy. Small intestine was found to be entrapped by the uterus,
   which had undergone torsion. The uterus was returned to its correct
   position, and the small intestine was decompressed. After surgery, the
   mare was treated with orally administered progestin to prevent abortion.
   The mare recovered and delivered a live foal at 354 days of gestation.
   Uterine torsion at such an early stage of gestation is not common in
   horses.}},
