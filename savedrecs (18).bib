@article{ WOS:000363295000003,
Author = {Praszalowicz, Michal and Francuz, Anna},
Title = {{Geometrical scaling in inelastic inclusive particle production at the
   LHC}},
Journal = {{PHYSICAL REVIEW D}},
Year = {{2015}},
Volume = {{92}},
Number = {{7}},
Month = {{OCT 23}},
Abstract = {{Analyzing recent ALICE data on inelastic pp scattering at the LHC
   energies, we show that charged particle distributions exhibit
   geometrical scaling (GS). We show also that the inelastic cross section
   is scaling as well and that in this case the quality of GS is better
   than for multiplicities. Moreover, exponent. characterizing the
   saturation scale is for the cross-section scaling compatible with the
   one found in deep inelastic ep scattering at HERA. Next, by
   parametrizing charged particle distributions by the Tsallis-like
   formula, we find a somewhat unexpected solution that still exhibits GS,
   but differs from the ``standard{''} one where the Tsallis temperature is
   proportional to the saturation scale.}},
