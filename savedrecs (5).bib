@article{ WOS:000600891900002,
Author = {Rasulov, T. H. and Dilmurodov, E. B.},
Title = {{INFINITE NUMBER OF EIGENVALUES OF 2x2 OPERATOR MATRICES: ASYMPTOTIC
   DISCRETE SPECTRUM}},
Journal = {{THEORETICAL AND MATHEMATICAL PHYSICS}},
Year = {{2020}},
Volume = {{205}},
Number = {{3}},
Pages = {{1564-1584}},
Month = {{DEC}},
Abstract = {{We study an unbounded 2x2 operator matrix A in the direct product of two
   Hilbert spaces. We obtain asymptotic formulas for the number of
   eigenvalues of A. We consider a 2x2 operator matrix A(mu), where mu > 0
   is the coupling constant, associated with the Hamiltonian of a system
   with at most three particles on the lattice Z(3). We find the critical
   value mu(0) of the coupling constant mu for which A mu(0) has an
   infinite number of eigenvalues. These eigenvalues accumulate at the
   lower and upper bounds of the essential spectrum. We obtain an
   asymptotic formula for the number of such eigenvalues in both the left
   and right parts of the essential spectrum.}},
