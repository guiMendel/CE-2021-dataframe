@article{ WOS:000362578900007,
Author = {Wang, Si and Dhima, Khalid and Steinberg, Christian and Papenheim, Marc
   and Scheer, Hella-Christin and Helfer, Anke and Polywka, Andreas and
   Goerrn, Patrick},
Title = {{Morphology of organic semi-crystalline polymer after thermal nanoimprint}},
Journal = {{APPLIED PHYSICS A-MATERIALS SCIENCE \& PROCESSING}},
Year = {{2015}},
Volume = {{121}},
Number = {{2, SI}},
Pages = {{357-362}},
Month = {{NOV}},
Abstract = {{The morphology of highly regular, semi-crystalline P3HT (poly
   3-hexylthiophene-2.5-diyl) after thermal nanoimprint is studied and
   compared to that of its amorphous counterpart. Differential scanning
   calorimetry measurements of both materials provide the glass transition
   temperature (below room temperature) as well as the melting temperature
   (T (m) a parts per thousand 235 A degrees C) and the crystallization
   temperature (T (c) a parts per thousand 200 A degrees C) of the
   semi-crystalline polymer. Imprint experiments are performed at
   temperatures below and above the melting temperature of the
   crystallites. The samples imprinted with line structures in the range of
   135-500 nm are investigated by scanning electron microscopy and
   transmission measurements. In agreement with T (m), the investigations
   indicate that the crystallites do not melt when semi-crystalline P3HT is
   imprinted below T (m). After imprint above T (m), no more indication of
   the crystallites is found. Furthermore, physical self-assembly only
   occurred after imprint beyond T (m) with semi-crystalline P3HT,
   indicating a fully amorphous state. The morphology observed-the
   roughness of the surface within partly filled cavities when crystallites
   are present-correlates well with the results from optical measurements.}},
