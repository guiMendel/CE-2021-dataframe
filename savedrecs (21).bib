@article{ WOS:000284421600004,
Author = {Eden, Svetlana K. and An, Angel Q. and Horner, Jeffrey and Jenkins,
   Cathy A. and Scott, Theresa A.},
Title = {{A two-step process for graphically summarizing spatial temporal
   multivariate data in two dimensions}},
Journal = {{COMPUTATIONAL STATISTICS}},
Year = {{2010}},
Volume = {{25}},
Number = {{4}},
Pages = {{587-601}},
Month = {{DEC}},
Abstract = {{Graphical illustration and exploration of atmospheric data, like those
   from the 2006 ASA Data Exposition, is challenging. We chose to employ a
   two-step process, which consisted of exploring the presented data (1)
   from a `global' view by simultaneously displaying the spatial and
   temporal components of individual variables in the data set using a
   geographic grid of polar coordinate plots or circular histograms that
   incorporated elevation as a background color, and (2) from a `local'
   view by displaying the relationships between multiple variables at
   specific geographic locations and/or time points/periods (selected via
   step 1) using scatterplot matrices.}},
Publisher = {{SPRINGER HEIDELBERG}},
Address = {{TIERGARTENSTRASSE 17, D-69121 HEIDELBERG, GERMANY}},
