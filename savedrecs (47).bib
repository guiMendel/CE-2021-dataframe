@article{ WOS:000089230900015,
Author = {Wu, TX and Thompson, DJ},
Title = {{The vibration behavior of railway track at high frequencies under
   multiple preloads and wheel interactions}},
Journal = {{JOURNAL OF THE ACOUSTICAL SOCIETY OF AMERICA}},
Year = {{2000}},
Volume = {{108}},
Number = {{3, 1}},
Pages = {{1046-1053}},
Month = {{SEP}},
Abstract = {{The track foundation is preloaded by multiple wheel loads due to the
   train weight and, as the pad and ballast are nonlinear, their stiffness
   depends upon the preload in them. Due to the influence of these
   resilient components of the track, the track vibration is affected by
   the wheel loads. It is also affected by the wheel/rail interactions. In
   this article the preloads in the pad and ballast are calculated by
   considering the nonlinear properties of the track foundation, and thus
   the preloaded pad and ballast stiffnesses are determined. The vibration
   properties are explored for the track under multiple wheel loads and
   multiple wheel/rail interactions by comparing the results from different
   track models with and without these effects. It is found that the point
   receptance of the track is reduced and the vibration decay rate is
   enhanced at low frequencies due to the wheel loads. The effects of the
   wheel/rail interactions are most significant for frequencies 400-2000
   Hz. Because of the wheel/rail interactions, the point receptance
   fluctuates and the vibration decay is enhanced in the regions around the
   wheels. (C) 2000 Acoustical Society of America.
   {[}S0001-4966(00)02409-7].}},
