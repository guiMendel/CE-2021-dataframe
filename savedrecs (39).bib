@article{ WOS:000231247300061,
Author = {Torricelli, A and Pifferi, A and Spinelli, L and Cubeddu, R and
   Martelli, F and Del Bianco, S and Zaccanti, G},
Title = {{Time-resolved reflectance at null source-detector separation: Improving
   contrast and resolution in diffuse optical imaging}},
Journal = {{PHYSICAL REVIEW LETTERS}},
Year = {{2005}},
Volume = {{95}},
Number = {{7}},
Month = {{AUG 12}},
Abstract = {{We propose a novel approach to imaging in diffusive media based on
   time-resolved reflectance measurements at null source-detector
   separation. This approach yields better spatial resolution and contrast
   as compared to the classical approach, which typically employs a
   separation of 20-40 mm. Results are obtained by an analytical
   perturbation approach to diffusion theory and on Monte Carlo
   simulations. Practical implementation with state-of-the-art technology
   and performance of a complementary approach based on the use of small
   but not null source-detector separation are also discussed.}},
