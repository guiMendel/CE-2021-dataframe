@article{ WOS:000242443200015,
Author = {Gu, Wenjin and Zhao, Hongchao},
Title = {{Fuzzy sliding mode control for skid-to-turn missiles based on
   output-redefinition}},
Journal = {{DYNAMICS OF CONTINUOUS DISCRETE AND IMPULSIVE SYSTEMS-SERIES
   B-APPLICATIONS \& ALGORITHMS}},
Year = {{2005}},
Volume = {{12}},
Number = {{S}},
Pages = {{162-171}},
Month = {{DEC}},
Note = {{International Conference on Complex Systems, Control and Optimization,
   NE Univ, Shenyang, PEOPLES R CHINA, AUG 08-10, 2004}},
Abstract = {{The output-redefinition method is applied to the overload control system
   design of skid-to-turn (STT) missiles. The modified output easily
   removes the nonminimum phase characteristics of missile normal overload
   output. In order to improve the system robustness and to reduce the
   chattering phenomenon, a fuzzy sliding mode control (FSMC) method is
   developed to control the modified output. A new type of anti-ship
   missile is illustrated to research in the simulation, which is a
   tail-controlled STT missile. The FSMC based on output-redefinition
   method is applied to design its overload control system. The missile's
   aerodynamic coefficients are perturbed up and down from their nominal
   values to test the robustness of the overload control system. The
   simulation results show the stability and robustness of the designed
   overload control system.}},
