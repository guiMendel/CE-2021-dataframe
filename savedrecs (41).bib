@article{ WOS:000165879400083,
Author = {Rossberg, AG},
Title = {{Three-dimensional pattern formation, multiple homogeneous soft modes,
   and nonlinear dielectric electroconvection}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2000}},
Volume = {{62}},
Number = {{6, A}},
Pages = {{8114-8132}},
Month = {{DEC}},
Abstract = {{Patterns forming spontaneously in extended, three-dimensional,
   dissipative systems are likely to excite several homogeneous soft modes
   (approximate to hydrodynamic modes) of the underlying physical system,
   much more than quasi-one- (1D) and two-dimensional (2D) patterns are.
   The reason is the lack of damping boundaries. This paper compares two
   analytic techniques to derive the pattern dynamics from hydrodynamics,
   which are usually equivalent but lead to different results when applied
   to multiple homogeneous soft modes. Dielectric electroconvection in
   nematic liquid crystals is introduced as a model for 3D pattern
   formation. The 3D pattern dynamics including soft modes are derived. For
   slabs of large but finite thickness the description is reduced further
   to a 2D one. It is argued that the range of validity of 2D descriptions
   is limited to a very small region above threshold. The transition from
   2D to 3D pattern dynamics is discussed. Experimentally testable
   predictions for the stable range of ideal patterns and the electric
   Nusselt numbers are made. For most results analytic approximations in
   terms of material parameters are given. {[}S1063-651X(00)09512-X].}},
