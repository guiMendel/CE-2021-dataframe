@article{ WOS:000284575400002,
Author = {Caraveo, P. A. and De Luca, A. and Marelli, M. and Bignami, G. F. and
   Ray, P. S. and Parkinson, P. M. Saz and Kanbach, G.},
Title = {{X-RAY PULSATIONS FROM THE RADIO-QUIET GAMMA-RAY PULSAR IN CTA 1}},
Journal = {{ASTROPHYSICAL JOURNAL LETTERS}},
Year = {{2010}},
Volume = {{725}},
Number = {{1}},
Pages = {{L6-L10}},
Month = {{DEC 10}},
Abstract = {{Prompted by the Fermi-LAT discovery of a radio-quiet gamma-ray pulsar
   inside the CTA 1 supernova remnant, we obtained a 130 ks XMM-Newton
   observation to assess the timing behavior of this pulsar. Exploiting
   both the unprecedented photon harvest and the contemporary Fermi-LAT
   timing measurements, a 4.7 sigma single-peak pulsation is detected,
   making PSR J0007+7303 the second example, after Geminga, of a
   radio-quiet gamma-ray pulsar also seen to pulsate in X-rays.
   Phase-resolved spectroscopy shows that the off-pulse portion of the
   light curve is dominated by a power-law, non-thermal spectrum, while the
   X-ray peak emission appears to be mainly of thermal origin, probably
   from a polar cap heated by magnetospheric return currents, pointing to a
   hot spot varying throughout the pulsar rotation.}},
