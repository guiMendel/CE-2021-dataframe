@article{ WOS:000595545200004,
Author = {Pandey, Gaurav and Dey, Diptesh and Tiwari, Ashwani K.},
Title = {{Controlling the Ultrafast Dynamics of HD plus by the Carrier-Envelope
   Phases of an Ultrashort Laser Pulse: A Quasi-Classical Dynamics Study}},
Journal = {{JOURNAL OF PHYSICAL CHEMISTRY A}},
Year = {{2020}},
Volume = {{124}},
Number = {{47}},
Pages = {{9710-9720}},
Month = {{NOV 25}},
Abstract = {{A theoretical study on the coupled electron-nuclear dynamics of HD+
   molecular ions under ultrashort, intense laser pulses is performed by
   employing a well-established quasi-classical model. The influence of the
   laser carrier-envelope phase on various channel (H + D+, D + H+, and H+
   + D+) probabilities is investigated at different laser field
   intensities. The carrier-envelope phase is found to govern the
   dissociation (H + D+ and D + H+) and Coulomb explosion (H+ + D+) channel
   probabilities. The kinetic energy release distributions of the fragments
   are also found to be sensitive to the carrierenvelope phase of the laser
   pulse. Our results are in agreement with the previously reported quantum
   dynamics studies and experiments.}},
