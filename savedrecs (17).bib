@article{ WOS:000364913700009,
Author = {Mueller, K. and LHCb Collaboration},
Title = {{LHCb results in proton-nucleus collisions at the LHC}},
Journal = {{ASTRONOMISCHE NACHRICHTEN}},
Year = {{2015}},
Volume = {{336}},
Number = {{8-9}},
Pages = {{773-777}},
Month = {{NOV}},
Abstract = {{The forward acceptance of the LHCb detector allows it to probe
   proton-ion collisions in a unique kinematic range, complementary to the
   other LHC experiments. The production of J/ and ?-mesons decaying into
   two muons is studied at the LHCb experiment in proton-lead collisions at
   a proton-nucleon centre-of-mass energy sNN = 5 TeV. The analysis is
   based on a data sample corresponding to an integrated luminosity of 1.6
   nb(-1). The nuclear modification factor and the forward-backward
   production ratio are determined for J/ and ?(1S) mesons. Clear
   suppression of prompt J/ production is observed with respect to the
   production in proton-proton collisions at large rapidity, while the
   suppression of J/ from b-hadron decays is less pronounced. The nuclear
   modification factor for ?(1S) mesons in the forward region is found to
   be similar to those for J/ from b-hadron decays. Furthermore, a first
   observation of Z bosons at large rapidities in proton-lead collisions is
   reported. ((c) 2015 WILEY-VCH Verlag GmbH \& Co. KGaA, Weinheim)}},
