@article{ WOS:000165877600050,
Author = {Mukouda, I and Shimomura, Y and Iiyama, T and Harada, Y and Katano, Y
   and Nakazawa, T and Yamaki, D and Noda, K},
Title = {{Microstructure in pure copper irradiated by simultaneous multi-ion beam
   of hydrogen, helium and self ions}},
Journal = {{JOURNAL OF NUCLEAR MATERIALS}},
Year = {{2000}},
Volume = {{283}},
Number = {{A}},
Pages = {{302-305}},
Month = {{DEC}},
Note = {{9th International Conference on Fusion Reactor Materials (ICFRM-9),
   COLORADO SPRINGS, COLORADO, OCT 10-15, 1999}},
Organization = {{US DOE, Off Fus Energy Sci; Argoone Natl Lab; Oak Ridge Natl Lab;
   Pacific NW Natl Lab; Univ Calif Santa Barbara; Amer Nucl Soc; Min Met \&
   Mat Soc}},
Abstract = {{Pure copper was irradiated at 300-500 degreesC by 5 MeV Cu ions (single
   beam) and Cu ions plus gas atoms (H and He) (dual beam irradiation)
   simultaneously. The high energy ion irradiation was carried out with the
   accelerator TIARA at the Takasaki-establishment of JAERI. The ions stop
   within a few microns from surface level and damage was formed up to this
   depth. The damage structure was observed as a function of the depth
   utilizing a focused ion beam (FIB) device. Below 300 degreesC
   irradiation with a single beam produced a high density of stacking fault
   tetrahedra (SFT) but void formation was not observed. Large voids were
   observed with single beam irradiation at 500 degreesC. In specimen
   irradiated with a dual beam of helium and Ni ions, the number density of
   voids was increased significantly. In copper irradiated with hydrogen
   and Ni ions, the number density of voids was not so large. Experimental
   results show that helium atoms promote void formation. Hydrogen atoms
   have less effect on void formation than helium atoms in pure copper. (C)
   2000 Elsevier Science B.V. All rights reserved.}},
