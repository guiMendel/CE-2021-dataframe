@article{ WOS:000232751900010,
Author = {Kenmoku, M and Nandi, KK and Shigemoto, K},
Title = {{Solution-independent analysis of black hole entropy in brick wall model}},
Journal = {{CLASSICAL AND QUANTUM GRAVITY}},
Year = {{2005}},
Volume = {{22}},
Number = {{19}},
Pages = {{3923-3934}},
Month = {{OCT 7}},
Abstract = {{Using the brick wall regularization of `t Hooft, the entropy of
   non-extreme and extreme black holes is investigated in a general static,
   spherically symmetric spacetime. We classify the singularity in the
   entropy by introducing a new index 6 with respect to the brick wall
   cut-off epsilon. The leading contribution to entropy for the non-extreme
   case (delta not equal 0) is shown to satisfy the area law with quadratic
   divergence with respect to the invariant cut-off epsilon(inv) while the
   extreme case (delta = 0) exhibits logarithmic divergence or constant
   value with respect to epsilon. The general formula is applied to
   Reissner-Nordstrom, dilaton and brane-world black holes and we obtain
   consistent results.}},
