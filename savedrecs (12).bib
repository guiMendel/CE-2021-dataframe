@article{ WOS:000367460400106,
Author = {He, Yang and Qin, Hong and Sun, Yajuan and Xiao, Jianyuan and Zhang,
   Ruili and Liu, Jian},
Title = {{Hamiltonian time integrators for Vlasov-Maxwell equations}},
Journal = {{PHYSICS OF PLASMAS}},
Year = {{2015}},
Volume = {{22}},
Number = {{12}},
Month = {{DEC}},
Abstract = {{Hamiltonian time integrators for the Vlasov-Maxwell equations are
   developed by a Hamiltonian splitting technique. The Hamiltonian
   functional is split into five parts, which produces five exactly
   solvable subsystems. Each subsystem is a Hamiltonian system equipped
   with the Morrison-Marsden-Weinstein Poisson bracket. Compositions of the
   exact solutions provide Poisson structure preserving/Hamiltonian methods
   of arbitrary high order for the Vlasov-Maxwell equations. They are then
   accurate and conservative over a long time because of the
   Poisson-preserving nature. (c) 2015 AIP Publishing LLC.}},
