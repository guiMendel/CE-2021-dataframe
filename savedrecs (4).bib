@article{ WOS:000610467400011,
Author = {Kim, Yu-Jeong and Choi, Yongkun and Lee, Seung-Han},
Title = {{A study on revised Korean Industrial Standards (KS) for estimation of
   mineral resources and reserves in Korea}},
Journal = {{JOURNAL OF THE GEOLOGICAL SOCIETY OF KOREA}},
Year = {{2020}},
Volume = {{56}},
Number = {{6}},
Pages = {{817-824}},
Month = {{DEC}},
Abstract = {{The estimation of the mineral resources and mineral reserves is the most
   critical factor to make design and investment decisions for a project.
   For this reason, standardized classification and terms are used in other
   countries. Also, international standards are used to provide specific
   and clear reports on estimation results. However, Korean industrial
   standards, called KS, has a limit in the industry due to different terms
   and estimation criteria from technologies and markets. In this study, we
   suggest the revised KS for estimation of mineral resources and reserves
   which meets on international standards and overcomes its limitation. The
   three existing standards are integrated into one unified standard as
   with international standards. In addition, we suggest the legal
   definition of mineral resources and reserves clearly and the new
   industry standard including estimation procedures, presenting results
   and restrictions.}},
