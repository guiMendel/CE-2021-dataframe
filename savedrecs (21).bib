@article{ WOS:000285138200001,
Author = {Gerner, Matthias},
Title = {{Compositional and constructional reduplication in Kam-Tai languages}},
Journal = {{FOLIA LINGUISTICA}},
Year = {{2010}},
Volume = {{44}},
Number = {{2}},
Pages = {{267-337}},
Month = {{DEC}},
Abstract = {{Direct compositionality is a property of empirical data (and of
   grammatical frameworks) where the meaning of an expression can be
   reliably computed from the meanings of its parts (Jacobson 2002). Using
   empirical data from Kam and Northern Zhuang, two Kam-Tai languages
   spoken in the P. R. of China, I define the notions of compositional and
   constructional reduplication rules. A rule is compositional if the host
   construction does not manifest selectional restrictions on the embedded
   output of the rule. By contrast, a reduplication rule is constructional
   if there are selectional restrictions. Based on the descriptive insights
   of this study and on Jacobson's two types of (direct) compositionality,
   I define four different degrees of compositionality that a
   morphosyntactic operation may exhibit: strong compositionality, weak
   compositionality, weak constructionality (non-compositionality) and
   strong constructionality.}},
