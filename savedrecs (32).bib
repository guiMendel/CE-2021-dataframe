@article{ WOS:000232957800009,
Author = {Bray, S and Lee, JS and Pilaftsis, A},
Title = {{Heavy Majorana neutrino production at e(-) gamma colliders}},
Journal = {{PHYSICS LETTERS B}},
Year = {{2005}},
Volume = {{628}},
Number = {{3-4}},
Pages = {{250-261}},
Month = {{NOV 10}},
Abstract = {{We study signatures of heavy Majorana neutrinos at e(-)gamma colliders.
   Since these particles violate lepton number, they can give rise to the
   reactions e(-) gamma -> W-W- e(+) and e(-) gamma -> W+ mu(-) mu(-) v.
   The Standard Model background contains extra light neutrinos that escape
   detection, and can be reduced to an unobservable level after imposing
   appropriate kinematical cuts. We analyze the physics potential of an
   e(-) gamma collider, with centre of mass energies root s = 0.5-1 TeV and
   a total integrated luminosity of 100 fb(-1), for detecting heavy
   Majorana neutrinos with masses m(N) = 100-400 GeV. Assuming that heavy
   neutrinos couple predominantly to only one lepton flavour at a time, we
   find that 10 expected background-free events can be established for a
   WeN-coupling vertical bar B-eN vertical bar greater than or similar to
   4.6 x 10(-3) or for a W mu N-coupling vertical bar B-mu N vertical bar
   greater than or similar to 9-0 X 10(-2). Instead, if no signal is
   observed, this will imply that vertical bar B-eN vertical bar less than
   or similar to 2.7 x 10(-3) and vertical bar B-mu N vertical bar less
   than or similar to 0.05 at the 90\% confidence level. (c) 2005 Published
   by Elsevier B.V.}},
