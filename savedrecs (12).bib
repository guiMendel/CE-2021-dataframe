@article{ WOS:000365716200009,
Author = {Lui, Semechah K. Y. and Helmberger, Don and Wei, Shengji and Huang, Yihe
   and Graves, Robert W.},
Title = {{Interrogation of the Megathrust Zone in the Tohoku-Oki Seismic Region by
   Waveform Complexity: Intraslab Earthquake Rupture and Reactivation of
   Subducted Normal Faults}},
Journal = {{PURE AND APPLIED GEOPHYSICS}},
Year = {{2015}},
Volume = {{172}},
Number = {{12, SI}},
Pages = {{3425-3437}},
Month = {{DEC}},
Abstract = {{Results from the 2011 Mw 9.1 Tohoku-Oki megathrust earthquake display a
   complex rupture pattern, with most of the high-frequency energy radiated
   from the downdip edge of the seismogenic zone and very little from the
   large shallow rupture. Current seismic results of smaller earthquakes in
   this region are confusing due to disagreements among event catalogs on
   both the event locations (> 30 km horizontally) and mechanisms. Here we
   present an in-depth study of a series of intraslab earthquakes that
   occurred in a localized region near the downdip edge of the main shock.
   We explore the validity of 1D velocity model and refine earthquake
   source parameters for selected key events by performing broadband
   waveform modeling combining regional networks. These refined source
   parameters are then used to calibrate paths and further simulate
   secondary source properties, such as rupture directivity and fault
   dimension. Calculation of stress changes caused by the main event
   indicate that the region where these intraslab events occurred are prone
   to thrust events. This group of intraslab earthquakes suggest the
   reactivation of a subducted normal fault, and are potentially useful in
   enhancing our understanding on the downdip shear zone and large
   outer-rise events.}},
