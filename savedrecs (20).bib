@article{ WOS:000284082300006,
Author = {Brezinski, C. and Redivo-Zaglia, M.},
Title = {{Extensions of Drummond's process for convergence acceleration}},
Journal = {{APPLIED NUMERICAL MATHEMATICS}},
Year = {{2010}},
Volume = {{60}},
Number = {{12, SI}},
Pages = {{1231-1241}},
Month = {{DEC}},
Note = {{Conference on Approximation and Extrapolation of Convergent and
   Divergent Sequences and Series, Marseille, FRANCE, SEP 28-OCT 02, 2009}},
Abstract = {{In this paper, we give some new algebraic properties of a sequence
   transformation due to Drummond for accelerating the convergence. In the
   particular case where the terms of the sequence to be transformed are
   the partial sums of a formal power series, this transformation leads to
   Fade-type approximants. Then, using various operators, some extensions
   of Drummond's process are proposed, and their properties are given. A
   new expression for the E-transformation, using generalized divided
   differences, is obtained, and it is also generalized. The recurrence
   relation for these differences can be used for its implementation, and
   the Ford-Sidi algorithm is also related to them. Lubkin's T and
   W-transformations, which could be related to Drummond's, are discussed
   in Appendix A. (C) 2010 IMACS. Published by Elsevier B.V. All rights
   reserved.}},
