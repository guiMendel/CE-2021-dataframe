@article{ WOS:000365243700016,
Author = {Yang, Wuu},
Title = {{A Brouwerian Model of the Run-Time Memory}},
Journal = {{JOURNAL OF INFORMATION SCIENCE AND ENGINEERING}},
Year = {{2015}},
Volume = {{31}},
Number = {{6}},
Pages = {{2103-2124}},
Month = {{NOV}},
Abstract = {{The run-time memory of a program may be described with a directed graph
   in which nodes represent chunks of memory and edges represent
   references. We define a closed cluster induced by a node n, denoted as
   CC(n), as the largest set of nodes that are reachable from n but are
   unreachable from nodes outside the closed cluster. Based on closed
   clusters, there is a Brouwerian structure under the run-time memory. We
   present the Brouwerian model and discuss its properties,
   transformations, and applications. We also propose a two-counter
   algorithm for calculating CC(n). The two-counter algorithm is never
   slower than a traditional one-counter algorithm. Our study of the
   Brouwerian structure is motivated by work on garbage collection.}},
