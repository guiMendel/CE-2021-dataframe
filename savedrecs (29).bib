Author = {Marlin, Thomas E.},
Title = {{Teaching ``operability{''} in undergraduate chemical engineering design
   education}},
Journal = {{COMPUTERS \& CHEMICAL ENGINEERING}},
Year = {{2010}},
Volume = {{34}},
Number = {{9, SI}},
Pages = {{1421-1431}},
