@article{ WOS:000232739300064,
Author = {Chen, JF and Hsiao, RS and Hsieh, MT and Huang, WD and Guo, PS and Lee,
   WI and Lee, SC and Lee, CL},
Title = {{Thickness dependence of current conduction and carrier distribution of
   GaAsN grown on GaAs}},
Journal = {{JAPANESE JOURNAL OF APPLIED PHYSICS PART 1-REGULAR PAPERS BRIEF
   COMMUNICATIONS \& REVIEW PAPERS}},
Year = {{2005}},
Volume = {{44}},
Number = {{10}},
Pages = {{7507-7511}},
Month = {{OCT}},
Abstract = {{Thickness dependence of the properties of GaAsN grown on GaAs was
   investigated by characterizing GaAs/GaAS(0.982)N(0.018)/ GaAs Schottky
   diodes by current-voltage (I-V), capacitance-voltage (C-V) profiling and
   deep-level transient spectroscopy (DLTS). I-V characteristics show a
   considerable increase in the saturation current when the GaAsN thickness
   is increased from 60 to 250 A. As GaAsN thickness is increased further,
   the I-V characteristic deviates from that of a normal Schottky diode
   with a large series resistance. These I-V characteristics correlate well
   with carrier distribution. In thick GaAsN samples, C-V profiling shows
   carrier depletion in the top GaAs layer and frequency-dispersion
   accumulation in the GaAsN layer. DLTS spectra show that the carrier
   depletion in the top GaAs layer is due to an EL2 trap and the
   frequency-dispersion accumulation is due to the removal of electrons
   from a trap at 0.35 eV in the GaAsN layer. Increasing the GaAsN
   thickness markedly increases the magnitude of both traps. The large
   series resistance in thick GaAsN samples is due to EL2 that markedly
   depletes the top GaAs layer.}},
