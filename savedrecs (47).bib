@article{ WOS:000165210700013,
Author = {Wang, XY and Liu, XD and Zhu, WY and Gu, SS},
Title = {{Analysis of C-plane fractal images from z <- z(alpha)+c for (alpha < 0)}},
Journal = {{FRACTALS-COMPLEX GEOMETRY PATTERNS AND SCALING IN NATURE AND SOCIETY}},
Year = {{2000}},
Volume = {{8}},
Number = {{3}},
Pages = {{307-314}},
Month = {{SEP}},
Abstract = {{This paper expounds the definition of the c-plane fractal images from
   the complex mapping z <-- z(<alpha>) + c (a < 0). A series of
   interesting and intriguing fractal images are generated by changing a
   single parameter <alpha>. When symbol alpha is a negative integer, the
   fractal image is a planetary structure consisting of a central planet
   surrounded by satellite structures. The number of satellite structures
   equals (1 + \textbackslash{}alpha\textbackslash{}). For negative
   non-integer values of alpha, additional embryonic satellite structure,
   proportional in size to the fractional part of alpha, are observed. The
   position and size of the planet and the positions of the satellite
   structures are analytically estimated. The four evolutions of the
   c-plane fractal image are given for the first time. The reasons
   resulting in the four evolutions of the c-plane fractal images are
   analyzed.}},
