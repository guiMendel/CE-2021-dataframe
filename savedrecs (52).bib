@article{ WOS:A1995TE36100010,
Author = {HAYES, DG and KLEIMAN, R},
Title = {{LIPASE-CATALYZED SYNTHESIS AND PROPERTIES OF ESTOLIDES AND THEIR ESTERS}},
Journal = {{JOURNAL OF THE AMERICAN OIL CHEMISTS SOCIETY}},
Year = {{1995}},
Volume = {{72}},
