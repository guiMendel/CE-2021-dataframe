@article{ WOS:000594337700005,
Author = {Wu, Jixuan and Mo, Fei and Saraya, Takuya and Hiramoto, Toshiro and
   Kobayashi, Masaharu},
Title = {{A Monolithic 3-D Integration of RRAM Array and Oxide Semiconductor FET
   for In-Memory Computing in 3-D Neural Network}},
