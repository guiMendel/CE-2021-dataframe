@article{ WOS:000285908200008,
Author = {Fernandez-Caballero, Antonio and Carlos Castillo, Jose and
   Martinez-Cantos, Javier and Martinez-Tomas, Rafael},
Title = {{Optical flow or image subtraction in human detection from infrared
