@article{ WOS:000591324700001,
Author = {More, Sayaji Arjun and Chao, Tzu-Hsuan and Cheng, Mu-Jeng and Liu,
   Rai-Shung},
Title = {{Gold-Catalyzed Bicyclic and {[}3+2]-Annulations of Internal Propargyl
