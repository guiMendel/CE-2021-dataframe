@article{ WOS:A1995RZ45300008,
Author = {JOHNSON, MD and GILLAM, ML},
Title = {{COMPOSITION AND CONSTRUCTION OF LATE PLEISTOCENE END MORAINES, DURANGO,
   COLORADO}},
Journal = {{GEOLOGICAL SOCIETY OF AMERICA BULLETIN}},
Year = {{1995}},
Volume = {{107}},
Number = {{10}},
Pages = {{1241-1253}},
Month = {{OCT}},
Abstract = {{The late Pleistocene Animas City 2 and Animas City 3 end moraines at the
   north edge of Durango, Colorado, were built at the terminus of the
   Animas glacier as ice-marginal aprons of primarily debris-flow sediment
   interbedded with sandy stream sediment. Sorted sediment and three types
   of diamicton present in the moraines are interpreted as fluvial
   sediment, lacustrine sediment, debris-flow sediment, and melt-out till.
   Melt-out till at one locality represents deposition from the basal,
   debris-rich parts of the Animas glacier onto the proximal side of the
   Animas City 3 moraine. Glaciotectonic features in a coarsening-upward
   sequence of sediment, previously deposited in a proglacial
   moraine-dammed lake, mark a readvance of the Animas glacier to the
   Animas City 3 moraine.
   Based on the character of the sediments and their interbedding, we
   conclude that the existing moraines were built rapidly (in <10 yr to a
   few tens of years), immediately before the glacier retreated from the
   terminus position. Earlier moraine ridges would have been built
   continuously at the ice margin, but continuously removed by laterally
   migrating outwash streams. That the glacier stood at the terminus
   position for a long time is indicated not by the moraines, but by the
   extensive outwash deposits to the south and by the geomorphic constraint
   that coarse sediment could only be delivered to the river south of the
   moraine position when the ice was at the terminus position; otherwise,
   the large, scoured bedrock basin north of the moraine ridges would have
   acted as a sediment trap.}},
Publisher = {{GEOLOGICAL SOC AMERICA}},
Address = {{PO BOX 9140 3300 PENROSE PLACE, BOULDER, CO 80301}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{JOHNSON, MD (Corresponding Author), GUSTAVUS ADOLPHUS COLL,DEPT GEOL,800 W COLL AVE,ST PETER,MN 56082, USA.
   UNIV COLORADO,DEPT GEOL SCI,BOULDER,CO 80309.}},
