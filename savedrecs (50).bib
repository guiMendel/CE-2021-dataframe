@article{ WOS:A1995TL32600026,
Author = {Eggen, OJ},
Title = {{Reality tests of superclusters in the young disk population}},
Journal = {{ASTRONOMICAL JOURNAL}},
Year = {{1995}},
Volume = {{110}},
Number = {{6}},
Pages = {{2862-2867}},
Month = {{DEC}},
Abstract = {{Six percent of the FK5 stars of fundamental proper motion pass the
   convergent point test for membership in the IC 2391 supercluster if
   stars within 30 degrees of the convergent and anticonvergent points (sin
   lambda<0,5) and values of V-TOT < 25 km/s are dismissed and a tolerance
   of 0.1 V-TOT (0.1/sin lambda) is placed on tau/nu. One third of these
   stars are eliminated by the luminosity test, using calibrations of
   available photometry, and an additional 40\% are removed by the
   imposition of the requirement that rho(Clus)-rho(Obs)<3 km/s. This
   leaves 1\% of the FK5 stars as supercluster members, which show a spread
   of about 10(7) yr, if 5x10(7) is the upper age limit of the early-type
   stars or 10(8) yr, if 5x10(7) is the minimum age. An age spread is also
   reflected in the lower main sequence stars. Interesting supercluster
   members include the spectroscopic triple HD 21364 (HR 1038), the
   ellipsoidal variable HD 1061 (UU Psc), the light variable HD 27290 (HR
   1338) which lies in the Bohm-Vitense decrement (BVD), the red variable
   HD 89388 (HR 4050=V337 Car), and the peculiar A type, iron and barium
   underabundant and calcium, silicon, and helium overabundant, star HD
   143459 (HR 5959). The FK5 stars were also tested with the contrived
   convergent points of delta=+/-90 degrees but only the expected random
   selection of stars was found. (C) 1995 American Astronomical Society.}},
