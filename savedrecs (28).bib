@article{ WOS:000282627800016,
Author = {Yuan, Zhi-shan and Lu, Zheng and Wu, Xiu-liang and Xie, You-hua and Dai,
   Sheng-long and Liu, Chang-sheng},
Title = {{Retrogression characteristics of a novel Al-Cu-Li-X alloy}},
Journal = {{INTERNATIONAL JOURNAL OF MINERALS METALLURGY AND MATERIALS}},
Year = {{2010}},
Volume = {{17}},
Number = {{5}},
Pages = {{624-628}},
Month = {{OCT}},
Abstract = {{Retrogression characteristics of a novel Al-Cu-Li-X alloy of 2A97 were
   studied by hardness testing, transmission electron microscopy (TEM), and
   differential scanning calorimetry (DSC). The retrogression treatments of
   aging at 155 degrees C for 12 h followed by aging at 220 and 240 degrees
   C were chosen by determining the peak temperature of delta`
   precipitation at 230 degrees C by DSC. The retrogression treatment at a
   lower temperature of 220 degrees C causes the precipitation and
   coarsening of delta` and theta` phases in the matrix, resulting in an
   increase in hardness. Retrogression at a higher temperature of 240
   degrees C causes the dissolution and coarsening of delta` and theta`
   precipitates in the matrix and on the grain boundaries, resulting in a
   decrease in hardness. Microstructural changes upon retrogression
   including the appearance of equilibrium precipitates such as T-1, T-2,
   delta`, and theta are confirmed by the selected area electron
   diffraction and the bright and dark field image analysis.}},
