@article{ WOS:A1995RK76200021,
Author = {MILLER, RS and MACPHAIL, RA},
Title = {{COMPLEX DYNAMICS AND THE STIMULATED GAIN SPECTRUM OF PENTYLCYANOBIPHENYL
