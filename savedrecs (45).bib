@article{ WOS:000171003700013,
Author = {Mitjans, MS and Sanchis, JB and Padro, XB and Garcia, I and Garolera,
   JMG and Garay, MR},
Title = {{Descending necrotizing mediastinitis}},
Journal = {{INTERNATIONAL SURGERY}},
Year = {{2000}},
Volume = {{85}},
Number = {{4}},
Pages = {{331-335}},
Month = {{OCT-DEC}},
Abstract = {{Between 1990-1999, six patients (mean age, 41.5 years) with the
   diagnosis of descending necrotizing mediastinitis were surgically
   treated at our institution. All patients underwent cervical drainage
   plus an aggressive mediastinal debridement and drainage via
