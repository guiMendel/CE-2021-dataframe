@article{ WOS:000363347300008,
Author = {Li-ChunHsieh, Kevin and Schob, Stefan and Zeller, Matthias W. G. and
   Pulli, Benjamin and Ali, Muhammad and Wang, Cuihua and Chiou, Terry
   Ting-Yu and Tsang, Yuk-Ming and Lee, Po-Shun and Stossel, Thomas P. and
   Chen, John W.},
Title = {{Gelsolin decreases actin toxicity and inflammation in murine multiple
   sclerosis}},
Journal = {{JOURNAL OF NEUROIMMUNOLOGY}},
Year = {{2015}},
Volume = {{287}},
Pages = {{36-42}},
Month = {{OCT 15}},
Abstract = {{Gelsolin is the fourth most abundant protein in the body and its
   depletion in the blood has been found in multiple sclerosis (MS)
   patients. How gelsolin affects the MS brain has not been studied. We
   found that while the secreted form of gelsolin (pGSN) decreased in the
   blood of experimental autoimmune encephalomyelitis (EAE) mice, pGSN
   concentration increased in the EAE brain. Recombinant human pGSN
   (rhp-GSN) decreased extracellular actin and myeloperoxidase activity in
   the brain, resulting in reduced disease activity and less severe
   clinical disease, suggesting that gelsolin could be a potential
   therapeutic target for MS. (C) 2015 Elsevier B.V. All rights reserved.}},
