@article{ WOS:000609027500007,
Author = {Agti, F. Zohra and Soltania, M. Toufik and Santos, Luis F. and
   Messaouclie, A. and De Ligny, D.},
Title = {{Study of New Antimony Phosphate Glasses}},
Journal = {{JOURNAL OF NEW TECHNOLOGY AND MATERIALS}},
Year = {{2020}},
Volume = {{10}},
Number = {{2}},
Pages = {{64-69}},
Month = {{DEC}},
Abstract = {{Crystallization kinetic studies of ternary antimonite glasses within the
   60 Sb2O3-(40-x) NaPO3-xWO3 system x= (5, 15) were prepared using the
   melt quenching technique were performed by non-isothermal methods using
   differential scanning calorimetric (DSC) its measurements showed glasses
   a very stable with the addition of WO3. The kinetic parameters Ea and n
   by applying Ozawa and Chen methods were found to be in good agreement
   with each other the values of crystallization activation energy were
   calculated in the range of 49, 30 Kcal.mol(-1) (Chen) and 52,
   33Kcal.mol(-1) (Ozawa) and Avrami constant, n(moy)(SN5W)=2.05,
