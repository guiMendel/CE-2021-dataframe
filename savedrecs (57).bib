@article{ WOS:A1995TF52300002,
Author = {HUANG, JM and BERLIN, CI and CULLEN, JK and WICKREMASINGHE, AR},
Title = {{DEVELOPMENT OF THE VIIITH-NERVE COMPOUND ACTION-POTENTIAL EVOKED BY
   LOW-INTENSITY TONE PIPS IN THE MONGOLIAN GERBIL}},
Journal = {{HEARING RESEARCH}},
Year = {{1995}},
Volume = {{88}},
Number = {{1-2}},
Pages = {{14-18}},
Month = {{AUG}},
Abstract = {{Maturation of the cochlea and afferent auditory units is reflected by
   changes in VIIIth nerve compound action potential (CAP) parameters. We
   studied auditory nerve CAPs evoked by low-intensity stimuli in Mongolian
   gerbils (Meriones unguiculatus) ranging in age from 22 to 92 days after
   birth. The gerbil CAP development is characterized by marked changes in
   latency, threshold, and amplitude during the first few weeks of life.
   CAP latency and CAP threshold reach adult-like values at about 1 month
   of age. In contrast, the CAP amplitude continues to grow in size even
   after 2 months. This dichotomy suggests that the development of afferent
   auditory nerve function in the gerbil is preceded by maturation of the
   mechanical processes of the middle ear and cochlea.}},
