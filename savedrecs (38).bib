@article{ WOS:000231764300002,
Author = {Singh, H},
Title = {{Hot corrosion studies on plasma spray coatings over some nickel- and
   iron-base superalloys}},
Journal = {{JOURNAL OF THERMAL SPRAY TECHNOLOGY}},
Year = {{2005}},
Volume = {{14}},
Number = {{3}},
Pages = {{311-312}},
Month = {{SEP}},
Abstract = {{Plasma sprayed deposits on selected nickel- and iron-base superalloys
   were characterized in the as-sprayed condition. Subsequently oxidation
   behavior of the uncoated as well as coated superalloys was investigated
   in air and in molten salt environment (Na2SO4-60\%V2O5) under cyclic
   conditions. The studies were performed in a silicon tube furnace for 50
   cycles; each cycle consisted of 1 h heating at 900 degrees C followed by
   20 min cooling. The coatings were deposited using 40 kW Miller Thermal
   (USA) plasma spray system. Four compositions of coatings were used:
   Ni-22Cr-10Al-1Y (NiCrAlY), Ni- 20Cr, Ni3Al, and Stellite-6.
   Approximately 150 mu m thick layer of NiCrAlY was applied as a bond coat
   before deposition of the top layer of approximately 200 mu m in all
   cases. All the coating powders were commercially available except the
   Ni3Al powder, which was prepared by mixing nickel and aluminum powders
   in the stoichiometric ratios in a ball mill. In as-sprayed condition,
   the formation of Ni3Al as main phase was confirmed by XRD analysis. The
   plasma sprayed coatings were found to have some porosity.}},
