@article{ WOS:000596076600019,
Author = {Das, Shaon Kumar and Ghosh, Goutam Kumar and Avasthe, Ravikant},
Title = {{Ecotoxicological responses of weed biochar on seed germination and
   seedling growth in acidic soil}},
Journal = {{ENVIRONMENTAL TECHNOLOGY \& INNOVATION}},
Year = {{2020}},
Volume = {{20}},
Month = {{NOV}},
Abstract = {{Ecotoxicological measurement is vital for unbeaten soil-based/soil-less
   amendment with bioenergy by-products like biochar and the germination
   experiment is a trustworthy method for different types of biochar
   phytotoxicity investigation. An experiment was conducted to study the
   ecotoxicological responses of four weed biochar on seed germination and
   seedling growth in acidic soil at three different level of application
   in maize and black gram. The weed derived biochar were prepared at 600
   degrees C followed by their characterization. With increase in biochar
   application rate the seed germination increased for both maize and
   black. The seedling growth parameter was significantly enhanced by low
   biochar application rate (5 t/ha) than high rate (10 t/ha). The
   Artemisia vulgaris (AVB) biochar performed better than all other biochar
   and it may be due to that the original biomass AV was medicinal in
   nature. The Bidens sp. (BB) and Neyraridia sp. (NB) biochar performed
   moderately and the worst performance observed by Chromolaena odorata
   (COB) biochar for both seed germination and seedling growth. At higher
   dose few seedling growth parameters were inhibited due to presence of
   toxic level elements selenium and chromium. The shoot/root ration (s/r)
   increased with augmentation in biochar application rate except NB for
   maize and irregular trend for black gram. Thus low cost biochar should
   be promoted for the welfare of agricultural production. (C) 2020
   Elsevier B.V. All rights reserved.}},
