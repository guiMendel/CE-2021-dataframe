@article{ WOS:000281315500031,
Author = {Li, Xianghong and Deng, Shuduan and Fu, Hui},
Title = {{Inhibition effect of methyl violet on the corrosion of cold rolled steel
   in 1.0 M HCl solution}},
Journal = {{CORROSION SCIENCE}},
Year = {{2010}},
Volume = {{52}},
Number = {{10}},
Pages = {{3413-3420}},
Month = {{OCT}},
Abstract = {{The inhibition effect of methyl violet (MV) on the corrosion of cold
   rolled steel (CRS) in 1.0 M HCl solution at 20-50 degrees C was studied
   by weight loss, potentiodynamic polarization, electrochemical impedance
   spectroscopy (EIS), and scanning electron microscopy (SEM) methods. The
   results show that MV is an excellent inhibitor even with very low
   concentration, and the adsorption of MV on CRS surface obeys Langmuir
   adsorption isotherm. Potentiodynamic polarization curves reveal that MV
   behaves as a mixed-type inhibitor. EIS spectra exhibit one capacitive
   loop and confirm the inhibitive ability. The inhibition action of MV is
   also evidenced by SEM. (C) 2010 Elsevier Ltd. All rights reserved.}},
