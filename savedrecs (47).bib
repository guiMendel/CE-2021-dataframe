@article{ WOS:000089275800010,
Author = {Stenull, O and Janssen, HK},
Title = {{Master operators govern multifractality in percolation}},
Journal = {{EUROPHYSICS LETTERS}},
Year = {{2000}},
Volume = {{51}},
Number = {{5}},
Pages = {{539-545}},
Month = {{SEP}},
Abstract = {{Using renormalization group methods, we study multifractality in
   percolation at the instance of noisy random resistor networks. We
   introduce the concept of master operators. The multifractal moments of
   the current distribution (which are proportional to the noise cumulants
   C-R((l)) (x, x') of the resistance between two sites x and x' located on
   the same cluster) are related to such master operators. The scaling
   behavior of the multifractal moments is governed exclusively by the
   master operators, even though a myriad of servant operators is involved
   in the renormalization procedure. We calculate the family of
   multifractal exponents \{psi(l)\} for the scaling behavior of the noise
   cumulants, C-R((l)) (x, x') similar to \textbackslash{}x -
   x'\textbackslash{}(psi l/nu), where nu is the correlation length
   exponent for percolation, to two-loop order.}},
