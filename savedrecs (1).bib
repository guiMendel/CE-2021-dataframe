@article{ WOS:000599510700003,
Author = {Faul, Fabian T. and Steiner, Hans-Juergen and Eibert, Thomas F.},
Title = {{Near-Field Antenna Measurements with Manual Collection of the
   Measurement Samples}},
Journal = {{ADVANCES IN RADIO SCIENCE}},
Year = {{2020}},
Volume = {{18}},
Pages = {{17-22}},
Month = {{DEC 10}},
Abstract = {{Near-field measurements are commonly performed in anechoic chambers
   which limits the flexibility of the measurements and requires high
   precision equipment to achieve exact results. In this contribution, we
   investigate a simple near-field measurement setup which does not use any
   sophisticated positioning system nor operates in a controlled
   environment. Instead, the probe antenna is moved by an operator person
   while the probe position is measured by a laser tracker. This implies
   that the measurement results will have a higher error level in
   comparison with antenna chamber measurements. However, excellent error
   levels are not always necessary, especially when it comes to on-site
   testing of the principle functionality of antennas. Measurement results
   are shown to illustrate the performance of the system.}},
