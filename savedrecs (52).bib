@article{ WOS:A1995TE94200016,
Author = {DIAS, AR and GARCIA, MH and RODRIGUES, JC and PETERSEN, JC and
   BJORNHOLM, T and GEISLER, T},
Title = {{3RD-HARMONIC GENERATION IN ORGANOMETALLIC RUTHENIUM(II) DERIVATIVES
   CONTAINING COORDINATED P-SUBSTITUTED BENZONITRILES}},
Journal = {{JOURNAL OF MATERIALS CHEMISTRY}},
Year = {{1995}},
Volume = {{5}},
Number = {{11}},
Pages = {{1861-1865}},
Month = {{NOV}},
Abstract = {{A family of eta 5-monocyclopentadienylruthenium(II) derivatives
   possessing coordinated p-substituted benzonitriles has been studied by
   optical third-harmonic generation (THG) at the fundamental wavelength of
   lambda=1064 nm. Thin films of the compounds in a polymer matrix of
   poly(methyl methacrylate) (PMMA) were studied using the Maker fringe
   technique. For the compound possessing the p-NCC6H4C6H4NO2 coordinated
   nitrile the second hyperpolarizability, lambda(- 3
   omega;omega,omega,omega), was determined to be 2.3 x10(-33) esu.
   Spectroscopic results show evidence of pi back-donation resulting in an
