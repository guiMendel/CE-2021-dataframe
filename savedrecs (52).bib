@article{ WOS:A1995TD65300057,
Author = {RIEBER, MS and RIEBER, M},
Title = {{SUPPRESSION OF CYCLIN D1 BUT NOT CDK4 OR CYCLIN-A WITH INDUCTION OF
   MELANOMA TERMINAL DIFFERENTIATION}},
Journal = {{BIOCHEMICAL AND BIOPHYSICAL RESEARCH COMMUNICATIONS}},
Year = {{1995}},
Volume = {{216}},
Number = {{1}},
Pages = {{422-427}},
Month = {{NOV 2}},
Abstract = {{To identify cyclins specifically associated with control of melanoma
   cell proliferation, we now compared expression of cyclin A, reported to
   be a marker for hematological malignancies, with. that of cyclin D and
   its cdk4 kinase partner. All these proteins were expressed in
   proliferating B16 melanoma. However, L-tyrosine which induces melanoma
   terminal differentiation, selectively decreased cyclin D with no
   comparable effect on cdk4 or cyclin A. A 5-hour exposure of the cells to
   the tyrosine phosphatase inhibitor, sodium vanadate,further decreased
   cyclin D from differentiated cells, suggesting that tyrosine
   phosphorylation regulates cyclin D turnover. Addition of serum to
   starved cells also revealed that tyrosine did not block the early cyclin
   D increase associated with serum stimulation, but accelerated its
   subsequent loss. Our data suggest that cyclin D decrease with melanoma
   terminal differentiation could be an alternative mode of growth arrest
   even in cells harbouring a mutant or transcriptionally silent cdk4
   inhibitor tumor suppressor p16(ink4) gene. These results also imply that
   cyclin D may be useful as a target and as a prognostic marker in
   melanoma therapy. (C) 1995 Academic Press, Inc.}},
