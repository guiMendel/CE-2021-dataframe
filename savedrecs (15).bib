@article{ WOS:000364408000010,
Author = {Centini, M. and Benedetti, A. and Larciprete, M. C. and Belardini, A.
   and Voti, R. Li and Bertolotti, M. and Sibilia, C.},
Title = {{Midinfrared thermal emission properties of finite arrays of gold dipole
   nanoantennas}},
Journal = {{PHYSICAL REVIEW B}},
Year = {{2015}},
Volume = {{92}},
Number = {{20}},
Month = {{NOV 9}},
Abstract = {{We studied the far-field thermal emission properties of finite arrays of
   resonant gold dipole nanoantennas at equilibrium temperature. We
   numerically investigated the transition from the super-Planckian
   emission of the single resonant antenna to the sub-Planckian emission
   inherent to infinite periodic arrays. Increasing the number of unit
   cells of the array, the overall size of the system increases, and the
   relative emissivity quickly converges to values lower than the unity.
   Nevertheless, if the separation between nanoantennas in the array is
   small compared to the wavelength, the near-field interaction makes the
   emission of each unit cell multipolar. This opens the doors for
   additional tailoring of the emitted power and directionality of thermal
   radiation.}},
