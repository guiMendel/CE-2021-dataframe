@article{ WOS:000608978900014,
Author = {Han, Ruodong and Xiao, Han and Qian, Beili and Yue, Yajie and Li, Bowen},
Title = {{Effects of miR-663a on Kidney Injury in Septic Rats by Regulating Tumor
   Growth Factor-beta 1 Level}},
Journal = {{JOURNAL OF BIOMATERIALS AND TISSUE ENGINEERING}},
Year = {{2020}},
Volume = {{10}},
Number = {{11}},
Pages = {{1644-1648}},
Month = {{NOV}},
Abstract = {{TGF-beta 1 is associated with sepsis renal injury. miR-663a regulates
   TGF-beta 1 level by MiRDB analysis. Our study investigated whether
   miR-663a involves in renal damage of sepsis via TGF-beta 1. 30 healthy
   SD rats were divided into control group, Sepsis group and Sepsis +
   miR-663a inhibitor group followed by analysis of creatinine and urea
   nitrogen (BUN) by automatic biochemical analyzer, renal cell apoptosis
   by TUNES method, miR-663a level by qRT-PCR and TGF-beta 1 and Tubulin
   protein level by Western blot. Compared with Control group, blood
   creatinine and BUN were significantly elevated in Sepsis group, and
   renal cell apoptosis was increased (P < 0.01). Compared with Sepsis
   group, creatinine and BUN levels of the Sepsis +miR-663a inhibitor group
   were decreased significantly and apoptosis of renal tissues were
   decreased (P < 0.05). The miR-663a level in Sepsis group was
   significantly increased (P < 0.01), which was significantly reduced by
   miR-663a inhibitor (P < 0.05). TGF-beta 1 is the target of miR-663a.
   Compared to Control group, TGF-beta 1 level in Sepsis group was
   significantly reduced (P < 0.01) and increased in Sepsis + miR-663a
   inhibitor group (P < 0.05). When sepsis occurs, miR-663a level is
   elevated and TGF-beta 1 level is decreased. MiR-663a binds TGF-beta
   1-UTR and reduce its level. miR-663a causes sepsis kidney damage by
   reducing TGF-beta 1 and miR-663a inhibitor can ameliorate sepsis kidney
   damage by antagonizing the effect of miR-663a.}},
