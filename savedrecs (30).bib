@article{ WOS:000235061100016,
Author = {Dadashev, SY and Grishaeva, TM and Bogdanov, YF},
Title = {{In silico identification and characterization of meiotic DNA: AluJb
   possibly participates in the attachment of chromatin loops to
   synaptonemal complex}},
Journal = {{RUSSIAN JOURNAL OF GENETICS}},
Year = {{2005}},
Volume = {{41}},
Number = {{12}},
Pages = {{1419-1424}},
Month = {{DEC}},
Abstract = {{Earlier, using bioinformatic methods, we reported the identification of
   repeated DNA sequences (RSs), presumably responsible for the attachment
   of chromatin loops to the lateral elements of synaptonemal complex in
   meiotic chromosomes. In the present study, consensus sequences for this
   class of RS were identified. It was demonstrated that at least part of
   these sequences belonged to the AluJb subfamily of Alu sequences. The
   Alu copies distribution along the major human histocompatibility complex
   (MHC) and their spatial separation from the sites of meiotic
   recombination was examined. It was demonstrated that simple sequences,
   like (GT/CA)(n), were flanking meiotic recombination sites. A model of
   the RS organization in meiotic chromosome, most efficiently linking
   experimental data on the meiotic recombination in MHC and the in silico
   data on the RS localization (the coefficient. of multiple correlation, r
   = 0.92) is suggested.}},
