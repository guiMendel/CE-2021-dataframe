@article{ WOS:A1995TH79600010,
Author = {LEE, JM and KIM, KC},
Title = {{VIBRATION ANALYSIS OF RECTANGULAR ISOTROPIC THICK PLATES USING MINDLIN
   PLATE CHARACTERISTIC FUNCTIONS}},
Journal = {{JOURNAL OF SOUND AND VIBRATION}},
Year = {{1995}},
Volume = {{187}},
Number = {{5}},
Pages = {{865-877}},
Month = {{NOV 16}},
Abstract = {{An iterative Kantorovich method is presented for the vibration analysis
   of rectangular isotropic thick plates. Mindlin plate characteristic
   functions are derived in general forms by the Kantorovich method
   initially starting with Timoshenko beam functions consistent with the
   boundary conditions of the plate. Through numerical calculations of
   natural pairs and dynamic responses of appropriate models, it has been
