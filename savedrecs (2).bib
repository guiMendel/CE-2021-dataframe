@article{ WOS:000598575700001,
Author = {Plummer, Paul and Van Poeck, Katrien},
Title = {{Exploring the role of learning in sustainability transitions: a case
   study using a novel analytical approach}},
Journal = {{ENVIRONMENTAL EDUCATION RESEARCH}},
Year = {{2021}},
Volume = {{27}},
Number = {{3}},
Pages = {{418-437}},
Abstract = {{Based on an exploratory case study of a community food waste initiative
   in Gothenburg, Sweden, this article presents a novel analytical approach
   to studying the role of learning within sustainability transition
   niches. The article's theoretical perspective draws from pragmatist
   educational philosophy, practice theory, and sustainability transition
   studies. Proceeding from this theoretical perspective, a two-stage
   empirical analysis is conducted using dramaturgical analysis and
   practical epistemology analysis, methods from sociology and the
   educational sciences. Building on this analysis, the article introduces
   the concept of educative practices, through which it is argued that a
   role of learning at the niche level within sustainability transitions is
   to interrupt the reproduction of norms and attitudes within
   socio-technical systems. The article thus offers original analytical and
   conceptual tools for investigating sustainability transitions in
   relation to learning.}},
