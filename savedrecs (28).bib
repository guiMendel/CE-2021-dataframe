@article{ WOS:000281535600003,
Author = {Sierminska, Eva M. and Frick, Joachim R. and Grabka, Markus M.},
Title = {{Examining the gender wealth gap}},
Journal = {{OXFORD ECONOMIC PAPERS-NEW SERIES}},
Year = {{2010}},
Volume = {{62}},
Number = {{4}},
Pages = {{669-690}},
Month = {{OCT}},
Abstract = {{Economic research on the determinants of gender differences in economic
   outcomes particularly in income and consumption is well established.
   Extending these investigations to other outcomes such as wealth up till
   now has been limited due to lack of individual-level data. Using the
   German Socio-Economic Panel (SOEP) we find a significant `raw' gender
   wealth gap of 50,000euro for married partners. Decomposition analyses
   reveal that the gap is largely driven by differences in characteristics
   between men and women (observables), particularly by individual's own
   income and labour market experience. This is especially true at the
   bottom and at the top of the wealth distribution, which we show using
   semi-parametric decomposition techniques. Differences in the lower half
   of the distribution are mostly driven by the wealth function, i.e., the
   way in which women transform their characteristics into wealth.}},
