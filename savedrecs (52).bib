@article{ WOS:A1995TF60200002,
Author = {GRITZO, LA and NICOLETTE, VF},
Title = {{COUPLED THERMAL RESPONSE OF OBJECTS AND PARTICIPATING MEDIA IN FIRES AND
   LARGE COMBUSTION SYSTEMS}},
Journal = {{NUMERICAL HEAT TRANSFER PART A-APPLICATIONS}},
Year = {{1995}},
Volume = {{28}},
Number = {{5}},
Pages = {{531-545}},
Month = {{NOV}},
Abstract = {{When an object is subjected to a flowing, participating medium at a
   different temperature, such as a flame, the thermal response of the
   object and the surrounding medium becomes coupled. The differences
   inherent in treating the medium as a blackbody at some approximate
   temperature as opposed to a participating medium are assessed and
   quantified in this work by solving the transient, coupled, radiation,
   and conduction heat transfer problem for the flow of an absorbing and
   emitting medium adjacent to a vertical flat plate. The results are
   presented in terms of nondimensional parameters and include both average
   and local heat fluxes as a function of time. Early in the transient, a
   reduction in net heat flux of up to 65\% was observed by accounting for
   absorption and emission in the medium. Accordingly, a longer time is
   required for the object to reach an equilibrium temperature (Icp to
   100\% for values of the radiation parameter, N-rad, of 5.0) than is
   predicted using the blackbody assumption. For radiation Blot numbers
   greater than 5 or values of N-rad less than 10(-2), the differences
   inherent in the two approaches are negligible, and the blackbody
   assumption is valid.}},
Publisher = {{HEMISPHERE PUBL CORP}},
Address = {{1900 FROST ROAD, SUITE 101, BRISTOL, PA 19007-1598}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{GRITZO, LA (Corresponding Author), SANDIA NATL LABS,DEPT THERMAL \& FLUID ENGN 1513,POB 5800,ALBUQUERQUE,NM 87185, USA.}},
