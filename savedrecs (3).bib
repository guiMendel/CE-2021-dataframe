@article{ WOS:000581178900013,
Author = {Chou, An-Kuo and Chiu, Chong-Chi and Liu, Chen-Chih and Wang, Jhi-Joung
   and Chen, Yu-Wen and Hung, Ching-Hsia},
Title = {{PULSED ULTRASOUND REMEDIES POST-THORACOTOMY HYPERSENSITIVITY AND
   INCREASES SPINAL ANTI-INFLAMMATORY CYTOKINE IN RATS}},
Journal = {{ULTRASOUND IN MEDICINE AND BIOLOGY}},
Year = {{2020}},
Volume = {{46}},
Number = {{12}},
Pages = {{3296-3304}},
Month = {{DEC}},
Abstract = {{The purpose of the experiment was to study the effect of pulsed
   ultrasound (PUS) on post-thoracotomy pain and local tissue temperature
   and to correlate the findings with the alteration in spinal
   anti-inflammatory and pro-inflammatory cytokines. Mechanical
   sensitivity, subcutaneous temperature and spinal interleukin-10 (IL-10),
   IL-6 or tumor necrosis factor-alpha (TNF-alpha) expression were examined
   in a rat model of experimental post-thoracotomy pain. Group 1 received a
   sham surgery where thoracotomy was performed except for rib retraction.
   Group 2 underwent thoracotomy with rib retraction (TRR). Group 3
   received the TRR procedure followed by PUS. Group 4 underwent the TRR
   procedure followed by only the massage with the ultrasound turned off.
   Compared with group 1 (sham), groups 2-4 showed a decrease in mechanical
   withdrawal thresholds on postoperative days (PODs) 10 and 11. On PODs
   16, 23 and 30, group 3 (TRR+PUS-1) displayed an increase in mechanical
   withdrawal thresholds compared with groups 2 and 4. Subcutaneous and
   body temperatures in group 3 were not prominently different from group
   1, group 2 (TRR only) or group 4 (TRR+PUS-0). Compared with group 2,
   group 3 had an increase in spinal IL-10 level on POD 30 and a decrease
   in spinal IL-6 or TNF-alpha expression on PODs 16 and 30. We concluded
   that mechanical hypersensitivity after TRR is postponed by PUS, and its
   effect continues for 3 wk. A PUS dose not increase local tissue
   temperature. The beneficial effect of PUS appears related to
   upregulation of spinal anti-inflammatory cytokine and downregulation of
   spinal pro-inflammatory cytokines. (E-mail: chhung@mail.ncku.edu.tw) (C)
   2020 World Federation for Ultrasound in Medicine \& Biology. All rights
   reserved.}},
