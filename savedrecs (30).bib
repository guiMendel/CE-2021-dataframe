@article{ WOS:000234652700002,
Author = {P'Ng, KMY and Bushby, AJ and Dunstan, DJ},
Title = {{Strength of coherently strained layered superlattices}},
Journal = {{PHILOSOPHICAL MAGAZINE}},
Year = {{2005}},
Volume = {{85}},
Number = {{36}},
Pages = {{4429-4444}},
Month = {{DEC 21}},
Abstract = {{Electronic-grade single-crystal semiconductor structures provide a means
   to study the mechanical effects of coherency strain in isolation. In
   this work, thin coherently strained InGaAs superlattices grown on thick
   InP substrates were tested in three-point bending at 500 degrees C. The
   force-deflection curves of the specimens were measured and the beams
   were found to be significantly strengthened by the presence of the
   superlattices. Analysis provides estimates of the stress supported by
   the thin superlattices in the plastic regime. The superlattices are
   found to display mechanical strength up to a hundred times greater than
   the strength of the bulk substrate material. This effect can be
   attributed only to the coherency strain in the superlattices.}},
