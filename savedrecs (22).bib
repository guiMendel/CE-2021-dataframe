@article{ WOS:000284531700012,
Author = {Sundstrom, Teather J. and Wright, Dennis L.},
Title = {{Azaviridins as New Scaffolds for the Development of PI-3K Inhibitors}},
Journal = {{SYNLETT}},
Year = {{2010}},
Number = {{19}},
Pages = {{2875-2878}},
Month = {{DEC}},
Abstract = {{We have designed a new class of non-natural furanosteroids to serve as
   more chemically stable analogues of the viridins, potent inhibitors of
   PI-3K. Central to the design is the incorporation of a nitrogen atom
