@article{ WOS:000286026200050,
Author = {Park, H. K. and Nam, K. O. and Kim, D. J. and Ahn, H. J. and Lee, J. H.
   and Im, K. and Shaw, R.},
Title = {{A conceptual design of assembly strategy and dedicated tools for
   assembly of 40 degrees sector}},
Journal = {{FUSION ENGINEERING AND DESIGN}},
Year = {{2010}},
Volume = {{85}},
Number = {{10-12}},
Pages = {{1981-1985}},
Month = {{DEC}},
Note = {{9th International Symposium on Fusion Nuclear Technology, Dalian,
   PEOPLES R CHINA, OCT 11-16, 2009}},
Abstract = {{The International Thermanuclear Experimental Reactor (ITER) tokamak
   device is composed of 9 vacuum vessel (VV)/toroidal field coils
   (TFCs)/vacuum vessel thermal shields (VVTS) 40 degrees sectors. Each
   VV/TFCs/VVTS 40 degrees sector is made up of one 40 degrees VV, two 20
   degrees TFCs and associated VVTS segments. The 40 degrees sectors are
   sub-assembled at assembly hall respectively and then nine 40 degrees
   sectors sub-assembled at assembly hall are finally assembled at tokamak
   in-pit hall.
   The assembly strategy and tools for the 40 degrees sector sub-assembly
   and final assembly should be developed to satisfy the basic assembly
   requirements of the ITER tokamak device. Accordingly, the purpose-built
   assembly tools should be designed and manufactured considering assembly
   plan, available space, cost, safety, easy operation, efficient
   maintenance, and so on. The 40 degrees sector assembly tools are
   classified into 2 groups. One group is the sub-assembly tools including
   upending tool, lifting tool, sub-assembly tool, VV supports and bracing
   tools used at assembly hall and the other group is the in-pit assembly
