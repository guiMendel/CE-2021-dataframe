Title = {{Heterosis and combining ability in sunflower (Helianthus annuus)}},
Journal = {{INDIAN JOURNAL OF AGRICULTURAL SCIENCES}},
Year = {{2000}},
Volume = {{70}},
Number = {{8}},
Pages = {{525-529}},
