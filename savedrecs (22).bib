@article{ WOS:000286075700122,
Author = {Shamoto, Shin-ichi and Ishikado, Motoyuki and Wakimoto, Shuichi and
   Kodama, Katsuaki and Kajimoto, Ryoichi and Arai, Masatoshi and Fukuda,
   Tatsuo and Nakamura, Hiroki and Machida, Masahiko and Eisaki, Hiroshi},
Title = {{Spin excitations in iron oxypnictide superconductor system}},
Journal = {{PHYSICA C-SUPERCONDUCTIVITY AND ITS APPLICATIONS}},
Year = {{2010}},
Volume = {{470}},
Number = {{1, SI}},
Pages = {{S284-S287}},
Month = {{DEC}},
Note = {{9th International Conference on Materials and Mechanisms of
   Superconductivity, Tokyo, JAPAN, SEP 07-12, 2009}},
Abstract = {{The spin excitations in the LaFeAsO1-xFx system have been studied by
   inelastic neutron scattering measurements with powder samples. In the
   parent compound, the spin excitation was observed as a magnetic rod at
   (pi, pi, l) in the downfolded tetragonal Brillouin zone. It persisted
   even in the tetragonal phase at T = 280 K. In the superconducting
   compounds with x = 0.057 and 0.082, the spin excitations developed below
   the superconducting transition temperatures up to the same order of
   magnitude as that in the parent compound, and they were peaked at about
   11 meV. The spin excitation, however, almost disappeared in an overdoped
   compound with x = 0.158, which exhibited only low temperature
   superconductivity below 7 K. These results indicate that the present
   superconductivity intimately correlates with the spin fluctuation
   possibly in relation to the nesting condition between the Fermi surfaces
   at I-and M points. (C) 2009 Elsevier B.V. All rights reserved.}},
