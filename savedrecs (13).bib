@article{ WOS:000361920300014,
Author = {O'Reilly, Colin},
Title = {{Household Recovery from Internal Displacement in Northern Uganda}},
Journal = {{WORLD DEVELOPMENT}},
Year = {{2015}},
Volume = {{76}},
Pages = {{203-215}},
Month = {{DEC}},
Abstract = {{Northern Uganda experienced violent conflict for over 15 years,
   resulting in the internal displacement of over 1,800,000 Ugandans. In
   the five years that followed a cease fire agreement in 2006 nearly all
   the displaced persons returned home. The difference in the growth of
   consumption between returnee households and a comparison group of
   non-displaced households is estimated using propensity score matching.
   After an initial shock to consumption and assets upon return, returnee
   households experience a period of catch-up growth. These results
   contribute to understanding the dynamics of recovery from displacement
   and have implications for the policy response during recovery. (C) 2015
   Elsevier Ltd. All rights reserved.}},
