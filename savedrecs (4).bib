@article{ WOS:000604404900008,
Author = {Liu, Chengyong and Wu, Jianbo and Ding, Song and Wang, Zihan and Zhang,
   Yunfei},
Title = {{Magnetic Barkhausen Noise Testing for Subsurface Discontinuities under
   DC Magnetization}},
Journal = {{MATERIALS EVALUATION}},
