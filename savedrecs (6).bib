@article{ WOS:000595546000019,
Author = {Gui, Xin and Chang, Tay-Rong and Wei, Kaya and Daum, Marcus J. and Graf,
   David E. and Baumbach, Ryan E. and Mourigal, Martin and Xie, Weiwei},
Title = {{A Novel Magnetic Material by Design: Observation of Yb3+ with Spin-1/2
   in YbxPt5P}},
Journal = {{ACS CENTRAL SCIENCE}},
Year = {{2020}},
Volume = {{6}},
Number = {{11}},
Pages = {{2023-2030}},
Month = {{NOV 25}},
Abstract = {{The localized f-electrons enrich the magnetic properties in
   rare-earth-based intermetallics. Among those, compounds with heavier 4d
   and 5d transition metals are even more fascinating because anomalous
   electronic properties may be induced by the hybridization of 4f and
   itinerant conduction electrons primarily from the d orbitals. Here, we
   describe the observation of trivalent Yb3+ with S = 1/2 at low
   temperatures in YbxPt5P, the first of a new family of materials. YbxPt5P
   (0.23 <= x <= 0.96) phases were synthesized and structurally
   characterized. They exhibit a large homogeneity width with the Yb ratio
   exclusively occupying the la site in the anti-CeCoIn5 structure.
   Moreover, a sudden resistivity drop could be found in YbxPt5P below
   similar to 0.6 K, which requires further investigation. First-principles
   electronic structure calculations substantiate the antiferromagnetic
   ground state and indicate that two-dimensional nesting around the Fermi
   level may give rise to exotic physical properties, such as
   superconductivity. YbxPt5P appears to be a unique case among materials.}},
