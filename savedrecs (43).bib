@article{ WOS:000165634300009,
Author = {Tachikawa, D and Arima, S and Futami, K},
Title = {{Immunohistochemical expression of thymidylate synthase as a prognostic
   factor and as a chemotherapeutic efficacy index in patients with
   colorectal carcinoma}},
Journal = {{ANTICANCER RESEARCH}},
Year = {{2000}},
Volume = {{20}},
Number = {{6A}},
