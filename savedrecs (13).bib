@article{ WOS:000365187700004,
Author = {Perret, Anthony and Comtet, Alain and Majumdar, Satya N. and Schehr,
   Gregory},
Title = {{On Certain Functionals of the Maximum of Brownian Motion and Their
   Applications}},
Journal = {{JOURNAL OF STATISTICAL PHYSICS}},
Year = {{2015}},
Volume = {{161}},
Number = {{5}},
Pages = {{1112-1154}},
Month = {{DEC}},
Abstract = {{We consider a Brownian motion (BM) and its maximal value on a fixed time
   interval {[}0, t]. We study functionals of the maximum of the BM, of the
   form where V(x) can be any arbitrary function and develop various
   analytical tools to compute their statistical properties. These tools
