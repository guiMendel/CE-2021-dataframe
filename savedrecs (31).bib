@article{ WOS:000234310200012,
Author = {Serezhkin, VN and Pushkin, DV and Serezhkina, LB and Sevast'yanov, VG
   and Kuznetsov, NT},
Title = {{A new method for analyzing intermolecular interactions in crystal
   structures: Metal carbonyls}},
Journal = {{RUSSIAN JOURNAL OF INORGANIC CHEMISTRY}},
Year = {{2005}},
Volume = {{50}},
Number = {{12}},
Pages = {{1893-1902}},
Month = {{DEC}},
Abstract = {{A new method of crystal-chemical analysis of intermolecular interactions
   using the characteristics of the Voronoi-Dirichlet (VD) polyhedra of
   atoms in crystal structures was exemplified by transition metal
   carbonyls. From the crystal-chemical data on the compounds under
   consideration, the area (S-m) of the VD polyhedron faces corresponding
   to all the intermolecular contacts of one molecule, the total volume of
   the pyramids (V-m) whose bases are such faces and whose apices are
   occupied by the nuclei of the atoms involved in intermolecular contacts,
   and the total solid angle (Omega(m)) at which the intermolecular faces
   of VD polyhedra are seen from the corresponding nuclei of molecule atoms
   were determined. Most of the intermolecular interactions were shown to
   correspond to the contacts between nonmetal atoms of neighboring
   molecules. It was found that the enthalpy of sublimation of carbonyls is
   a linear function of V-m. This correlation dependence was used for
   calculating the enthalpy of sublimation of some carbonyls with known
   crystal structures but unknown thermochemical properties.}},
