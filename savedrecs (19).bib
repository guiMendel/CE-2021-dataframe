@article{ WOS:000369169000009,
Author = {Du, Ximing and Zhang, Yuxi and Jo, Sae Rom and Liu, Xiaoyun and Qi,
   Yanfei and Osborne, Brenna and Byrne, Frances L. and Smith, Greg C. and
   Turner, Nigel and Hoehn, Kyle L. and Brown, Andrew J. and Yang, Hongyuan},
Title = {{Akt activation increases cellular cholesterol by promoting the
   proteasomal degradation of Niemann-Pick C1}},
Journal = {{BIOCHEMICAL JOURNAL}},
Year = {{2015}},
Volume = {{471}},
Number = {{2}},
Pages = {{243-253}},
Month = {{OCT 15}},
Abstract = {{Null mutations of the Niemann-Pick type C1 (NPC1) gene cause NPC
   disease, a lysosomal storage disorder characterized by cholesterol
   accumulation in late endosomes (LE) and lysosomes (Ly). Nascent
   ormutated NPC1 is degraded through the ubiquitin-proteasome pathway, but
   how NPC1 degradation is regulated remains currently unknown. In the
   present study, we demonstrated a link between NPC1 degradation and the
   Akt (protein kinase B)/mTOR {[}mammalian (or mechanistic) target of
   rapamycin] signalling pathway in cervical cancer cell lines. We provided
   evidence that activated Akt/mTOR pathway increased NPC1 degradation by
   similar to 50\% in C33A cells when compared with SiHa or HeLa cells.
   NPC1 degradation in C33A cells was reversed when Akt/mTOR activation was
   blocked by specific inhibitors or when mTORC1 (mTOR complex 1) was
   disrupted by regulatory associated protein of mTOR ( Raptor) knockdown.
   Importantly, inhibition of the Akt/mTOR pathway led to decreased NPC1
   ubiquitination in C33A cells, pointing to a role of Akt/mTOR in the
   proteasomal degradation of NPC1. Moreover, we found that NPC1 depletion
   in several cancer cell lines inhibited cell proliferation and migration.
   Our results uncover Akt as a key regulator of NPC1 degradation and link
   NPC1 to cancer cell proliferation and migration.}},
