@article{ WOS:A1995RT00500010,
Author = {FRANCA, JG and VOLCHAN, E},
Title = {{NADPH DIAPHORASE HISTOCHEMISTRY AS A MARKER FOR BARRELS IN RAT
   SOMATOSENSORY CORTEX}},
Journal = {{BRAZILIAN JOURNAL OF MEDICAL AND BIOLOGICAL RESEARCH}},
Year = {{1995}},
Volume = {{28}},
Number = {{7}},
Pages = {{787-790}},
Month = {{JUL}},
Abstract = {{The primary somatosensory area (S1) of rodents presents multicellular
   units called barrels which can be identified by different techniques
   (e.g., Nissl staining, cytochrome oxidase or succinate dehydrogenase
   histochemistry). We applied NADPH diaphorase histochemistry to
   tangential sections of rat neocortex to determine if the reactive
   neuropil also shows the same remarkable array observed with other
   techniques. We demonstrated NADPH diaphorase-positive barrels in all
   hemispheres tested. The barrels are recognized as patches where the
   neuropil is most reactive. Each barrel is separated from the other by a
   less labeled neuropil. Many NADPH diaphorase-positive neurons are seen
   along the section, but very few cells are found in the barrel field. In
   this region, most of the labeled neurons are localized in the less
   reactive region between the barrels, although a few NADPH
   diaphorase-positive cells can also be found inside the barrels.}},
