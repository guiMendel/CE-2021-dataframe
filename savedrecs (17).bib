@article{ WOS:000467464600036,
Author = {Naseem, Muhammad and Moton, Rahat Zahoor and Siddiqui, Muhammad Aslam},
Title = {{Comparison of titanium elastic nails versus Thomas splint traction for
   treatment of pediatric femur shaft fracture}},
Journal = {{JOURNAL OF THE PAKISTAN MEDICAL ASSOCIATION}},
Year = {{2015}},
Volume = {{65}},
Number = {{11, 3}},
Pages = {{S160-S162}},
Month = {{NOV}},
Abstract = {{Objective: To compare titaniumelastic nails with Thomas splint traction
   treatment for femur shaft fracture in children.
   Methods: The descriptive randomised study was conducted at Abbasi
   Shaheed Hospital, Karachi, from January 2013 to December 2014, and
   comprised children aged 6-12 years with simple femur shaft fractures.
   The patients were randomised into two equal groups. Group A patients
   were treated with titanium elastic nail and group B patients with Thomas
   splint traction. Patients were mobilised without weight-bearing on the
   5th to 7th postoperative day. In group B, traction was maintained for
   about 8 to 10 weeks. After splint removal, patients were referred to
   physical therapy. Patients were available for one-year follow-up.
