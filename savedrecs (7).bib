@article{ WOS:000593180100006,
Author = {Stockton, Patrick A. and Field, Jeffrey J. and Squier, Jeff and
   Pezeshki, Ali and Bartels, Randy A.},
Title = {{Single-pixel fluorescent diffraction tomography}},
Journal = {{OPTICA}},
Year = {{2020}},
Volume = {{7}},
Number = {{11}},
Pages = {{1617-1620}},
Month = {{NOV 20}},
Abstract = {{Optical diffraction tomography (ODT) is an indispensable tool for
   studying objects in three dimensions. Until now, ODT has been limited to
   coherent light because spatial phase information is required to solve
   the inverse scattering problem. We introduce a method that enables ODT
   to be applied to imaging incoherent contrast mechanisms such as
   fluorescent emission. Our strategy mimics the coherent scattering
   process with two spatially coherent illumination beams. The
   interferometric illumination pattern encodes spatial phase in temporal
   variations of the fluorescent emission, thereby allowing incoherent
   fluorescent emission to mimic the behavior of coherent illumination. The
   temporal variations permit recovery of the spatial distribution of
   fluorescent emission with an inverse scattering model. Simulations and
   experiments demonstrate isotropic resolution in the 3D reconstruction of
   a fluorescent object. (C) 2020 Optical Society of America under the
   terms of the OSA Open Access Publishing AgreementY}},
