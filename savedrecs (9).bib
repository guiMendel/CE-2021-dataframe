@article{ WOS:000589165000001,
Author = {Hahn, Hyunggu and Lee, Dong-Eun and Jang, Dong Man and Kim, Jiyoun and
   Lee, Yeon and Cheong, Heesun and Han, Byung Woo and Kim, Hyoun Sook},
Title = {{Structural Insight on Functional Regulation of Human MINERVA Protein}},
