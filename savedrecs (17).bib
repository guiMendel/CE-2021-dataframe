@article{ WOS:000364557700009,
Author = {Pan, Chen and Hu, Yong-lin and Jiang, Xiang-ning and Gai, Ying},
Title = {{Cloning, expression, crystallization and crystallographic analysis of
   CouR from Rhodopseudomonas palustris}},
Journal = {{ACTA CRYSTALLOGRAPHICA SECTION F-STRUCTURAL BIOLOGY COMMUNICATIONS}},
Year = {{2015}},
Volume = {{71}},
Number = {{11}},
Pages = {{1416-1420}},
Month = {{NOV}},
Abstract = {{CouR from Rhodopseudomonas palustris is a member of the MarR
   transcriptional regulator family. It regulates the expression of CouA
   and CouB, enzymes that are involved in the degradation of p-coumarate.
   In vivo, CouR binds to a DNA fragment containing the couAB promoter and
   suppresses the expression of CouA and CouB, while binding of
   p-coumaroyl-CoA attenuates its affinity towards DNA and activates the
   expression of CouA and CouB. Here, the crystallization and X-ray
   diffraction analyses of CouR alone and in complex with p-coumaroyl-CoA
   are reported. Apo and ligand-complexed CouR crystals diffracted to 2.5
   and 3.3 angstrom resolution, respectively. The crystals of apo CouR
   belonged to space group P22(1)2(1), with unit-cell parameters a = 62.78,
   b = 76.15, c = 87.38 angstrom, whereas the crystals of the CouR-ligand
   complex belonged to space group P2(1)2(1)2(1), with unit-cell parameters
   a = 61.37, b = 69.82, c = 70.32 angstrom. The crystals were predicted to
   contain two CouR molecules or CouR-ligand complexes per asymmetric unit.}},
