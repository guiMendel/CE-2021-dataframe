@article{ WOS:000365450000013,
Author = {Smirnov, M. Yu. and Kalinkin, A. V. and Vovk, E. I. and Bukhtiyarov, V.
   I.},
Title = {{Size effect in the oxidation-reduction processes of platinum particles
   supported onto silicon dioxide}},
Journal = {{KINETICS AND CATALYSIS}},
Year = {{2015}},
Volume = {{56}},
Number = {{6}},
Pages = {{801-809}},
Month = {{NOV}},
Abstract = {{The interaction of the Pt/SiO2 model catalysts as thin films on the
   surface of tantalum supports with a mixture of NO + O-2 (1: 1) was
   studied by X-ray photoelectron spectroscopy. The pressure of the
   reaction mixture was varied from 6 to 64 mbar, and the temperature was
   varied from room temperature to 500A degrees C. Two types of the
   catalysts, in which the Pt/Si atomic ratios were similar to 0.1 and
   similar to 0.3 (0.1-Pt/SiO2 and 0.3-Pt/SiO2, respectively) according to
   the XPS data, were studied. In 0.1-Pt/SiO2, the particles of platinum
   predominantly had a size from 1 to 2.5 nm; a wide Pt particle size
   distribution in a range from 1 to 15 nm with a maximum at similar to 4
   nm was characteristic of 0.3-Pt/SiO2. The interaction of all of the
   samples with NO + O-2 at room temperature led to the dissolution of
   oxygen atoms in the bulk of platinum metal particles. As the reaction
   temperature was increased, PtO (x) platinum oxide particles were formed:
   from small Pt particles in 0.1-Pt/SiO2 at 300A degrees C and from larger
   particles in 0.3-Pt/SiO2 at 400-500A degrees C. It was established that
   the reactivity of platinum oxide particles toward hydrogen also depended
   on the particle size. The small particles of platinum oxide were
   converted into platinum metal under the action of hydrogen (16 mbar) at
   300A degrees C. The coarse particles of PtO (x) in the samples of
   0.3-Pt/SiO2 were reduced much more easily starting with room
   temperature.}},
