@article{ WOS:000600992000060,
Author = {Ren, Qiang and Zhao, Yujing and Wu, Xiulan and Zheng, Jinle and Ren,
   Yuhan and Hai, Ou},
Title = {{A novel white Ba3-ySryY1-x(BO3)3:xDy3+phosphor with lower correlated
   color temperature and superior thermal stability}},
Journal = {{JOURNAL OF SOLID STATE CHEMISTRY}},
Year = {{2020}},
Volume = {{292}},
Month = {{DEC}},
Abstract = {{The different concentrations of the Ba3-ySryY1-x (BO3)(3):xDy(3+)
   phosphors was prepared by solid state reaction. In order to further
   identify and describe the crystal structure, luminescence
   characteristics, fluorescence lifetime and thermal stability of the
   sample, XRD and luminescence spectrum tests were performed. The XRD
   showed that the Dy3+ and Sr2+ ions were successfully doped into the
   Ba3Y(BO3)(3) host materials. The white light emitted by the phosphor is
   composed of two emission peaks at 490 nm (blue) and 580 nm (yellow). The
   ratios of yellow-to-blue increased as well as the CCT reduced from 6974
   K, 5876 K-4910 K with the Sr2+ ion increasing. When the temperature
   reached to 373 K, the Ba3Y0.95(BO3)(3):0.05Dy(3+) and
   Ba2.85Sr0.15Y0.95(BO3)(3):0.05Dy(3+) phosphors maintained 85.31\% and
   76.22\% of the thermal stability, whose activation energy were 0.238 eV
   and 0.244 eV. These all show the potential application value of the
   novel white Ba3-ySryY1-x (BO3)(3):xDy(3+) phosphor in the field of white
   LED.}},
