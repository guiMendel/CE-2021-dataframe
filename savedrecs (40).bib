@article{ WOS:000165565600001,
Author = {Felli, L and Villas-Boas, JM},
Title = {{Renegotiation and collusion in organizations}},
Journal = {{JOURNAL OF ECONOMICS \& MANAGEMENT STRATEGY}},
Year = {{2000}},
Volume = {{9}},
Number = {{4}},
Pages = {{453-483}},
Month = {{WIN}},
Abstract = {{It has been argued that collusion among the members of an organization
   may lead to inefficiencies and hence should be prevented in equilibrium.
   This paper shows that whenever the parties to an organization can
   renegotiate their incentive scheme after collusion, these inefficiencies
   can be greatly reduced. Moreover, it might not be possible to prevent
   collusion and renegotiation in equilibrium. Indeed, if collusion is
   observable but not verifiable, then the organization's optimal incentive
   scheme will always be renegotiated. If, instead, collusion is not
   observable to the principal, both collusion and renegotiation will occur
   in equilibrium with positive probability. The occurrence of collusion
   and renegotiation should therefore not be taken as evidence of the
   inefficiency of an organization.}},
