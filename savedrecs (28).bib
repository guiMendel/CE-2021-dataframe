Title = {{Dialkynyl Carbene Derivatives: Generation and Characterization of
   Triplet tert-Butylpentadiynylidene (t-Bu-C C-C-C C-H) and
   Dimethylpentadiynylidene (Me-C C-C-C C-Me)}},
Journal = {{JOURNAL OF ORGANIC CHEMISTRY}},
