@article{ WOS:000583809400028,
Author = {Beach, I and Rotman, R.},
Title = {{THE LENGTH OF A SHORTEST CLOSED GEODESIC ON A SURFACE OF FINITE AREA}},
Journal = {{PROCEEDINGS OF THE AMERICAN MATHEMATICAL SOCIETY}},
Year = {{2020}},
Volume = {{148}},
Number = {{12}},
Pages = {{5355-5367}},
Month = {{DEC}},
Abstract = {{In this paper we prove new upper bounds for the length of a shortest
   closed geodesic, denoted l(M), on a complete, non-compact Riemannian
