@article{ WOS:000362184100039,
Author = {Wang, Nengli and Zhang, Xiyan and Wang, Penghe},
Title = {{Synthesis of Er3+:Lu2O3 nanopowders by carbonate co-precipitation
   process and fabrication of transparent ceramics}},
Journal = {{JOURNAL OF ALLOYS AND COMPOUNDS}},
Year = {{2015}},
Volume = {{652}},
Pages = {{281-286}},
Month = {{DEC 15}},
Abstract = {{Er3+ ions-doped Lu2O3 nanopowders were synthesized by co-precipitation
   method using NH4HCO3 as a precipitant, then Er3+:Lu2O3 transparent
   ceramics were fabricated by vacuum sintering using the as-prepared
   powder. The structure, crystallite size and upconversion luminescence
   (UCL) spectra of the as synthesized powders were characterized. The
   results indicated that the UCL properties of the powders varied with the
   calcination temperature and the Er3+ doping concentrations. Under the
   excitation of 980 nm laser diode, the ceramic sample showed a visually
   dominant green emission. Power studies revealed that the green (S-4(3/2)
   -> I-4(15/2)) and red (F-4(9/2) -> I-4(15/2)) upconversion emission of
   the sample occurred via a two-photon process. (C) 2015 Elsevier B.V. All
