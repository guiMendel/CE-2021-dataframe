@article{ WOS:000365804100043,
Author = {Shida, Keisuke and Watanabe, Yuko and Gomi, Hiroki and Takahashi, Akira
   and Tomita, Norikazu},
Title = {{Quantum Lattice Fluctuations in the Charge Density Wave State beyond the
   Adiabatic Approximation}},
Journal = {{JOURNAL OF THE PHYSICAL SOCIETY OF JAPAN}},
Year = {{2015}},
Volume = {{84}},
Number = {{12}},
Month = {{DEC 15}},
Abstract = {{We have developed a tractable numerical method in which large-amplitude
   quantum lattice fluctuations can be described beyond the adiabatic
   approximation using the coherent state representation of phonons. A
   many-body wave function is constructed by the superposition of direct
   products of non-orthogonal Slater determinants for electrons and
   coherent states of phonons. Both orbitals in all the Slater determinants
   and the amplitudes of all the coherent states are simultaneously
   optimized. We apply the method to the one-dimensional
   Su-Schrieffer-Heeger model with the on-site and nearest-neighbor-site
   Coulomb interactions. It is shown the lattice fluctuations in doped
   charge density wave (CDW) systems are described by the translational and
   vibrational motion of lattice solitons. Such lattice solitons induce
   bond alternation in the doped CDW system while the lattice becomes
   equidistant in the half-filled CDW system.}},
