@article{ WOS:000603252200009,
Author = {Goldbart, Aviv D. and Gannot, Meital and Haddad, Hen and Gopas, Jacob},
Title = {{Nuclear factor kappa B activation in cardiomyocytes by serum of children
   with obstructive sleep apnea syndrome}},
Journal = {{SCIENTIFIC REPORTS}},
Year = {{2020}},
Volume = {{10}},
Number = {{1}},
Month = {{DEC 17}},
Abstract = {{Obstructive sleep apnea syndrome (OSA) is associated with cardiovascular
   morbidity in adults and children. NF kappa B activity is enhanced in
   circulating monocytes of adults with OSA, that decreases following
   positive pressure therapy. OSA children's serum activates NF kappa B in
   a cell line. We hypothesized that OSA children's serum can activate NF
   kappa B in cardiomyocytes (CM) and effect their viability. In order to
   explore the role played by NF kappa B in OSA cardiovascular
   pathophysiology, rat, mouse and human immortalized CM were exposed to
   human serum drawn from OSA children and matched controls. Increased
   expression of NF kappa B classical subunits p65/p50 as well as major
   morphological changes occurred in cardiomyocytes following OSA's serum
   exposure. OSA children's serum induced NF kappa B activity as measured
   by p65 nuclear translocation in immortalized human CM and rat
   cardiomyocytes as well as dense immunostaining of the nucleus. Trypan
   blue and XTT assays showed that OSA sera induced CM apoptosis. We
   conclude that NF kappa B is systemically activated in cardiomyocytes,
   who also demonstrate decreased viability and contractility following
   exposure to OSA serum. It supports the hypothesis NF kappa B plays a
   role in the evolution of cardiovascular morbidity in OSA. It may support
   the search for new therapeutic interventions controlling NF kappa B
   activation in OSA.}},
