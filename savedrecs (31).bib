@article{ WOS:000233669800005,
Author = {Girondo, E and Wolfart, J},
Title = {{Conjugators of Fuchsian groups and quasiplatonic surfaces}},
Journal = {{QUARTERLY JOURNAL OF MATHEMATICS}},
