@article{ WOS:000089558800012,
Author = {Lai, ACK and Thatcher, TL and Nazaroff, WW},
Title = {{Inhalation transfer factors for air pollution health risk assessment}},
Journal = {{JOURNAL OF THE AIR \& WASTE MANAGEMENT ASSOCIATION}},
Year = {{2000}},
Volume = {{50}},
Number = {{9}},
Pages = {{1688-1699}},
Month = {{SEP}},
Abstract = {{To facilitate routine health risk assessments, we develop the concept of
   an inhalation transfer factor (ITF). The ITF is defined as the pollutant
   mass inhaled by an exposed individual per unit pollutant mass emitted
   from an air pollution source. A cumulative population inhalation
   transfer factor (PITF) is also defined to describe the total fraction of
   an emitted pollutant inhaled by all members of the exposed population.
   In this paper, ITFs and PITFs are calculated for outdoor releases from
   area, point, and line sources, indoor releases in single zone and
   multizone indoor environments, and releases within motor vehicles.
   Typical PITFs for an urban area from emissions outdoors are similar to
   10(-6)-10(-3). PITFs associated with emissions in buildings or in moving
   vehicles are typically much higher, similar to 10(-3)-10(-1).}},
