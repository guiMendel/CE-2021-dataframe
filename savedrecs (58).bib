@article{ WOS:A1995RT73600005,
Author = {SHINDO, Y and KOBORI, T and HORIGUCHI, K and YAMADA, E and NAKAJIMA, H
   and TSUJI, H},
Title = {{FRACTURE-TOUGHNESS AND TEMPERATURE RISE DURING CRACK-PROPAGATION IN
   FORGED JJ1 TYPE AUSTENITIC STAINLESS-STEEL PLATE AT 4.2 K}},
Journal = {{JOURNAL OF THE JAPAN INSTITUTE OF METALS}},
Year = {{1995}},
Volume = {{59}},
Number = {{8}},
Pages = {{793-798}},
Month = {{AUG}},
Abstract = {{This paper discusses fracture toughness and temperature rise for JJ1
   austenitic stainless steel forged plate at liquid helium temperature for
   fusion reactor magnets of next generation. Elastic-plastic fracture
   toughness (J(IC)) testing was conducted in general accordance with ASTM
   standards E813-81 and E813-87, and unloading-compliance technique was
   applied using compact specimens 25.0 mm and 12.5 mm thick. Thermocouples
   were used to measure the temperature rise near the crack tip. The
   effects of specimen size and side-groove on the cryogenic fracture
   properties of this alloy are examined. Examination of the fracture
