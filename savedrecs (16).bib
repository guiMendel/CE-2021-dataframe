@article{ WOS:000365177600002,
Author = {Bukhalev, V. A. and Boldinov, V. A. and Pryadkin, S. P. and Skrynnikov,
   A. A.},
Title = {{Optimal control of linear stochastic system using smoothed estimate of
   phase coordinates}},
Journal = {{AUTOMATION AND REMOTE CONTROL}},
Year = {{2015}},
Volume = {{76}},
Number = {{11}},
Pages = {{1918-1930}},
Month = {{NOV}},
Abstract = {{Consideration was given to the problem of optimizing the control of a
   linear stochastic plant in the linearly quadratic performance index. The
   optimality criterion is represented by the minimal root-mean-square form
   of deviation of the plant state vector from some desired vector time
   function under a limited root-mean-square form of the control vector.
   The required time function represents the optimal linear estimate of
   smoothing with constant delay of the state vector of another stochastic
   plant which is based on imprecise observations. The differential
   equations of the optimal information and control algorithm were
