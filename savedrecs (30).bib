@article{ WOS:000234533600030,
Author = {Yao, KL and Qin, Y and Liu, QM and Liu, ZL},
Title = {{DMRG studies on properties of undoped and doped molecule-based
   ferromagnetic chain}},
Journal = {{COMMUNICATIONS IN THEORETICAL PHYSICS}},
Year = {{2005}},
Volume = {{44}},
Number = {{6}},
Pages = {{1099-1102}},
Month = {{DEC 15}},
Abstract = {{By using density matrix renormalization group (DMRG) method a model for
   organic molecule-based ferromagnetic chain is proposed. It is found that
   the ground states of undoped and doped systems both exhibit
   ferrimagnetic ordering. The e-e repulsion plays an important role in the
   stability of the ferromagnetic state either in doped system or undoped
   system. For the undoped system, each unit cell contains half of the
   total spins, which is consistent with Lieb's theorem. It is convinced
   that when the system is doped with one electron, a charge density wave
   is excited, which decreases the amplitude of spin density wave,
   therefore acting against the stability of ferromagnetic state.}},
