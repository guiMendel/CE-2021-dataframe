@article{ WOS:A1995TW61300043,
Author = {Takaki, T and Yamada, S and Noda, Y and Tanaka, M},
Title = {{Relationship between the individual difference of exploratory locomotor
   activity and dopaminergic system in rats.}},
Journal = {{NEUROSCIENCES-JAPAN}},
Year = {{1995}},
Volume = {{21}},
Number = {{2}},
Pages = {{P169-P171}},
Month = {{DEC}},
Abstract = {{Male Wistar rats (200-250g) were placed individually in cages
   (25x15x40cm), equipped with photocell beams across the long axis 5 cm
   above the cage floor, and locomotor activity was recorded for 3 days.
   After the 7th day of the recording, the rats were killed by
   decapitation. The brains were rapidly removed and tissues of striatum,
   nucleus accumbens, and frontal cortex were homogenized with 1 mi of
   formic acid (1N, pH 2.3). After centrifugation, dopamine(DA),
   dihydroxyphenylacetic acid(DOPAC), and homovanillic acid(HVA) levels in
   the supernatant were measured by HPLC-ECD. A significant correlation was
   found between the decrease in DOPAC/DA ratio in the frontal cortex and
   the increase in exploratory locomotor activity (2 hours), and was also
   found between the increase in DOPAC/DA ratio in the striatum and the
   increase in exploratory locomotor activity(1 hour). Furthermore, a
   significant correlation was found between the decrease in DOPAC/DA ratio
   in the frontal cortex and the increase in DOPAC/DA ratio in the
   striatum. Thus, dopaminergic activity in the frontal cortex has opposite
   effect in the striatum and suppresses exploratory locomotor activity.}},
