@article{ WOS:000231503600072,
Author = {Zemlin, C and Mukund, K and Wellner, M and Zaritsky, R and Pertsov, A},
Title = {{Asymmetric bound states of spiral pairs in excitable media}},
Journal = {{PHYSICAL REVIEW LETTERS}},
Year = {{2005}},
Volume = {{95}},
Number = {{9}},
Month = {{AUG 26}},
Abstract = {{We present a stable regime of asymmetric bound states for spiral pairs
   in a generic numerical model of a homogeneous excitable medium. In this
   regime, one spiral tip (slave) rotates around the other (master).
   Master-slave dynamics occur for both same-chirality and
   opposite-chirality spiral pairs in a range of parameters and initial
   conditions. We study the dependency of master-slave characteristics on
   the medium's excitation threshold and present a phenomenological model
   that accounts for the qualitative properties of master-slave dynamics.}},
