@article{ WOS:000167021600079,
Author = {Aoki, N and Wall, MJ and Demsar, J and Zupan, B and Granchi, T and
   Schreiber, MA and Holcomb, JB and Byrne, M and Liscum, KR and Goodwin, G
   and Beck, JR and Mattox, KL},
Title = {{Predictive model for survival at the conclusion of a damage control
   laparotomy}},
Journal = {{AMERICAN JOURNAL OF SURGERY}},
Year = {{2000}},
Volume = {{180}},
Number = {{6}},
Pages = {{540-544}},
Month = {{DEC}},
Note = {{52nd Annual Meeting of the Southwestern-Surgical-Congress, COLORADO
   SPRINGS, COLORADO, APR 09-12, 2000}},
Organization = {{SW Surg Congress}},
Abstract = {{BACKGROUND: We employed modern statistical and data mining methods to
   model survival based on preoperative and intraoperative parameters for
   patients undergoing damage control surgery.
   METHODS: One hundred seventy-four parameters were collected from 68
   damage control patients in prehospital, emergency center, operating
   room, and intensive care unit (ICU) settings. Data were analyzed with
   logistic regression and data mining. Outcomes were survival and death
   after the initial operation.
   RESULTS: Overall mortality was 66.2\%. Logistic regression identified pH
   at initial ICU admission (odds ratio: 4.4) and worst partial
   thromboplastin time from hospital admission to ICU admission (odds
   ratio: 9.4) as significant. Data mining selected the same factors, and
   generated a simple algorithm for patient classification. Model accuracy
   was 83\%.
   CONCLUSIONS: Inability to correct pH at the conclusion of initial
   damage-control laparotomy and the worst PTT can be predictive of death.
   These factors may be useful to identify patients with a high risk of
   mortality. (C) 2001 by Excerpta Medica, Inc.}},
