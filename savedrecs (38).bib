@article{ WOS:000236086400016,
Author = {Ndahi, NP and Kolawole, GA},
Title = {{Cobalt(III) complexes of diphenylglyoxime as possible non-organometallic
   models of vitamin B-12}},
Journal = {{SOUTH AFRICAN JOURNAL OF SCIENCE}},
Year = {{2005}},
Volume = {{101}},
Number = {{9-10}},
Pages = {{457-463}},
Month = {{SEP-OCT}},
Abstract = {{Cobalt(III) non-organometallic model complexes of vitamin B-12 With
   diphenylglyoxime (H(2)dpg) have been prepared with various axial
   nitrogen or phosphorus bases (py, phen, bipy and PPh3) as ligands, and
   halides and pseudohalides as anions. The complexes were characterized by
   microanalyses, infrared spectra, room temperature magnetic moment,
   thermal gravimetric analysis, cyclic voltammetry and UV/visible
   electronic spectra. All the complexes are diamagnetic and are therefore
   octahedral d(6) Co(III) complexes. All the complexes decomposed below
   400 degrees C and showed one major TGA profile below 400 degrees C
   corresponding to the loss of one major component, which was either the
   base or the diphenylglyoxime. The profile between 400 and 800 degrees C
   was poorly defined. The percentage residue could therefore not be
   estimated. The Co(II)/Co(I) couples were reversible for most of the
   complexes, whereas the Co(III)/Co(II) couples are either
   quasi-reversible or irreversible and were therefore considered to be
   possible models for vitamin B-12.}},
