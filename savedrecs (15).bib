@article{ WOS:000365462900043,
Author = {Skrzynska, Anna and Przydacz, Artur and Albrecht, Lukasz},
Title = {{Organocatalytic Nonclassical Trienamine Activation in the Remote
   Alkylation of Furan Derivatives}},
Journal = {{ORGANIC LETTERS}},
Year = {{2015}},
Volume = {{17}},
Number = {{22}},
Pages = {{5682-5685}},
Month = {{NOV 20}},
Abstract = {{A new approach for the stereoselective remote alkylation of furan
   derivatives is reported. The reaction of 5-alkylfurfurals With
   nitroolefins as electrophilic counterparts occurs at their exocyclic
   epsilon-position and proceeds through the intermediacy of a nonclassical
   catalytic trienamine intermediate. The aminocatalyst bearing a H-bonding
   unit is used to control the stereochemical reaction outcome confirming
   the usefulness of such catalytic systems for the remote
   functionalizations of carbonyl compounds. Target products with two
   adjacent stereogenic centers are obtained in excellent yields and with
   good to moderate stereoselectivities.}},
