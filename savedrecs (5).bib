@article{ WOS:000603281200007,
Author = {Seo, Jeonghwa and Kim, Dae Hyuk and Ha, Jeongsoo and Rhee, Shin Hyung
   and Yoon, Hyeon Kyu and Park, Jongyeol and Seok, Woo-Chan and Rhee, Key
   Pyo},
Title = {{Captive Model Tests for Assessing Maneuverability of a Damaged Surface
   Combatant with Initial Heel Angle}},
Journal = {{JOURNAL OF SHIP RESEARCH}},
Year = {{2020}},
Volume = {{64}},
Number = {{4}},
Pages = {{392-406}},
Month = {{DEC}},
Abstract = {{The present study is about the application of a four-degree-of-freedom
   (4DOF) maneuvering mathematical model based on Abkowitz's model for
   assessing damaged ship maneuverability with initial asymmetry. A scaled
   model of the Office of Naval Research Tumblehome hull with a damaged
   compartment was used as the test model. Based on the survivability
   regulations for naval vessels, the damaged compartment was designed and
   located near the bow, such that it had an initial heel and trim. Static
   and dynamic captive model tests were performed on the damaged ship model
   to determine the maneuvering coefficients for the maneuvering
   mathematical model. Maneuvering simulations were carried out with the
   captive model test data and 4DOF maneuvering mathematical model. The
   advance speed in the maneuver reduced more in the damaged condition than
   in the intact condition, and maneuverability was severely degraded
   during starboard turning.}},
