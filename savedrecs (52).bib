@article{ WOS:A1995TL45700024,
Author = {Zivkovic, Z and Strbac, N and Sestak, J},
Title = {{Influence of fluorides on polymorphous transformation of alpha-Al2O3
   formation}},
Journal = {{THERMOCHIMICA ACTA}},
Year = {{1995}},
Volume = {{266}},
Pages = {{293-300}},
Month = {{NOV 15}},
Abstract = {{Results of investigation into the influence of added fluorides (AIF(3),
   CaF2, MgF2, LiF, NaF and Na(3)A1F(6), up to 10\%) at the temperature of
   the polymorphous transformation gamma-Al2O3 double right arrow
   alpha-Al2O3 are presented. It was determined that, at a heating rate of
   10 degrees C min(-1), addition of 2\% fluoride decreases the
   transformation temperature by 400 degrees C for AIF(3), by 220 degrees C
   for MgF2, and by 130 degrees C for CaF2. The other fluorides have no
   effect. The mechanism of the influence is determined, and it can be
   explained by the formation of fluorine bridges between fluoride cations
   and aluminium on the gamma-Al2O3 surface.}},
