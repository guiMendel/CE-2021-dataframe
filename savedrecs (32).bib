@article{ WOS:000227787300011,
Author = {Teal, JM and Peterson, S},
Title = {{Restoration benefits in a watershed context}},
Journal = {{JOURNAL OF COASTAL RESEARCH}},
Year = {{2005}},
Number = {{40}},
Pages = {{132-140}},
Month = {{WIN}},
Abstract = {{Wetland restoration requires good science, good engineering, and
   political will. The process has evolved in fits and starts over the past
   30+ years as scholars have enumerated the functions lost as a result of
   wetland destruction. Measuring effects of wetland restoration,
   especially watershed-wide effects, requires agreement among proponents
   on restoration goals. The goals should express a balance among
   ecological, social, political, and economic functions, and recognize
   that the functions are multi-dimensional and intertwined. A thorough
   ecological assessment of wetland restoration and monitoring should have
   a parallel component focusing on societal values as well as ecological
   values.
   Four U.S. watershed examples-The Mississippi River watershed, the
   Delaware Bay watershed, the Lower Fox River, Wisconsin, and South Cape
   Beach Marsh on Cape Cod, Massachusetts-illustrate the importance of the
   scale in restoration projects and the importance of measuring societal
   benefits from wetland restoration.}},
