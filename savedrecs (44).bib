@article{ WOS:000089869600004,
Author = {Heijdra, BJ and Ligthart, JE},
Title = {{The dynamic macroeconomic effects of tax policy in an overlapping
   generations model}},
Journal = {{OXFORD ECONOMIC PAPERS-NEW SERIES}},
Year = {{2000}},
Volume = {{52}},
Number = {{4}},
Pages = {{677-701}},
Month = {{OCT}},
Abstract = {{The paper studies the dynamic allocation effects of tax policy within
   the context of an overlapping-generations model of the Blanchard-Yaari
   type. The model is extended to allow for endogenous labour supply and
   three tax instruments, viz. a capital tax, labour income tax, and
   consumption tax. Both analytical expressions and simple diagrams are
   used to analyze the impact, transition, and long-run effects of tax
   policy changes. It is shown that a part of the long-run incidence of
   capital and consumption taxes falls on capital when households' horizons
   are finite whereas labour would fully bear the burden of these taxes in
   an infinite-horizon model.}},
