@article{ WOS:000233203600003,
Author = {Abbey, A and Parkhill, MR and Koss, MP},
Title = {{The effects of frame of reference on responses to questions about sexual
   assault victimization and perpetration}},
Journal = {{PSYCHOLOGY OF WOMEN QUARTERLY}},
Year = {{2005}},
Volume = {{29}},
Number = {{4}},
Pages = {{364-373}},
Month = {{DEC}},
Abstract = {{Self-reports of sexual assault are affected by a variety of factors
   including the number of questions, question phrasing, and context.
   Participants (307 women, 166 men) were randomly assigned to one of two
   forms of a questionnaire. One form had the tactics used to obtain forced
   sex as the initial frame of reference, whereas the other form had the
   type of sex that was forced as the initial frame of reference.
   Seventy-five percent of the women who received the tactics-first version
   reported that they had at least one victimization experience since the
   age of 14, as compared to 62\% of the women who received the
   type-of-sex-first version. Sixty-nine percent of the men who received
   the tactics-first version reported that they had at least one
   perpetration experience since the age of 14, as compared to 36\% of the
   men who received the type-of-sex-first version. These findings have
   implications for how questionnaires should be designed to maximize
   reporting of sexual assault incidents.}},
