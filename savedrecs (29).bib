@article{ WOS:000282370900032,
Author = {Bella, Gideon and Etzion, Erez and Hod, Noam and Oz, Yaron and Silver,
   Yiftah and Sutton, Mark},
Title = {{A search for heavy Kaluza-Klein electroweak gauge bosons at the LHC}},
Journal = {{JOURNAL OF HIGH ENERGY PHYSICS}},
Year = {{2010}},
Number = {{9}},
Month = {{SEP}},
Abstract = {{The feasibility for the observation of a certain leptonic Kaluza-Klein
   (KK) hard process in pp interactions at the LHC is presented. Within the
   S-1/Z(2) TeV-1 extra dimensional theoretical framework with the focus on
   the KK excitations of the Standard Model gamma and Z(0) gauge bosons,
   the hard-process, f (f) over bar -> Sigma(n) (gamma{*}/Z{*})(n) -> F (F)
   over bar, has been used where f is the initial state parton, F the final
   state lepton and (gamma{*}/Z{*})(n) is the n(th) KK excitation of the
   gamma/Z(0) boson. For this study the analytic form for the hard process
   cross section has been independently calculated by the authors and has
   been implemented using the Moses framework. The Moses framework itself,
   that has been written by the authors, was used as an external process
   within the PYTHIA8 Monte Carlo generator which provides the phase space
   generation for the final state leptons and partons from the initial
   state hadrons, and the simulation of initial and final state radiation
   and hadronization. A brief discussion of the possibility for observing
   and identifying the unique signature of the KK signal given the current
   LHC program is also presented.}},
