@article{ WOS:A1995RN31300010,
Author = {WONG, MW},
Title = {{STRUCTURES AND STABILITIES OF XCCY(2+) DICATIONS (X, Y=O, S AND NH),
   DOUBLY-CHARGED ISOELECTRONIC ANALOGS OF CYANOGEN}},
Journal = {{JOURNAL OF MASS SPECTROMETRY}},
Year = {{1995}},
Volume = {{30}},
Number = {{8}},
Pages = {{1144-1148}},
Month = {{AUG}},
Abstract = {{Consistent with experimental observations, high-level ab initio
   calculations, at the G2(MP2) level of theory, predict that the XCCY(2+)
   (X, Y = O, S and NH) dications are remarkably stable species in the gas
   phase. OCCSZ+ SCCS2+, HNCCO2+, HNCCS2+ and HNCCNH2+ are calculated to be
   thermodynamically stable species, whereas OCCO2+ is predicted to lie in
   a deep potential well, separated from exothermic C-C cleavage by a large
   barrier. All the doubly charged species are calculated to have a linear
   skeleton. Despite the inherent strong Conlombic repulsion, these species
   are characterized by short C=X or C=Y triple bonds. Vertical and
   adiabatic ionization energies of the XCCY(+.) monocations and the heats
   of formation of the resulting dications are reported. There are
   remarkable changes in the structures and stability ordering of isomers
   on going from the XCCY neutrals to the XCCY(+.) monocations and to the
   XCCY(2+) dications. The rationalization of these results is discussed.}},
