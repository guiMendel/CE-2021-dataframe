@article{ WOS:000566786400011,
Author = {Garrione, Maurizio and Gazzola, Filippo},
Title = {{Linear theory for beams with intermediate piers}},
Journal = {{COMMUNICATIONS IN CONTEMPORARY MATHEMATICS}},
Year = {{2020}},
Volume = {{22}},
Number = {{8}},
Month = {{DEC}},
Abstract = {{The full linear theory for hinged beams with intermediate piers is
   developed. The analysis starts with the variational setting and the
   study of the linear stationary problem. Well-posedness results are
   provided and the possible loss of regularity, due to the presence of the
   piers, is analyzed. A complete spectral theorem is then proved,
   explicitly determining the eigenvalues according to the position of the
   piers and exhibiting the fundamental modes of oscillation. A related
   second-order eigenvalue problem is also studied, showing that it may
   display nonsmooth eigenfunctions and that the fourth-order problem
   cannot be seen as the square of a second-order problem.}},
