@article{ WOS:A1995TA75800006,
Author = {MORRISON, DS and SCHAEFER, RK and FRIEDMAN, RL},
Title = {{THE RELATIONSHIP BETWEEN SUBACROMIAL SPACE PRESSURE, BLOOD-PRESSURE, AND
   VISUAL CLARITY DURING ARTHROSCOPIC SUBACROMIAL DECOMPRESSION}},
Journal = {{ARTHROSCOPY-THE JOURNAL OF ARTHROSCOPIC AND RELATED SURGERY}},
Year = {{1995}},
Volume = {{11}},
Number = {{5}},
Pages = {{557-560}},
Month = {{OCT}},
Abstract = {{Twenty-two consecutive patients with subacromial impingement syndrome
   underwent arthroscopic subacromial decompression. Measurements of the
   subacromial space pressure and the blood pressure were recorded along
   with the clarity of the visual field. The clarity of the visual field
   was objectively determined based on the pressure at which bleeding was
   observed from trabecular bone or the soft tissue capillaries. A direct
   correlation was found between systolic blood pressure (SEP), subacromial
   space pressure (SASP), and the clarity of the visual field. On average,
   maintaining a pressure difference (SEP - SASP) of 49 mm Hg or less
   prevented bleeding and permitted good visualization. With a greater
   differential, significant bleeding occurred. Furthermore, the use of
   relative hypotensive anesthesia permits lower irrigation pressures and
   significantly reduces the risk of fluid extravasation into the
   subcutaneous tissues of the shoulder.}},
