@article{ WOS:000235065000001,
Author = {Balankin, AS and Susarrey, O and Paredes, RG and Morales, L and Samayoa,
   D and Lopez, JA},
Title = {{Intrinsically anomalous roughness of admissible crack traces in concrete}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2005}},
Volume = {{72}},
Number = {{6, 2}},
Month = {{DEC}},
Abstract = {{We study the roughness of postmortem cracks in concrete plates of
   different size. We find that the set of admissible crack paths exhibits
   an intrinsically anomalous roughness; nevertheless, any individual crack
   trace in concrete is essentially self-affine. We also find that both the
   local and the global amplitudes of crack traces are distributed
   according to a log-logistic distribution characterized by the same
   scaling exponent, whereas the mean-square width distribution is best
   fitted by the Pearson distribution, while the log-normal distribution
   also provides quite good adjustments and cannot be clearly rejected.}},
