@article{ WOS:000366079600017,
Author = {Compton, Philip D. and Fornelli, Luca and Kelleher, Neil L. and Skinner,
   Owen S.},
Title = {{Probing asymmetric charge partitioning of protein oligomers during
   tandem mass spectrometry}},
Journal = {{INTERNATIONAL JOURNAL OF MASS SPECTROMETRY}},
Year = {{2015}},
Volume = {{390}},
Number = {{SI}},
Pages = {{132-136}},
Month = {{NOV 15}},
Abstract = {{Dissociation of gaseous protein complexes produced by native
   electrospray often induces an asymmetric partitioning of charge between
   ejected subunits. We present a simple asymmetric charge partitioning
   factor (ACPF) to quantify the magnitude of asymmetry in this effect.
   When applied to monomer ejection from the cytochrome c dimer and
   beta-amylase tetramer, we found that the similar to 60-70\% of precursor
   charge ending up in the ejected monomers corresponds to ACPFs of 1.38
   and 2.51, respectively. Further, we used site-specific fragmentation
   from electron transfer dissociation (ETD) to identify differences in
   fragmentation and characterize domains of secondary-structure present in
   the dimer, ejected monomers, and monomers obtained directly from
   electrospray ionization (ESI). We found evidence of structural changes
