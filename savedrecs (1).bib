@article{ WOS:000597557900001,
Author = {Mizutori, Fumiya and Kohiyama, Masayuki},
Title = {{Experiment of torsional response induced by the Q-delta resonance}},
Journal = {{STRUCTURAL DESIGN OF TALL AND SPECIAL BUILDINGS}},
Year = {{2021}},
Volume = {{30}},
Number = {{4}},
Month = {{MAR}},
Abstract = {{Because of geometric nonlinearities, vibrations in two horizontal
   directions can generate a torsional moment that induces a torsional
   response, even in a structure with no eccentricity; we term this
   phenomenon Q-Delta effect. In this study, the torsional response caused
   by the Q-Delta resonance was investigated by performing a shaking table
   test that involves a single-layer symmetric specimen. The specimen was
   designed and fabricated by focusing on one of two resonance conditions.
   Its moment of inertia was adjustable by changing locations of weights,
   and the natural frequency in the torsional mode could be modified. We
   developed a numerical model of the specimen, in which the columns
   connected to the slab were integrated into an elastic Euler beam. It was
   confirmed that the torsional response increased near the predicted
   Q-Delta resonance point. In addition, the acceleration at the corner of
   the slab was significantly increased. The formulated equations of motion
   provided a better prediction of the actual phenomena. Because the
   specimen corresponds to a stiff and slender high-rise building when
   converted into a full scale, the result suggests a need to be aware of
   the risk of torsional response increase due to the Q-Delta resonance.}},
