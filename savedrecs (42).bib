@article{ WOS:000165224500081,
Author = {Kromer, F and Lang, M and Oeschler, N and Hinze, P and Langhammer, C and
   Steglich, F and Kim, JS and Stewart, GR},
Title = {{Thermal expansion studies of superconducting U(1-x)ThxBe(13) (0 <= x <=
