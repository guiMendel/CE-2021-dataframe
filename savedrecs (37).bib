@article{ WOS:000231885600076,
Author = {Barba, D and Salem, B and Morris, D and Aimez, V and Beauvais, J and
   Chicoine, M and Schiettekatte, F},
Title = {{Ion channeling effects on quantum well intermixing in
   phosphorus-implanted InGaAsP/InGaAs/InP}},
Journal = {{JOURNAL OF APPLIED PHYSICS}},
Year = {{2005}},
Volume = {{98}},
Number = {{5}},
Month = {{SEP 1}},
Abstract = {{Photoluminescence, time-resolved photoluminescence, and Raman
   characterization techniques have been used to study
   In0.73Ga0.27As0.57P0.43/In0.53Ga0.47As/InP single quantum well
   heterostructure after 20-keV phosphorus ion implantation followed by
   rapid thermal annealing. The annealing process induces intermixing in
   the heterostructures and results in the blueshift of the quantum well
   peak emission. In order to investigate ion channeling effects on this
   band-gap tuning process, room-temperature implantations have been
   performed at tilt angles of 0 degrees and 7 degrees with respect to the
   sample (001)-growth axis. We show that the ion channeling increases the
   blueshift from 24 to 42 nm, while it reduces both the density of the
   nonradiative defects within the active layer and the structure
   disordering. These features are attributed to the nature of the damage
   generated by channeled ions. The band-gap increase observed in the
   sample implanted at 0 degrees is consistent with the formation of a
   compressive strain at the barrier/quantum well interface, whose
   intensity is measured by Raman spectroscopy. (c) 2005 American Institute
   of Physics.}},
