@article{ WOS:000285171700004,
Author = {Sotnikov, O. S. and Paramonova, N. M. and Archakova, L. I.},
Title = {{Dynamics of Ultrastructural Transformations of Paired Biological
   Membranes in the Nervous System}},
Journal = {{BIOLOGICHESKIE MEMBRANY}},
Year = {{2010}},
Volume = {{27}},
Number = {{6}},
Pages = {{489-497}},
Month = {{NOV-DEC}},
Abstract = {{The paper demonstrates ultrastructural mechanisms of formation of
   nanoscopic pores and large syncytial perforations of paired membranes of
   neurons, neuron and glia, multiglial envelopes as well as of the
   endoplasmic reticulum and Golgi apparatus. Mechanisms of formation of
   the pores and perforations in all cases are similar. Approaching of the
   membranes leads to their fusion with formation of continuous and dotted
   tight junctions. The pores and their enlargements perforations are
   formed only on the basis of the tight junctions; the intermembrane cleft
   acquires a varicose, beads-like shape. The pore between the ``beads{''}
   is formed as a result of conversion of the ellipsoid shape of the beads
   into the spherical one. Upon autoamputation of varicose vesicles, they
   are transformed into the residual bodies in the lumen of perforations.}},
