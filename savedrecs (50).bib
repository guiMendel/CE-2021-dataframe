@article{ WOS:A1995TM60200031,
Author = {Han, JK and Lee, SK},
Title = {{Reducing PIP2 hydrolysis, Ins(1,4,5)P-3 receptor availability, or
   calcium gradients inhibits progesterone-stimulated Xenopus oocyte
   maturation}},
Journal = {{BIOCHEMICAL AND BIOPHYSICAL RESEARCH COMMUNICATIONS}},
Year = {{1995}},
Volume = {{217}},
Number = {{3}},
Pages = {{931-939}},
Month = {{DEC 26}},
Abstract = {{In this report, we investigated the possibility that a product of
   phosphatidylinositol 4,5-bisphosphate (PIP2) breakdown, inositol
   1,4,5-trisphosphate (Ins(1,4,5)P-3), and its downstream effector, Ca2+,
   is involved in oocyte maturation. Microinjection of monoclonal antibody
   specifically directed to PIP2 into oocytes prior to progesterone
   addition inhibited meiotic maturation. In addition, preventing or
   suppressing the progesterone-induced increase in {[}Ca2+](i) and
   maintaining cytosolic free Ca2+ of oocyte at 0.1 or 0.3 mu M by
   Br-2-BAPTA buffered Ca2+ solutions, completely blocked maturation.
   However, raising cytosolic Ca2+ concentration of oocyte to 1.0 or 3.0 mu
   M, the inhibitory Br-2-BAPTA effects on oocyte maturation was greatly
