@article{ WOS:000285328500003,
Author = {Park, Seul-Ki and Nam, Suk-Woo and Lee, Mi-Young},
Title = {{Ethylbenzene-induced differential protein profiles in rat liver}},
Journal = {{BIOCHIP JOURNAL}},
Year = {{2010}},
Volume = {{4}},
Number = {{4}},
Pages = {{272-278}},
Month = {{DEC 20}},
Abstract = {{Ethylbenzene (EB) is an important industrial chemical that has recently
   been classified as a possible human carcinogen. We investigated the
   effects of administration of EB equivalent to 10\% and 50\% of LD50 dose
   on rats and analyzed their liver proteomes at 72 h after dosing.
   Approximately 1,100 protein spots that were detected by 2-dimensional
   gel electrophoresis showed reproducible abundance. 11 of these proteins
   showed changes in expression level of at least 1.5-fold after EB
   treatment. 3 of these proteins were down-regulated and the rest were
   up-regulated by 10\% LD50 of EB dose, whereas 4 proteins were
   down-regulated and the rest were up-regulated by 50\% LD50. The
   differentially expressed proteins might act as a candidate of biomarker
   set for ED toxicity.}},
