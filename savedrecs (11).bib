@article{ WOS:000366885500005,
Author = {Castelluccio, Stephane},
Title = {{Stephane Castelluccio: Madeleine de Scudery and Versailles}},
Journal = {{REVUE DE L ART}},
Year = {{2015}},
Number = {{190}},
Pages = {{39-47}},
Month = {{DEC}},
Abstract = {{From 1640 to 1669, Madeleine de Scudery published epic novels that
   encountred great success. Her characters and imaginary castles were
   inspired by contemporary figures and private mansions. She exaggerated
   their beauty by insisting on the use of precious materials and elements
   repeatd infinitly, such as columns, porticos, fountains... It is
   striking to notice that some particularities of her imaginary castles
   and gardens actually infuenced the architecture of the initial
   Versailles ``maison de plaisance{''}. In Scudery's 1669 text, the castle
   appears as a place where realty passed beyond fiction and Louis XIV is
   portrayed as a novel's magician hero. Thirteen years later, Versailles
   became the official french Monarchy palace. Consequently, in 1684,
   Scudery described it as a unique castle and presented Louis XIV as the
   true ideal monarch. Notwithstanding, successive generations of writers
   did not use her ``novel mode{''} style and opted for more historical and
   rigorous descriptions: reality did not need fiction any more.}},
