@article{ WOS:A1995RU47800016,
Author = {MELLONI, A and POLITI, M and GENTILI, GG},
Title = {{MODE-MATCHING ANALYSIS OF TE(011)-MODE WAVE-GUIDE BANDPASS-FILTERS}},
Journal = {{IEEE TRANSACTIONS ON MICROWAVE THEORY AND TECHNIQUES}},
Year = {{1995}},
Volume = {{43}},
Number = {{9, 1}},
Pages = {{2109-2116}},
Month = {{SEP}},
Abstract = {{A mode-matching technique for the analysis of waveguide bandpass filters
   employing circular TE(011) resonators, coupled by rectangular apertures,
   is presented. Such a technique takes rigorously into account thickness
   and angular offset of the two coupling irises, and higher modes
