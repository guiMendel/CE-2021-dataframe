@article{ WOS:000232711800003,
Author = {Calleo-Cross, A and Sharma, N and Aronne, LJ},
Title = {{Medical assessment of obese older patients}},
Journal = {{CLINICS IN GERIATRIC MEDICINE}},
Year = {{2005}},
Volume = {{21}},
Number = {{4}},
Pages = {{689+}},
Month = {{NOV}},
Abstract = {{Obesity is a chronic disease associated with many other chronic
   illnesses. Assessment and treatment of obesity in the older patient can
   be recommended if in the clinical opinion of the practitioner, weight
   loss would have a positive impact on the patient's health status.
   Management of obesity follows the rules used in the management of other
   chronic diseases. A sympathetic, supportive approach by the practitioner
   is recommended. A reasonable goal for treatment (eg, a weight loss of
   5\% to 10\% of body weight) should be established, and the main focus
   should be health benefit. In the older patient, building lean body mass
   with resistance training seems to be of primary importance. A
   low-glycemic diet composed of healthy foods is recommended.}},
