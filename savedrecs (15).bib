@article{ WOS:000366611500089,
Author = {Chen, Yuanyuan and Kotnala, Abhay and Yu, Li and Zhang, Jiasen and
   Gordon, Reuven},
Title = {{Wedge and gap plasmonic resonances in double nanoholes}},
Journal = {{OPTICS EXPRESS}},
Year = {{2015}},
Volume = {{23}},
Number = {{23}},
Pages = {{30227-30236}},
Month = {{NOV 16}},
Abstract = {{We study the plasmonic resonances of double nanoholes (DNHs) in metal
   films. These apertures exhibit the usual gap-mode Fabry-Perot
   resonances, where the zeroth order resonance is determined by the
   waveguide cut-off and the first order resonance shows sensitivity to the
   film thickness. An additional wedge resonance is observed, which is
   sensitive to the curvature of the cusps in the DNHs, analogous to the
   wedge modes of single wedges. While the gap mode intensity increases
   dramatically with decreasing gap-width, the wedge mode intensity
   saturates since its field enhancement arises from the curvature of the
   metal film, like cylindrical Sommerfeld waves. Experimental transmission
   spectra agree well with finite-difference time-domain simulations
   showing these separate resonances. The controlled design of these
   resonances is critical for applications including optical tweezers,
   nonlinear conversion, sensing and spectroscopy. (C) 2015 Optical Society
   of America}},
