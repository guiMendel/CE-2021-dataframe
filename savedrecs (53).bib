@article{ WOS:A1995TD71500009,
Author = {WEIRAUCH, DA and LAZAROFF, JE and OWNBY, PD},
Title = {{WETTING IN AN ELECTRONIC PACKAGING CERAMIC SYSTEM .2. WETTING OF ALUMINA
   BY A SILICATE GLASS MELT UNDER CONTROLLED P(O2) CONDITIONS}},
