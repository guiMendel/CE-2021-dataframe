@article{ WOS:000231543300046,
Author = {Peng, GY and Guo, Z and Kiniwa, Y and Voo, KS and Peng, WY and Fu, TH
   and Wang, DY and Li, YC and Wang, HY and Wang, RF},
Title = {{Toll-like, receptor 8-mediated reversal of CD4(+) regulatory T cell
   function}},
Journal = {{SCIENCE}},
Year = {{2005}},
Volume = {{309}},
Number = {{5739}},
Pages = {{1380-1384}},
Month = {{AUG 26}},
Abstract = {{CD4(+) regulatory T (Treg) cells have a profound ability to suppress
   host immune responses, yet little is understood about how these cells
   are regulated. We describe a mechanism {[}inking Toll-like receptor
   (TLR) 8 signaling to the control of Treg cell function, in which
   synthetic and natural ligands for human TLR8 can reverse Treg cell
   function. This effect was independent of dendritic cells but required
   functional TLR8-MYD88-IRAK4 signaling in Treg cells. Adoptive transfer
   of TLR8 ligand-stimulated Treg cells into tumor-bearing mice enhanced
   antitumor immunity. These results suggest that TLR8 signaling could play
   a critical role in controlling immune responses to cancer and other
   diseases.}},
