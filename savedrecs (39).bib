@article{ WOS:000231831300023,
Author = {Carpi, F and De Rossi, D},
Title = {{Improvement of electromechanical actuating performances of a silicone
   dielectric elastomer by dispersion of titanium dioxide powder}},
Journal = {{IEEE TRANSACTIONS ON DIELECTRICS AND ELECTRICAL INSULATION}},
Year = {{2005}},
Volume = {{12}},
Number = {{4}},
Pages = {{835-843}},
Month = {{AUG}},
Abstract = {{This paper presents the first reported data on the embedding of highly
   dielectric ceramic inclusions in a rubbery host medium as a means to
   increase the electromechanical material response for dielectric
   elastomer actuation. The studied polymer/ceramic composite, consisting
   of a silicone matrix in which titanium dioxide powder was dispersed,
   exhibited, in comparison with pure silicone, a decreased elastic
   modulus, as well as an increased dielectric constant. The measured low
   frequency permittivity resulted in accordance with several classical
   dielectric mixing rules. The use of this material as elastomeric
   dielectric for planar actuators enabled a reduction of the driving
   electric fields, so that a transverse strain of 11\% at 10 V/mu m and a
   transverse stress of 16.5 kPa at 9 V/mu m were obtained. These levels of
   strain and stress were respectively more than eight and four times
   higher than the corresponding values generated with the pure polymer
   matrix for analogous electrical stimuli.}},
