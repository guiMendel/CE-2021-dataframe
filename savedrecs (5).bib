@article{ WOS:000601460900001,
Author = {Serban, Bogdan A. and Barrett-Catton, Emma and Serban, Monica A.},
Title = {{Tetraethyl Orthosilicate-Based Hydrogels for Drug Delivery-Effects of
   Their Nanoparticulate Structure on Release Properties}},
Journal = {{GELS}},
Year = {{2020}},
Volume = {{6}},
Number = {{4}},
Month = {{DEC}},
Abstract = {{Tetraethyl orthosilicate (TEOS)-based hydrogels, with shear stress
   response and drug releasing properties, can be formulated simply by TEOS
   hydrolysis followed by volume corrections with aqueous solvents and pH
   adjustments. Such basic thixotropic hydrogels (thixogels) form via the
   colloidal aggregation of nanoparticulate silica. Herein, we investigated
   the effects of the nanoparticulate building blocks on the drug release
   properties of these materials. Our data indicate that the age of the
   hydrolyzed TEOS used for the formulation impacts the nanoparticulate
   structure and stiffness of thixogels. Moreover, the mechanism of
   formation or the disturbance of the nanoparticulate network
   significantly affects the release profiles of the incorporated drug.
   Collectively, our results underline the versatility of these basic,
   TEOS-only hydrogels for drug delivery applications.}},
