
@article{ WOS:000424617000013,
Author = {Mackedanz, Christian Ferreira and Gill, Lorena and Rigo, Luiz Carlos},
Title = {{Afro-descendants and Pelotan football in post-abolition (1925-1938)}},
Journal = {{ESTUDIOS HISTORICOS}},
Year = {{2015}},
Number = {{15}},
Month = {{DEC}},
Abstract = {{This paper discusses how football was related to the exclusionary
   context that blacks confronted in the post-abolition period in Pelotas,
   from a social and cultural perspective. In the survey, by analyzing
   periodicals, photographs and oral testimony, it made a debate on what
   the similarities and differences between the national football
   democratization context and the sport situation in this city. It
   analyzed also the role that football will perform in this environment
   already referred, being sometimes reproducer of that social exclusion
   context and at other times acting as an organizing instrument for
   Afro-descendants.}},
