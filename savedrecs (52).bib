Author = {FUKUDA, R and XUANTHANH, N and ISHIMURA, N and ISHIHARA, S and
   CHOWDHURY, A and KOHGE, N and AKAGI, S and WATANABE, M and FUKUMOTO, S},
Title = {{X-GENE AND PRECORE REGION MUTATIONS IN THE HEPATITIS-B VIRUS GENOME IN
   PERSONS POSITIVE FOR ANTIBODY TO HEPATITIS-B E-ANTIGEN - COMPARISON
   BETWEEN ASYMPTOMATIC HEALTHY CARRIERS AND PATIENTS WITH SEVERE CHRONIC
   ACTIVE HEPATITIS}},
Journal = {{JOURNAL OF INFECTIOUS DISEASES}},
Year = {{1995}},
Volume = {{172}},
Number = {{5}},
Pages = {{1191-1197}},
Month = {{NOV}},
Abstract = {{Hepatitis B virus (HBV) carriers with antibody to hepatitis e antigen
   comprise asymptomatic carriers (ASCs), who have low replication levels
   of HBV, and patients with chronic active hepatitis (CAH), who have high
   levels of viral replication. To investigate whether defects in the X
   protein might be responsible for this difference in the level of viral
   replication, nucleotide sequences of X and precore gene regions in serum
   HBV were analyzed in 19 ASCs and 9 CAH patients. All patients had a
   point mutation creating a stop codon in the precore region. Seventeen
   ASCs (87.3\%) had identical mutations consisting of 4 noncontiguous 1-bp
   deletions or an 8-bp deletion, both of which truncate the normal X
   protein, whereas no CAH patient had an X gene mutation (P < .001). Thus,
   deletion of the X protein might be responsible for the low levels of
   viral replication in ASCs.}},
