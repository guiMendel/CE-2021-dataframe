@article{ WOS:A1995TG13700006,
Author = {SEEGERS, JFML and ZHAO, AC and MEIJER, WJJ and KHAN, SA and VENEMA, G
   and BRON, S},
Title = {{STRUCTURAL AND FUNCTIONAL-ANALYSIS OF THE SINGLE-STRAND ORIGIN OF
   REPLICATION FROM THE LACTOCOCCAL PLASMID PWVO1}},
Journal = {{MOLECULAR AND GENERAL GENETICS}},
Year = {{1995}},
Volume = {{249}},
