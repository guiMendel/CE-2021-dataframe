@article{ WOS:000284741500012,
Author = {Caceres, J. and Hernando, C. and Mora, M. and Pelayo, I. M. and Puertas,
   M. L.},
Title = {{On the geodetic and the hull numbers in strong product graphs}},
Journal = {{COMPUTERS \& MATHEMATICS WITH APPLICATIONS}},
Year = {{2010}},
Volume = {{60}},
Number = {{11}},
Pages = {{3020-3031}},
Month = {{DEC}},
Abstract = {{A set S of vertices of a connected graph G is convex, if for any pair of
   vertices u, v is an element of S, every shortest path joining u and v is
   contained in S. The convex hull CH (S) of a set of vertices S is defined
   as the smallest convex set in G containing S. The set S is geodetic, if
   every vertex of G lies on some shortest path joining two vertices in S,
   and it is said to be a hull set if its convex hull is V (G). The
   geodetic and the hull numbers of G are the minimum cardinality of a
   geodetic and a minimum hull set, respectively. In this work, we
   investigate the behavior of both geodetic and hull sets with respect to
   the strong product operation for graphs. We also establish some bounds
   for the geodetic number and the hull number and obtain the exact value
   of these parameters for a number of strong product graphs. (C) 2010
   Elsevier Ltd. All rights reserved.}},
