@article{ WOS:000232933200003,
Author = {Civantos, C},
Title = {{Race/class/language: ``El negro{''} speaks Cuban whiteness in the Teatro
   Bufo}},
Journal = {{LATIN AMERICAN THEATRE REVIEW}},
Year = {{2005}},
Volume = {{39}},
Number = {{1}},
Pages = {{49+}},
Month = {{FAL}},
Abstract = {{This essay focuses on the intersections between race, class, and
   language hierarchical conceptions of ``proper,{''} yet ``authentic,{''}
   Cuban speech - in representations of pseudo-intellectual Afro-Cubans in
   19(th)-Century Cuban blackface theatre. The analysis of three key
   figures that emerge in the theatrical genres of the day (the negro bozal
   and two different versions of the negro catedratico) indicates that each
   one is part of the simultaneous appropriation and rejection of the
   Afro-Cuban, as well as the intertwined presence of the Cuban
   intellectual and the linguistic and cultural authority for which lie
   (she) stands. Thus, whites carry out self-definition through the
   representation of blacks speaking different conceptions of
   ``improper{''} language. The desired white, yet creole, Cuban identity
   is always intertwined with the Afro-Cuban, as well as with definitions
   of what constitutes ``true{''} or ``proper{''} Cuban language.}},
