@article{ WOS:000089859300006,
Author = {Ramharter, G},
Title = {{On the densities of certain lattice packings by parallelepipeds}},
Journal = {{ACTA MATHEMATICA HUNGARICA}},
Year = {{2000}},
Volume = {{88}},
Number = {{4}},
Pages = {{331-340}},
Month = {{SEP}},
Abstract = {{Given ally (non-degenerate) n-dimensional lattice L, let kappa(L) denote
   the supremum of the numbers kappa such that there exists a lattice
   packing Q + L of density kappa where Q is some o-symmetric
   parallelepiped with faces parallel to the coordinate axes, Many efforts
   have been made to determine or estimate the minimal such density
   kappa(n), taken over all n-dimensional lattices. It is known that. lim
   sup(n-->infinity) (kappa(n))(1/n) < 1 and lim inf(n-->infinity)
   (kappa(n))(1/n2) > 0. Here we investigate a sequence of lattices L,
   which are known to minimize the function kappa(L) in dimensions n less
   than or equal to 3 and are likely to provide the minima kappa(n) =
   kappa(L-n) in certain higher dimensions. We establish the inequality
   kappa(L-n) greater than or equal to n(-n/2) which supports the
   conjecture that lim sup(n-->infinity) (kappa(n))(1/(n log n)) is
   positive.}},
