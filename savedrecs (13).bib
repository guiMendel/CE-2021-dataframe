@article{ WOS:000368509300018,
Author = {Ooghe, Erwin and Peichl, Andreas},
Title = {{FAIR AND EFFICIENT TAXATION UNDER PARTIAL CONTROL}},
Journal = {{ECONOMIC JOURNAL}},
