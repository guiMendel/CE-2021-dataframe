@article{ WOS:000367214200189,
Author = {He Gang and Li Yuyang and Bu Conghao and Liu Guanghua and Li Jiangtao},
Title = {{Fabrication of YAG:Ce Glass-Ceramic by Combustion Synthesis under High
   Gravity}},
Journal = {{RARE METAL MATERIALS AND ENGINEERING}},
Year = {{2015}},
Volume = {{44}},
Number = {{1}},
Pages = {{765-768}},
Month = {{NOV}},
Abstract = {{YAG:Ce glass-ceramic was fabricated by combustion synthesis under high
   gravity. The phase composition, microstructure and photoluminescence
   properties of the prepared YAG:Ce glass-ceramic were determined by X-ray
   diffraction (XRD), scanning electron microscope (SEM), micro-Raman
   spectrometer (Raman) and spectrophotometer, respectively. It is found
   that the metallic and ceramic products of the combustion reaction are
   separated from each other, and high purity YAG glass-ceramic is obtained
   with the application of high gravity field. Morphology of YAG crystals
   changes gradually from dendritic to polygonal, and finally to columnar
   shape along high gravity direction. The Raman spectra and compositions
   of residual glasses were analyzed along high gravity direction. The
   formation of graded microstructure in YAG glass-ceramic may be caused by
   the compositional sedimentation and thermal diffusion along high gravity
   direction. The density and hardness of the polygons layer are 4.0 g
   cm(-3) and 9.0 GPa, respectively. The photoluminescence spectrum of
   YAG:Ce glass-ceramic suggests that it can be considered as a candidate
   packaging material in lighting field.}},
