@article{ WOS:A1995TB02000049,
Author = {GHOSH, D and LAHIRI, M and DEB, A and DAS, S and PURKAIT, K and BISWAS,
   B and ROYCHOUDHURY, J and CHATTERJEE, R and ZAFRI, AK},
Title = {{FACTORIAL CORRELATOR STUDY IN S-32-AG/BR INTERACTION AT 200A GEV}},
Journal = {{PHYSICAL REVIEW C}},
Year = {{1995}},
Volume = {{52}},
Number = {{4}},
Pages = {{2092-2096}},
Month = {{OCT}},
Abstract = {{In this paper we present an analysis of our S-32-induced interaction
   data at 200A GeV in terms of factorial correlators. The correlated
   moments are found to increase with decreasing bin-bin separation, D.
   This increase follows a power law within the region D less than or equal
   to 1. Bin-size independence of the factorial correlators is observed for
   the values of D=0.4 and D=0.8. Our data are consistent with the
   dimension-independent scaling relation proposed by Seixas, which gives a
   way out of the `'intermittent''-''nonintermittent'' ambiguity.}},
Publisher = {{AMER PHYSICAL SOC}},
Address = {{ONE PHYSICS ELLIPSE, COLLEGE PK, MD 20740-3844 USA}},
