@article{ WOS:A1995RK73200003,
Author = {TOMITA, H and KOMIYA, S and HORII, Y and NAKAMURA, T},
Title = {{GRAZING-INCIDENCE X-RAY-DIFFRACTION STUDY ON EFFECT OF IMPLANTED BF2+
   AND LINEWIDTH ON TITANIUM SILICIDATION}},
Journal = {{JAPANESE JOURNAL OF APPLIED PHYSICS PART 2-LETTERS}},
Year = {{1995}},
Volume = {{34}},
Number = {{7B}},
Pages = {{L876-L878}},
Month = {{JUL 15}},
Abstract = {{The C19 and C54 fractions in TiSi2 are examined quantitatively by the
   grazing incidence X-ray diffraction method in order to study the effect
   of BF2+ implanted in Si substrates and linewidth on TiSi2 silicidation
   and the phase transition from C49 to C54. BF2+ implantation suppresses
   both silicidation and the phase transition, and the phase transition is
   also markedly suppressed on narrow lines. As a result, all the C49 TiSi2
   remains for 0.5 mu m lines with BF2+ dose of 2 x 10(15) cm(-2) after the
   2nd rapid thermal annealing (800 degrees C, 30 s). Furthermore, we have
   found that the grains of C49 TiSi2 are preferentially oriented to highly
   implanted substrates. This indicates that the phase of these grains is
   stable thermally and difficult to transform to C54 phase.}},
