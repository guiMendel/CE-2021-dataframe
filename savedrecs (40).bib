@article{ WOS:000166245300040,
Author = {Praptowidodo, VS and Khalik, A},
Title = {{Studies on seawater desalination by reverse osmosis at the Badak natural
   gas liquefaction plant, Bontang, East Kalimantan}},
Journal = {{DESALINATION}},
Year = {{2000}},
Volume = {{132}},
