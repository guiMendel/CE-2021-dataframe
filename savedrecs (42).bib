@article{ WOS:000165999200019,
Author = {Kang, JH and Hong, SB and Kim, JI and Chung, ST and Row, KH},
Title = {{Effect of packing size on chomatographic separation of catechin
   compounds in green tea}},
Journal = {{KOREAN JOURNAL OF CHEMICAL ENGINEERING}},
Year = {{2000}},
Volume = {{17}},
Number = {{6}},
Pages = {{723-726}},
Month = {{NOV}},
Abstract = {{EGCG (Epigallocatechin Gallate), one of the catechin compounds abundant
   mainly in green tea, has chemopreventive effects against carcinogenesis.
   The extract at 50 degreesC water from the powder of green tea was
   partitioned with chloroform and ethyl acetate. The resulting solution
   was further purified on three different columns, 4.6x250 mm (40/63 mum),
   4.6x250 mm (15 mum), 3.9x300 mm (10 mum), in order to separate EGCG
   among catechin compounds from green tea. In a given packing size, the
   composition of the binary mobile phase, water and acetonitrile, with
   0.1\% acetic acid was changed to operate in optimized experimental
   conditions. In a packed column of 40/63 mum packing, catechin compounds
   were not well separated. With packing sizes of 15 and 10 mum, the
   resolution of EGCG among catechin compounds was better, and maximum
   injection volume to separate EGCG purely was 100 and 480 mul,
   respectively. The study on the effect of packing size may be utilized
   for preparative chromatographic separation.}},
