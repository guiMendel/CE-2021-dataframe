@article{ WOS:A1995RX76500005,
Author = {AGUGGIA, M and FEBBRARO, A and GILLI, M and RICCIO, A},
Title = {{EXTEROCEPTIVE SUPPRESSION OF TEMPORALIS MUSCLE-ACTIVITY IN A TENSION
   HEADACHE - RECENT FINDINGS REGARDING A CENTRAL PATHOGENIC MECHANISM}},
Journal = {{GIORNALE DI NEUROPSICOFARMACOLOGIA}},
Year = {{1995}},
Volume = {{17}},
Number = {{4}},
Pages = {{107-112}},
Month = {{JUL-AUG}},
Abstract = {{The electrical stimulation of the trigeminal fibers determines the
   inhibition of the voluntary contraction of the temporal muscle. This
   response, called `'exteroceptive suppression'' (ES), is divided in two
   periods of electric silence respectively named ES1 and ES2. The period
   ES2, could represent an antinociceptive reflex of the brainstem, and is
   being revealed an important method in the study of the
