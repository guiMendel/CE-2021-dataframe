@article{ WOS:000233209400001,
Author = {Mohler, DL and Downs, JR and Hurley-Predecki, AL and Sallman, JR and
   Gannett, PM and Shi, XL},
Title = {{DNA cleavage by the photolysis of cyclopentadienyl metal complexes:
   Mechanistic studies and sequence selectivity of strand scission by
   CPW(CO)(3)CH3}},
Journal = {{JOURNAL OF ORGANIC CHEMISTRY}},
Year = {{2005}},
Volume = {{70}},
Number = {{23}},
Pages = {{9093-9102}},
Month = {{NOV 11}},
Abstract = {{The photolysis of CpW(CO)(3)Me has been shown to produce methyl radicals
   and to cleave DNA in a single-stranded manner, and preliminary evidence
   implicated a carbon-centered radical in this process. In this work, the
   mechanism of strand scission in this reaction was determined to occur by
   hydrogen atom abstraction from the 4'- and 5'-positions of the
   deoxyribose moiety of the backbone of DNA. Additionally, in a side
   reaction that does not lead to frank strand scission, all four bases of
   DNA are methylated under these conditions; however, none of these base
   or backbone modifications lead to the formation of abasic sites.}},
