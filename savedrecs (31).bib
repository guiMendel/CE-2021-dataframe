@article{ WOS:000234428500003,
Author = {Pi, YZ and Schumacher, J and Jekel, M},
Title = {{The use of para-chlorobenzoic acid (pCBA) as an ozone/hydroxyl radical
   probe compound}},
Journal = {{OZONE-SCIENCE \& ENGINEERING}},
Year = {{2005}},
Volume = {{27}},
Number = {{6}},
Pages = {{431-436}},
Month = {{DEC}},
Abstract = {{Since OH circle radicals cannot be measured directly during an ozonation
   process, para-chlorobenzoic acid (pCBA) has been used recently as an OH
   circle radical probe compound during ozonation based on its very slow
   direct reaction with ozone and fast reaction with OH circle radicals.
   However, in experiments of this study it was shown that pCBA accelerated
   ozone decay. Furthermore, the formation oh hydrogen peroxide was
   observed during this process. The formed H2O2 increases the
   decomposition of aqueous ozone and leads to enhanced formation of OH
   circle radicals. The chain reaction therefore changes to HO2- ion
   initiated decay of ozone instead of hydroxide ion, OH-. Thus, an error
   in applying pCBA as a probe compound in low scavenger containing waters
   is likely to occur if the scavenging rate of pCBA makes up more than 5\%
   of the total scavenging rate.}},
