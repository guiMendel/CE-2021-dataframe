@article{ WOS:000363825500007,
Author = {Branger, Nicole and Mahayni, Antje and Zieling, Daniel},
Title = {{Robustness of stable volatility strategies}},
Journal = {{JOURNAL OF ECONOMIC DYNAMICS \& CONTROL}},
Year = {{2015}},
Volume = {{60}},
Pages = {{134-151}},
Month = {{NOV}},
Abstract = {{The paper analyzes the robustness of stable volatility strategies, i.e.
   strategies in which the portfolio weight of the stock is inversely
   proportional to its local volatility. These strategies are optimal for a
   CRRA investor if the stock follows a diffusion process, the expected
   excess return is proportional to its volatility, and the hedging demand
   is zero. We assess the performance of stable volatility strategies when
   these restrictive assumptions do not hold, in particular, when the risk
   premium is not proportional to volatility and when the stock price is
   subject to jumps. We find that stable volatility strategies are indeed
   robust or close to robust under a maxmin decision rule. In addition to
   our theoretical results, we perform a simulation analysis to evaluate
   strategies that scale the portfolio weight by the volatility, variance
   or a constant portfolio weight, and also analyze the strategies using
   empirical excess returns. Both analyses confirm the robustness of stable
   volatility strategies. (C) 2015 Elsevier B.V. All rights reserved.}},
