@article{ WOS:A1995RM67700009,
Author = {BUCKLAND, JR and HUNTLEY, JM and TURNER, SRE},
Title = {{UNWRAPPING NOISY PHASE MAPS BY USE OF A MINIMUM-COST-MATCHING ALGORITHM}},
Journal = {{APPLIED OPTICS}},
Year = {{1995}},
Volume = {{34}},
Number = {{23}},
Pages = {{5100-5108}},
Month = {{AUG 10}},
Abstract = {{An algorithm for unwrapping noisy phase maps by means of branch cuts has
   been proposed recently. These cuts join discontinuity sources that mark
   the beginning or end of a 2 pi phase discontinuity. After the placement
   of branch cuts, the unwrapped phase map is unique and independent of the
   unwrapping route. We show how a minimum-cost-matching graph-theory
   method can be used to find the set of cuts that has the global minimum
   of total cut length, in time approximately proportional to the square of
   the number of sources. The method enables one to unwrap unfiltered
   speckle-interferometry phase maps at higher source densities (0.1
   sources pixel(-1)) than any previous branch-cut placement algorithm.}},
