@article{ WOS:000088546800107,
Author = {Phimister, JR and Seider, WD},
Title = {{Semicontinuous, middle-vessel, extractive distillation}},
Journal = {{COMPUTERS \& CHEMICAL ENGINEERING}},
Year = {{2000}},
Volume = {{24}},
Number = {{2-7}},
