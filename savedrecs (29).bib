@article{ WOS:000281009800011,
Author = {Antonellini, Marco and Mollema, Pauline Nella},
Title = {{Impact of groundwater salinity on vegetation species richness in the
   coastal pine forests and wetlands of Ravenna, Italy}},
Journal = {{ECOLOGICAL ENGINEERING}},
Year = {{2010}},
Volume = {{36}},
