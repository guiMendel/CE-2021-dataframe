@article{ WOS:A1995TE56500010,
Author = {OLIVECRONA, T and BERGO, M and HULTIN, M and OLIVECRONA, G},
Title = {{NUTRITIONAL REGULATION OF LIPOPROTEIN-LIPASE}},
Journal = {{CANADIAN JOURNAL OF CARDIOLOGY}},
Year = {{1995}},
Volume = {{11}},
Number = {{G}},
