@article{ WOS:000576753600033,
Author = {Qi, Siming and Chen, Yanan and Lin, Zhichen and Jiang, Wei and Cai,
   Xinhao and Zhou, Qingjun},
Title = {{PrSr(3)F(e)3O(10-delta) as cobalt-free cathode for
