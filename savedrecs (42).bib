@article{ WOS:000165286200005,
Author = {Cloutier, J},
Title = {{Peculiarities and inconsistencies in the Test Multidimensionnel de
   Personnalite}},
Journal = {{CANADIAN PSYCHOLOGY-PSYCHOLOGIE CANADIENNE}},
Year = {{2000}},
Volume = {{41}},
Number = {{4}},
Pages = {{257-266}},
Month = {{NOV}},
Abstract = {{The assessment of psychopathology must address the issue of the so
   called `test taking attitude'. Protocols made at random, faked good or
   bad should be identified and no interpretation of clinical scales should
   be provided for invalid protocols. Three concepts are used in the
   assessment of scale validity. An incongruency scale for the yes-sayers
   is designed to evaluate the agreement between responses of a specific
   yes-sayer and those of yes-sayers in general. An infrequency scale is
   composed of items rarely endorsed by patients and normals. Finally, an
   inconsistency scale is derived from pairs of items highly correlated and
   usually answered in a similar or opposite way. Concomitant validity of
   the three scales is established by mean of correlations with similar
   MMPI-2 scales. By excluding yes-sayers with incongruent responses and
   protocols with many infrequent or inconsistent responses, the assessment
   of psychopathology can be more accurate and the clinical scales more
   specific.}},
