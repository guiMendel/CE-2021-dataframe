@article{ WOS:000579839600188,
Author = {Sharma, Kedar and Morla, Sudhir and Khaire, Kaustubh Chandrakant and
   Thakur, Abhijeet and Moholkar, Vijayanand Suryakant and Kumar, Sachin
   and Goyal, Arun},
Title = {{Extraction, characterization of xylan from Azadirachta indica (neem)
   sawdust and production of antiproliferative xylooligosaccharides}},
Journal = {{INTERNATIONAL JOURNAL OF BIOLOGICAL MACROMOLECULES}},
Year = {{2020}},
Volume = {{163}},
Pages = {{1897-1907}},
Month = {{NOV 15}},
Abstract = {{Xylan extracted from neem sawdust gave 22.5\%, (w/w) yield. The
   extracted xylan was composed of xylose and glucuronic acid at a molar
   ratio of 8:1 and with a molecular mass, similar to 66 kDa. FTIR and NMR
   analyses indicated a backbone of xylan substituted with 4-O-methyl
   glucuronic acid at the O-2 position. FESEM analysis showed a highly
   porous and granular surface structure of xylan. A thermogravimetric
   study of xylan showed thermal denaturation at 271 degrees C. The
   hydrolysis of xylan by recombinant endo-beta-1,4-xylanase produced a
   mixture of xylooligosaccharides ranging from degree of polymerization
   2-7. Xylooligosaccharides inhibited cell growth of human colorectal
   cancer (HT-29) cells but did not affect the mouse fibroblast cells
   confirming its biocompatibility. Western blotting, DNA laddering and
   flow cytometric analysis displayed inhibition of HT-29 cells by
   xylooligosaccharides. FLICA staining and mitochondrial membrane
   potential analyses confirmed the activation of the intrinsic pathway of
   apoptosis. The study amply indicated that the xylooligosaccharides
   produced from neem xylan could be potentially used as an
   antiproliferative agent. (C) 2020 Elsevier B.V. All rights reserved.}},
Publisher = {{ELSEVIER}},
Address = {{RADARWEG 29, 1043 NX AMSTERDAM, NETHERLANDS}},
Type = {{Article}},
Language = {{English}},
