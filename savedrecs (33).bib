@article{ WOS:000232750200003,
Author = {Klein, EL and Moura, CAV and Harris, C and Giret, A},
Title = {{Reconnaissance stable isotope (C, O, H, S) study of Paleoproterozoic
   gold deposits of the Sao Luis craton and country rocks, northern Brazil:
   Implications for gold metallogeny}},
Journal = {{INTERNATIONAL GEOLOGY REVIEW}},
Year = {{2005}},
Volume = {{47}},
Number = {{11}},
Pages = {{1131-1143}},
Month = {{NOV}},
Abstract = {{Caxias, Areal, and Pedra de Fogo are structurally controlled syn- to
   late tectonic and post-metamorphic gold deposits Of Paleoproterozoic age
   located in the Sao Luis craton, northern Brazil. Previous fluid
   inclusion studies indicated reduced, low-salinity CO2-H2O (CH4-N-2)
   fluids trapped at 260 degrees-300 degrees C (Caxias, Areal) to 330
   degrees-400 degrees C (Pedra de Fogo), and at 2 kbar as responsible for
   the mineralization. Oxygen isotope ratios of quartz (+10.4 to +16.2
   parts per thousand), chlorite (+7.6 parts per thousand), and sericite
   (+5.1 parts per thousand), and hydrogen isotope ratios of chlorite (-46
   parts per thousand), sericite (-58 parts per thousand), and fluid
   inclusion water (-32 to -70 parts per thousand) indicate fluid
   delta(18)O values of +2.6 to +5.6 per mil (Caxias), +0.6 to +3.5 per mil
   (Areal), and +10.3 to +12.1 per mil (Pedra de Fogo) and fluid 5D
   composition of -24 to -53 per mil (Caxias), -49 to -62 per mil (Areal),
   and -70 per mil (Pedra de Fogo) at the assumed temperatures. These
   estimated fluid compositions are consistent with metamorphic sources.
   Calcite and fluid inclusion CO2 delta(13)C values between -3.1 and -10.9
   per mil are not diagnostic of a particular origin, but a more negative
   value (-20.2 parts per thousand) found at Caxias indicates a greater
   organic component, at least locally. Sulfide delta(34)S values of -2.8
   parts per thousand to -11.0 parts per thousand reflect possible
   achievement of more oxidized conditions. Hydrogen and oxygen isotope
   analyses of the country rocks suggest minor isotope disequilibrium and
   resetting of oxygen isotope geothermometers. This might indicate
   subsolidus post-crystallization isotopic exchange, linked with
   metamorphism and/or hydrothermal alteration.}},
