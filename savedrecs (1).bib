@article{ WOS:000598302400001,
Author = {Zhang, Fan and Cheng, Susu},
Title = {{Behavioral choices in a dynamic duopoly with process innovation and
   sticky price: Myopia versus farsightedness}},
Journal = {{MANAGERIAL AND DECISION ECONOMICS}},
Year = {{2021}},
Volume = {{42}},
Number = {{3}},
Pages = {{662-674}},
Month = {{APR}},
Abstract = {{This paper investigates the impacts of sticky price and behavioral
   choices on the operation decisions, firm's profits, and social welfare.
   Each player has either myopic behavior or farsighted behavior. Results
   show that (i) players prefer farsighted behavior, which can achieve the
   Pareto optimality, and (ii) when both players are farsighted, they will
   set a higher price but will have an insufficient-effort problem in
   process innovation. Furthermore, by comparing the social welfare in four
   different scenarios, we find that the social welfare is the highest when
   both players are myopic.}},
