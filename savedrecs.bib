@article{ WOS:000651747200001,
Author = {Ding, Yannan},
Title = {{A Late Enlightenment Enterprise: The British East India Company's Survey
   of the Paracels in 1808}},
Journal = {{CARTOGRAPHIC JOURNAL}},
Year = {{2020}},
Volume = {{57}},
Number = {{4, SI}},
Pages = {{366-378}},
Month = {{OCT 1}},
Abstract = {{This paper describes the survey of the Paracels organized by the British
   East India Company (EIC) in 1808 in the context of the late
   Enlightenment. It documents the preparatory work for and execution of
   the survey to show that it overturned the erroneous representations of
   the Paracels on maps and charts from the sixteenth to the early
   nineteenth century. Using new archival material, including ship's
   journals, memoirs, and private letters, this paper examines details of
   this survey, including patronage networks and the role played by
   institutions to argue that the late Enlightenment was a period of
   transformation in the epistemology and methodology of geography.}},
