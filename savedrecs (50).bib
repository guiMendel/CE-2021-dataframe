@article{ WOS:A1995TM60900022,
Author = {Miura, H and Tazuma, S and Kajiyama, G},
Title = {{Partial characterization of regulation of biliary lecithin
   hydrophobicity: Association with organic anion-induced solute
   cholestasis in rats}},
Journal = {{BIOCHEMICAL JOURNAL}},
