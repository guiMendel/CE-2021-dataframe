@article{ WOS:000285071400022,
Author = {Higashi, Kiyoshi and Mikami, Toshiyuki and Yamada, Toru and Kawashima,
   Hitoshi and Kimura, Toru and Kaneko, Hideo},
Title = {{A novel adipokine GM2AP impairs insulin signaling}},
Journal = {{BIOCHEMICAL AND BIOPHYSICAL RESEARCH COMMUNICATIONS}},
Year = {{2010}},
Volume = {{402}},
Number = {{3}},
Pages = {{571-576}},
Month = {{NOV 19}},
Abstract = {{In an attempt to discover novel adipokines, we performed proteomics
   analyses using culture medium from differentiated 3T3-L1 adipocytes, and
   first identified GM2AP The levels of GM2AP mRNA and protein were
   augmented by adipogenesis in cultured adipocytes and expression in
   adipose tissue and serum of obese mice or human subjects was found to be
   significantly higher than in lean counterparts. Exposure of 3T3-L1
   adipocytes to GM2AP protein accelerated dissociation of insulin
   receptor-beta (IR beta) from caveolin-1, and interrupted insulin signal
   transduction Abrogation of GM2AP function by specific antibodies
   augmented glucose uptake. Furthermore, treatment of rat pheochromocytoma
   PC12 NS1 cells with GM2AP impaired NGF signal transduction Taken
   together, these results provide novel insights into the physiological
   functions of GM2AP in obesity (C) 2010 Elsevier Inc All rights reserved}},
