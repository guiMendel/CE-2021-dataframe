@article{ WOS:A1995RL05600012,
Author = {DESIRE, S and COUILLEROT, JP and HILBERT, JL and VASSEUR, J},
Title = {{PROTEIN-CHANGES IN SOLANUM-TUBEROSUM DURING STORAGE AND DORMANCY
   BREAKING OF IN-VITRO MICROTUBERS}},
Journal = {{PLANT PHYSIOLOGY AND BIOCHEMISTRY}},
Year = {{1995}},
Volume = {{33}},
Number = {{4}},
Pages = {{479-487}},
Month = {{JUL-AUG}},
Abstract = {{The mechanisms controling potato tuber dormancy maintenance and release
   are not understood. To identify molecular events associated with
   dormancy release in potato microtubers (Solanum tuberosum L.) produced
   in vitro, changes in soluble proteins were investigated by the use of
   two-dimensional gel electrophoresis. Potato microtubers (var. Desiree)
   used in this study were dormant at the harvest time. The duration of
   dormancy corresponded to 22 weeks in cold storage condition at 4 degrees
   C. During this period, the germination observed after transfer of the
   microtubers at 19 degrees C for 3 weeks, showed a sharp increase from
   8\% to 80\%, between 14 and 22 weeks respectively. The water content of
   the microtubers decreased during the first weeks of storage whereas the
   total soluble protein content was more variable. Two-dimensionnal
   polyacrylamide gel electrophoresis of proteins extracted from potato
   microtubers revealed changes during microtubers storage. The major
   changes appeared after 3 weeks and were characterized by the
   accumulation of three polypeptides (b(5), b(6) and b(7)), with an
   apparent molecular mass of 31 kDa. A protein set (a(2) to a(6)) with
   molecular mass of 40 kDa, which are likely to be part of the patatin
   family, and one polypeptide with a molecular mass of 29.8 kDa (b(8)),
   increased strongly between 4 and 9 weeks. When the first germinations
   were potentially possible after 9 weeks, these polypeptides decreased in
   amount or were not longer detected. When the germination capacity
   increased from the 10th week no protein changes were detected. The
   possible roles of the accumulated proteins on dormancy breakage were
   discussed.}},
