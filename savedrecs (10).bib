Language = {{English}},
Affiliation = {{Burgess, DJ (Corresponding Author), Univ Connecticut, Sch Pharm, Dept Pharmaceut Sci, 69 North Eagleville Rd,U-3092, Storrs, CT 06269 USA.
   Gu, Bing; Wang, Yan; Burgess, Diane J., Univ Connecticut, Sch Pharm, Storrs, CT 06269 USA.}},
DOI = {{10.1016/j.ijpharm.2015.10.056}},
ISSN = {{0378-5173}},
EISSN = {{1873-3476}},
Keywords = {{Polymer blends; PLGA; Microspheres; Foreign body reaction; Glucose
   biosensors; Long-term drug release; Accelerated release testing}},
Keywords-Plus = {{FOREIGN-BODY REACTION; RELEASE; DELIVERY}},
