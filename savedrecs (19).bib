@article{ WOS:000363345900045,
Author = {Satou, Yukihiko and Sueki, Keisuke and Sasa, Kimikazu and Matsunaka,
   Tetsuya and Takahashi, Tsutomu and Shibayama, Nao and Izumi, Daiki and
   Kinoshita, Norikazu and Matsuzaki, Hiroyuki},
Title = {{Technological developments for strontium-90 determination using AMS}},
Journal = {{NUCLEAR INSTRUMENTS \& METHODS IN PHYSICS RESEARCH SECTION B-BEAM
   INTERACTIONS WITH MATERIALS AND ATOMS}},
Year = {{2015}},
