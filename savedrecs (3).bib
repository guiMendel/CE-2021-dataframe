@article{ WOS:000592199000009,
Author = {Goncalves, Alice Souza and Gerardo, Camila Ferreira and Alves Franca,
   Silvia Cristina and Bastos, Daniele Cruz and Santos, Shirleny Fontes},
Title = {{PLASTIC WOOD/INORGANIC FILLER COMPOSITES: INFLUENCE OF FILLER TYPE}},
Journal = {{REVISTA UNIVAP}},
Year = {{2020}},
Volume = {{26}},
Number = {{51}},
Pages = {{108-119}},
Month = {{DEC}},
Abstract = {{The aim of the present work was to study the influence of the addition
   of two types of inorganic fillers, silica (SiO2) and muscovite mica, to
   obtain composites with post-industrial plastic wood residues. Mica is a
   natural product, derived from mining, and silica is obtained by a
   chemical synthesis route (Pechini Method). The composite materials were
   obtained in double screw extruder, pressed to obtain films and later
   characterized by density analysis, Shore D hardness, melt flow index
   (MFI) and optical microscopy. The results indicate that the addition of
   up to 10 \% of muscovite did not modify the properties of the polymeric
   matrix; however, the addition of 10 \% silica caused a reduction in
   density, hardness and MFI values, which may be related to poor
   polymer-filler interaction. From the FTIR spectra, it can be inferred
   that there is no chemical interaction between the matrix and the fillers
   used.}},
