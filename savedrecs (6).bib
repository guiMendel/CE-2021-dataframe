@article{ WOS:000595547400106,
Author = {Kim, Hyeon Ung and Roh, Yoon Ho and Mun, Seok Joon and Bong, Ki Wan},
Title = {{Discontinuous Dewetting in a Degassed Mold for Fabrication of
   Homogeneous Polymeric Microparticles}},
