@article{ WOS:A1995TQ71500003,
Author = {Zhou, YR and Watkins, LR},
Title = {{Rigorous approach to performance modelling of nonlinear, optically
   amplified IM/DD systems}},
Journal = {{IEE PROCEEDINGS-OPTOELECTRONICS}},
Year = {{1995}},
Volume = {{142}},
Number = {{6}},
Pages = {{271-278}},
Month = {{DEC}},
Abstract = {{A method is reported that, through a rigorous treatment of the combined
   effect of chromatic dispersion, self-phase modulation and in-line
   amplifier noise, provides a sound basis for quantitative performance
   analysis in long-haul systems. This is achieved by using a moment
   generating function based approach in which the effect of intersymbol
   interference and the randomness of waveform distortion are fully
   accommodated. Additionally, an improved bound on the bit error rate for
   optically preamplified receivers is proposed which is tighter than the
   Chernoff and modified Chernoff bounds and is independent of gain for all
   gains of practical interest. With the aid of this new bound, the
   influence of chromatic dispersion, self-phase modulation and in-line
   amplifier noise as a function of various system parameters is examined
   and quantified.}},
