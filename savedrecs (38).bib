@article{ WOS:000231284800005,
Author = {Sekiguchi, T},
Title = {{Uniqueness of equilibrium payoffs in finitely repeated games with
   imperfect monitoring}},
Journal = {{JAPANESE ECONOMIC REVIEW}},
Year = {{2005}},
Volume = {{56}},
Number = {{3}},
Pages = {{317-331}},
Month = {{SEP}},
Abstract = {{This paper examines when a finitely repeated game with imperfect
   monitoring has a unique equilibrium payoff vector. This problem is
   nontrivial under imperfect monitoring, because uniqueness of equilibrium
   (outcome) in the stage game does not extend to finitely repeated games.
   A (correlated) equilibrium is equilibrium minimaxing if any player's
   equilibrium payoff is her minimax value when the other players choose a
   correlated action profile from the actions played in the equilibrium.
   The uniqueness result holds if all stage game correlated equilibria are
   equilibrium minimaxing and have the same payoffs. The uniqueness result
   does not hold under weaker conditions.}},
