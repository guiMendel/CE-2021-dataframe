@article{ WOS:000088829500005,
Author = {Malone, RE},
Title = {{Dimensions of vulnerability in emergency nurses' narratives}},
Journal = {{ADVANCES IN NURSING SCIENCE}},
Year = {{2000}},
Volume = {{23}},
Number = {{1}},
Pages = {{1-11}},
Month = {{SEP}},
Abstract = {{Two notions of vulnerability dominate in the nursing literature. In one
   model, vunerability is equated with susceptibility to particular harmful
   agents, conditions, or events at particular times and is considered
   something to be avoided or resisted. Another view regards vulnerability
   as the ever-present, common condition of all sentient beings and a
   condition of nurses' access to understanding patients' experiences. This
   article uses data from an ethnographic study conducted in two hospital
   emergency departments to illustrate tensions between these two stances
   toward vulnerability as they are reflected in emergency nurses'
   narratives.}},
