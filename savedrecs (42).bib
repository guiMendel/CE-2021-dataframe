@article{ WOS:000166283600005,
Author = {Grave, L and Dreyer, D and Dieterle, A and Leroy, P and Michou, AI and
   Doderer, C and Pavirani, A and Lusky, M and Mehtali, M},
Title = {{Differential influence of the E4 adenoviral genes on viral and cellular
   promoters}},
Journal = {{JOURNAL OF GENE MEDICINE}},
Year = {{2000}},
Volume = {{2}},
Number = {{6}},
Pages = {{433-443}},
Month = {{NOV-DEC}},
Abstract = {{Background Strong and stable transgene expression is fundamental to the
   success of recombinant adenovirus vectors in human gene therapy.
   However, control of transgene expression is a complex process, involving
   both viral and cellular factors. In this study, the influence of the E4
   adenoviral region on the activity of various promoters was investigated
   in vitro and in vivo.
   Methods Pairs of isogenic E1 degrees and E1 degrees E4 degrees vectors
   were generated and compared. Levels of transgene expression were
   determined by Northern blot, ELISA and FAGS analysis. Initiation of
   transcription was studied by nuclear run-on assays.
   Results Similar to the viral CMV and RSV promoters, the activity of the
   ubiquitous cellular PGK promoter required the presence of the E4 genes
   in vitro and in vivo. In contrast, transgene expression from selected
   liver- and tumor-specific promoters did not require E4 functions.
   Conclusion Together with the reported low liver toxicity of E1 degrees
   E4 degrees vectors, the independence of E4 of liver-specific promoters
   renders such vectors interesting alternatives to the use of gutless
   vectors. Copyright (C) 2000 John Wiley \& Sons, Ltd.}},
