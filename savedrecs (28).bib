@article{ WOS:000280543500029,
Author = {Li, Xueliang and Shi, Yongtang},
Title = {{On a relation between the Randic index and the chromatic number}},
Journal = {{DISCRETE MATHEMATICS}},
Year = {{2010}},
Volume = {{310}},
Number = {{17-18}},
Pages = {{2448-2451}},
Month = {{SEP 28}},
Abstract = {{The Randic index of a graph G, denoted by R(G), is defined as the sum of
   1/root d(u)d(v) over all edges uv of G, where d(u) denotes the degree of
   a vertex u in G. Caporossi and Hansen proposed a conjecture on the
   relation between the Randic index R(G) and the chromatic number chi(G)
   of a graph G: for any connected graph G of order n >= 2, R(G) >=
   chi(G)-2/2 + 1/root n-1 (root chi(G)-1 + n-chi(G)), and furthermore the
   bound is sharp for all n and 2 <= chi(G) <= n. We prove this conjecture.
   (C) 2010 Elsevier B.V. All rights reserved.}},
