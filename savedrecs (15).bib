@article{ WOS:000364931000002,
Author = {Kritskov, Leonid V. and Sarsenbi, Abdizhahan M.},
Title = {{BASICITY IN L-p OF ROOT FUNCTIONS FOR DIFFERENTIAL EQUATIONS WITH
   INVOLUTION}},
Journal = {{ELECTRONIC JOURNAL OF DIFFERENTIAL EQUATIONS}},
Year = {{2015}},
Month = {{NOV 4}},
Abstract = {{We consider the differential equation
   alpha u{''}(-x) - u{''}(x) = lambda u(x), -1 < x < 1,
   with the nonlocal boundary conditions u(-1) = 0, u'(-1) = u'(1) where
   alpha is an element of (-1,1). We prove that if r = root(1 - alpha)/(1 +
   alpha) is irrational then the system of its eigenfunctions is complete
   and minimal in L-p(-1, 1) for any p > 1, but does not constitute a
   basis. In the case of a rational value of r we specify the way of
   choosing the associated functions which provides the system of all root
   functions of the problem forms a basis in L-p(-1, 1).}},
