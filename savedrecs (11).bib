@article{ WOS:000213840500005,
Author = {da Costa, Silvia and Paez, Dario and Sanchez, Flor and Garaigordobil,
   Maite and Gondim, Sonia},
Title = {{Personal factors of creativity: A second order meta-analysis}},
Journal = {{JOURNAL OF WORK AND ORGANIZATIONAL PSYCHOLOGY-REVISTA DE PSICOLOGIA DEL
   TRABAJO Y DE LAS ORGANIZACIONES}},
Year = {{2015}},
Volume = {{31}},
Number = {{3}},
Pages = {{165-173}},
Month = {{DEC}},
Abstract = {{This article integrates seven meta-analyses of individual factors
   associated with creativity and innovation and reports effects based on
   one (r) or two or more ((r) over bar) meta-analyses. Emotional
   intelligence (r=.31), divergent thinking ((r) over bar=.27), openness to
   experience ((r) over bar=.22), creative personality ((r) over bar=.21),
   intrinsic motivation ((r) over bar=.20), positive affect ((r) over
   bar=.19), and androgyny (r=.19) are related to creativity. Age (r=.17),
   intelligence ((r) over bar=.17), extraversion ((r) over bar=.13),
   self-efficacy ((r) over bar=.13), and extrinsic motivation (r=.11) were
   also moderately associated with innovation. Pro-risk attitudes (r=.08)
   and being female (r=.07) were weakly associated with creativity. Results
   are discussed, explanatory processes described, and practical
   implications for organizations examined. (C) 2015 Colegio Oficial de
   Psicologos de Madrid. Published by Elsevier Espana, S.L.U.}},
