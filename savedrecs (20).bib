@article{ WOS:000285438900020,
Author = {Mishra, Sabyashachi and Meuwly, Markus},
Title = {{Quantitative Analysis of Ligand Migration from Transition Networks}},
Journal = {{BIOPHYSICAL JOURNAL}},
Year = {{2010}},
Volume = {{99}},
Number = {{12}},
Pages = {{3969-3978}},
Month = {{DEC 15}},
Abstract = {{In this work we use transition network analysis for the first time to
   investigate ligand migration in truncated hemoglobin (trHbN) and obtain
   kinetic information about the docking-site dynamics in the protein. A
   comparison with explicit water molecular dynamics simulations (100 ns in
   total) shows that the rate constants derived from the network analysis
   are realistic. The transition network analysis provides 1) The
   time-resolved connectivity network in the protein; 2) The half-lives of
   the docking sites; 3) The transition timescales between two given
   docking sites; and 4) The extent of population transfer among different
   docking sites of the protein as a function of lag time. We investigate
   the role of the Tyr(33) and Gln(58) residues in ligand migration by
   studying ligand migration in four mutants of trHbN. The mutation study
   suggests that residues Tyr(33) and Gln(58) stabilize the NO ligand in
   the Xe2 docking site of trHbN, thus facilitating the efficiency of the
   NO detoxification reaction.}},
