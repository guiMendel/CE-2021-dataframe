@article{ WOS:000167332700024,
Author = {Gravier-Bonnet, N and Migotto, AE},
Title = {{Gonangium development and medusoid of Nemalecium lighti (Hargitt, 1924)
   (Cnidaria : Hydrozoa, Haleciidae)}},
