@article{ WOS:A1995TB58700003,
Author = {RANDZIO, SL and DEITERS, UK},
Title = {{THERMODYNAMIC TESTING OF EQUATIONS OF STATE OF DENSE SIMPLE LIQUIDS}},
Journal = {{BERICHTE DER BUNSEN-GESELLSCHAFT-PHYSICAL CHEMISTRY CHEMICAL PHYSICS}},
