@article{ WOS:000165446600011,
Author = {Kahru, A and Pollumaa, L and Reiman, R and Ratsep, A and Liiders, M and
   Maloveryan, A},
Title = {{The toxicity and biodegradability of eight main phenolic compounds
   characteristic to the oil-shale industry wastewaters: A test battery
   approach}},
