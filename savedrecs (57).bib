@article{ WOS:A1995TA47700001,
Author = {POSTLES, D},
Title = {{NOTIONS OF THE FAMILY, LORDSHIP AND THE EVOLUTION OF NAMING PROCESSES IN
   MEDIEVAL ENGLISH RURAL SOCIETY - A REGIONAL EXAMPLE}},
Journal = {{CONTINUITY AND CHANGE}},
Year = {{1995}},
Volume = {{10}},
Number = {{2}},
Pages = {{169+}},
Month = {{AUG}},
Abstract = {{This article, a detailed study of change within local societies,
   examines the radical transformation of personal naming processes which
   occurred in England between the eleventh and the thirteenth centuries.
   Emerging from the details are finely nuanced aspects of change informed
   by the interrelationship of several influences, including not just
   demographic and cultural change but also the influence of social
   organization and lordship. Whilst social diffusion was important in the
   changing patterns of naming, there were differences in the reception of
   change within social groups, illustrated most importantly for the unfree
   peasantry. Despite the problem that records provide only a formal,
   imposed representation of naming, it is possible to see through them and
   to observe some processes of more colloquial naming.}},
