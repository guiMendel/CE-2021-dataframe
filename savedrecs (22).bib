@article{ WOS:000498210000010,
Author = {Ruessmann, Helmut},
Title = {{KAM ITERATION WITH NEARLY INFINITELY SMALL STEPS IN DYNAMICAL SYSTEMS OF
   POLYNOMIAL CHARACTER}},
Journal = {{DISCRETE AND CONTINUOUS DYNAMICAL SYSTEMS-SERIES S}},
Year = {{2010}},
Volume = {{3}},
Number = {{4, SI}},
Pages = {{683-718}},
Month = {{DEC}},
Abstract = {{In this paper we present a new variant of the KAM theory, containing an
   artificial parameter q, 0 < q < 1, which makes the steps of the KAM
   iteration infinitely small in the limit q NE arrow 1. This KAM procedure
   can be compared for q < 1 with a Riemann sum which tends, for q NE arrow
   1, to the corresponding Riemann integral. As a consequence this limit
   has all advantages of an integration process compared with its
   preliminary stages: Simplification of the conditions for the involved
   parameters and global linearization which therefore improves numerical
   results. But there is a difference from integrals: The KAM iteration
   itself works only for q < 1, however, q can be chosen as near to 1 as we
   want and the limit q NE arrow 1 exists for all involved parameters.
   Hence, the mentioned advantages remain mainly preserved. The new
   technique of estimation differs completely from all what has appeared
   about KAM theory in the literature up to date. Only Kolmogorov's idea of
   local linearization and Moser's modifying terms are left. The basic idea
   is to use the polynomial structure in order to transfer, at least
   partially, the whole KAM procedure outside of the original domain of
   definition of the given dynamical system.}},
