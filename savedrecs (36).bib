@article{ WOS:000232739800013,
Author = {Park, SS and Min, BW and Kim, WB and Han, HJ and Yong, HS and Kim, SJ
   and Kim, CS and Mok, YJ},
Title = {{Double cancer of the stomach and oesophagus with situs ambiguus with
   polysplenia: The importance of preoperative evaluation}},
Journal = {{DIGESTIVE AND LIVER DISEASE}},
Year = {{2005}},
Volume = {{37}},
Number = {{10}},
Pages = {{799-802}},
Month = {{OCT}},
Abstract = {{A rare case of double cancer with situs ambiguus with polysplenia is
   presented. A 58-year-old patient was initially diagnosed with an early
   gastric cancer. On evaluation, the computed tomography of the abdomen
   demonstrated situs ambiguus with polysplenia. We performed a subtotal
   gastrectomy with the stomach being reconstructed in a Billroth-II
   fashion. Three months after the operation, he again visited our
   department complaining nausea and dysphagia. Examinations confirmed the
   other oesophageal malignancy with advanced stage. Because of
   unfamiliarity to situs anomaly and rarity of double cancer, we missed
   anther coexistent cancer. This is the first case presentation of a
   double carcinoma occurring in a patient with situs ambiguus with
   polysplenia. The literature is reviewed and the importance of
   preoperative evaluation is discussed. (c) 2005 Editrice
   Gastroenterologica Italiana S.r.l. Published by Elsevier Ltd. All fights
