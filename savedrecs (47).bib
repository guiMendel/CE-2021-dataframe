@article{ WOS:000088666600010,
Author = {van de Witte, SV and Groenewegen, HJ and Drukarch, B and Voorn, P},
Title = {{Dynorphin modulates dopamine D1-receptor mediated turning behavior in
   6-hydroxydopamine-lesioned rats}},
Journal = {{NEUROSCIENCE LETTERS}},
Year = {{2000}},
Volume = {{290}},
Number = {{1}},
Pages = {{37-40}},
Month = {{AUG 18}},
Abstract = {{We investigated if the potentiated turning response to a challenge with
   the partial dopamine D1 receptor agonist SKF-38393, as seen after
   priming with L-dihydroxyphenytalanine (DOPA) of unilaterally
   6-hydroxydopamine-lesioned rats, can be modulated by infusion of
   dynorphin A (1-17) in the striatum. Seventeen days after the
   6-hydroxydopamine lesion, rats received intrastriatal dynorphin (0.08 or
   3.85 mu g) followed by L-DOPA (50 mg/kg i.p.) and were challenged 3 days
   later with SKF-38393 (3.0 mg/kg s.c.). Compared to controls, the lower
   dose of dynorphin caused an earlier onset of turning, while the higher
   dose decreased the response to SKF-38393. These findings suggest a
   dose-dependent modulatory role for striatal dynorphin in L-DOPA-priming
   of a D1-mediated behavioral response. (C) 2000 Elsevier Science Ireland
   Ltd. All rights reserved.}},
