@article{ WOS:000233914800001,
Author = {Mokhov, II and Khon, VC},
Title = {{Interannual variability and long-term tendencies of change in
   atmospheric centers of action in the northern hemisphere: Analyses of
   observational data}},
Journal = {{IZVESTIYA ATMOSPHERIC AND OCEANIC PHYSICS}},
Year = {{2005}},
Volume = {{41}},
Number = {{6}},
Pages = {{657-666}},
Month = {{NOV-DEC}},
Abstract = {{Interannual and long-term variations of the characteristics of
   atmospheric centers of action (ACAs) in the Northern Hemisphere are
   analyzed. Different empirical data covering the period from the late
   19th to the early 21st century and the reanalysis data are used. The
   emphasis is on the detection of significant changes in ACA
   characteristics in winter, when the long-term surface warming is most
   considerable. Statistically significant tendencies toward the
   intensification of the Aleutian Low and of the North Atlantic centers of
   action in the second half of the 20th century are noted. The tendencies
   of change in the behavior of the Siberian High obtained from different
   empirical data differ from one another. A statistically significant
   connection of the North Pacific centers of action with El Nino/La Nina
   events is noted. During El Nino (La Nina), the Aleutian High deepens
   (weakens) and shifts eastward (westward) and the Hawaiian High weakens
   (intensifies) and , moves southward (northward). It is pointed out that
   there was an increase in the correlation of the characteristics of the
   North Pacific centers of action with sea-surface temperature in the El
   Nino and La Nina region at the end of the 20th century. Wavelet analysis
   shows that the four- to six-year cyclicity of the Aleutian and Hawaiian
   centers of action, which is inherent in El Nino, rengthened by the end
   of the 20th century.}},
