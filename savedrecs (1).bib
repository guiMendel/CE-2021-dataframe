@article{ WOS:000682551700001,
Author = {Wei, Renfeng and Ye, Zhifeng},
Title = {{Experimental and numerical analysis of fluid-solid-thermal coupling on
   electric fuel pump}},
Journal = {{PROCEEDINGS OF THE INSTITUTION OF MECHANICAL ENGINEERS PART G-JOURNAL OF
   AEROSPACE ENGINEERING}},
Year = {{2021}},
Volume = {{235}},
Number = {{11}},
Pages = {{1427-1440}},
Month = {{SEP}},
Abstract = {{This paper designs an axial partition fuel cooling shell to solve the
   problem of temperature rise in the motor of the electric fuel pump
   (EFP). And describes a simplified method in conjunction with the
   computational fluid dynamics(CFD) to analyze heat generations and fuel
   cooling effects in integrated EFPs. Furthermore, CFD is used to
   numerically simulate the coupling effects among the fluid-solid-thermal
   based on multiple physical field. With varying different working
   conditions of the pump, cooling characteristics of the fuel cooling
   shell are obtained through CFD results. Finally, an experimental system
   for the EFP is established to verify reliability of the simplified
   method and the effectiveness of the fuel cooling scheme. Results show
   that fuel cooling shell plays an essential role in heat dissipating,
   with a maximum reduction of up to approximately 42 K in temperature.
   Temperature error between simulations and experiments is less than 4\%,
   which indicates reliabilities of the simplified model and fuel cooling
   shell.}},
