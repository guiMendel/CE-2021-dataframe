@article{ WOS:000283177800002,
Author = {Heffer, Jon and Lewins, Jeffery},
Title = {{Boltzmann's knock: auto-ignition delay times from Arrhenius theory}},
Journal = {{JOURNAL OF NON-EQUILIBRIUM THERMODYNAMICS}},
Year = {{2010}},
Volume = {{35}},
Number = {{3, SI}},
Pages = {{199-211}},
Month = {{OCT}},
Note = {{Joint European Thermodynamics Conference (JETC10), Univ Copenhagen,
   Niels Bohr Inst, Copenhagen, DENMARK, JUN 22-24, 2009}},
Abstract = {{An exact series solution is given for the first-order equation
   describing delay times in chemical kinetics, especially auto-ignition of
   fuels, employing the Arrhenius rate factor. Provision is made for both a
   constant pressure and a constant volume process by means of an index
   that describes the temperature dependence of the reaction rate. To allow
   for the common circumstance that the activation temperature is well
