@article{ WOS:000234470100026,
Author = {Utsunomiya, S and Ewing, RC and Wang, LM},
Title = {{Radiation-induced decomposition of U(VI) phases to nanocrystals of UO2}},
Journal = {{EARTH AND PLANETARY SCIENCE LETTERS}},
Year = {{2005}},
Volume = {{240}},
Number = {{2}},
Pages = {{521-528}},
Month = {{DEC 1}},
Abstract = {{U6+-phases are common alteration products, under oxidizing conditions,
   of uraninite and the UO2 in spent nuclear fuel. These U6+-phases are
   subjected to a radiation field caused by the alpha-decay of U, or in the
   case of spent nuclear fuel, incorporated actinides, such as Pu-239 and
   Np-237. In order to evaluate the effects of alpha-decay events on the
   stability of the U6+-phases, we report, for the first time, the results
   of ion beam irradiations (1.0 McV Kr2+) of U6+-phases. The
   heavy-particle irradiations are used to simulate the ballistic
   interactions of the recoil-nucleus of an a-decay event with the
   surrounding structure. The Kr2+-irradiation decomposed the U6+-phases to
   UO2 nanocrystals at doses as low as 0.006 displacements per atom (dpa).
   U6+-phases accumulate substantial radiation doses (similar to 1.0
   displacement per atom) within 100,000 yr if the concentration of
   incorporated Pu-239 is as high as 1 wt.\%. Similar nanocrystals of UO2
   were observed in samples from the natural fission reactors at Oklo,
   Gabon. Multiple cycles of radiation-induced decomposition to UO2
   followed by alteration to U-64-phases provide a mechanism for the
   remobilization of incorporated radionuclides. (C) 2005 Elsevier B.V. All
   rights reserved.}},
