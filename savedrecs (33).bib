@article{ WOS:000233348500022,
Author = {Kondoh, O and Inagaki, Y and Fukuda, H and Mizuguchi, E and Ohya, Y and
   Arisawa, M and Shimma, N and Aoki, Y and Sakaitani, M and Watanabe, T},
Title = {{Piperazine propanol derivative as a novel antifungal targeting
   1,3-beta-D-glucan synthase}},
Journal = {{BIOLOGICAL \& PHARMACEUTICAL BULLETIN}},
Year = {{2005}},
Volume = {{28}},
Number = {{11}},
Pages = {{2138-2141}},
Month = {{NOV}},
Abstract = {{1,3-beta-D-Glucan synthase, which synthesizes a main component of fungal
   cell wall, is one of the promising targets for antifungal agents. In
   order to identify novel chemical classes of 1,3-beta-D-glucan synthase
   inhibitors, we screened a chemical library monitoring inhibition of the
   Candida albicans 1,3-beta-D-glucan synthase activity. The piperazine
   propanol derivative GS1578 1(2,6-difluoro-phenyl)-carbamic acid
   3-(4-benzothiazol-2-yl-piperazine-1-yl)-propyl esterl was identified as
   a potent inhibitor against 1,3-beta-D-glucan synthase with an IC50 value
   of 0.16 mu m. GS1578 exhibited in vitro antifungal activity against
   pathogenic fungi including C. albicans and Aspergillus fumigatus.
   Temperature-sensitive mutations of the FKS1 gene in the Delta fks2
   background of Saccharomyces cerevisiae, where FKS1 and FKS2 encode
   putative catalytic subunits of 1,3-beta-D-glucan synthase, altered
   sensitivity to GS1578. This suggests that the antifungal activity of the
   piperazine propanol derivative has an effect on 1,3-beta-D-glucan
