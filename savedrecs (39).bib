Title = {{Stabilization of sulfide and sulfite and ion-pair chromatography of
   mixtures of sulfide, sulfite, sulfate and thiosulfate}},
Journal = {{JOURNAL OF CHROMATOGRAPHY A}},
Year = {{2005}},
Volume = {{1085}},
Number = {{1}},
