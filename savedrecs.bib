@article{ WOS:000602988800001,
Author = {Rast, Erich},
Title = {{The theory theory of metalinguistic disputes}},
Journal = {{MIND \& LANGUAGE}},
Abstract = {{According to the theory theory of metalinguistic disputes, disagreements
   in metalinguistic disputes are based on diverging underlying theories,
   opinions, or world views. An adequate description of metalinguistic
   disagreement needs to consider the compatibility and topics of such
   theories. Although topic continuity can be spelled out in terms of
   measurement operations, it is argued that even metalinguistic disputes
   about a term used in different, mutually compatible theories can be
   substantive because the dispute is indirectly about the virtues of the
   underlying theories. The account is defended against externalist and
   holist objections.}},
