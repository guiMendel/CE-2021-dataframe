@article{ WOS:000089534800006,
Author = {Egyed, B and Furedi, S and Angyal, M and Boutrand, L and Vandenberghe, A
   and Woller, J and Padar, Z},
Title = {{Analysis of eight STR loci in two Hungarian populations}},
Journal = {{FORENSIC SCIENCE INTERNATIONAL}},
