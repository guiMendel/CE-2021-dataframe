@article{ WOS:000176458900028,
Author = {Pereira, JC and Vieira, RAM and Gonzalez, J and Alvir, M and de Queiroz,
   AC},
Title = {{Ruminal degradability of agroindustrial by-products}},
Journal = {{REVISTA BRASILEIRA DE ZOOTECNIA-BRAZILIAN JOURNAL OF ANIMAL SCIENCE}},
Year = {{2000}},
Volume = {{29}},
Number = {{6, 2}},
Pages = {{2359-2366}},
Month = {{NOV-DEC}},
Abstract = {{The ruminal degradation kinetics of the dry matter (DM) and crude
   protein (CP) of citrus pulp, tomato pomace (dehy), soybean, sunflower,
   and oat hulls, and the apple pomace were evaluated by the In situ
   technique. Three rumen fistulated weathers were used. The citrus pulp
   undegradable fraction was low (0.34 and 1\% for the DM and CP,
   respectively), which resulted on the high effective degradation values
   (76.07 and 64.3\% for the DM and CP, respectively). The tomato pomace
   (dehy) soluble fraction was low, as far as the DM (12.96\%) and CP
   (14.85\%); were observed also a high lignin contents and acid detergent
   insoluble nitrogen; and the effective degradability was 34.9 and 41.72\%
   for the DM and CP respectively. The soybean hulls undegradable DM
   fraction was high (18.94\%), however, its DM degradation rate was high
   (0.095 h(-1)) and the effective degradations attained values of 53.59\%
   and 54.01\% for DM and CP, respectively. The main characteristic of the
   oat hulls was its high soluble protein fraction (72.24\%). The apple
   pomace DM and CP ruminal degradation profiles showed logistical behavior
   with long discrete lag times, being the parameters estimated by a
   specific model developed to fit them. Values of 72.95 and 54.82\% for
   the DM and CP ruminal degradations, respectively, were estimated for
   this by-product.}},
