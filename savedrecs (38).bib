@article{ WOS:000233769600017,
Author = {Shulga, G and Solodovniks, P and Shakels, V},
Title = {{Some features of the flow behaviour of lignin-containing aqueous blends}},
Journal = {{CELLULOSE CHEMISTRY AND TECHNOLOGY}},
Year = {{2005}},
Volume = {{39}},
Number = {{5-6}},
Pages = {{563-572}},
Month = {{SEP-DEC}},
Abstract = {{The properties of aqueous concentrated polymer solutions, containing
   sulphite lignin and a high-molecular acrylic polymer, were studied by
   FT-IR spectroscopy, rheology, as well as by determining their
   adhesiveness with respect to the wood Surface. The obtained aqueous
   blends were structured liquids owing to the formation of a
   semi-interpenetrating polymer network. The non-linear dependence of the
   viscosity logarithm oil inverse temperature, as well as the values of
   the activation energy of the viscous flow testified a major contribution
   of the inter-polymer H-bonds to the cohesion strength of the spatial
   network. At the same time, the adhesiveness of polymer aqueous blends
   largely depended on the content of a long-chain acrylic polymer present
   in them. The local increase in structure viscosity with increasing shear
   stress made it possible to regard the flow of the aqueous blends of
   polymers as a reversible chemical reaction, proceeding directly with the
   formation of new physicochemical bonds between interpolymer
   supramolecular Structures, but. in Opposite direction with their
   cleavage. Ageing at room temperature resulted in the enhancement of
   structure formation in aqueous polymer blends.}},
