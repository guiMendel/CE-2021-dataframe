@article{ WOS:000232118000006,
Author = {Yang, FZ and Wan, SA and Chang, YL and Wu, HR},
Title = {{A novel objective no-reference metric for digital video quality
   assessment}},
Journal = {{IEEE SIGNAL PROCESSING LETTERS}},
Year = {{2005}},
Volume = {{12}},
Number = {{10}},
Pages = {{685-688}},
Month = {{OCT}},
Abstract = {{A novel objective no-reference metric is proposed for video quality
   assessment of digitally coded videos containing natural scenes. Taking
   account of the temporal dependency between adjacent images of the videos
   and characteristics of the human visual system, the spatial distortion
   of an image is predicted using the differences between the corresponding
   translational regions of high spatial complexity in two adjacent images,
   which are weighted according to temporal activities of the video. The
   overall video quality is measured by pooling the spatial distortions of
   all images in the video. Experiments using reconstructed video sequences
   indicate that the objective scores obtained by the proposed metric agree
   well with the subjective assessment scores.}},
