@article{ WOS:000282837400011,
Author = {Hennequin, C. and Rivera, S. and Quero, L. and Latorzeff, I.},
Title = {{Prostate cancer: Doses and volumes of radiotherapy}},
Journal = {{CANCER RADIOTHERAPIE}},
Year = {{2010}},
Volume = {{14}},
Number = {{6-7}},
Pages = {{474-478}},
Month = {{OCT}},
Abstract = {{Radiotherapy is nowadays a major therapeutic option in prostate cancer.
   Technological improvements allowed dose escalation without increasing
   late toxicity. Some randomized trials have shown that dose escalation
   decreases the biochemical failure rate, without any benefit in survival
   with the present follow-up. However, some studies indicate that the
   distant metastases rate is also decreased. Most of these studies have
   been done without hormonal treatment, and the role of dose escalation in
   case of long-term androgen deprivation is unknown. The target volume
   encompassed the whole gland: however, complete or partial focal
   treatment of the prostate can be done with sophisticated IMRT technique
   and must be evaluated. Proximal part of the seminal vesicles must be
   included in the target volumes. The role of nodal irradiation is another
   debate, but it could be logically proposed for the unfavourable group.
   (C) 2010 Published by Elsevier Masson SAS on behalf of the Societe
   francaise de radiotherapie oncologique (SFRO).}},
