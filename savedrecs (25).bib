@article{ WOS:000284974900021,
Author = {Tamasan, M. and Vulpoi, A. and Vanea, E. and Simon, V.},
Title = {{Textural properties of the medical Algo clay as influenced by
   calcination}},
Journal = {{APPLIED CLAY SCIENCE}},
Year = {{2010}},
Volume = {{50}},
Number = {{3}},
Pages = {{418-422}},
Month = {{NOV}},
Abstract = {{A pharmaceutically available clay Algo clay from the Tibet plateau
   recommended for a large range of medical applications was investigated
   by X-ray diffraction (XRD) thermal analysis scanning electron microscopy
   (SEM) nitrogen adsorption particle size distribution measurements and
   electron paramagnetic resonance (EPR) The major crystalline phase was
   kaolinite with layered structure confirmed by SEM analysis The specific
   surface area of the clay was about 28 m(2)/g the particle size <100 nm
