@article{ WOS:000089989300005,
Author = {Hill, SA and Fekken, GC and Bond, SL},
Title = {{Factor structure integrity of the personal attributes questionnaire: An
   English-French comparison}},
Journal = {{CANADIAN JOURNAL OF BEHAVIOURAL SCIENCE-REVUE CANADIENNE DES SCIENCES DU
   COMPORTEMENT}},
Year = {{2000}},
Volume = {{32}},
Number = {{4}},
Pages = {{234-242}},
Month = {{OCT}},
Abstract = {{The Personal Attributes Questionnaire (PAQ) is one of the pre-eminent
   measures of gender-specific personality characteristics. To evaluate the
   PAQ's viability 20 years after its creation, we assessed the
   dimensionality and psychometric properties of the original PAQ and a
   French version using samples of English-speaking and French-speaking
   university administrators. The structure of the English form of the PAQ
   was supported for both women and men when the data were submitted to
   confirmatory analyses. Support was not as convincing for the
   dimensionality of the French form of the PAQ however. Some evidence for
   expected differences in mean Instrumentality and Expressivity scores was
   found between women and men. Our findings support the continued use of
   the English PAQ as a measure of instrumental and Expressive personality
   traits. Further study of the French PAQ is recommended.}},
