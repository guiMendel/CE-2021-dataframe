@article{ WOS:A1995TR00400015,
Author = {Ashbaugh, MS and Benguria, RD},
Title = {{Sharp upper bound to the first nonzero Neumann eigenvalue for bounded
   domains in spaces of constant curvature}},
Journal = {{JOURNAL OF THE LONDON MATHEMATICAL SOCIETY-SECOND SERIES}},
Year = {{1995}},
Volume = {{52}},
Number = {{2}},
Pages = {{402-416}},
Month = {{OCT}},
Abstract = {{The main result of this paper is that for a domain Omega contained in a
   hemisphere of the n-dimensional sphere S-n the first nonzero Neumann
   eigenvalue mu(1)(Omega) is less than or equal to the first nonzero
   Neumann eigenvalue mu(1)(D) where D is a geodesic ball in S-n of the
   same measure as Omega. Equality occurs if and only if Omega is isometric
   to D. This result generalizes old results of Szego and Weinberger which
   gave the corresponding upper bound For mu(1)(Omega) in the Euclidean
   case, and a result of Chavel for domains in S-n which restricted n to
   lie in a geodesic ball of radius 1/4 pi when n = 2 and to even smaller
   geodesic balls for larger n. The techniques used are analogous to those
   for our recent proof of the Payne-Polya-Weinberger conjecture:
