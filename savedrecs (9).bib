@article{ WOS:000598515200004,
Author = {Dematagoda, Udith},
Title = {{`Machinic Desire': Wyndham Lewis, Masculinity and the Sublime Horror of
   Technological War}},
Journal = {{MODERNIST CULTURES}},
Year = {{2020}},
Volume = {{15}},
Number = {{4}},
Pages = {{488-514}},
Month = {{NOV}},
Abstract = {{This article explores Wyndham Lewis's experience of the First World War,
   and its influence on his varied artistic output. It interrogates how
   Lewis's initial ambivalence towards an emergent technological society
   shifted through direct encounters with mechanized warfare, and
   speculates on the effect of these upon his post-war writing and
   criticism. By contrasting Lewis's thought against that of his Italian
   Futurist contemporaries, I will demonstrate the centrality of their
   divergent conceptions of masculinity in accounting for this
   opposition-and how Lewis's critique of technological society prefigures
   contemporary opposition towards the post-humanist philosophy of
   Accelerationism.}},
