   Jan, Eric},
Title = {{Global shape mimicry of tRNA within a viral internal ribosome entry site
   mediates translational reading frame selection}},
Journal = {{PROCEEDINGS OF THE NATIONAL ACADEMY OF SCIENCES OF THE UNITED STATES OF
   AMERICA}},
Year = {{2015}},
