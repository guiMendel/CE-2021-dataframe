@article{ WOS:000597239600011,
Author = {Mueller, Grit and Freude, Gabriele and Kersten, Norbert},
Title = {{Neuroenhancement in four occupations in Germany}},
Journal = {{GESUNDHEITSWESEN}},
Year = {{2020}},
Volume = {{82}},
Number = {{12}},
Pages = {{971-976}},
Month = {{DEC}},
Abstract = {{Objective The aim of this study was to assess the prevalence of
   pharmacological neuroenhancement in 4 occupations and to investigate its
   association with psychosocial working conditions and mental health.
   Methods In the study on ``Neuroenhancement Behaviour in the Context of
   Stressful Workload - empirical Studies on Employees{''}, 4166 employed
   physicians, advertising experts, publishers and software developers were
   surveyed with computer-assisted personal interviews. Lifetime, 12 months
   and 4 weeks prevalence rates with corresponding confidence intervals
   were calculated. The association of neuroenhancement with psychosocial
   working conditions and mental health were estimated in Poisson
   regression models.
   Results The lifetime prevalence of pharmacological neuroenhancement was
   8.4 \%, the 12 months prevalence 2.9\% and the 4 weeks prevalence 1.3
   \%. Advertising experts and female software developers showed elevated
   prevalence rates for neuroenhancement. Psychosocial working conditions,
   such as support from colleagues, role clarity, emotional demands and job
   insecurity were associated with neuroenhancement in men. Burnout and
   depressive symptoms were related to an increase in neuroenhancement.
   Conclusions Compared to samples from the general population or the
   overall working population, the estimated prevalence rates among the
