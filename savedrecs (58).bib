@article{ WOS:A1995RQ23000005,
Author = {BRUHWILER, E and SAOUMA, VE},
Title = {{WATER FRACTURE INTERACTION IN CONCRETE .2. HYDROSTATIC-PRESSURE IN
   CRACKS}},
Journal = {{ACI MATERIALS JOURNAL}},
Year = {{1995}},
Volume = {{92}},
Number = {{4}},
Pages = {{383-390}},
Month = {{JUL-AUG}},
Abstract = {{Current stability analysis of cracked concrete dams assumes fill uplift
   pressure along the crack. Given the stress continuity known to prevail
   in the fracture process zone (FPZ), it is postulated that internal
   uplift pressure is also a function of crack opening displacements (COD).
   To assess this assumption, fracture experiments in which hydrostatic
   pressure is applied during crack formation and growth are presented.
   Results indicate that there is a gradual transition from zero to full
   hydrostatic pressure along the fracture process zone, and that it is
   independent of the concrete mix and specimen size; however; it is a
   function of both the COD and hydrostatic pressure applied at the crack
   mouth. Simplified models are presented to facilitate the use of these
   results for the stability analysis of a cracked concrete dam.}},
