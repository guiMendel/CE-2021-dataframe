@article{ WOS:000284135400009,
Author = {Cakmak, Hasan and Jansen, Martin},
Title = {{Intrinsic photoluminescence properties of amorphous Si3B3N7 ceramic}},
Journal = {{JOURNAL OF LUMINESCENCE}},
Year = {{2010}},
Volume = {{130}},
Number = {{12}},
Pages = {{2322-2326}},
Month = {{DEC}},
Abstract = {{Amorphous Si3B3N7 ceramic shows intrinsic photoluminescence. Three
   distinctive emission regions have been observed. Excitation with
   wavelengths between 254 and 330 nm stimulates intense blue emissions
   with peak maxima ranging from 447 to 479 nm. Exciting between 315 and
   420 nm results in two broad emission peaks; the first in the range
   between 400 and 440 nm, and the second between 560 and 690 nm. Of these
   two latter emissions, the one in the violet region dominates in
   intensity over the other one in the red region, thus a violet color is
   perceived with the eye via excitation with 330 nm or higher wavelengths.
   The three distinct intrinsic emissions in the photoluminescence spectrum
   of amorphous Si3B3N7 ceramic are related to defect centers in three
   topologically different regions in its structure. Dependence of emission
   on excitation wavelength is associated with the Red-Edge Effect and can
   be explained with a reabsorption mechanism at the red excitation edge.
   (C) 2010 Elsevier B.V. All rights reserved.}},
