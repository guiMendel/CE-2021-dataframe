@article{ WOS:000597990500001,
Author = {Simao, Ana Y. and Goncalves, Joana and Gradillas, Ana and Garcia,
   Antonia and Restolho, Jose and Fernandez, Nicolas and Rodilla, Jesus M.
   and Barroso, Mario and Duarte, Ana Paula and Cristovao, Ana C. and
   Gallardo, Eugenia},
Title = {{Evaluation of the Cytotoxicity of Ayahuasca Beverages}},
Journal = {{MOLECULES}},
Year = {{2020}},
Volume = {{25}},
Number = {{23}},
Month = {{DEC}},
Abstract = {{Ayahuasca is a beverage consumed at shamanic ceremonies and currently
   has gained popularity on recreational scenarios. It contains
   beta-carboline alkaloids and N,N-dimethyltryptamine, which possesses
   hallucinogenic effects. Only a few studies have elicited the
   psychoactive effects and the dose of such compounds on neurological
   dopaminergic cells or animals. In this work, we aimed to study the
   cytotoxic effects of these compounds present in ayahuasca beverages and
   on five different teas (Banisteriopsis caapi, Psychotria viridis,
   Peganum harmala, Mimosa tenuiflora and Dc Ab (commercial name))
   preparations on dopaminergic immortalized cell lines. Moreover, a
   characterization of the derivative alkaloids was also performed. All the
   extracts were characterized by chromatographic systems and the effect of
   those compounds in cell viability and total protein levels were analyzed
   in N27 dopaminergic neurons cell line. This is the first article where
   cytotoxicity of ayahuasca tea is studied on neurological dopaminergic
   cells. Overall, results showed that both cell viability and protein
   contents decreased when cells were exposed to the individual compounds,
   as well as to the teas and to the two mixtures based on the traditional
   ayahuasca beverages.}},
