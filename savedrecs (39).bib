@article{ WOS:000231106200003,
Author = {Capi, G and Doya, K},
Title = {{Evolution of recurrent neural controllers using an extended parallel
   genetic algorithm}},
Journal = {{ROBOTICS AND AUTONOMOUS SYSTEMS}},
Year = {{2005}},
Volume = {{52}},
Number = {{2-3}},
Pages = {{148-159}},
Month = {{AUG 31}},
Abstract = {{Autonomous intelligent agents often must complete non-Markovian
   sequential tasks, which require complex recurrent neural controllers. In
   order to improve the convergence of evolution and reduce the computation
   time, this paper proposes application of an extended evolutionary
   algorithm. We implemented an extended multi-population genetic algorithm
   (EMPGA), where subpopulations apply different evolutionary strategies.
   In addition, subpopulations compete and cooperate among each other.
   Results show that EMPGA outperformed single population genetic algorithm
   (SPGA) by efficiently distributing the number of individuals among
   subpopulations as different strategies became successful during the
   course of evolution. In addition, the comparison with other
   multi-population GA shows that competition between subpopulations
   improved the quality of solution. The evolved neural controllers were
   also tested in the real hardware of Cyber Rodent robot. (C) 2005
   Elsevier B.V. All rights reserved.}},
