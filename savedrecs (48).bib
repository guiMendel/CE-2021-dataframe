@article{ WOS:000088880100036,
Author = {Iannazzo, L and Majewski, H},
Title = {{No involvement of nicotinic receptors in the facilitation of
   acetylcholine outflow in mouse cortex in the presence of neostigmine and
   atropine}},
Journal = {{BRITISH JOURNAL OF PHARMACOLOGY}},
Year = {{2000}},
Volume = {{130}},
Number = {{8}},
Pages = {{2008-2014}},
Month = {{AUG}},
Abstract = {{1 The role of nicotinic and muscarinic receptors in the modulation of
   acetylcholine release was studied using field stimulated mouse cortex
   slices incubated with {[}H-3]-choline.
   2 Both acetylcholine (100 mu M) and the cholinesterase inhibitor
   neostigmine (100 mu M) inhibited the stimulation-induced (S-I) outflow
   of radioactivity but in the presence of atropine (0.3 mu M) an
   enhancement was seen, which may be indicative of facilitatory nicotinic
   receptors.
   3 Mecamylamine (100 mu M) was unable to antagonize the enhancement seen
   in the presence of acetylcholine and atropine. The nicotinic agonist
   dimethylphenylpiperazinium (30 mu M) did not facilitate S-I outflow of
   radioactivity.
   4 A range of nicotinic blockers had no effect on the enhancement seen in
   the presence of neostigmine and atropine, nor did indomethacin, the
   5HT(3) antagonist MDL 7222 nor the NMDA antagonist MK-801.
   5 The inability to block this effect suggests that nicotinic receptors
   are not involved. We postulate, at least for neostigmine, that the
   facilitation is an artefact because of the use of {[}H-3]-choline as a
   radiotracer whereby the efflux of radioactivity is enhanced because the
   radiolabelled acetylcholine is not metabolized to choline and therefore
   flows out of the tissue more readily.}},
