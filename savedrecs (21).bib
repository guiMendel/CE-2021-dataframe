Journal = {{CHEMISTRY OF HETEROCYCLIC COMPOUNDS}},
Year = {{2010}},
Volume = {{46}},
Number = {{9}},
Pages = {{1076-1083}},
Month = {{DEC}},
Abstract = {{Organic ligands containing terpyridine and disulfide groupings linked
   between one another by a bonding -O(CH(2))(n)- (n = 6, 12) fragment have
