@article{ WOS:000165262300003,
Author = {Johansen, PO},
Title = {{Bait attraction studies on the scavenging deepwater isopod Natatolana
   borealis (Crustacea, Isopoda)}},
Journal = {{OPHELIA}},
Year = {{2000}},
Volume = {{53}},
Number = {{1}},
Pages = {{27-35}},
Month = {{OCT}},
Abstract = {{Natatolana borealis (Lilljeborg) was collected at 85 m depth in
   Osterfjord, Western Norway from January to December 1995 in traps baited
   with 300 g pieces of fish tissue. The catches varied from 62 to 237
   individuals after 24 hours deployment. The catches of N. borealis were
   negatively correlated to the tidal amplitude. Occasional current peaks
   above 1.6 cm . s(-1) occurred at the collection site at irregular
   intervals. The current speed was less than 1.6 cm . s(-1) during more
   than 98 \% of deployment time. Estimated mean current speeds during the
   24 hour deployments were generally 0.03-0.45 cm . s(-1) The maximum
   swimming speeds of unfed specimens in the laboratory ranged from 4.4 to
   18.7 cm . s(-1). Adults swam significantly faster than juveniles. The
   maximum distance over which N. borealis was drawn to bait, was estimated
   to a maximum of 190 m. About 95 \% of the trapped N. borealis were
   caught less than 1 m above the bottom at 85 m depth, but the isopods
   swam at least 2.3-2.5 m above the bottom.}},
