@article{ WOS:000089047400008,
Author = {Legerstee, M and Barna, J and DiAdamo, C},
Title = {{Precursors to the development of intention at 6 months: Understanding
   people and their actions}},
Journal = {{DEVELOPMENTAL PSYCHOLOGY}},
Year = {{2000}},
Volume = {{36}},
Number = {{5}},
Pages = {{627-634}},
Month = {{SEP}},
Note = {{International Conference on Infant Studies, ATLANTA, GA, APR, 1998}},
Abstract = {{Under investigation was whether 6-month-old infants expect people to
   behave differently toward persons and inanimate objects. Infants were
   randomly assigned to experimental and control conditions. In the
   experimental conditions, infants were habituated to an actor who either
   talked to or reached for and swiped with something hidden behind an
   occluder. In the test events the actor was occluded, but the infants
   were shown either a person or an object. In the control condition,
   infants only saw the person or object stimulus. Results showed that
   infants who had been habituated to an actor who was talking looked
   longer at the object, and infants who had been habituated to an actor
   who was reaching and swiping looked longer at the person. No difference
   in looking at the stimuli was observed in the control condition. This
   suggests that infants expect people's actions to be related to objects
   in ways that are continuous with more mature, intentional
   understandings.}},
