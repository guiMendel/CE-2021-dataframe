@article{ WOS:000364618000014,
Author = {Jimenez-Reyes, Pedro and Pareja-Blanco, Fernando and
   Balsalobre-Fernandez, Carlos and Cuadrado-Penafiel, Victor and
   Ortega-Becerra, Manuel and Gonzalez-Badillo, Juan J.},
Title = {{Jump-Squat Performance and Its Relationship With Relative Training
   Intensity in High-Level Athletes}},
Journal = {{INTERNATIONAL JOURNAL OF SPORTS PHYSIOLOGY AND PERFORMANCE}},
Year = {{2015}},
Volume = {{10}},
Number = {{8}},
Pages = {{1036-1040}},
Month = {{NOV}},
Abstract = {{Purpose: To examine the relationship between the relative load in full
   squats and the height achieved in jump-squat (JS) exercises and to
   determine the load that maximizes the power output of high-level
   athletes. Method: Fifty-one male high-level track-and-field athletes
   (age 25.2 +/- 4.4 y, weight 77. +/- 6.2 kg, height 179.9 +/- 5.6 cm) who
   competed in sprinting and jumping events took part in the study.
   Full-squat 1-repetition-maximum (1-RM) and JS height (JH) with loads
   from 17 to 97 kg were measured in 2 sessions separated by 48 h. Results:
   Individual regression analyses showed that JH (R-2 = .992 +/- .005) and
   the jump decrease (JD) that each load produced with respect to the
   unloaded countermovement jump (CMJ) (R-2 = .992 +/- 0.007) are highly
   correlated with the full-squat \%1-RM, which means that training
   intensities can be prescribed using JH and JD values. The authors also
   found that the load that maximizes JS's power output was 0\%RM (ie,
   unloaded CMJ). Conclusions: These results highlight the close
   relationship between JS performance and relative training intensity in
   terms of \%1-RM. The authors also observed that the load that maximizes
   power output was 0\%1-RM. Monitoring jump height during JS training
   could help coaches and athletes determine and optimize their training
   loads.}},
