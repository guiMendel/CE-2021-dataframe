@article{ WOS:000088191600009,
Author = {Silverman, GJ and Cary, SP and Dwyer, DC and Luo, L and Wagenknecht, R
   and Curtiss, AE},
Title = {{A B cell superantigen-induced persistent ``Hole{''} in the B-1
   repertoire}},
