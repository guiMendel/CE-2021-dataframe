@article{ WOS:000089304200024,
Author = {Liu, XM and Liu, L and Sun, XH and Zhang, MD},
Title = {{Theoretical analysis of wavelength conversion for cascading second-order
   nonlinearities in silica fiber}},
Journal = {{ACTA PHYSICA SINICA}},
Year = {{2000}},
Volume = {{49}},
Number = {{9}},
Pages = {{1792-1797}},
Month = {{SEP}},
Abstract = {{A novel wavelength conversion method for wavelength division
   multiplexing(WDM) nets by use of cascaded second-order nonlinearities
   (chi((2)):chi((2))) in D-fiber grating after periodically
   thermal/electric-field poling is proposed,and is theoretically analyzed
   for the first time. The coupled mode equations of the chi((2)):chi((2))
   nonlinearities are derived,and the analytic expressions for the
   electric-field intensity amplitude of the conversion light waves and
   conversion efficiency eta are obtained under the small signal
   condition,which are well consistent with numerical calculations. Both
   analytic expressions and numerical results show that,under the suitabl
   phase-matching condition, ? is proportional to the logarithm of the
   square of pump light power,and the 4th power of the grating length L \&
   second-order nonlinearity d. The calculated results also show that the
   wavelength conversion efficiency eta and conversion bandwidth Delta
   lambda of over -17 dB and 120 nm respectively can be reached with this
   fiber grating. With the increase of L, eta increases quickly white Delta
   lambda decreases quickly. The results of simulated calculation and
   theoretical analysis show that the cascaded chi((2)):chi((2)) process is
   different from that of quasi-phases-matched
   difference-frequency-generation.}},
Publisher = {{CHINESE PHYSICAL SOC}},
Address = {{P O BOX 603, BEIJING 100080, PEOPLES R CHINA}},
