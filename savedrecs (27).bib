@article{ WOS:000283109000010,
Author = {Liew, Jin-Yi and Chen, Yuan and Hu, Xiao-Ping},
Title = {{Speed Control of an Ultrasonic Motor Considering the Influence of
   Temperature}},
Journal = {{JOURNAL OF THE KOREAN PHYSICAL SOCIETY}},
Year = {{2010}},
Volume = {{57}},
Number = {{4, 1, SI}},
Pages = {{886-888}},
Month = {{OCT}},
Abstract = {{Ultrasonic motors (USM) have good prospects in various application
   fields, such as actuators in mobile-phone auto-focus (AF) modules due to
   their prominent characteristics. From the perspective use, we must
   understand the influence of the temperature on characteristics such as
   the resonant frequency of the USM. In our experiment, an octagonal
   nut-type USM was produced and optimized by using a finite-element method
   (FEM). The resonant frequency and the rotational speed of the USM were
   measured at temperatures between -20 degrees C and 50 degrees C. With
   decreasing temperature, the results show an increasing resonant
   frequency. However, the speed decreases with decreasing temperature due
   to an excursion of the resonant frequency. By using the feedback voltage
   from an isolated electrode from the piezoelectric ceramic bonded on the
   stator of the USM, we could self adjust the output frequency of the
   driver to stabilize the speed of the USM while changing the temperature.}},
