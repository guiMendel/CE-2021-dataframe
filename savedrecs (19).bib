@article{ WOS:000361153700151,
Author = {Gierlotka, Wojciech and Jendrzejczyk-Handzlik, Dominika and Fitzner,
   Krzysztof and Handzlik, Piotr},
Title = {{On the ternary Ag - Cu - Ga system: Electromotive force measurement and
   thermodynamic modeling}},
Journal = {{JOURNAL OF ALLOYS AND COMPOUNDS}},
Year = {{2015}},
Volume = {{646}},
Pages = {{1023-1031}},
Month = {{OCT 15}},
Abstract = {{The ternary silver-copper-gallium system found application as a solder
   material in jewel crafting and electronics, thus a phase diagram of this
   system seems to be important tool, which is necessary for a proper
   application of different alloys. The activity of gallium in liquid phase
   was determined by electromotive measurement technique and after that the
   equilibrium diagram of Ag - Cu - Ga was modeled based on available
   experimental data using Calphad approach. A set of Gibbs energies was
