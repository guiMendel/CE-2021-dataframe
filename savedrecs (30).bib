@article{ WOS:000234120800012,
Author = {Praprotnik, M and Delle Site, L and Kremer, K},
Title = {{Adaptive resolution molecular-dynamics simulation: Changing the degrees
   of freedom on the fly}},
Journal = {{JOURNAL OF CHEMICAL PHYSICS}},
Year = {{2005}},
Volume = {{123}},
Number = {{22}},
Month = {{DEC 8}},
Abstract = {{We present a new adaptive resolution technique for efficient
   particle-based multiscale molecular-dynamics simulations. The presented
   approach is tailor-made for molecular systems where atomistic resolution
   is required only in spatially localized domains whereas a lower
   mesoscopic level of detail is sufficient for the rest of the system. Our
   method allows an on-the-fly interchange between a given molecule's
   atomic and coarse-grained levels of description, enabling us to reach
   large length and time scales while spatially retaining atomistic details
   of the system. The new approach is tested on a model system of a liquid
   of tetrahedral molecules. The simulation box is divided into two
   regions: one containing only atomistically resolved tetrahedral
   molecules, and the other containing only one-particle coarse-grained
   spherical molecules. The molecules can freely move between the two
   regions while changing their level of resolution accordingly. The hybrid
   and the atomistically resolved systems have the same statistical
   properties at the same physical conditions. (c) 2005 American Institute
   of Physics.}},
