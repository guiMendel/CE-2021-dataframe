@article{ WOS:000089413500013,
Author = {Marsen, B and Lonfat, M and Scheier, P and Sattler, K},
Title = {{Energy gap of silicon clusters studied by scanning tunneling
   spectroscopy}},
Journal = {{PHYSICAL REVIEW B}},
Year = {{2000}},
Volume = {{62}},
Number = {{11}},
Pages = {{6892-6895}},
Month = {{SEP 15}},
Abstract = {{Scanning tunneling spectroscopy was performed on small pristine silicon
   clusters on graphite. For the investigated clusters with sizes between
   2.5 and 40 Angstrom, two distinct size regimes, were found: below 15
   Angstrom, Si clusters show energy gaps up to 450 meV, above that size
   only zero gaps are observed. We conclude that a structural transition
   occurs at the size of 15 Angstrom. Larger clusters assume the diamond
   structure, and the zero gaps originate from the dangling bonds on the
   cluster surface. Smaller clusters appear in high-coordination
   structures, which have eliminated the dangling bonds.}},
