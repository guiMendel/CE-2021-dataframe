@article{ WOS:000599460900019,
Author = {Mulder, Tom and Mortimer, Beth and Vollrath, Fritz},
Title = {{Functional flexibility in a spider's orb web}},
Journal = {{JOURNAL OF EXPERIMENTAL BIOLOGY}},
Year = {{2020}},
Volume = {{223}},
Number = {{23}},
Month = {{DEC}},
Abstract = {{Web spiders rely on vibrations propagated via their web to identify,
   locate and capture entangled prey. Here, we experimentally tested the
   robustness of the orb weaver's predation strategy when webs are severely
   distorted and silk tensions are drastically altered throughout the web,
   a common occurrence in the wild. We assessed prey identification
   efficiency by comparing the spider's initial reaction times towards a
   fruit fly trapped in the web, we measured location efficiency by
   comparing times and number of tugging bouts performed, and we determined
   capture efficiency by comparing capture times. It emerged that spiders
   are capable of identifying, locating and capturing prey in distorted
   webs, albeit taking somewhat longer to do so.}},
