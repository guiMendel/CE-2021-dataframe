@article{ WOS:000600992000013,
Author = {Yang, Yong and Jiang, Wenbin and Jiang, Jinlong and Qiu, Qianqian and
   Mao, Ping and Wu, Mei and Zhang, Lijing},
Title = {{Synthesis of hierarchical ZSM-5 zeolites templated by sodium alginate
   toward enhanced catalytic activity for esterification}},
Journal = {{JOURNAL OF SOLID STATE CHEMISTRY}},
Year = {{2020}},
Volume = {{292}},
Month = {{DEC}},
Abstract = {{Hierarchical zeolites containing secondary mesopores exhibit excellent
   catalytic performance in the liquid phase reaction due to their low
   diffusion resistance. In this study, hierarchical ZSM-5 zeolite
   (ZSM-5-SA) has been successfully prepared via sodium alginate (SA) as
   macro- and mesoporous template. ZSM-5-SA possessed larger particle size
   and enhanced crystallinity owing to SA shortening the nucleation period
   of zeolite crystals. The introduction of SA also promotes Al atoms to
   enter ZSM-5 zeolite, where the particle size and Al content gradually
   increase with increasing SA concentration. Compared to conventional
   ZSM-5 zeolite, ZSM-5-SA possesses higher catalytic activity toward the
   esterification of acetic acid with n-butyl alcohol due to the
   synergistic effect of stronger acid sites and hierarchical porosity of
   ZSM-5-SA. The conversion of acetic acid can reach 47.34\% after five
   cycles.This work paves the way for the development of highly active
   ZSM-5 zeolite catalysts by simple SA template.}},
