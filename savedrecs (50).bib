@article{ WOS:A1995TE65500013,
Author = {CLARKE, RA and SINGH, S and MCKENZIE, H and KEARSLEY, JH and YIP, MY},
Title = {{FAMILIAL KLIPPEL-FEIL SYNDROME AND PARACENTRIC INVERSION
