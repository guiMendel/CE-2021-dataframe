@article{ WOS:000280744100039,
Author = {Bao, Zhi-Hua and Chen, Jian-Xin and Tang, Hui and Zhou, Li-Heng},
Title = {{COMPACT DUAL-BAND BANDPASS FILTER USING SLOT-LINE RESONATOR}},
Journal = {{MICROWAVE AND OPTICAL TECHNOLOGY LETTERS}},
Year = {{2010}},
Volume = {{52}},
Number = {{10}},
Pages = {{2299-2300}},
Month = {{OCT}},
Abstract = {{This letter presents a novel compact dual-hand bandpass filter (BPF),
   which consists of two pairs of half-guided wavelength slot-line
   resonators. The two pairs of resonators, operating at different
   frequencies, generate two passbands for the proposed BPF by utilizing a
   novel coupling scheme. Tight coupling between the two resonators in each
   of the pairs can be simultaneously achieved, and the interaction between
   the two pairs is negligible. Thus the operation frequencies of the two
   passbands can be controlled independently. Several attenuation poles are
   created to enhance the selectivity of the proposed filter. The simulated
   and measured results are presented, showing good agreement. (C) 2010
   Wiley Periodicals, Inc. Microwave Opt Technol Lett 52:2299-2300, 2010;
