@article{ WOS:000231029200006,
Author = {Punnen, AP and Chapovska, O},
Title = {{The bottleneck k-MST}},
Journal = {{INFORMATION PROCESSING LETTERS}},
Year = {{2005}},
Volume = {{95}},
Number = {{5}},
Pages = {{512-517}},
Month = {{SEP 15}},
Abstract = {{The k-MST is a well known NP-hard problem and several approximation
   algorithms exist to solve this problem with a guaranteed performance
   bound. A closely related problem, called the bottleneck k-MST (BMST(k))
   can however be solved in O(m log n) time on graph with n nodes and m
   edges. We propose two algorithms to solve BMST(k), one of complexity O(m
   + n log n) and the other of O(m) time. We also consider a generalization
   of BMST(k) which subsumes many bottleneck problems studied in the
   literature and show that this generalized problem can also be solved in
   0(in) time. (c) 2005 Elsevier B.V. All rights reserved.}},
