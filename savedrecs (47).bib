@article{ WOS:000089084900008,
Author = {Kandarakov, O and Titel, C and Volkova, L and Nosov, A and Ehwald, R},
Title = {{Additional phosphate stabilises uninterrupted growth of a Dioscorea
   deltoidea cell culture}},
Journal = {{PLANT SCIENCE}},
Year = {{2000}},
Volume = {{157}},
Number = {{2}},
Pages = {{209-216}},
Month = {{AUG 22}},
Abstract = {{Suspension cells of Dioscorea deltoidea Wall (strain D-1) were
   maintained in a semicontinuous culture (SCC) in shake flasks at a high
   growth rate. It was shown that continuous propagation growth of this
   culture is unstable on Murashige's and Skoog's (MS) medium due to P
   starvation. On a P-enriched MS-medium the SCC was stable even at mean
   specific growth rates >0.3 day(-1). Highest volumetric concentrations of
   furostanol glycosides were obtained, when a P-enriched SCC was not
   further subcultivated but fed with sucrose. The investigated culture is
   able to control phosphate uptake and to prevent toxicity on media with
   excess P. High concentrations of cellular P-i did not effect the ratio
   of furostanol to starch. (C) 2000 Elsevier Science Ireland Ltd. All
   rights reserved.}},
