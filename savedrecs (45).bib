@article{ WOS:000090125700004,
Author = {Singh, K and Srivastava, AK},
Title = {{Pharmacokinetics and urinary excretion of ciprofloxacin in crossbred cow
   calves}},
Journal = {{INDIAN JOURNAL OF ANIMAL SCIENCES}},
Year = {{2000}},
Volume = {{70}},
Number = {{10}},
Pages = {{1021-1024}},
Month = {{OCT}},
Abstract = {{The pharmacokinetics, urinary excretion and dosage regimen of
   ciprofloxacin after it's single intravenous administration of 5
   mg.kg(-1) was investigated in crossbred cow calves. At 1 min the
   concentration of ciprofloxacin in plasma was 10.3 +/- 0.10 mug.ml which
   rapidly declined to 3.07 +/- 0.02 mug.ml(-1) at 10 min. The minimum
   therapeutic concentration (0.1 mu gml(-1)) in plasma was maintained up
   to 6 hr. The elimination half life and volume of distribution were 3.75
   +/- 0.06 hr and 6.80 +/- 0.15 L.kg(-1) respectively. The distribution
   half life and AUC were 0.21 +/- 0.003 hr and 3.98 +/- 0.05 mug.ml(-1) hr
   respectively. The total body clearance (Cl-B) and tissue/plasma(T/P)
   ratio were 1.24 +/- 0.02 L.kg(-1)hr and 8.01 +/- 0.22 respectively.
   Approximately 18\% of the total administered dose of ciprofloxacin was
   recovered in urine within 24 hr after administration. A satisfactory
   intravenous dosage regimen of ciprofloxacin in cow calves would be 6 mg.
   . kg(-1) followed by 5 mg kg(-1) repeated at 12 hr intervals.}},
