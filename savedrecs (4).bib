@article{ WOS:000600907200004,
Author = {Nagorny, Serge and Ferrante, Marco and Laubenstein, Matthias and Nisi,
   Stefano and Ueki, Tatsuya},
Title = {{Characterization of vanadium of biological origin for possible
   applications in physics experiments}},
Journal = {{JOURNAL OF ENVIRONMENTAL RADIOACTIVITY}},
Year = {{2020}},
Volume = {{225}},
Month = {{DEC}},
Abstract = {{For the first time, vanadium of biological origin, extracted from
   centrifugal fraction of vanadium-storing blood cells of the Ascidia
   sydneiensis samea species, was characterized as regards its isotopic
   composition and content of natural radioactive elements potassium (K),
   thorium (Th) and uranium (U). The natural abundance of vanadium isotopes
   has been confirmed with high accuracy, thus excluding a possible
   selectivity within bio-chemical reactions of vanadium concentration in
   blood cells from seawater. A large potassium concentration (up to 5500 x
   10(-6) g g(-1)) was found in the blood cell samples. The concentration
   of thorium was determined to be about 30 x 10(-9) g g(-1), while the
   uranium concentration was about 150 x 10(-9) g g(-1). Hence, a highly
   efficient two-stage purification approach with a total vanadium recovery
   of better than 70\% was developed and applied. The final concentrations
   of K < 100 x 10(-6) g g(-1) and of U/Th < 0.5 x 10(-9) g g(-1) in the
   purified vanadium-containing samples were achieved. Vanadium extracted
   from centrifugal fraction of vanadium-storing blood cells after
   two-stage purification approach could be utilized in various
   applications, where a high chemical purity compound is required.
   However, to be used as a source of radiopure vanadium in
   ultra-low-background experiment aimed to search for V-50 beta decay, it
   should be further purified by Electron Beam Melting against residual
   potassium.}},
