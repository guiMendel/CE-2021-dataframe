@article{ WOS:A1995QV79800002,
Author = {AWAIDA, M and BEECH, JR},
Title = {{CHILDRENS LEXICAL AND SUBLEXICAL DEVELOPMENT WHILE LEARNING TO READ}},
Journal = {{JOURNAL OF EXPERIMENTAL EDUCATION}},
Year = {{1995}},
Volume = {{63}},
Number = {{2}},
Pages = {{97-113}},
Month = {{WIN}},
Abstract = {{A sample of 236 4- to 6-year-olds was tested on reading, spelling,
   vocabulary, short-term memory, visual discrimination, nonverbal
   intelligence, phonological processing, acid fluency, The children were
   retested 1 year later. The 5-year-olds' reading quotients were predicted
   by their phonological processing and visual discrimination scores 1 year
   earlier The 6-year-olds' reading quotients were predicted by their
   previous year's reading quotients, socioeconomic status, phonological
   processing, and nonword reading scores. The 7-year-olds' reading
   quotients were predicted by their previous year's reading quotients and
   nonword reading scores. Factors influencing the development of lexical
   and sublexical processes also were examined. At the initial stage,
   phonemic and pattern-recognition skills were influential for the lexical
   route, but, when formal teaching of reading began, there was a strong
   Matthew effect: Children already good at reading tended to do even
   better the following year. Similar factors affected sublexical
   development initially, but prior phonemic processes and spelling ability
   became important once formal teaching began. There appeared to be an
   initial lack of differentiation between lexical and sublexical routes,
   but, by the 3rd year of the developmental period studied, they were
   sharply differentiated.}},
