@article{ WOS:000166043000004,
Author = {Sekiguchi, H and Sudo, M and Watanabe, T and Kanzawa, A},
Title = {{Generation system for heat sink at low temperature using chemical heat
   pump with reaction of isobutylene/water/t-butyl alcohol}},
Journal = {{SEKIYU GAKKAISHI-JOURNAL OF THE JAPAN PETROLEUM INSTITUTE}},
Year = {{2000}},
Volume = {{43}},
Number = {{6}},
Pages = {{396-402}},
Month = {{NOV}},
Abstract = {{A new heat-driven chemical heat pump was proposed using
   isobutylene/water/t-butyl alcohol reaction system. It provides a low
   temperature heat sink and uses waste energy at temperatures below 373 K
   which is abundant in industry. This system was composed of evaporator,
   condenser, exothermic reactor, and endothermic reactor. The cold thermal
   energy was generated at the evaporator. The near was released from the
   condenser and the exothermic reactor at ambient temperature. The
   endothermic reactor absorbed energy from the low temperature heat
   source. A rectifying column was also located at the exit of the
   endothermic reactor to concentrate isobutylene in the evaporator.
   Moreover two heat exchangers were added to the system to reduce the heat
   input. The system analyses including equilibrium calculations, heat and
   mass balances indicated that optima of the reflux ratio and the
   endothermic reactor temperature existed where the maximum of the
   coefficient of performance (COP) was obtained.}},
