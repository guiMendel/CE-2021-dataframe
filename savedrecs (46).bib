@article{ WOS:000089919300007,
Author = {Khadaroo, AJ},
Title = {{Testing restrictions on cointegrating vectors: A comment}},
Journal = {{JOURNAL OF MACROECONOMICS}},
Year = {{2000}},
Volume = {{22}},
Number = {{4}},
Pages = {{695-705}},
Month = {{FAL}},
Abstract = {{This paper questions the methodology of Cutler, Davies, Rhodd and
   Schwarn (CDRS) (199'i) when testing restrictions on cointegrating
   vectors in a macroeconomic system. CDRS use the Johansen and Juselius
   (1992) H-5 test for testing single-vector and multiple-vector
   restrictions. I here argue that the single-vector H-5 test as applied by
   CDRS is redundant and only serves to inflate the appropriate p-value
   which arises from the Johansen and Juselius (1992) H-6 test. The p-value
   for the Hg test is theoretically predictable given the Hs test results.
   Regarding multiple vector restrictions, I argue that a test by Johansen
   (1996) is appropriate. From an economic perspective, my findings do
   reduce support for the existence of a U.S. long-run M1 equation in the
   macroeconomic system used by CDRS.}},
