@article{ WOS:000228098400007,
Author = {Abramowitz, JS and Whiteside, SR and Deacon, BJ},
Title = {{The effectiveness of treatment for pediatric obsessive-compulsive
   disorder: A meta-analysis}},
Journal = {{BEHAVIOR THERAPY}},
Year = {{2005}},
Volume = {{36}},
Number = {{1}},
Pages = {{55-63}},
Month = {{WIN}},
Abstract = {{The last decade has seen a noticeable increase in the number of
   treatment outcome studies for pediatric Obsessive-compulsive disorder
   (OCD). The present article describes a meta-analysis of this literature
   with the aim of quantifying treatment effects and examining the extent
   to which various patient or treatment variables are related to outcome.
   Results showed that pharmacotherapy with serotonergic antidepressants
   and cognitive-behavioral therapy involving exposure and response
   prevention are each effective in reducing OCD symptoms.
   Cognitive-behavioral therapy produced larger effect sizes and greater
   rates of clinically significant improvement compared to medication,
   although there were methodological differences between medication and
   psychotherapy studies.}},
