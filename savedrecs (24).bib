@article{ WOS:000283509900023,
Author = {Hussain, S. M. and Mohan, S. P. and Jonathan, M. P.},
Title = {{Ostracoda as an aid in identifying 2004 tsunami sediments: a report from
   SE coast of India}},
Journal = {{NATURAL HAZARDS}},
Year = {{2010}},
Volume = {{55}},
Number = {{2}},
Pages = {{513-522}},
Month = {{NOV}},
Abstract = {{Four short core samples were collected from the creek, estuarine regions
   of southeast (SE) coast of India affected by the 2004 Asian Tsunami. The
   study is aimed to signify the importance of ostracoda species in
   identifying major natural events (e.g. Tsunamis) in the coastal regions.
   The presence of many marine ostracoda species in the beach areas and the
   comparative studies with earlier reports from SE coast indicate that
   these species were brought by the high-energy tsunami waves. The
   depositional feature of ostracoda species in the beach and estuarine
   region also infers on the nature and force of tsunami waves in a
   particular region. The results clearly support that microfossils can be
   used to identify the major natural events close to coastal regions.}},
