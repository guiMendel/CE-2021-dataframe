@article{ WOS:000166222800005,
Author = {Kravchenko, SM and Okrugin, AV and Kosolapova, MV},
Title = {{Mechanisms of the concentration of platinum group elements in the Konder
   Massif (Aldan Shield, Russia)}},
Journal = {{GEOLOGY OF ORE DEPOSITS}},
Year = {{2000}},
Volume = {{42}},
Number = {{6}},
Pages = {{503-514}},
Month = {{NOV-DEC}},
Abstract = {{New analyses of isoferroplatinum (216) and iridium-osmium alloys (66)
   from placers associated with the dunite stock of the Konder
   alkaline-ultrabasic massif, as well as published analyses of dunite
   chromites, have been correlated. It has been shown that the distribution
   of elements in isoferroplatinum as well as in osmium-iridium alloys and
   chromites corresponds to the Rayleigh fractional crystallization model.
   This means that these elements belong to rocks of comagmatic series. A
   unimodal logarithmic statistic of distribution of the Ir/Pd ratios in
   isoferroplatinum has been established. For the first time, we have found
   the presence of structural cells in which the microfractionation of
   melts and formation of chromites and platinum group minerals (PGM) of
   varying compositions took place. The results of our study testify to the
   orthomagmatic origin of platinum mineralization. These results do not
   agree with the model of PGM formation at a postmagmatic stage during the
   process of recrystallization of dunites and chromites.}},
