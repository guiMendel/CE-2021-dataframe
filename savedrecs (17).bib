@article{ WOS:000363461900006,
Author = {LaLonde, Kate B. and Zimmermann, Zachary J. and Poling, Alan},
Title = {{Behavioral Gerontology Research: Where are the Male Participants?}},
Journal = {{BEHAVIORAL INTERVENTIONS}},
Year = {{2015}},
Volume = {{30}},
Number = {{4}},
Pages = {{378-385}},
Month = {{NOV}},
Abstract = {{It has been reported that females are underrepresented in several areas
   of psychological research, which may have significant clinical and
   theoretical implications. No one has reported, however, whether women
   are underrepresented in behavioral gerontology research. To make this
   determination we recorded the gender of participants in behavioral
   gerontology studies published in six journals. In 129 published studies
   that included older adults (i.e. people 55 or more years of age) there
   were a total of 3070 participants, and most (69\%) were female.
   Underrepresentation of men is significant insofar as treatment effects
   are sometimes gender specific, making it inappropriate to generalize
   results from females to males (or vice versa). Moreover,
   underrepresentation of older men suggests that their behavioral needs
   may not be generating the attention they deserve. Copyright (c) 2015
   John Wiley \& Sons, Ltd.}},
