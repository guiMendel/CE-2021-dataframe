@article{ WOS:000599947100001,
Author = {Song, Yang and Qu, Wenwen and He, Yuhang and Yang, Hanxiao and Du, Miao
   and Wang, Aijuan and Yang, Qing and Chen, Yuanqing},
Title = {{Synthesis and processing optimization of N-doped hierarchical porous
   carbon derived from corncob for high performance supercapacitors}},
Journal = {{JOURNAL OF ENERGY STORAGE}},
Year = {{2020}},
Volume = {{32}},
Month = {{DEC}},
Abstract = {{In this paper, N-doped hierarchical porous carbon-based supercapacitor
   electrode materials have been synthesized by means of pyrolysis methods
   using corncob, KOH and urea as biomass sources, activator and nitrogen
   source, respectively. The influence of carbonization temperature on the
   morphology and structure and N doping content on supercapacitor
   performance of corncob derived porous carbons has been investigated
   systematically. The specific capacitance of sample with carbonization
   temperature at 800 degrees C and urea: corncob mass ratio of 4:3 reaches
   382.6 F/g at 0.5 A/g, and still maintains as high as 232.7 F/g at high
   current density of 20 A/g. The symmetric supercapacitor achieves a high
