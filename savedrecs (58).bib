@article{ WOS:A1995RK72000001,
Author = {WU, XP and DENG, ZG and ZHOU, ZL and FANG, LZ and QIN, B},
Title = {{ON THE MEASUREMENT OF THE HUBBLE CONSTANT IN A LOCAL LOW-DENSITY
   UNIVERSE}},
Journal = {{ASTROPHYSICAL JOURNAL}},
Year = {{1995}},
Volume = {{448}},
Number = {{2, 2}},
Pages = {{L65-L68}},
Month = {{AUG 1}},
Abstract = {{Astrophysical observations indicate that the `'local universe'' has a
   relatively lower matter density (Omega(0)) than the predictions of the
   standard inflation cosmology and the large-scale motions of galaxies
   which provide a mean mass density to be very close to unity. In such a
   local underdense region, the Hubble expansion may not be representative
   of the global behavior. Utilizing an underdense sphere embedded in a
   flat universe as the model of our `'local universe,'' we show that the
   local Hubble constant would be 1.2-1.4 times larger than the global
   value on scale of similar to 80 Mpc, depending on the variation of
   Omega(0). This may account for the recent measurements of the
   unpleasantly large Hubble constant of similar to 80 km s(-1) Mpc(-1)
   using the Cepheid variables in the Virgo cluster and the relative
   distance between Virgo and Coma cluster and removes the resulted
   apparent paradox of the age of our universe.}},
