@article{ WOS:000593389300001,
Author = {Zhang, Hao and Zhao, Yuxuan and Fang, Chunyu and Li, Guo and Zhang, Meng
   and Zhang, Yu-hui and Fei, Peng},
Title = {{Exceeding the limits of 3D fluorescence microscopy using a
   dual-stage-processing network}},
