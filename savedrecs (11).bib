@article{ WOS:000367028400006,
Author = {Ding MingShuai and Han WeiZhong and Li Ju and Ma Evan and Shan ZhiWei},
Title = {{In situ study of the mechanical properties of airborne haze particles}},
Journal = {{SCIENCE CHINA-TECHNOLOGICAL SCIENCES}},
Year = {{2015}},
Volume = {{58}},
Number = {{12}},
Pages = {{2046-2051}},
Month = {{DEC}},
Abstract = {{Particulate pollution has raised serious concerns regarding its
   potential impacts on human health in developing countries. However, much
   less attention has been paid to the threat of haze particles to
   machinery and industry. By employing a state-of-the-art in situ scanning
   electron microscope compression testing technique, we demonstrate that
   iron-rich and fly ash haze particles, which account for nearly 70\% of
   the total micron-sized spherical haze particles, are strong enough to
   generate abrasive damage to most engineering alloys, and therefore can
   generate significant scratch damage to moving contacting surfaces in
   high precision machineries. Our finding calls for preventive measures to
   protect against haze related threat.}},
Publisher = {{SCIENCE PRESS}},
Address = {{16 DONGHUANGCHENGGEN NORTH ST, BEIJING 100717, PEOPLES R CHINA}},
