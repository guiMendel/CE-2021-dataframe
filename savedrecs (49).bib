@article{ WOS:000087589700002,
Author = {Lindell, MK},
Title = {{An overview of protective action decision-making for a nuclear power
   plant emergency}},
Journal = {{JOURNAL OF HAZARDOUS MATERIALS}},
Year = {{2000}},
Volume = {{75}},
Number = {{2-3, SI}},
Pages = {{113-129}},
Month = {{JUL 28}},
Abstract = {{Protection of the public in a nuclear power plant emergency requires
   decision-makers to balance the time requirements from two chains of
   events: the events associated with a radiological release and the events
   involved with the response to that release. The management of these
   events is distributed among personnel at the nuclear plant, in the local
   community, and in state and federal agencies. All of these parties must
