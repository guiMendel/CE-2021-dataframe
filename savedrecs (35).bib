@article{ WOS:000232544800055,
Author = {Boyko, O and Valentin, C and Rey, G and Antonucci, L and Balcou, P and
   Coudreau, S},
Title = {{Temporal superresolution of ultrashort laser pulses}},
Journal = {{OPTICS EXPRESS}},
Year = {{2005}},
Volume = {{13}},
Number = {{20}},
Pages = {{8231-8239}},
Month = {{OCT 3}},
Abstract = {{We introduce the notion of temporal superresolution for ultrashort laser
   pulses, by analogy with the well-known method of optical superresolution
   in imaging systems. Simple linear spectral masks are presented, that
   shape a laser pulse into a central peak of very short duration, well
   below the Fourier limited width of Gaussian pulses, but accompagnied by
   low intensity satellite pulses. A proof-of-principle experiment is
   presented, using a short pulse, high intensity laser system. Such lasers
   may induce very strongly non linear phenomena in laser-matter
   interactions, suppressing the effect of the satellite pulses and
   therefore fully mimicking shorter light pulses. As an example, we show
   theoretically that the field ionization asymmetry induced by cosine few
   cycle pulses is strongly enhanced using temporally superresolved laser
   pulses. (c) 2005 Optical Society of America.}},
