@article{ WOS:A1995RT64100013,
Author = {FUNG, YS and WONG, LWY},
Title = {{APPORTIONMENT OF AIR-POLLUTION SOURCES BY RECEPTOR MODELS IN HONG-KONG}},
Journal = {{ATMOSPHERIC ENVIRONMENT}},
Year = {{1995}},
Volume = {{29}},
Number = {{16}},
Pages = {{2041-2048}},
