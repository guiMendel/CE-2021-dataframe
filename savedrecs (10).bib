@article{ WOS:000368290300011,
Author = {Fields, Alexander P. and Rodriguez, Edwin H. and Jovanovic, Marko and
   Stern-Ginossar, Noam and Haas, Brian J. and Mertins, Philipp and
   Raychowdhury, Raktima and Hacohen, Nir and Carr, Steven A. and Ingolia,
   Nicholas T. and Regev, Aviv and Weissman, Jonathan S.},
Title = {{A Regression-Based Analysis of Ribosome-Profiling Data Reveals a
   Conserved Complexity to Mammalian Translation}},
Journal = {{MOLECULAR CELL}},
Year = {{2015}},
Volume = {{60}},
Number = {{5}},
Pages = {{816-827}},
Month = {{DEC 3}},
Abstract = {{A fundamental goal of genomics is to identify the complete set of
   expressed proteins. Automated annotation strategies rely on assumptions
   about protein-coding sequences (CDSs), e.g., they are conserved, do not
   overlap, and exceed a minimum length. However, an increasing number of
   newly discovered proteins violate these rules. Here we present an
   experimental and analytical framework, based on ribosome profiling and
   linear regression, for systematic identification and quantification of
   translation. Application of this approach to lipopolysaccharide-
   stimulated mouse dendritic cells and HCMV-infected human fibroblasts
   identifies thousands of novel CDSs, including micropeptides and variants
   of known proteins, that bear the hallmarks of canonical translation and
   exhibit translation levels and dynamics comparable to that of annotated
   CDSs. Remarkably, many translation events are identified in both mouse
   and human cells even when the peptide sequence is not conserved. Our
   work thus reveals an unexpected complexity to mammalian translation
   suited to provide both conserved regulatory or protein-based functions.}},
