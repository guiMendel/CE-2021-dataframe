@article{ WOS:000232721400030,
Author = {Casey, SJ and Newell, FN},
Title = {{The role of long-term and short-term familiarity in visual and haptic
   face recognition}},
Journal = {{EXPERIMENTAL BRAIN RESEARCH}},
Year = {{2005}},
Volume = {{166}},
Number = {{3-4}},
Pages = {{583-591}},
Month = {{OCT}},
Note = {{5th International Multisensory Research Forum, Barcelona, SPAIN, JUN
   02-05, 2004}},
Abstract = {{Recent studies have suggested that the familiarity of a face leads to
   more robust recognition, at least within the visual domain. The aim of
   our study was to investigate whether face familiarity resulted in a
   representation of faces that was easily shared across the sensory
   modalities. In Experiment 1, we tested whether haptic recognition of a
   highly familiar face (one's own face) was as efficient as visual
   recognition. Our observers were unable to recognise their own face
   models from tactile memory alone but were able to recognise their faces
   visually. However, haptic recognition improved when participants were
   primed by their own live face. In Experiment 2, we found that short-term
   familiarisation with a set of previously unfamiliar face stimuli
   improved crossmodal recognition relative to the recognition of
   unfamiliar faces. Our findings suggest that familiarisation provides a
   strong representation of faces but that the nature of the information
   encoded during learning is critical for efficient crossmodal
   recognition.}},
