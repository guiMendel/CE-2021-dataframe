@article{ WOS:000285121200010,
Author = {Mundhenk, Martin and Schneider, Thomas and Schwentick, Thomas and Weber,
   Volker},
Title = {{Complexity of hybrid logics over transitive frames}},
Journal = {{JOURNAL OF APPLIED LOGIC}},
Year = {{2010}},
Volume = {{8}},
Number = {{4, SI}},
Pages = {{422-440}},
Month = {{DEC}},
Abstract = {{This article examines the complexity of hybrid logics over transitive
   frames, transitive trees, and linear frames. We show that satisfiability
   over transitive frames for the hybrid language extended with the
   downarrow operator down arrow is NEXPTIME-complete. This is in contrast
   to undecidability over arbitrary frames (Areces et al. (1999) {[}2]). We
   also show that adding the @ operator or the past modality leads to
   undecidability over transitive frames. This is again in contrast to the
   case of transitive trees and linear frames, where we show these
   languages to be nonelementarily decidable. Furthermore, we establish
   2EXPTIME and EXPTIME upper bounds for satisfiability over transitive
   frames and transitive trees, respectively, for the hybrid Until/Since
   language and complement them with an EXPTIME lower bound for the modal
   Until language. (C) 2010 Elsevier B.V. All rights reserved.}},
