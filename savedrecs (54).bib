@article{ WOS:A1995TD11900006,
Author = {HEBL, MR},
Title = {{GENDER BIAS IN LEADER SELECTION}},
Journal = {{TEACHING OF PSYCHOLOGY}},
Year = {{1995}},
Volume = {{22}},
Number = {{3}},
Pages = {{186-188}},
Month = {{OCT}},
Abstract = {{In this classroom exercise, students experience how stereotypes can
   result in biased leader selection and learn some introductory
   information about task-oriented competitive and social cooperative
   leaders. Students are placed in initially leaderless, mixed-sex (two
   men, two women) groups and asked to select leaders in preparation for a
   group activity. Half of the groups receive instructions that focus on
   competition; the other half receive instructions that emphasize
   cooperation. Overall, a disproportionate number of men are selected as
   leaders, substantiating part research that shows gender stereotypes
   guide individuals in selecting leaders. However, this bias appeared only
   after task-oriented competitive instructions. Men and women were
   selected as group leaders equally often after receiving social
   cooperative instructions. Questions that probe these findings are
   provided for class discussion. Students find this simple demonstration
   to be provocative, and they indicate that it helps them understand
   difficult concepts.}},
