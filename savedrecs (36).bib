@article{ WOS:000232478500004,
Author = {Taherian, H and Ramiar, A},
Title = {{A novel technique for radiation shape factor calculation using CAD
   software}},
Journal = {{NUMERICAL HEAT TRANSFER PART B-FUNDAMENTALS}},
Year = {{2005}},
Volume = {{48}},
Number = {{4}},
Pages = {{387-403}},
Month = {{OCT}},
Abstract = {{The most useful method in calculating radiation heat transfer between
   surfaces is using the concept of radiation shape factor. There are many
   methods for calculating the shape factor, but only a small number of
   software or computer programs exist that can calculate it, and most of
   them have limited capabilities for this procedure. In this article, a
   computer program written in VBA is presented that uses the hemisphere
   method to calculate the shape factor between two arbitrary 2-D (region)
   or 3-D (3dsolid) objects in AutoCAD environment. First and second
   objects are selected by the user from the AutoCAD screen. Then the
   program calculates the shape factor and writes the final shape factor in
   a text file.}},
