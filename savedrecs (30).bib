@article{ WOS:000234187300025,
Author = {Narazaki, A and Kawaguchi, Y and Niino, H and Shojiya, M and Koyo, H and
   Tsunetomo, K},
Title = {{Formation of a TiO2 micronetwork on a UV-absorbing SiO2-based glass
   surface by excimer laser irradiation}},
Journal = {{CHEMISTRY OF MATERIALS}},
Year = {{2005}},
Volume = {{17}},
Number = {{26}},
Pages = {{6651-6655}},
Month = {{DEC 27}},
Abstract = {{We report on the preparation of TiO2 micronetworks on a UV-absorbing
   Na2O-TiO2-SiO2 glass surface by excimer laser irradiation, a technique
   we have called laser-induced Superficial phase separation. The TiO2
   micronetworks that were formed oil the etched glass surface were in the
   rutile crystalline phase and exhibited a photocatalytic capability. We
   were also able to form a range of TiO2 micronetworks in an array of
   20-mu m holes. This flexible formation of photocatalytic TiO2 networks
   on a glass surface without any heat treatments could enable a new route
   for developing novel microdevices, such as monolithic microfluidics with
   light-catalyzed reactions and miniaturized solar batteries.}},
