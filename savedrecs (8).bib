@article{ WOS:000595149800011,
Author = {Senjo, Hajime and Onozawa, Masahiro and Hidaka, Daisuke and Yokoyama,
   Shota and Yamamoto, Satoshi and Tsutsumi, Yutaka and Haseyama, Yoshihito
   and Nagashima, Takahiro and Mori, Akio and Ota, Shuichi and Sakai,
   Hajime and Ishihara, Toshimichi and Miyagishima, Takuto and Kakinoki,
   Yasutaka and Kurosawa, Mitsutoshi and Kobayashi, Hajime and Iwasaki,
   Hiroshi and Hashimoto, Daigo and Kondo, Takeshi and Teshima, Takanori},
Title = {{A novel nutritional index ``simplified CONUT{''} and the disease risk
   index independently stratify prognosis of elderly patients with acute
   myeloid leukemia}},
Journal = {{SCIENTIFIC REPORTS}},
Year = {{2020}},
Volume = {{10}},
Number = {{1}},
Month = {{NOV 10}},
Abstract = {{Elderly patients aged 65 or older with acute myeloid leukemia (AML) have
   poor prognosis. The risk stratification based on genetic alteration has
   been proposed in national comprehensive cancer network (NCCN) guideline
   but its efficacy was not well verified especially in real world elderly
   patients. The nutritional status assessment using controlling
   nutritional status (CONUT) score is a prognostic biomarker in elderly
   patients with solid tumors but was not examined in elderly AML patients.
   We performed prospective analysis of genetic alterations of 174 patients
   aged 65 or older with newly diagnosed AML treated without hematopoietic
   stem cell transplantation (HSCT) and developed simplified CONUT (sCONUT)
   score by eliminating total lymphocyte count from the items to adapt AML
   patients. In this cohort, both the NCCN 2017 risk group and sCONUT score
   successfully stratified the overall survival (OS) of the elderly
   patients. A multivariable analysis demonstrated that adverse group in
   NCCN 2017 and high sCONUT score were independently associated with poor
   2-year OS. Both risk stratification based on NCCN 2017 and sCONUT score
   predict prognosis in the elderly patients with newly diagnosed AML.}},
