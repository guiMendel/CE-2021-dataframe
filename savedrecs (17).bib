@article{ WOS:000364885400007,
Author = {Khalifeh, Hind and Brauer, Ruth and Toulmin, Hilary and Howard, Louise
   M.},
Title = {{Perinatal mental health: What every neonatologist should know}},
Journal = {{EARLY HUMAN DEVELOPMENT}},
Year = {{2015}},
Volume = {{91}},
Number = {{11}},
Pages = {{649-653}},
Month = {{NOV}},
Abstract = {{Perinatal mental disorders are common and can impact adversely both on
   maternal functioning and on foetal and neonatal outcomes. For the more
   severe disorders, such as schizophrenia, bipolar disorder and severe
   depression, medication may be needed during pregnancy and breastfeeding,
   and there is a growing but complex evidence based on the effects of
   psychotropic medication on the foetus and neonate. In addition, the
   neonatologist needs to be aware of the co-morbid problems that women
   with mental disorders are more likely to have as these may also impact
   on the neonate. Close liaison with family physicians and primary care
   where there are concerns about mental health is important to ensure
   maternal mental health is optimal for the mother and her infant. (C)
   2015 Elsevier Ireland Ltd. All rights reserved.}},
