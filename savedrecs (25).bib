@article{ WOS:000285406900003,
Author = {Taibi, Ludmia and Giton, Frank and Moutereau, Stephane and Fiet, Jean
   and Guechot, Jerome},
Title = {{Testosterone assays using non recommended methods lead to misdiagnosis
   in women}},
Journal = {{ANNALES DE BIOLOGIE CLINIQUE}},
Year = {{2010}},
Volume = {{68}},
Number = {{6}},
Pages = {{649-656}},
Month = {{NOV-DEC}},
Abstract = {{French and US endocrine societies recommend using GC-MS or RIA after
   purification (extraction + chromatography) to assess blood levels of
   testosterone in women. However, most of laboratories use automatized
   methods that have to be reserved to measure testosterone levels in men.
   The aim of this study was to show the consequences of analytical
   discrepancies of some immunological methods on the diagnostics values of
   testosterone levels assayed in women. Compared to GC-MS the correlations
   of the assayed levels varied (Spearman's rank correlation coefficients:
   0.935; 0.793; 0.841; 0.852 respectively for RIA Immunotech (TM) with
   extraction and chromatographic purification; Testosterone Access-DxI800
   (R); Testosterone Immulite 2000 (R); Testosterone II Cobas E601 (R)).
   The testosterone levels allowed an accurate conclusion in 95.2 \%; 75.8
   \%; 77.4 \%; 89.8 \% of patients, respectively. The agreement with GC-MS
   results was very good for RIA method (kappa=0,840), moderate for DxI800
   (R) method (kappa=0,414), moderate for Immulite (R) method
   (kappa=0,467), good for Cobas (R) method (kappa=0,667). Most of
   discordances are false hypertestosteronemia. The use of non recommended
   methods may leads to nosological errors (misclassification rates of 10
   to 25\% with automatized methods) that causes loss of chance in part of
   female patients.}},
