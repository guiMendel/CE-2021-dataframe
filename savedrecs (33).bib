@article{ WOS:000233886800061,
Author = {Bibet, P and Beaumont, B and Belo, JH and Delpech, L and Ekedahl, A and
   Granucci, G and Kazarian, F and Litaudon, X and Mailloux, J and Mirizzi,
   F and Pericoli, V and Prou, M and Rantamaki, K and Tuccillo, A},
Title = {{Toward a LHCD system for ITER}},
