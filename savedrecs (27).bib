@article{ WOS:000282855800004,
Author = {Kowalczyk, Lidia and Steinbacher, Roswitha and Doerfelt, Rene},
Title = {{Perioperative hypothermia - an underestimated complication}},
Journal = {{TIERAERZTLICHE UMSCHAU}},
Year = {{2010}},
Volume = {{65}},
Number = {{10}},
Pages = {{384+}},
Month = {{OCT}},
Abstract = {{Hypothermia is a common, often underestimated perianesthetic
   complication. It is especially common in small patients. Hypothermia has
   adverse effects on metabolism and many organ systems. Many drugs used in
   anesthesia, such as Acepromazin and Isoflurane, reduce body temperature
   by decreasing the temperature set point at the thermal control center in
   the brain and by vasodilatation. Long surgery time, low room temperature
   and cold operation tables as well as surgery of open body cavities lead
   to an additional decrease of the temperature of the patient. Warming of
   the patient can be done by passive and active methods such as a short
   surgery time, force air blankets, and warm body cavity lavage. These
   methods help to prevent hypothermia and to increase patient temperature.}},
