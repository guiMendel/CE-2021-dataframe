@article{ WOS:000166815100008,
Author = {Madrid, R and Boronat, M and Valverde, M and Lax, A},
Title = {{Evolution of macronutrients in the adjustment of nutrient solutions for
   hydroponically cultivated bell peppers (var. Orlando)}},
Journal = {{AGROCHIMICA}},
Year = {{2000}},
Volume = {{44}},
Number = {{5-6}},
Pages = {{240-249}},
Month = {{SEP-DEC}},
Abstract = {{The study of the evolution of pH, EC, and macronutrients N, P, K, Ca, Mg
   and S of the nutritive and drainage solutions is the objective of this
   paper. We have employed bell pepper (Cv. Orlando Fl)on perlite as
   substrate. It is carried out the comparative study between the nutritive
   and drainage (totals and suction probes) solutions during the
   cultivation cycle, Decreasing of pH in the nutritive solution is mainly
   a consequence of the HNO3 apportations and to the acid character of some
   apported ions of the same (H2PO4-, Mg2+ y NH4+). The EC reaches the
   higest values in the final stage for the drainage solution as a result
   to the growing accumulation of ions (NO3-, Ca2+ and Mg2+) in the
   substrate. We found an excess in N and Ca apportations, in relations
   with the plant uptake, giving this high accumulations of these nutrients
   in the drainage solutions. Phosphorus and potassium are incorporated in
   equilibrated quantities, and the K/Ca relation, near 1 could be
   interpreted as a loss of part of the incorporated Ca for its competition
   with K.}},
