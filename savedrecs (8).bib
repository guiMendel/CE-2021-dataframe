@article{ WOS:000587556700001,
Author = {El Kassar, Nadja},
Title = {{The Powers of Individual and Collective Intellectual Self-Trust in
   Dealing with Epistemic Injustice}},
Journal = {{SOCIAL EPISTEMOLOGY}},
Year = {{2021}},
Volume = {{35}},
Number = {{2, SI}},
Pages = {{197-209}},
Month = {{MAR 4}},
Abstract = {{The literature on epistemic injustice is increasingly turning to the
   question of countering epistemic injustice. But few authors note that
   the strategies against epistemic injustice are complemented by a more
   fundamental disposition, intellectual self-trust. Most of the time,
   intellectual self-trust is merely mentioned as an individual disposition
   that is harmed by epistemic injustice. In this article, I argue that
   intellectual self-trust as an optimistic disposition is a central tool
   for countering epistemic injustice. Both individual intellectual
   self-trust and collective intellectual self-trust, as well as groups and
   communities themselves are central in obstructing the effects of
   epistemic injustice and engaging in resistance against epistemic
   injustice. I start with an overview of the effects of epistemic
   injustice. After that, I spell out the powers of individual intellectual
   self-trust, then move from the role of communities for individual
   intellectual self-trust to collective intellectual self-trust, i.e.,
   intellectual self-trust of collectives. Finally, I outline the function
   of collective intellectual self-trust for obstructing and resisting
   epistemic injustice.}},
