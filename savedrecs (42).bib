@article{ WOS:000166120600011,
Author = {Annin, VK},
Title = {{Benthic foraminiferal assemblages in Pos'et Bay (Sea of Japan) and their
   habitats}},
Journal = {{OCEANOLOGY}},
Year = {{2000}},
Volume = {{40}},
Number = {{6}},
Pages = {{830-838}},
Month = {{NOV-DEC}},
Abstract = {{The taxonomic composition of the present-day benthic foraminifers in
   Pos'et Bay (Sea of Japan) was studied using 60 samples of the surface
   deposits. Fifty-one species of benthic foraminifers belonging to 32
   genera were identified, and the patterns of benthic foraminiferal
   distribution were analyzed depending on such abiotic factors as the
   roughness of the coastline, the degree of closure of the bay water
   areas, the type of the bottom sediments, as well as the depth,
   near-bottom water temperature, and the influence of the riverine runoff.
   The zones with high bioproductivity were recognized, and the species
   diversity was assessed. Four types and five subtypes of benthic
   foraminiferal assemblages corresponding to certain depths, bottom
   temperatures, deposit structures, and types of bay water area were
   recognized with respect to the prevailing abundance of the dominant
   species and the ratio between the subdominant and accessory species. The
   benthic foraminiferal assemblages from the upper littoral zone differ
   from those living in the bay water area.}},
