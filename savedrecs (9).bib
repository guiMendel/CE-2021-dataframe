@article{ WOS:000597149000025,
Author = {Anand, Sharath Kumar and Macki, Mohamed},
Title = {{What have we learned from C5 palsy - A short communication}},
Journal = {{JOURNAL OF CLINICAL NEUROSCIENCE}},
Year = {{2020}},
Volume = {{81}},
Pages = {{111-112}},
Month = {{NOV}},
Abstract = {{A study group on C5 palsy retrospectively reviewed 1001 cervical
   operations at their institutions in order to understand the incidence,
   prognosticators, pathogenesis, and outcome of C5 palsy after cervical
   operations. Three studies are summarized. C5 palsy was higher after
   posterior versus anterior operations. C4-C5 foraminotomy and age were
   the strongest predictors of C5 palsy after posterior surgeries and
   anterior cervical decompression-fusion, respectively. Among patients
   undergoing C4-C5 posterior laminoforaminotomy with instrumented fusion,
   cord shift on postoperative imaging was thought to be implicated in the
   pathogenesis of C5 palsy. Among affected patients, 81.4\% recovered.
   Median time to resolution of C5 palsy was between 6 months to 1 year.
   (C) 2020 Elsevier Ltd. All rights reserved.}},
