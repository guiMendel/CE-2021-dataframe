Title = {{Influence of the deposition parameters of graphene oxide nanofilms on
   the kinetic characteristics of the SAW humidity sensor}},
Journal = {{SENSORS AND ACTUATORS B-CHEMICAL}},
Year = {{2015}},
Volume = {{217}},
