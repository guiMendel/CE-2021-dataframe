@article{ WOS:000165347500008,
Author = {Zwartz, G and Drake, TE and Cromaz, M and Ward, D and Janzen, V and
   Galindo-Uribarri, A and Prevost, D and Waddington, J and Mullins, SM},
Title = {{Investigation of the use of an alpha+Xn reaction channel to enhance the
   population of superdeformed states in Hg-193 and Hg-195}},
Journal = {{JOURNAL OF PHYSICS G-NUCLEAR AND PARTICLE PHYSICS}},
Year = {{2000}},
Volume = {{26}},
Number = {{11}},
Pages = {{1723-1733}},
Month = {{NOV}},
Abstract = {{A study was made to determine whether the population of superdeformed
   states in Hg-193 and Hg-195 can be enhanced by using reactions in which
   alpha particles are emitted. The search utilized a W-184 (O-18, alpha
   Xn) reaction at 115 and 120MeV for the Hg-193 study and a W-186(O-18,
   alpha Xn) reaction at 105 and 110 MeV for the Hg-195 study. Two known
   superdeformed states of Hg-193 were observed. The intensities of the
   superdeformed states in Hg-193 populated by a reaction involving the
   emission of an alpha particle were found to be reduced by at least a
   factor of four relative to the intensities of these stares produced in
   reactions involving only the emission of neutrons and gamma -rays. No
   rotational bands built on superdeformed states in Hg-195 With transition
   intensities > 0.8\% of the total 195Hg yield were found. Evidence is
   shown that the energy of the alpha particle that is associated with
   superdeformed states may be lower than that of alpha particles
   associated with normally deformed states.}},
