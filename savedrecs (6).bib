@article{ WOS:000596289900001,
Author = {Schebesch, Karl-Michael and Doenitz, Christian and Haj, Amer and Hohne,
   Julius and Schmidt, Nils Ole},
Title = {{Application of the Endoscopic Micro-Inspection Tool QEVO (R) in the
   Surgical Treatment of Anterior Circulation Aneurysms-A Technical Note
   and Case Series}},
Journal = {{FRONTIERS IN SURGERY}},
Year = {{2020}},
Volume = {{7}},
Month = {{NOV 24}},
Abstract = {{Introduction: The application of neuro-endoscopes in cerebral aneurysm
   surgery may help to avoid unintended aneurysm remnants and the
   accidental clipping of perforating arteries and aid the detection of
   blood collecting in the subdural spaces. Here, we present our experience
   with the novel endoscopic micro-inspection tool QEVO (R) (Carl Zeiss
   Meditec, Germany) in aneurysm surgery.
   Materials and Equipment: In all patients the surgical microscope KINEVO
   (R) (Carl Zeiss Meditec, Germany) and the Microinspection tool QEVO (R)
   were applied.
   Methods: The case series comprises 22 unruptured cerebral aneurysms of
   the anterior circulation. All aneurysms were treated surgically. All
   patients routinely underwent computed tomography and digital subtraction
   angiography within 10 days after surgery.
   Results: No aneurysm remnants, cerebral ischemic deficits, or subdural
   hematomas were detected.
   Discussion: In this technical note, we discuss the benefits and
   limitations of the QEVO (R) tool and illustrate the major paradigms by
   means of intraoperative photographs.}},
