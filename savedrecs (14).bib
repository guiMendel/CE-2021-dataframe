@article{ WOS:000358929500003,
Author = {Wang, Guanyao and Hu, Zhiwei and Huang, Xingyi and Jiang, Pingkai},
Title = {{Enhanced Dielectric Properties and Low Percolation Threshold of Ternary
   PVDF/BT/rGO Nanocomposites}},
Journal = {{JOURNAL OF ADVANCED PHYSICS}},
Year = {{2015}},
Volume = {{4}},
Number = {{4}},
Pages = {{314-323}},
Month = {{DEC}},
Abstract = {{Novel ternary nanocomposites consisting of poly(vinylidene fluoride)
   (PVDF), surface-modified BaTiO3 (BT) nanoparticles, and reduced graphene
   oxide (rGO) were successfully prepared via a facile solution-processed
   method. The mutual electrostatic assemble of surface modified BT
   nanoparticles and rGO nanosheets paves the way for their homogeneous
   dispersion in the PVDF matrix, which was further confirmed by the SEM
   results. Similar with previously reported percolative systems, the
   obtained ternary PVDF/BT/rGO nanocomposites exhibit a typical
   percolation transition with the percolation threshold lied in the range
   from 0.6 wt\% to 0.8 wt\% of rGO filler. More importantly, the ternary
   nanocomposites not only show a high dielectric constant but also
   maintain rather low dielectric loss below the percolation threshold
   compared with binary PVDF/BT nanocomposites. For instance, a high
   dielectric constant (similar to 87) and relatively low dielectric loss
   (similar to 0.03) at 1 kHz could be achieved for our proposed
   PVDF/BT/rGO nanocomposites at 0.6 wt\% fraction of rGO filler. In
   addition, the dielectric constant of the proposed nanocomposites
   exhibits a temperature dependency over a wide frequency range. On the
   other hand, the fraction of rGO filler also has a remarkable effect on
   the dielectric properties of the nanocomposites. We expect that these
   proposed nanocomposites would be potential candidates for utilization in
   modern electronic devices.}},
