@article{ WOS:000369435700010,
Author = {Woelkerling, William J. and Harvey, Adele S. and de Reviers, Bruno},
Title = {{Jania pedunculata (Rhodophyta: Corallinaceae): Typification,
   nomenclature, and taxonomic status relative to J. crassa, J. verrucosa
   sensu Johansen \& Womersley, and J. ungulata}},
Journal = {{TAXON}},
Year = {{2015}},
Volume = {{64}},
Number = {{6}},
Pages = {{1280-1293}},
Month = {{DEC 31}},
Abstract = {{New nomenclatural and morphological-anatomical data support the
   following conclusions: (1) Jania pedunculata (Rhodophyta: Corallinaceae)
   is a name underpinned by a newly designated lectotype and epitype whose
   features are consistent with placement in the genus Jania; (2) J.
   pedunculata and J. crassa should not be treated as heterotypic synonyms;
   and (3) Australian specimens previously referred to J. verrucosa, a name
   of uncertain application, belong to J. crassa and not to J. pedunculata.
   Detailed accounts of the lectotype and epitype of J. pedunculata are
