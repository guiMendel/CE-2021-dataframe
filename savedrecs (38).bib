@article{ WOS:000232229900056,
Author = {Titov, AI and Kapfer, B and Date, S and Ohashi, Y},
Title = {{Coherent Theta(+) and Lambda(1520) photoproduction off the deuteron}},
Journal = {{PHYSICAL REVIEW C}},
Year = {{2005}},
Volume = {{72}},
Number = {{3}},
Month = {{SEP}},
Abstract = {{We analyze coherent Theta(+)Lambda(1520) photoproduction in the gamma d
   interaction near the threshold. We demonstrate that the effect of the
   coherent production becomes manifest in a comparison of the nK(+)
   invariant mass distribution when the pK(-) system is on the Lambda(1520)
   mass. Our model calculations indicate a sizable contribution of resonant
   and nonresonant background processes in the gamma d -> npK(+)K(-)
   reaction that generally exceeds the contribution of the coherent
   resonance channel. However, we find that coherent Theta(+)Lambda(1520)
   photoproduction is enhanced relative to the background processes in the
   forward hemisphere of pK(-) pair photoproduction. Moreover, the
   coherence effect does not depend on the Theta(+) photoproduction
   amplitude and is defined by the probabilities of Lambda(1520)
   photoproduction and the Theta(+)-> NK transition. Therefore, this effect
   may be used as an independent method for studying the production
   mechanism and properties of Theta(+).}},
