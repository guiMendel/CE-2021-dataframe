@article{ WOS:000362843300024,
Author = {Hein, Alexander and Lambrechts, Diether and von Minckwitz, Gunter and
   Haeberle, Lothar and Eidtmann, Holger and Tesch, Hans and Untch, Michael
   and Hilfrich, Joern and Schem, Christian and Rezai, Mahdi and Gerber,
