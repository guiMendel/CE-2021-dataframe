@article{ WOS:A1995UF13000008,
Author = {Zignego, AL and Ferri, C and Monti, M and LaCivita, L and Giannini, C
   and Careccia, G and Giannelli, F and Pasero, G and Bombardieri, S and
   Gentilini, P},
Title = {{Hepatitis C virus as a lymphotropic agent: Evidence and pathogenetic
   implications}},
