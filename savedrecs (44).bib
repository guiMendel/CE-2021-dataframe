@article{ WOS:000165590800005,
Author = {Fan, YC and Jiang, LL and Tang, XG and Zhou, QF and Zhang, JX},
Title = {{Structure phase transition of nanocrystalline barium titanate doped with
   lead prepared by sol-gel methods}},
Journal = {{JOURNAL OF INORGANIC MATERIALS}},
Year = {{2000}},
Volume = {{15}},
Number = {{5}},
Pages = {{797-801}},
Month = {{OCT}},
Abstract = {{Nanocrystalline (Ba,Pb)TiO3 (5 mol\% Pb) with various grain sizes were
   prepared by a sol-gel process. The gain size, structure and phase
   properties of nanocrystalline BPT mere studied by TEM, XRD and DSC. The
   results show that the structure phase transition is from ferroelectric
   tetragonal to paraelectric cubic phase and becomes more diffusive with
   decreasing particle size. On the other hand, the ferroelectric
   orthorhombic to ferroelectric tetragonal phase transition disappears
   with increasing particle size, it means that the formation of the
   unwanted orthorhombic phase of BaTiO3 is inhibited.}},
