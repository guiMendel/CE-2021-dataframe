@article{ WOS:A1995RN68200002,
Author = {ICHI, T and HIGASHIMURA, Y and KATAYAMA, T and KODA, T and TADA, M},
Title = {{DETERMINATION OF A FOOD COLORANT, CARDENIA YELLOW PIGMENT, IN PROCESSED
   FOODS WITH A HIGH-PERFORMANCE LIQUID CHROMATOGRAPH PHOTODIODE-ARRAY
   DETECTOR MASS-SPECTROMETER SYSTEM}},
