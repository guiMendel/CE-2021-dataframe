@article{ WOS:000088294300009,
Author = {Berna, JL and Bengoechea, C and Moreno, A and Rounds, RS},
Title = {{Evaluation of the rheological properties of sulfonic acids and sodium
   sulfonates}},
Journal = {{JOURNAL OF SURFACTANTS AND DETERGENTS}},
Year = {{2000}},
Volume = {{3}},
Number = {{3}},
Pages = {{353-359}},
Month = {{JUL}},
Abstract = {{Sulfonic acids of linear alkylbenzene (LAB) are converted into the
   corresponding sodium salts to produce the most widely used anionic
   surfactant worldwide, linear alkylbenzene sulfonate (LAS). Used in many
   industrial applications and consumer products, the physical and
   mechanical properties of the sulfonates are strongly dependent on the
   LAB manufacturing process. Until recently, commercial alkylation of
   benzene has employed aluminum chloride or hydrogen fluoride catalysts,
   but a new fixed-bed alkylation process (DETAL) has been developed with
   improved 2-phenyl isomer selectivity and low tetralin concentration. In
   order to better understand the rheology of LAS in aqueous media, a
   comprehensive comparative evaluation of sulfonic acids and sodium
   sulfonates of the three LAB process derivatives has been done using
   dynamic mechanical rheometry, steady shear viscometry, and X-ray
   diffraction for phase identification. LAB sulfonic acids are Newtonian
   fluids in the temperature range of 20-60 degrees C. The neat AlCl3, HF,
   and DETAL sulfonic acids are Newtonian fluids within the temperature
   range of 20-60 degrees C. At 30 wt\%, all three sulfonates are Newtonian
   at 20-60 degrees C, and the 40 wt\% AlCl3 sodium sulfonate remains in
   the Newtonian regime within this temperature range. Lamellar liquid
   crystalline phases have been identified for the sulfonates in the
   concentration range of 40-60 wt\% in water at 20-60 degrees C, and a
   hexagonal lattice phase also has been identified for DETAL sodium
   alkylbenzene sulfonate at 40 wt\%, 60 degrees C. The presence of
   anisotropic phases results in non-Newtonian pseudoplastic behavior with
   time-dependent viscosity functions.}},
