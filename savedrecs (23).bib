@article{ WOS:000283567800020,
Author = {Cao, Zhikun and Gu, Bo and Mills, Graham and Han, Hua},
Title = {{A novel strategy for predicting the performance of open vertical
   refrigerated display cabinets based on the MTF model and ASVM algorithm}},
Journal = {{INTERNATIONAL JOURNAL OF REFRIGERATION-REVUE INTERNATIONALE DU FROID}},
Year = {{2010}},
Volume = {{33}},
Number = {{7}},
Pages = {{1413-1424}},
Month = {{NOV}},
Abstract = {{This paper presents a novel strategy for predicting the performance of
   open vertical refrigerated display cabinets which is based on a modified
   two-fluid (MTF) model and an adaptive support vector machine (ASVM)
   algorithm. A MTF model (physical model) was built for open vertical
   refrigerated display cabinets, and then an ASVM algorithm (machine
   learning algorithm) was built. To verify the quantity of air leakage
   from the cabinet during operation, an important performance factor of
   display cabinets, an MTF model was built. After the training and
   validation data sets were constructed from the output of the MTF model,
   the problem was solved using an ASVM algorithm. The defrosting water
   quantity and total energy consumption / total display area (TEC/TDA),
   achieved from the experiments by using the predicted combination of the
   controlled parameters, were found to be reduced by 39.2\% and 19.3\%,
   respectively, from the experimental results of the original display
   cabinet. (c) 2010 Elsevier Ltd and IIR. All rights reserved.}},
