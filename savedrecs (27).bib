@article{ WOS:000283108900012,
Author = {Kang, Dong Shik and Lee, Sang Chil and Kim, Suck Whan},
Title = {{Magnetophonon Resonance by Quantum Size Effects in n-Ge-based and
   n-GaAs-based Quantum-dot Superlattices}},
Journal = {{JOURNAL OF THE KOREAN PHYSICAL SOCIETY}},
Year = {{2010}},
Volume = {{57}},
Number = {{4}},
Pages = {{760-767}},
Month = {{OCT}},
Abstract = {{Magnetophonon resonance (MPR) effects in quantum-dot superlattices
   (QDSL's) of n-Ge nonpolar and n-GaAs polar semiconductors with an
   isotropic parabolic potential well for optical phonons are investigated.
   The MPR conditions as a function of the magnetic field strength for the
   various confinement frequencies in a parabolic potential well are also
   studied to determine the quantum size effects. Furthermore, the
   qualitative features of the MPR effects, such as the appearance of a
   plateau between neighboring MPR peaks and the change of the MPR. peak
   amplitude with the confinement frequency, the temperature, and the
   miniband width, are discussed in detail.}},
