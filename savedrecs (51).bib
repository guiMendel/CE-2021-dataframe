@article{ WOS:A1995TT42500009,
Author = {Tikhonravova, PI},
Title = {{Thermophysical properties of the gray forest soil and their changes in
   the course of cultivation}},
Journal = {{EURASIAN SOIL SCIENCE}},
Year = {{1995}},
Volume = {{27}},
Number = {{12}},
Pages = {{79-88}},
Month = {{DEC}},
Abstract = {{Thermophysical parameters (heat capacity, heat and temperature
   conductivity) have been under study in gray forest soils with varying
   organic matter content. Conditions of heat transfer and heat
   accumulation were found to correlate with humous content in soil, with
