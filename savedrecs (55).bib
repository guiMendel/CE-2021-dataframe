@article{ WOS:A1995RU16800032,
Author = {CALLY, PS},
Title = {{EFFECTS OF WEAK-TO-MODERATE VERTICAL MAGNETIC-FIELDS ON SOLAR F-MODES
   AND P-MODES}},
Journal = {{ASTROPHYSICAL JOURNAL}},
Year = {{1995}},
Volume = {{451}},
Number = {{1, 1}},
Pages = {{372-380}},
Month = {{SEP 20}},
Abstract = {{Observations indicate that solar p-modes encountering regions of
   enhanced magnetic field such as active region plage are partially
   absorbed. We adopt a simple vertical held model and investigate the
   possiblity that the responsible mechanism is slow mode leakage, which
   has been at least partially successful in explaining absorption by
   sunspots. It is found that there are two frequency ranges with very
   different behavior: (1) below the chromospheric acoustic cutoff
   frequency, and (2) above it. In the first case, which includes the 5
   minute band, the f- and p(1)-modes would be expected to suffer
   substantial absorption, but p-modes of higher radial order would not be
   damped sufficiently to account for observations. On the other hand, in
   the higher frequency range (greater than or equal to 5 mHz), damping
   lengths off and p(1)-p(3) modes are smaller than or comparable to
   typical plage sizes, indicating that substantial absorption should
   occur.}},
