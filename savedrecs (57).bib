@article{ WOS:A1995RU40700202,
Author = {IKEDA, S and YAMADA, Y},
Title = {{PHASE-TRANSITION IN HYDROGEN-BONDED FERROELECTRIC COMPOUNDS - QUANTUM
   FLUCTUATIONS VERSUS THERMAL FLUCTUATIONS}},
Journal = {{PHYSICA B}},
