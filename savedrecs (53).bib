@article{ WOS:A1995UE60300015,
Author = {Porse, BT and RodriguezFonseca, C and Leviev, I and Garrett, RA},
Title = {{Antibiotic inhibition of the movement of tRNA substrates through a
   peptidyl transferase cavity}},
Journal = {{BIOCHEMISTRY AND CELL BIOLOGY-BIOCHIMIE ET BIOLOGIE CELLULAIRE}},
Year = {{1995}},
Volume = {{73}},
Number = {{11-12}},
Pages = {{877-885}},
Month = {{NOV-DEC}},
Note = {{International Conference on the Structure and Function of the Ribosome,
   VICTORIA, CANADA, MAY 20-25, 1995}},
Organization = {{Int Union Biochem \& Molec Biol; Nat Sci \& Engn Res Council Canada; Med
   Res Council Canada; Natl Sci Fdn; NIH, US; Int Fed Cell Biol; Univ
   Victoria}},
Abstract = {{The present review attempts to deal with movement of tRNA substrates
   through the peptidyl transferase centre on the large ribosomal subunit
   and to explain how this movement is interrupted by antibiotics. It
   builds on the concept of hybrid tRNA states forming on ribosomes and on
   the observed movement of the 5' end of P-site-bound tRNA relative to the
   ribosome that occurs on peptide bond formation. The 3' ends of the tRNAs
   enter, and move through, a catalytic cavity where antibiotics are
   considered to act by at least three primary mechanisms: (i) they
   interfere with the entry of the aminoacyl moiety into the catalytic
   cavity before peptide bond formation; (ii) they inhibit movement of the
   nascent peptide along the peptide channel, a process that may generally
   involve destabilization of the peptidyl tRNA, and (iii) they prevent
   movement of the newly deacylated tRNA between the PIP and hybrid P/E
   sites on peptide bond formation.}},
