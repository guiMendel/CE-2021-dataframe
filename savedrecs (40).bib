@article{ WOS:000166124800006,
Author = {Stronski, S and Michaud, PA},
Title = {{Prevention as on integral part of adolescent health care}},
Journal = {{MEDECINE ET HYGIENE}},
Year = {{2000}},
Volume = {{58}},
Number = {{2327}},
Pages = {{2577+}},
Month = {{DEC 20}},
Abstract = {{During adolescence, health care and health prevention activities are
   closely linked. In his office, every physician can have an impact on his
   young patients' health in different ways: screening for unmet health
   needs, increasing this adolescent's compliance in providing him with
   adequate information, discussing the consultant's behaviour and
   lifestyles, etc. Routine examinations (like before sports camp or entry
   into apprenticeship) represent excellent opportunities for such
   preventive activities. In every day practice, physicians should evaluate
   and make use of their patients' resources; they should not underestimate
   the impact of the adolescents' environment (climate in the family,
   social environment, working conditions,...). Training initiatives are
   badly needed in this underestimated field.}},
