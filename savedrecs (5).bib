@article{ WOS:000590984800004,
Author = {Sun, Shangqing and Ge, Liangquan and Li, Yuandong and Hu, Chuanhao},
Title = {{Study on the gamma plus X, gamma plus beta, gamma plus alpha Coincidence
   Summing Effects of the Intrinsic Background Instrument Spectrum of a
   LaBr3(Ce) Scintillation Counter}},
Journal = {{JOURNAL OF THE KOREAN PHYSICAL SOCIETY}},
Year = {{2020}},
Volume = {{77}},
Number = {{12}},
Pages = {{1091-1099}},
Month = {{DEC}},
Abstract = {{The energy resolution of a LaBr3(Ce) scintillation counter can reach
   2.7\% (662 keV) at room temperature. As a radiation measuring device, it
   has remarkably good characteristics. However, the LaBr3(Ce) crystal has
   its own intrinsic radioactivity background, which mainly comes from
   La-138 and from Ac-227 and its daughters. La-138 can emit beta, gamma
   and X-rays through beta and gamma decay; Ac-227 and its daughters can
   emit alpha, beta and gamma-rays through a, beta and gamma decay. alpha,
   beta and X-ray energy are characteristic while beta-ray energy is
   continuous. The energy of alpha, beta and X-rays is mainly deposited in
   the crystal unless the alpha, beta and X-rays at the edge of the crystal
   can escape from the crystal. Therefore, the alpha, beta, gamma and
   X-rays generated by the intrinsic radioactivity of the crystal are
   superimposed on the instrument spectrum, which makes the instrument
   spectrum more complicated and produces the coincidence summing effects
   of gamma + X, gamma + beta and gamma + alpha.In this research, the
   GEANT4.9.5 software package was used to simulate the spectra of alpha,
   beta, gamma and X-rays. On the basis of a combination of fitting
   decomposition and reconstruction, the simulation spectrum of the
   LaBr3(Ce) self-radioactive background was obtained accurately and was
   verified by using physical experiments.}},
