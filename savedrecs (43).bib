@article{ WOS:000165327100025,
Author = {Widlicka, DW and Wong, EH and Weisman, GR and Lam, KC and Sommer, RD and
   Incarvito, CD and Rheingold, AL},
Title = {{A preconstrained tricyclic biimidazoline ligand adaptable to diverse
   coordination modes}},
Journal = {{INORGANIC CHEMISTRY COMMUNICATIONS}},
Year = {{2000}},
Volume = {{3}},
Number = {{11}},
Pages = {{648-652}},
Month = {{NOV}},
Abstract = {{The tricyclic biimidazoline ligand L possesses a preconstrained
   synperiplanar bisamidine N = C-C = N and has been shown to have a
   versatile coordination chemistry. In its manganese(II) tris-chelate
   complex {[}MnL3](ClO4)(2), a distorted trigonal prismatic coordination
   geometry is adopted. By contrast, a square-planar copper(II) complex,
   {[}CuL3](ClO4)(2) features one chelating as well as two monodentate L's,
