@article{ WOS:000286625000004,
Author = {El-Nagar, Kh and Amer, M. and Mekawy, M. and Mahmoud, Essam El Din},
Title = {{Development of Viscosity Transfer Standards from Chitosan/gelatin
   Mixtures}},
Journal = {{MAPAN-JOURNAL OF METROLOGY SOCIETY OF INDIA}},
Year = {{2010}},
Volume = {{25}},
Number = {{4}},
Pages = {{251-257}},
Month = {{DEC}},
Abstract = {{This paper reports the degree of stability of the viscosity of
   Chitosan/gelatin mixtures having viscosity from 1 mm(2)/s to 161
   mm(2)/s. The kinematic viscosity of the five liquids, was determined at
   20 degrees C and 25 degrees C. To test the stability of the viscosity of
   these oils, we have repeated the measurements of the kinematic and
   dynamic viscosity at 20 degrees C and 25 degrees C, several times for a
   period of two years. The measurements were carried out using standard
   viscometers and water bath, where temperature stability was about +/-
   0.02 degrees C. The variations of the viscosity for the oils under test
   during two years interval were within the ranges of uncertainty of
   measurements. The results obtained show that these oils can be used for
   a period of two years with stable physical and chemical composition and
   in turn their measured viscosity values. These recommended viscosity
   standards will be of great importance for many applications e. g.,
   thickeners, food, textile, sizings, petrol and medical industries
   because they verify a wide calibration ranges of viscosity values for
   different industries.}},
