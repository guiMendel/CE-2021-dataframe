@article{ WOS:000368827600003,
Author = {Zhong, Suni and Chen, Yanzhong and Song, Fenyun and Tian, Yong and Wang,
   Xiufang},
Title = {{Novel Synthesis of Magnetic Mesoporous Carbon Spheres and Release of
   Poorly Water-Soluble Indomethacin}},
Journal = {{SCIENCE OF ADVANCED MATERIALS}},
Year = {{2015}},
Volume = {{7}},
Number = {{12}},
Pages = {{2508-2514}},
Month = {{DEC}},
Abstract = {{The synthesis of magnetic mesoporous carbon spheres (FeMCs) with
   sucrose, silica, and hydrated iron nitrite Fe(NO3)(3)center dot 9H(2)O
   under neutral conditions was achieved by the ultrasonication method.
   Indomethacin (INC) was selected as the model drug. FeMCs before and
   after INC-loading FeMCs were characterized by scanning electron
   microscopy (SEM), wide-angle X-ray diffraction (WXRD), nitrogen sorption
   isotherms, and differential scanning calorimetry (DSC). Release tests
   were conducted to confirm improvements in drug release kinetics. The
   experimental results showed that the mesoporous carbon spheres exhibited
   BET surface areas of 615-876 m(2)/g and pore volumes of 1.36-2.25
   cm(3)/g. INC showed an amorphous state after it was incorporated into
   the mesoporous channels of the FeMCs. Release kinetics revealed that the
   release rate of INC from FeMCs was significantly higher than that of
   pure INC. This finding may be attributed to the reduced crystallinity of
   INC in the composites. FeMCs could be exploited as an alternative
   delivery system for poorly water-soluble indomethacin. The crystallinity
   of the loaded drug and the drug release kinetics could be well
   controlled by FeMCs developed in this method.}},
