@article{ WOS:000233808600008,
Author = {Webb, AA and Cullen, CL and Rose, P and Eisenbart, D and Gabor, L and
   Martinson, S},
Title = {{Intracranial meningioma causing internal ophthalmoparesis in a dog}},
Journal = {{VETERINARY OPHTHALMOLOGY}},
Year = {{2005}},
Volume = {{8}},
Number = {{6}},
Pages = {{421-425}},
Month = {{NOV-DEC}},
Abstract = {{A 10-year-old, spayed female, Irish water spaniel was presented with a
   2-week history of anisocoria characterized by mydriasis of the right eye
   compared to the left eye in ambient light. Ophthalmic and neurological
   examinations, combined with pharmacological testing, identified a
   disease process affecting the right parasympathetic nucleus of cranial
   nerve 3 (CN III) and/or the parasympathetic component of CN III.
   Magnetic resonance imaging (MRI) identified a mass involving the right
   midbrain and extending caudally to the rostral border of the medulla
   oblongata. The dog became comatose within 12 h following MRI and was
   euthanized. Histopathology identified the intracranial mass as a
   meningioma.}},
