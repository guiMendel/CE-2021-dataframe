@article{ WOS:000362979100002,
Author = {Kuang, Lulin and Lan, Yu and Zhao, Huaisong and Feng, Shiping},
Title = {{High-energy magnetic excitations in cuprate superconductors}},
Journal = {{INTERNATIONAL JOURNAL OF MODERN PHYSICS B}},
Year = {{2015}},
Volume = {{29}},
Number = {{25-26, SI}},
Month = {{OCT 20}},
Note = {{10th International Conference on New Theories, Discoveries and
   Applications of Superconductivity New3SC, Chongqing Sheraton, PEOPLES R
   CHINA, OCT 26-29, 2014}},
Abstract = {{In this paper, the high-energy magnetic excitations in cuprate
   superconductors are studied based on the kinetic energy driven
   superconducting mechanism. The spin self-energy is evaluated explicitly
   in terms of the collective charge carrier modes in the particle-hole and
   particle-particle channels, and employed to calculate the dynamical spin
   structure factor. Our results show the existence of damped but
   well-defined dispersive spin excitations in the whole doping phase
   diagram. In particular, the charge carrier doping has a more modest
   effect on the high-energy spin excitations, and then the high-energy
   magnetic excitations retain roughly constant energy as a function of
   doping, with spectral weights and dispersion relations comparable to
   those found in the parent compound.}},
