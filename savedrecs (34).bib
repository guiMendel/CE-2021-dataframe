@article{ WOS:000232937500027,
Author = {Sieber, B},
Title = {{Degradation of gallium nitride quantum dots under 10 keV electron-beam
   injection at low and high excitation densities}},
Journal = {{JOURNAL OF APPLIED PHYSICS}},
Year = {{2005}},
Volume = {{98}},
Number = {{8}},
Month = {{OCT 15}},
Abstract = {{The time evolution of gallium nitride quantum dots luminescence has been
   recorded in situ in a scanning electron microscope. Electrons of 10 keV
   were injected in a stationary spot in a scanning electron microscope
   during 30 min. Two different regimes were evidenced depending on the
   beam current density. At low density, the cathodoluminescence (CL)
   intensity decreased rapidly and a dark spot appeared. At high beam
   density, the CL intensity decrease was slower, and a dark ring centered
   on the beam injection location was formed. All the results are
   coherently interpreted by an enhanced diffusion of nonradiative defects
   promoted by nonradiative recombinations.}},
