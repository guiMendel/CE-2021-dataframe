Author = {Bauer, Judith S. and Ghequire, Maarten G. K. and Nett, Markus and
   Josten, Michaele and Sahl, Hans-Georg and De Mot, Rene and Gross, Harald},
Title = {{Biosynthetic Origin of the Antibiotic Pseudopyronines A and B in
   Pseudomonas putida BW11M1}},
Journal = {{CHEMBIOCHEM}},
