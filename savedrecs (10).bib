@article{ WOS:000364257300058,
Author = {Zhang, Wencong and Wang, Mengmeng and Chen, Wenzhen and Feng, Yangju and
   Yu, Yang},
Title = {{Evolution of inhomogeneous reinforced structure in TiBw/Ti-6AL-4V
   composite prepared by pre-sintering and canned beta extrusion}},
Journal = {{MATERIALS \& DESIGN}},
Year = {{2015}},
Volume = {{88}},
Pages = {{471-477}},
Month = {{DEC 25}},
Abstract = {{2.5 vol.\% TiBwfri-6Al-4V composite with a discontinuous columnar
   reinforced structure was successfully fabricated by pre-sintering and
   subsequent canned beta extrusion process. Detail investigation on
   microstructural evolution revealed that intermediate Ti3B4 rather than
   TiB was substantially synthesized on the surface of coarse Ti64 powders
   due to insufficient reaction during pre-sintering, but the growth of TiB
   whiskers particularly along c-axis was significantly accelerated by
   subsequent canned beta extrusion, mainly resulting from the introduced
   high pressure and severe plastic deformation, while highly preferred
   orientation of TiB whiskers along extrusion direction was gradually
   developed. As a result, a discontinuous columnar reinforced structure in
   TiBwfri64 composite was achieved and led to superior mechanical
   properties of similar to 1450 MPa in ultimate tensile strength and
   similar to 8.2\% in fracture elongation at ambient temperature for the
   as-extruded composite. (C) 2015 Elsevier Ltd. All rights reserved.}},
