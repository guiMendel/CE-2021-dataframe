@article{ WOS:000575065100014,
Author = {Cardells-Peris, Jesus and Gonzalvez, Moises and Ortega-Porcel, Joaquin
   and Ruiz de Ybanez, Maria Rocio and Carmen Martinez-Herrero, Maria and
   Magdalena Garijo-Toledo, Maria},
Title = {{Parasitofauna survey of song thrushes (Turdus philomelos) from the
   eastern part of Spain}},
Journal = {{PARASITOLOGY INTERNATIONAL}},
Year = {{2020}},
Volume = {{79}},
Month = {{DEC}},
Abstract = {{Thrushes (Turdus spp.) are migratory passerine birds found in northern
   Europe during the summer months and in southern Europe and north of
   Africa during the winter. They constitute an important small game bird
   group very appreciated by Spanish hunters. Between October 2013 and
   February 2014, 90 adult song thrushes were collected for their exam.
   After necropsies, three species of helminths were macroscopically
   recovered from 15 birds (16.7\%): Morishitium sp. (16.7\%),
   Splendidofilaria mavis (6.7\%) and Dilepis undula (7.8\%). One of them
   showed an adult cestode in the lung. Moreover, 12 of the positive
   thrushes (80\%) harboured microfilaria in pulmonary blood vessels and
   three of them (20\%) were infected by Sarcocystis sp. on skeletal
   musculature. All parasitized birds showed lesions, ranging from mild to
   moderate airsacculitis, bronchitis and coelomitis associated to
   Morishitium sp. infection. To the authors' knowledge, this is the first
   description of Sarcocystis spp. in song thrushes from Spain. Our results
   reveal the need for further studies to evaluate the epidemiological role
   of song thrushes as spreaders of parasites during their annual
   migration.}},
