@article{ WOS:A1995TR61000002,
Author = {Moloney, AP and OKiely, P},
Title = {{Growth, digestibility and nitrogen retention in finishing continental
   steers offered concentrates ad libitum}},
Journal = {{IRISH JOURNAL OF AGRICULTURAL AND FOOD RESEARCH}},
Year = {{1995}},
Volume = {{34}},
Number = {{2}},
Pages = {{115-121}},
Month = {{DEC}},
Abstract = {{Two concentrate rations based on a mixture of barley and soyabean (BS)
   or a mixture of industrial by-products (BP) (distillers grains,
   fishmeal, maize gluten, citrus pulp, molasses, rolled wheat and tallow)
   were formulated, using published information and chemical analysis, to
   be isonitrogenous and isoenergetic on a dry matter (DM) basis. When
   offered ad libitum, together with 1.2 kg grass-silage DM per animal, to
   finishing Simmental-x-Friesian steers (mean liveweight 596, s.d. 40.4,
   kg; 10/treatment), neither group concentrate DM intake (10.3 and 10.2
   kg/day, for BS- and BP-based rations, respectively), daily bodyweight
   gain (1.25 and 1.12, s.e. 0.057, kg for BS- and BP-based rations,
   respectively) nor daily carcass gain (827 and 716, s.e. 50.8, g for BS-
   and BP-based rations, respectively) differed significantly. Based on the
   in vivo digestible organic matter concentration measured in growing
   steers (749 and 658, s.e. 5.3, g/kg DM for BS- and BP-based rations,
   respectively), less metabolisable energy was supplied by the BP-based
   ration. Both absolute nitrogen retention (41 v. 58, s.e. 5.1, g/day) and
   nitrogen retention as a proportion of absorbed nitrogen (274 v. 367,
   s.e. 28.0, g/kg) were higher (P<0.07) for the BP-based ration. It is
   concluded that cattle offered the BP-based ration had a greater
   efficiency of utilisation of dietary nitrogen despite a lower intake of
   energy than those offered the BS-based ration.}},
