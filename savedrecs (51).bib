@article{ WOS:A1995TQ95800001,
Author = {Wolfert, HP},
Title = {{Use of the catena principle in geomorphological impact assessment: A
   functional approach}},
Journal = {{ZEITSCHRIFT FUR GEOMORPHOLOGIE}},
Year = {{1995}},
Volume = {{39}},
Number = {{4}},
Pages = {{417-431}},
Month = {{DEC}},
Abstract = {{The assessment of landscape qualities is an essential theme in applied
   geomorphological research incorporated in environmental impact
   assessments. An integral method for geomorphological impact assessment
   is presented, based on a functional approach. Five groups of landform
   functions can be distinguished in the Netherlands: orientation
   functions, information functions, ordering functions, regulation
   functions and carriage functions. The catena principle is used to select
   relevant criteria to assess landform functions. Evaluation criteria
   chosen for the five groups of functions are: complexity and visibility;
   representativity, visibility, rarity and condition; geomorphological
   position, topographical position and complexity; geographical position
   and relief; and relief, respectively. The method was applied in an
   environmental impact assessment of sand and gravel extraction sites in a
   fluvial lowland area in the Netherlands. A 1:50000 analytical
   geomorphological map was used for the assessment. Results helped to
   differentiate between the various alternatives for extraction.}},
