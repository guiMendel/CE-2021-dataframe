@article{ WOS:A1995TG12600037,
Author = {STEPANOW, S},
Title = {{EXTENSION OF THE THEORY OF MICROPHASE SEPARATION IN BLOCK-COPOLYMER
   MELTS BEYOND THE RANDOM-PHASE-APPROXIMATION}},
Journal = {{MACROMOLECULES}},
Year = {{1995}},
Volume = {{28}},
Number = {{24}},
Pages = {{8233-8241}},
Month = {{NOV 20}},
Abstract = {{By using the diagram technique method, we introduce the collective
   variables in the theory of copolymer melts without appealing to the
   random phase approximation. The main result of this work is that the
   fluctuations in the collective structure factor directly couple to the
   individual structure factors of the block copolymer chain entering the
   Leibler expression of the collective structure factor. The expansion
   parameter, which controls the fluctuations, is found to be proportional
   to the Flory-Huggins chi parameter. We apply our method to compute the
   collective structure factor of a diblock copolymer melt above the
   microphase separation (MST) in a self-consistent way. Below the MST, we
