@article{ WOS:000284275600013,
Author = {Song, Xu Chun and Yang, E. and Liu, Gang and Zhang, Yong and Liu, Zhi
   Sheng and Chen, Hai Fang and Wang, Ying},
Title = {{Preparation and photocatalytic activity of Mo-doped WO3 nanowires}},
Journal = {{JOURNAL OF NANOPARTICLE RESEARCH}},
Year = {{2010}},
Volume = {{12}},
Number = {{8}},
Pages = {{2813-2819}},
Month = {{OCT}},
Abstract = {{Mo-doped WO3 nanowires were fabricated by a hydrothermal method in the
   presence of K2SO4. The physical properties of prepared nanowires were
   characterized by X-ray diffraction (XRD), energy-dispersive X-ray
   analysis (EDS), scanning electron microscopy (SEM), and transmission
   electron microscopy (TEM). The results show that the obtained products
   are nanowires with diameters ranging between 10 and 20 nm, and lengths
   of about 600 nm. Its photoactivity was evaluated through the
   photodegradation of methylene blue (MB) in aqueous solution. Effects of
