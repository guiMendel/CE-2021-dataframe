@article{ WOS:A1995TL80800033,
Author = {Goodman, FO and Nakamura, H},
Title = {{On the multiple peaks in charge-transfer probability versus laser
   frequency in the theory of laser assisted surface ion neutralization}},
Journal = {{PROGRESS IN SURFACE SCIENCE}},
Year = {{1995}},
Volume = {{50}},
Number = {{1-4}},
Pages = {{389-405}},
Month = {{SEP-DEC}},
Note = {{15th Biennial Canadian Conference on Surface Science (Surface Canada
   95), UNIV WATERLOO, WATERLOO, CANADA, MAY 25-27, 1995}},
Organization = {{Canadian Soc Chem, Div Surface Sci; Canadian Assoc Physicists}},
Abstract = {{Some aspects of the theory of LASIN (laser assisted surface ion
   neutralization) are discussed, with emphasis on the physical origins of
   the so-called double-peak structures found in some calculations of the
   charge-transfer (neutralization) probability, P, as a function of the
   laser frequency eta. These two peaks have been called the first peak at
   eta approximate to eta(m) = epsilon(0) - epsilon(m) (in a.u.), where
   epsilon(0) (epsilon(m)) is the electronic energy level of the ion/atom
   (middle of the solid's valence band) and the second peak, a much larger
   peak at eta approximate to 1.3 eta(m), respectively.
   We show that these double-peak structures are all special cases of
   multiple-peak structures which result from quantum interference effects,
   and that, in fact, the second peak is to be regarded as the main
   resonance peak. This result is interesting in itself, because it is the
   first peak which has heretofore been considered the main resonance peak,
   To simplify the discussion, a two-level model is adapted, which
   represents the solid valence band by a single level at epsilon(m).
   Clarification of the physical reason for the multi pie peaks is based on
   the semiclassical theory of nonadiabatic transitions, in which the peaks
   are due to the phase difference between the two adiabatic paths that
   arise from the diagonalization of the two-level hamiltonian.
   With the electronic hopping potential modelled by V(t) = V(0)sech(lambda
   t), and the laser potential by W(t) = W(0)sech(lambda t) cos(eta t +
   delta), in the usual notation, an approximate analytical expression for
   P(eta) is presented for the case \textbackslash{}W-0/V-0\textbackslash{}
   much less than 1, which covers most of the previous treatments, and is
   in good agreement with the exact results.}},
