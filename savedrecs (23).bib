@article{ WOS:000284965000041,
Author = {Li, Guowang and Cao, Yu and Xing, Huili Grace and Jena, Debdeep},
Title = {{High mobility two-dimensional electron gases in nitride heterostructures
   with high Al composition AlGaN alloy barriers}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2010}},
Volume = {{97}},
Number = {{22}},
Month = {{NOV 29}},
Abstract = {{We report high-electron mobility nitride heterostructures with >70\% Al
   composition AlGaN alloy barriers grown by molecular beam epitaxy. Direct
   growth of such AlGaN layers on GaN resulted in hexagonal trenches and a
   low mobility polarization-induced charge. By applying growth
   interruption at the heterojunction, the surface morphology improved
   dramatically and the room temperature two-dimensional electron gas
   (2DEG) mobility increased by an order of magnitude, exceeding 1300
   cm(2)/V s. The 2DEG density was tunable at 0.4-3.7 X 10(13)/cm(2) by
   varying the total barrier thickness (t). Surface barrier heights of the
   heterostructures were extracted and exhibited dependence on t. (C) 2010
   American Institute of Physics. {[}doi:10.1063/1.3523358]}},
