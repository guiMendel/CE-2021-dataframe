@article{ WOS:000089813800042,
Author = {Yamada, S and Ohta, J and Kusanagi, H},
Title = {{Evaluation of lattice distortion near c-ZrO2/alpha-Al2O3 interface by
   plasmon loss}},
Journal = {{JOURNAL OF APPLIED PHYSICS}},
Year = {{2000}},
Volume = {{88}},
Number = {{9}},
Pages = {{5206-5210}},
Month = {{NOV 1}},
Abstract = {{Plasmon energies near a cubic-ZrO2 (c-ZrO2)/alpha-Al2O3 interface were
   studied by electron energy loss spectroscopy using a field-emission
   transmission electron microscope. The plasmon energies measured by
   scanning an electron probe of 1 nm in diameter across the interface
   showed that, approaching the interface, the plasmon energy of c-ZrO2
   increases from 23.9 to 24.3 eV and that of alpha-Al2O3 decreases from
   24.3 to 23.4 eV. Energy dispersive x-ray spectroscopy revealed that
   fluctuation of the chemical composition across the interface is small
   (less than or equal to 5\%) and causes only a slight change in the
   plasmon energies (less than or equal to 0.16 eV). We conclude that the
   large energy changes (up to 0.9 eV) are mainly caused by lattice
   distortion. Using the Drude model, it was found that, 3 nm from the
   interface, the unit cell volume of c-ZrO2 decreases by 3\% and that of
   alpha-Al2O3 increases by 6\% compared with the volumes at 50 nm. The
   presence of lattice distortion near the interface was also confirmed by
   the convergent-beam electron diffraction method. The results show that
   plasmon loss is useful to evaluate lattice distortion. (C) 2000 American
   Institute of Physics. {[}S0021-8979(00)05921-1].}},
