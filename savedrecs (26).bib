@article{ WOS:000281338900010,
Author = {Akbari, Saieed and Alikhani, Saeid and Peng, Yee-hock},
Title = {{Characterization of graphs using domination polynomials}},
Journal = {{EUROPEAN JOURNAL OF COMBINATORICS}},
Year = {{2010}},
Volume = {{31}},
Number = {{7}},
Pages = {{1714-1724}},
Month = {{OCT}},
Abstract = {{Let G be a simple graph of order n. The domination polynomial of G is
   the polynomial D(G, x) = Sigma(n)(i=1) d(G, i)x(i), where d(G, i) is the
   number of dominating sets of G of size i. A root of D(G, x) is called a
   domination root of G. We denote the set of distinct domination roots by
   Z(D(G, x)). Two graphs G and H are said to D-equivalent, written as G
   similar to H, if D(G, x) = D(H, x). The D-equivalence class of G is
   {[}G] = \{H : H similar to G\}. A graph G is said to be D-unique if
   {[}G] = \{G\}. In this paper, we show that if a graph G has two distinct
   domination roots, then Z(D(G, x)) = \{-2, 0\}. Also, if G is a graph
   with no pendant vertex and has three distinct domination roots, then
   Z(D(G. x)) subset of \{0, -2 +/- root 2i, -3 +/-root 3i/2\}. Also, we
   study the D-equivalence classes of some certain graphs. It is shown that
   if n equivalent to 0, 2 (mod3), then C(n) is D-unique, and if n
   equivalent to 0 (mod 3), then {[}P(n)] consists of exactly two graphs.
   (C) 2010 Elsevier Ltd. All rights reserved.}},
