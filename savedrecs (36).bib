@article{ WOS:000233781700067,
Author = {Yasuda, K and Niraula, M and Kusama, H and Yamamoto, Y and Tominaga, M
   and Takagi, K and Agata, Y and Suzuki, K},
Title = {{Development of nuclear radiation detectors with energy discrimination
   capabilities based on thick CdTe layers grown by metalorganic vapor
   phase epitaxy}},
Journal = {{IEEE TRANSACTIONS ON NUCLEAR SCIENCE}},
Year = {{2005}},
Volume = {{52}},
Number = {{5, 3}},
Pages = {{1951-1955}},
Month = {{OCT}},
Abstract = {{We report on the development of nuclear radiation detectors based on
   epitaxially grown thick single crystalline CdTe layers. The optimization
   of the CdTe growth on the GaAs substrates in a metalorganic vapor phase
   epitaxy resulted high-structural quality and thick CdTe layers of
   thickness up to 200 mu m. Radiation detectors were fabricated in
   p-CdTe/n-CdTe/n(+) -GaAs structure, where a 2-5 mu m thick iodine-doped
   n-CdTe buffer layer was first grown on the n(+)-GaAs substrate, followed
   by about 100-mu m-thick undoped p-like CdTe layer. The detectors
   exhibited good rectification property and good charge transport
   property. They showed reverse bias leakage currents typically from 1 to
   5 mu A/cm(2) at 40V bias, and clearly demonstrated energy discrimination
   capability by resolving the 59.5-keV gamma peak from the Am-241
   radioisotope during the radiation detection test. Some results on direct
   growth of CdTe epilayers on Si substrates are also presented.}},
