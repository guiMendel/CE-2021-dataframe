@article{ WOS:000233111300012,
Author = {Romer, S and Durr, D and Moser, T},
Title = {{Asymptotic behaviour of Bohmian trajectories in scattering situations}},
Journal = {{JOURNAL OF PHYSICS A-MATHEMATICAL AND GENERAL}},
Year = {{2005}},
Volume = {{38}},
Number = {{39}},
Pages = {{8421-8443}},
Month = {{SEP 30}},
Abstract = {{We study the asymptotic behaviour of Bohmian trajectories in a
   scattering situation with short-range potential V and for wavefunctions
   Psi = Psi(ac) + Psi(pp) E L-2(R-3) with a scattering and a bound part.
   It is shown that the set of possible trajectories splits into
   trajectories whose long-time behaviour is governed by the scattering
   part Psi(ac) of the wavefunction (scattering trajectories) and
   trajectories whose long time behaviour is governed by the bound part
   Psi(pp) of the wavefunction (bound trajectories). Furthermore, the
   scattering trajectories behave like trajectories in classical mechanics
   in the limit t -> infinity. As an intermediate step, we show that the
   asymptotic velocity v(infinity) := lim(t ->infinity) Q/t exists almost
   surely and is randomly distributed with the density vertical bar(Psi)
   over cap (out)vertical bar(2), Psi(out) is the outgoing asymptote of the
   scattering part of the wavefunction.}},
