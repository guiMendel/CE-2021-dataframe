@article{ WOS:A1995TW56200003,
Author = {Dijkstra, HA},
Title = {{Surface tension driven cellular patterns in three-dimensional boxes .3.
   The formation of hexagonal patterns}},
Journal = {{MICROGRAVITY SCIENCE AND TECHNOLOGY}},
Year = {{1995}},
Volume = {{8}},
Number = {{3}},
Pages = {{155-162}},
Month = {{NOV}},
Abstract = {{In this study of Benard-Marangoni flow patterns in horizontally square
   three-dimensional boxes, flows in intermediate aspect ratio (7 < A < 13)
   boxes are considered. Main focus is on the formation of steady flow
   patterns with only a few hexagons. For fixed aspect ratio, the patterns
   arise as a weakly nonlinear interaction between modes which destabilize
   the motionless solution as the Marangoni number Ma is increased. It
   turns out to be interesting to follow the morphogenesis of these
   patterns, at fixed Ma, as the aspect ratio is increased. The hexagonal
   flow patterns then arise in two steps, both which involve regulative and
   generative processes at the lateral (no-slip) boundaries. The results
   provide a mechanistic description of the formation of successive layers
   of hexagons in which the presence and properties of the lateral walls
   play a crucial role.}},
