@article{ WOS:000583948500117,
Author = {Khadivi, Miaad and Javanbakht, Vahid},
Title = {{Emulsion ionic liquid membrane using edible paraffin oil for lead
   removal from aqueous solutions}},
Journal = {{JOURNAL OF MOLECULAR LIQUIDS}},
Year = {{2020}},
Volume = {{319}},
Month = {{DEC 1}},
Abstract = {{Fora variety of reasons, such as high selectivity, easy control, easy
   reduction, and environmental compatibility, there is the high potential
   for the use of emulsion ionic liquid membranes (EILM) has been expanded.
   The edible oils have great potential for using in food industry
   extraction processes. Edible paraffin used for this purpose is less
   toxic and more cost-effective than other membranes. The technique of
   EILM based on edible oils was used as a chemical process for Pb2+
   recovery from aqueous solution, using Aliquat 336 as an ionic liquid
   carrier and the ionic transport agent for Pb2+ ions in the membrane
   phase. The Tween 80 as a dispersive non-ionic surfactant was used for
   the emulsion formation. The kinetics and effective parameters on the
