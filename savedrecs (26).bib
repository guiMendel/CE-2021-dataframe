@article{ WOS:000283686500078,
Author = {Chew, Xiongyeu and Zhou, Guangya and Yu, Hongbin and Chau, Fook Siong
   and Deng, Jie and Loke, Yee Chong and Tang, Xiaosong},
Title = {{An in-plane nano-mechanics approach to achieve reversible resonance
   control of photonic crystal nanocavities}},
Journal = {{OPTICS EXPRESS}},
Year = {{2010}},
Volume = {{18}},
Number = {{21}},
Pages = {{22232-22244}},
Month = {{OCT 11}},
Abstract = {{Control of photonic crystal resonances in conjunction with large
   spectral shifting is critical in achieving reconfigurable photonic
   crystal devices. We propose a simple approach to achieve nano-mechanical
   control of photonic crystal resonances within a compact integrated
   on-chip approach. Three different tip designs utilizing an in-plane
   nano-mechanical tuning approach are shown to achieve reversible and
   low-loss resonance control on a one-dimensional photonic crystal
   nanocavity. The proposed nano-mechanical approach driven by a sub-micron
   micro-electromechanical system integrated on low loss suspended feeding
   nanowire waveguide, achieved relatively large resonance spectral shifts
   of up to 18 nm at a driving voltage of 25 V. Such designs may
   potentially be used as tunable optical filters or switches. (C) 2010
   Optical Society of America}},
