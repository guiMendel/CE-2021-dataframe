@article{ WOS:000234100100122,
Author = {Suleiman, M and Faupel, J and Borchers, C and Krebs, HU and Kirchheim, R
   and Pundt, A},
Title = {{Hydrogen absorption behaviour in nanometer sized palladium samples
   stabilised in soft and hard matrix}},
Journal = {{JOURNAL OF ALLOYS AND COMPOUNDS}},
Year = {{2005}},
