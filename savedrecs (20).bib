@article{ WOS:000285583400034,
Author = {Rinaldi, Ciro R. and Rinaldi, Paola and Alagia, Adele and Gemei, Marica
   and Esposito, Nicola and Formiggini, Fabio and Martinelli, Vincenzo and
   Senyuk, Vitalyi and Nucifora, Giuseppina and Pane, Fabrizio},
Title = {{Preferential nuclear accumulation of JAK2V617F in CD34(+) but not in
   granulocytic, megakaryocytic, or erythroid cells of patients with
   Philadelphia-negative myeloproliferative neoplasia}},
Journal = {{BLOOD}},
Year = {{2010}},
Volume = {{116}},
Number = {{26}},
Pages = {{6023-6026}},
Month = {{DEC 23}},
Abstract = {{Recently, Dawson et al identified a previously unrecognized nuclear role
   of JAK2 in the phosphorylation of histone H3 in hematopoietic cell
   lines. We searched nuclear JAK2 in total bone marrow (BM) cells and in 4
   sorted BM cell populations (CD34(+), CD15(+), CD41(+), and CD71(+)) of
   10 myeloproliferative neoplasia (MPN) patients with JAK2V617F mutation
   and 5 patients with wild-type JAK2 MPN. Confocal immunofluorescent
   images and Western blot analyses of nuclear and cytoplasmic fractions
   found nuclear JAK2 in CD34(+) cells of 10 of 10 JAK2-mutated patients
   but not in patients with wild-type JAK2. JAK2 was predominantly in the
   cytoplasmic fraction of differentiated granulocytic, megakaryocytic, or
   erythroid cells obtained from all patients. JAK2V617F up-regulates LMO2
   in K562 and in JAK2V617F-positive CD34(+) cells. The selective JAK2
   inhibitor AG490 normalizes the LMO2 levels in V617F-positive K562 and
   restores the cytoplasmic localization of JAK2. (Blood. 2010;
   116(26):6023-6026)}},
