@article{ WOS:000089857800040,
Author = {Zdebska, E and Wozniewicz, B and Adamowicz-Salach, A and Koscielak, J},
Title = {{Erythrocyte membranes from a patient with congenital dyserythropoietic
   anaemia type I (CDA-I) show identical, although less pronounced,
   glycoconjugate abnormalities to those from patients with CDA-II (HEMPAS)}},
Journal = {{BRITISH JOURNAL OF HAEMATOLOGY}},
Year = {{2000}},
Volume = {{110}},
Number = {{4}},
Pages = {{998-1001}},
Month = {{SEP}},
Abstract = {{Congenital dyserythropoietic anaemias (CDAs) are rare hereditary
   disorders characterized by ineffective erythropoiesis and
   multinuclearity of erythroblasts. Three main types of the disease have
   been described. Glycoconjugate abnormalities in erythrocyte membrane
   glycoconjugates, consisting of hypoglycosylation of band 3 and
   accumulation of certain glycosphingolipids including
   lactotriaosylceramide, neolactotriaosylceramide and
   polyglycosylceramides, have been described only in patients with CDA
   type II (CDA-II). We report on identical, although less pronounced,
   abnormalities in erythrocyte glycoconjugates from a patient with CDA-I.
   A low degree of hypoglycosylation of band 3 in our patient with CDA-I
   suggests that hypoglycosylation is not a cause, but, most probably, a
   consequence of dyserythropoiesis.}},
