@article{ WOS:A1995UR47200010,
Author = {Holevo, AS},
Title = {{Excessive maps, `'arrival times'' and perturbation of dynamical
   semigroups}},
Journal = {{IZVESTIYA MATHEMATICS}},
Year = {{1995}},
Volume = {{59}},
Number = {{6}},
Pages = {{1311-1325}},
Month = {{NOV-DEC}},
Abstract = {{The notion of excessive map for dynamical semigroup is introduced, and
   it is shown that an excessive map defines an operation-valued measure
   describing the measurement of an `'arrival time'' related to the
   irreversible dynamics described by the semigroup. Any such arrival time
   determines a positive perturbation of the dynamical semigroup describing
   the dynamics after `'arrivals''. Generators of the relevant
   perturbations are characterized, and several examples, both commutative
   and a non-commutative, are discussed, elucidating the problem of
   standard representation.}},
