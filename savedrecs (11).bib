@article{ WOS:000372616400003,
Author = {Duetting, Paul and Kesselheim, Thomas and Tardos, Eva},
Title = {{Algorithms as Mechanisms: The Price of Anarchy of Relax-and-Round}},
Journal = {{ACM SIGECOM EXCHANGES}},
Year = {{2015}},
Volume = {{14}},
Number = {{2}},
Pages = {{22-25}},
Month = {{DEC}},
Abstract = {{We show that algorithms that follow the relax-and-round paradigm
   translate approximation guarantees into Price of Anarchy guarantees,
   provided that the rounding is oblivious and the relaxation is smooth. We
   use this meta result to obtain simple, near-optimal mechanisms for a
   broad range of optimization problems such as combinatorial auctions, the
   maximum traveling salesman problem, and packing integer programs. In
   each case the resulting mechanism matches or beats the performance
   guarantees of known mechanisms.}},
