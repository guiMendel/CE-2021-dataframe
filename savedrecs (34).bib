@article{ WOS:000233223000010,
Author = {Schmitt, A and Spaeter, S},
Title = {{Improving the prevention of environmental risks with convertible bonds}},
Journal = {{JOURNAL OF ENVIRONMENTAL ECONOMICS AND MANAGEMENT}},
Year = {{2005}},
Volume = {{50}},
Number = {{3}},
Pages = {{637-657}},
Month = {{NOV}},
Abstract = {{This paper considers a limited liability firm that needs external funds
   in order to invest in an activity that presents an environmental risk
   for the Society. When the firm's risk-reducing activities cannot be
   observed by the lenders, we show that the issue of convertible bonds can
   create incentives to improve prevention. Convertible bonds allow the
   holder to exchange his bonds for a fixed number of the firm's shares.
   The use of such hybrid securities could either complement or be an
   alternative to the American CERCLA legislation about lender extended
   liability. We define an optimal convertible bond contract that induces
   unchanged economic profits for the bank, more prevention and higher
   expected net revenues for the firm and a higher expected social welfare
   than with standard debt. Our results hold true both with and without
   extended liability. (c) 2005 Elsevier Inc. All rights reserved.}},
