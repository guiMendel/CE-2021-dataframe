@article{ WOS:000234338700056,
Author = {Salis, G and Wang, R and Jiang, X and Shelby, RM and Parkin, SSP and
   Bank, SR and Harris, JS},
Title = {{Temperature independence of the spin-injection efficiency of a MgO-based
   tunnel spin injector}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2005}},
Volume = {{87}},
Number = {{26}},
Month = {{DEC 26}},
Abstract = {{The spin polarization of electrons injected into GaAs from a
   CoFe/MgO(100) tunnel spin injector is inferred from the circular
   polarization of light emitted from a GaAs-based quantum well (QW)
   detector. The circular polarization strongly depends on the spin and
   electron hole recombination lifetimes in the QW. Using time-resolved
   optical techniques, we show that these lifetimes are highly temperature
   dependent. A peak in the charge lifetime versus temperature is likely
   responsible for the previously observed dip in the electroluminescence
   polarization. Evidence for a temperature-independent spin injection
   efficiency of similar to 70\% from 10 K to room temperature is found.
   (c) 2005 American Institute of Physics.}},
