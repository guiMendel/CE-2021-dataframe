@article{ WOS:000089178600040,
Author = {Natsume, T and Nakayama, H and Jansson, O and Isobe, T and Takio, K and
   Mikoshiba, K},
Title = {{Combination of biomolecular interaction analysis and mass spectrometric
   amino acid sequencing}},
Journal = {{ANALYTICAL CHEMISTRY}},
Year = {{2000}},
Volume = {{72}},
Number = {{17}},
Pages = {{4193-4198}},
Month = {{SEP 1}},
Abstract = {{We describe an approach for the combination of biomolecular interaction
   analysis (BPA) and electrospray tandem mass spectrometry (ESI/MS/MS) to
   obtain sequence information on the affinity-bound proteins on the sensor
   chip of BW. The procedure is illustrated with stable and unstable
   interactions of recombinant proteins, i.e., histidine-tagged
   protein-Ni2+/NTA and 1,4,5-inositol trisphosphate receptor-ligand
   interactions. The E. coli lysates expressing the recombinant proteins
   were passed through the sensor chips, and biomolecular interactions were
   monitored iu real time. The molecules detected on the sensor chip were
   digested by delivering proteolytic enzyme to the sensing now cells. The
   resulting on-chip digested peptide mixture at the mid- to low-femtomole
   level was recovered on a microcapillary reversed-phase precolumn by an
   on-line system and analyzed using HPLC-MS/MS. In both cases, unambiguous
   sequence information on the recombinant proteins isolated on the sensor
   chip was obtained from only a single run of analysis. The combined
   BIA-MS/MS may prove to be a general and versatile system to discover
   novel biomolecular interactions and to analyze protein complexes.}},
