@article{ WOS:000363830700014,
Author = {Harris, Hila and Israeli, David and Minshew, Nancy and Bonneh, Yoram and
   Heeger, David J. and Behrmann, Marlene and Sagi, Dov},
Title = {{Perceptual learning in autism: over-specificity and possible remedies}},
Journal = {{NATURE NEUROSCIENCE}},
Year = {{2015}},
Volume = {{18}},
Number = {{11}},
Pages = {{1574-1576}},
Month = {{NOV}},
Abstract = {{Inflexible behavior is a core characteristic of autism spectrum disorder
   (ASD), but its underlying cause is unknown. Using a perceptual learning
   protocol, we observed initially efficient learning in ASD that was
   followed by anomalously poor learning when the location of the target
   was changed (over-specificity). Reducing stimulus repetition eliminated
   over-specificity. Our results indicate that inflexible behavior may be
   evident ubiquitously in ASD, even in sensory learning, but can be
   circumvented by specifically designed stimulation protocols.}},
