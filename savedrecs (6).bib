@article{ WOS:000590660800001,
Author = {Funck, Juliette and Bataille, Clement and Rasic, Jeffrey and Wooller,
   Matthew},
Title = {{A bio-available strontium isoscape for eastern Beringia: a tool for
   tracking landscape use of Pleistocene megafauna}},
Journal = {{JOURNAL OF QUATERNARY SCIENCE}},
Year = {{2021}},
Volume = {{36}},
Number = {{1}},
Pages = {{76-90}},
Month = {{JAN}},
Abstract = {{Numerous paleoecological questions concern the mobility of ancient fauna
   in eastern Beringia. Strontium (Sr) isotope ratio (Sr-87/Sr-86) analysis
   has emerged as a powerful tracer for determining the provenance of
   ancient biological materials. However, it is important to characterize
   Sr-87/Sr-86 variation across a landscape. We measured the Sr-87/Sr-86
   composition of teeth from present-day, herbivorous rodents (n = 162)
   sampled from across eastern Beringia to estimate bio-available
   Sr-87/Sr-86 values. We compiled these data with the very limited number
   of previously published Sr-87/Sr-86 values from the region. We then used
   this dataset and a machine learning, random-forest regression to predict
   bio-available Sr-87/Sr-86 variations across eastern Beringia. As a case
   study using our new Sr-87/Sr-86 map (isoscape), we measured the
   Sr-87/Sr-86 and oxygen stable isotope values (delta O-18) of five
   radiocarbon-dated steppe bison from eastern Beringia and compared these
   to our Sr-87/Sr-86 isoscape and a delta O-18 isoscape to estimate the
   probable landscape use of these ancient fauna. Our model and isoscape
   provide important foundations for a wide range of additional
   applications, including studies of the paleo-mobility of other fauna,
   ancient people and present-day fauna in eastern Beringia.}},
