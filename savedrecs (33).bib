@article{ WOS:000233986000003,
Author = {Orchard, DM and Ahmadi, M and Tang, FC},
Title = {{Application of Java and VRML visualisation tools in store release
   wind-tunnel testing}},
Journal = {{AERONAUTICAL JOURNAL}},
Year = {{2005}},
Volume = {{109}},
Number = {{1101}},
Pages = {{569-576}},
Month = {{NOV}},
Abstract = {{The use of Java and Virtual Reality Modeling Language (VRML) to view 3D
   animations related to store release testing is presented. The method
   offers a versatile means (platform independent) of generating
   interactive 3D animations without the need for specialised system
   requirements. The resulting animations are accessible on any network
   attached PC through a standard web browser. Animation is performed by
   displaying the 3D VRML file within a web browser and connecting it to a
   Java application. The animation is made possible through the use of an
   External Authoring Interface (EAI), which enables the VRML model to be
   displayed and controlled by the Java file. Details of the main
   components of the HTML, Java and VRML files are given. Examples of the
   technique are provided which include the animation of the store release
   trajectory calculated from wind-tunnel test data and the assessment of
   the kinematics of proposed designs of a Captive Trajectory Simulation
   (CTS) system.}},
