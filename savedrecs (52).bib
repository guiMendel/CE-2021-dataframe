@article{ WOS:A1995TY62500006,
Author = {Christidis, G and Markopoulos, T},
Title = {{Mechanisms of formation of kaolinite and halloysite in the bentonite
   deposits of Milos Island, Greece}},
Journal = {{CHEMIE DER ERDE-GEOCHEMISTRY}},
Year = {{1995}},
Volume = {{55}},
Number = {{4}},
Pages = {{315-329}},
Month = {{NOV}},
Abstract = {{Representative kaolinite and/or halloysite bearing-bentonites from Miles
   island, Greece were examined in order to establish the possible
   relationships holding between smectite and kaolinite/halloysite. The
   data suggest that kaolinite and halloysite have been formed at the
   expense of smectite. The replacement of smectite by kaolinite/halloysite
   rejects the possibility for precipitation of the two minerals during the
   same geological event. The proposed mechanisms for kaolinite/halloysite
   formation involve: a) dissolution of smectite and neoformation of
   kaolinite/halloysite, and b) precipitation of kaolinite as a by-product
   of solid state transformation of smectite with higher layer charge to
   one with lower layer charge, which is triggered by supply of Si. The
   dissolution mechanism involves provision of H+ i.e. acidic pH. On the
   contrary the role of pH does not seem to be important in the solid state
   transformation mechanism. Cheto and Wyoming montmorillonites dissolve at
   a similar pH while beidellite is more susceptible to acidic condition
   than montmorillonite, dissolving at a higher pH.}},
