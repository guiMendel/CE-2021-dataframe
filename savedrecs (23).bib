@article{ WOS:000282235600005,
Author = {Meneses, Lorena and Araya, Andrea and Pilaquinga, Fernanda and Espin,
   Michelle and Carrillo, Pamela and Sanchez, Freddy},
Title = {{Theoretical Studies of Reactivity and Selectivity in Some Organic
   Reactions}},
Journal = {{INTERNATIONAL JOURNAL OF QUANTUM CHEMISTRY}},
Year = {{2010}},
Volume = {{110}},
