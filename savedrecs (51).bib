@article{ WOS:A1995TJ07800004,
Author = {MAZUR, A},
Title = {{BIOSOCIAL MODELS OF DEVIANT-BEHAVIOR AMONG MALE ARMY VETERANS}},
Journal = {{BIOLOGICAL PSYCHOLOGY}},
Year = {{1995}},
Volume = {{41}},
Number = {{3}},
Pages = {{271-293}},
Month = {{NOV 16}},
Abstract = {{Measures of hormones (testosterone, cortisol and thyroxin) and of
   deviant or antisocial behavior are examined in a sample of 4179 Vietnam
   veterans. Hormone levels were found to vary with veterans' age, social
   status acid race, and all three hormones were significantly related to
   deviant behavior. Biosocial models, combining hormonal variables with
   social background variables, tended to explain more variance in deviance
   than did traditional models based on social background alone. Several
   propositions regarding hormones and behavior were assessed. While the
   role of hormones in causing deviance remains unclear, it is argued that
   hormones should be included in models of deviant behavior.}},
