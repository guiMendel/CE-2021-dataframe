@article{ WOS:000233064400007,
Author = {Te, JW and Liu, Y and Li, M and Gao, SJ and Tu, MJ},
Title = {{Evolution of Sm2Fe17 Alloys during hydrogenation-disproportion process}},
Journal = {{JOURNAL OF RARE EARTHS}},
Year = {{2005}},
Volume = {{23}},
Number = {{5}},
Pages = {{542-545}},
Month = {{OCT}},
Abstract = {{The evolution of phase compositions, phase change and microstructure in
   Sm2Fe17 alloys during hydrogenation-disproportion process were
   systematically studied with XRD, SEM, EDX methods. Research indicates
   that HD process of SM2Fe17 alloys is as follows: Sm2Fe17 alloy absorbs
   hydrogen first in the atmosphere of hydrogenation with a pressure of 0.1
   MPa. Disproportionation begins at T >= 500 degrees C, then the alloys
   turn into lots of SmHx and alpha-Fe phases which are partly in
   microcrystal or amorphous structures. Along with the increase of
   temperature, the microcrystal and amorphous structures transformed into
   crystal structure and this transformation thoroughly completed at 750
   degrees. The size of obtained crystal grain is about 20 similar to 100
   nm. Based on the experimental data, a microstructural transformation
   model of Sm2Fe17 alloys during hydrogenation-disproportion process was
   made.}},
