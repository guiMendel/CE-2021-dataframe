@article{ WOS:A1995RV60900016,
Author = {PELEGRI, SP and BLACKBURN, TH},
Title = {{EFFECT OF BIOTURBATION BY NEREIS SP, MYA-ARENARIA AND CERASTODERMA SP ON
   NITRIFICATION AND DENITRIFICATION IN ESTUARINE SEDIMENTS}},
Journal = {{OPHELIA}},
Year = {{1995}},
Volume = {{42}},
Pages = {{289-299}},
