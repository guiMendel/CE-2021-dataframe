@article{ WOS:000288427600005,
Author = {Znaor, L. and Shahrezaei, S. Ansari and Binder, S.},
Title = {{Low-fluence photodynamic therapy for acute central serous
   chorioretinopathy}},
Journal = {{SPEKTRUM DER AUGENHEILKUNDE}},
Year = {{2010}},
Volume = {{24}},
Number = {{5}},
Pages = {{271-274}},
Month = {{OCT}},
Abstract = {{Purpose: To evaluate the effectiveness of indocyanine green angiography
   (ICGA) guided photodynamic therapy (PDT) with low-fluence as a treatment
   modality for acute central serous chorioretinopathy (CSC).
   Design: Retrospective observational case report.
   Patient and methods: A 53-year-old men with acute CSC was evaluated with
   fluorescein angiography, ICGA, and optical coherence tomography.
   Subsequently, the patient was treated with ICGA-guided low-fluence PDT
   (25 J/cm(2)).
   Results: All subretinal fluid disappeared 4 weeks after the treatment.
   Conclusion: Data from this case report suggest that ICGA-guided
   low-fluence PDT might be an effective and safe treatment for acute CSC.
   This should be verified in future clinical trials.}},
Publisher = {{SPRINGER}},
Address = {{233 SPRING ST, NEW YORK, NY 10013 USA}},
