@article{ WOS:A1995TR23700010,
Author = {Dumer, I},
Title = {{Nonbinary double-error-correcting codes designed by means of algebraic
   varieties}},
Journal = {{IEEE TRANSACTIONS ON INFORMATION THEORY}},
Year = {{1995}},
Volume = {{41}},
Number = {{6, 1}},
Pages = {{1657-1666}},
Month = {{NOV}},
Abstract = {{Linear q-ary codes of growing length n --> infinity and designed
   distance delta are studied. At first, we examine cyclic codes defined by
   the sets of code zeros
   \{g(i)\textbackslash{}i = q(s) + 1, q(s+1)+1,...,g(s+delta-2) + 1\}
   over a primitive element g of GF(q(m)). Then special cubic varieties are
   designed and employed in order to attain distances delta = 5, 6. The
