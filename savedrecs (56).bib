@article{ WOS:A1995RT72900009,
Author = {WHITE, FV and ANTHONY, DC and YUNIS, EJ and TARBELL, NJ and SCOT, RM and
   SCHOFIELD, DE},
Title = {{NONRANDOM CHROMOSOMAL GAINS IN PILOCYTIC ASTROCYTOMAS OF CHILDHOOD}},
Journal = {{HUMAN PATHOLOGY}},
