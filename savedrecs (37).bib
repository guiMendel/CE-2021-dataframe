@article{ WOS:000233742400014,
Author = {Norisada, M and Kojima, K},
Title = {{Nitrogen form preference of six dipterocarp species}},
Journal = {{FOREST ECOLOGY AND MANAGEMENT}},
Year = {{2005}},
Volume = {{216}},
Number = {{1-3}},
Pages = {{175-186}},
Month = {{SEP 12}},
Abstract = {{We investigated the nitrogen form preference of six dipterocarp,
   species: Anisoptera costata Korth., Dipterocarpus obtusifolius Teijsin.
   ex Miq., Hopea odorata Roxb., Neobalanocaqws heimii (King) P. Ashton,
   Shorea fagiletiana Heim, and Shorea roxburghii G. Don. Seedlings were
   Supplied with nitrogen as nitrate, ammonium, or both in sand culture ill
   a controlled environment. Except for N. heimii, all species showed
   greater shoot growth when supplied with ammonium than with nitrate.
   Higher root rnass ratios were observed in all species with nitrate,
   which would be an adaptive response to limited nitrogen uptake. The five
   species, which preferred ammonium, showed a higher light-saturated
   photosynthetic rate with ammonium supply. The lower light-saturated
   photosynthetic rate with nitrate supply was a result of lower
   photosynthetic capacity, as indicated by a lower CO,saturated
   photosynthetic rate. The lower leaf nitrogen content in seedlings
   supplied with nitrate would be the cause of the lower photosynthetic
   performance. Nitrate reductase activity in leaf and root of D.
   obtusifolius, N. heimii, and S. roxburghii showed generally low
   inducibility with nitrate. (c) 2005 Elsevier B.V. All rights reserved.}},
