@article{ WOS:000283899500029,
Author = {Yogui, G. T. and Santos, M. C. O. and Bertozzi, C. P. and Montone, R. C.},
Title = {{Levels of persistent organic pollutants and residual pattern of DDTs in
   small cetaceans from the coast of Sao Paulo, Brazil}},
