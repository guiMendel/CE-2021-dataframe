@article{ WOS:000089862900040,
Author = {Galli-Resta, L and Novelli, E and Volpini, M and Strettoi, E},
Title = {{The spatial organization of cholinergic mosaics in the adult mouse
   retina}},
Journal = {{EUROPEAN JOURNAL OF NEUROSCIENCE}},
Year = {{2000}},
Volume = {{12}},
Number = {{10}},
Pages = {{3819-3822}},
Month = {{OCT}},
Abstract = {{We analysed the spatial organization of the cholinergic amacrine cell
   mosaics in the mouse retina, as part of a general study of the major
   mouse retinal arrays, aiming at providing intrinsic cellular reference
   grids to monitor anomalies in retinal growth and/or functional
   organization in mouse models of retinal degeneration. The spatial
   organization of the cells was analysed by means of the nearest neighbour
   distance analysis, as well as by the analysis of Voronoi and Delaunay
   tesselations. We found non random cell spacing in both cholinergic
   arrays, although the mosaic in the ganglion cell layer tiles the retina
   scarcely better than a random distribution. Autocorrelation analysis
   revealed no detectable pattern in cell positioning, but there was a
   tendency towards a minimal spacing between array elements. Finally, we
   found no correlation in the spatial organization of the two arrays.}},
