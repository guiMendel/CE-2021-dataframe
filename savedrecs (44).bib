@article{ WOS:000089895000053,
Author = {Maeda, Y and Sato, R and Wakahara, T and Okamura, M and Akasaka, T and
   Fujitsuka, M and Ito, O and Kobayashi, K and Nagase, S and Kako, M and
   Nakadaira, Y and Horn, E},
Title = {{C-60-sensitized bis-silylation of nitrile and carbonyl compounds with
   disilirane}},
Journal = {{JOURNAL OF ORGANOMETALLIC CHEMISTRY}},
Year = {{2000}},
Volume = {{611}},
Number = {{1-2}},
Pages = {{414-419}},
Month = {{OCT 6}},
Abstract = {{The photochemical reaction of disilirane with unsaturated compounds such
   as nitrile, ketone and aldehyde in the presence of C-60 with a halogen
   lamp (cutoff <400 nm) affords the corresponding adducts of disilirane
   and unsaturated compounds as bis-silylated products. The experimental
