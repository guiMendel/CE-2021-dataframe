@article{ WOS:000232044000011,
Author = {Lobo-Guerrero, S and Vallejo, LE},
Title = {{Discrete element method evaluation of granular crushing under direct
   shear test conditions}},
Journal = {{JOURNAL OF GEOTECHNICAL AND GEOENVIRONMENTAL ENGINEERING}},
Year = {{2005}},
