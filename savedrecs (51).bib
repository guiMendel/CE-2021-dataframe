@article{ WOS:A1995TK14900014,
Author = {PIETZ, J and LANDWEHR, R and KUTSCHA, A and SCHMIDT, H and DESONNEVILLE,
   L and TREFZ, FK},
Title = {{EFFECT OF HIGH-DOSE TYROSINE SUPPLEMENTATION ON BRAIN-FUNCTION IN ADULTS
   WITH PHENYLKETONURIA}},
Journal = {{JOURNAL OF PEDIATRICS}},
Year = {{1995}},
Volume = {{127}},
Number = {{6}},
Pages = {{936-943}},
Month = {{DEC}},
Abstract = {{Objectives: To characterize abnormalities of brain function in patients
   with phenylketonuria (PKU) who had relaxed or stopped the dietary
   regimen and to test whether oral high-dose tyrosin (Tyr) supplementation
   has a beneficial effect.
   Design: Comparison with a control group; double-blind,
   placebo-controlled study comprising six times; crossover treatment
   groups; oral high-dose Tyr therapy (100 mg/kg body weight per day) or
   placebo administration for 4 weeks.
   Subjects: Twentry-four early-treated patients with PKU aged 20.8 (16 to
   25) years; 24 control subjects.
   Methods: Plasma concentrations of phenylalanine and Typ were monitored.
   Neuropsychologic tasks, visual evoked potentials, and spectral analysis
   of electro-encephalographic activity were used to evaluate brain
   function.
   Results: When patients with PKU were compared with control subjects,
   deficits in certain aspects of brain function were confirmed (i.e., a
   decreased ability to sustain attention, prolonged latencies of visual
   evoked potential peaks N1 and P2, and a reduced amount of fast-wave
   activity on the electroencephalogram). Baseline plasma phenylalanine and
   Tyr concentrations were in the typical range of adult patients with PKU.
   The plasma Tyr concentration increased approximately 200\% during Tyr
   supplementation, but no beneficial effects were observed.
   Conclusions: High-dose Tyr supplementation cannot be recommended as an
   `'alternative'' treatment for patients with PKU after relaxation or
   termination of strict dietary adherence.}},
