@article{ WOS:000602882500001,
Author = {Gan, Yong X. and Gan, Jeremy B.},
Title = {{Measuring the Electrical and Photonic Properties of Cobalt
   Oxide-Containing Composite Carbon Fibers}},
Journal = {{JOURNAL OF COMPOSITES SCIENCE}},
Year = {{2020}},
Volume = {{4}},
Number = {{4}},
Month = {{DEC}},
Abstract = {{In this work, cobalt acetate was incorporated into polyacrylonitrile
   (PAN) polymer through electrospinning as the cobalt oxide source. After
   oxidization and pyrolysis, a PAN-derived composite carbon fiber
   containing cobalt oxide was obtained. Measuring the electrical and
   photonic properties of the composite fiber under visible light
   irradiation was performed to evaluate the photoelectric behavior of the
   composite fiber. The p-type semiconducting behavior of the composite
   fiber was confirmed by measuring the open circuit voltage of a
   photochemical fuel cell consisting of the photosensitive electrode made
   from the composite fiber. The application of the composite fiber for
   glucose sensing was demonstrated.}},
