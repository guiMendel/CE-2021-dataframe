@article{ WOS:000281323900018,
Author = {Khamei, Aliakbar and Dehghani, Kamran},
Title = {{Microstructural Evolution During the Hot Deformation of Ti-55Ni (at.
   pct) Intermetallic Alloy}},
Journal = {{METALLURGICAL AND MATERIALS TRANSACTIONS A-PHYSICAL METALLURGY AND
   MATERIALS SCIENCE}},
Year = {{2010}},
Volume = {{41A}},
Number = {{10}},
Pages = {{2595-2605}},
Month = {{OCT}},
Abstract = {{The hot deformation behavior of Ti-55Ni (at. pct) alloy was studied
   using compression testing at 1173 K (900 A degrees C) to 1323 K (1050 A
   degrees C) and at the strain rates of 0.001 to 0.35 s(-1). The
   microstructure evolution was characterized using optical and scanning
   electron microscopy (SEM). The influences of hot-working parameters on
   the flow stress and microstructural features of this alloy were then
   analyzed. The results indicate that, depending on the temperature and
   strain rate, the dynamic recrystallization (DRX) is the dominate
   mechanism. Besides, the particle-stimulated nucleation (PSN) mechanism
   could partially recrystallize the structure. The PSN phenomenon is of
   significant importance for the Ti-55Ni (at. pct) that suffers from
   insufficient workability because of its high content of intermetallic
   phases. It is of interest that the discontinuous yielding phenomenon has
   been observed when the specimens were deformed at 1173 K (900 A degrees
   C). Finally, the optimum parameters for hot working of Ti-55Ni (at. pct)
   alloy are determined as well.}},
