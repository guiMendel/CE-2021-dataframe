Author = {Fang, Zhuangxi and Newstein, Maurice C. and Garetz, Bruce A. and
   Balsara, Nitash P.},
Title = {{Measurement and Analysis of the Angular Guided-Wave Depolarized Light
   Scattering Patterns from Block Copolymer Thin Films}},
Journal = {{MACROMOLECULES}},
Year = {{2010}},
Volume = {{43}},
Number = {{23}},
Pages = {{10071-10077}},
Month = {{DEC 14}},
Abstract = {{We have measured and analyzed the angular scattering patterns in
   guided-wave depolarized light scattering (GWDLS) from 500 nm thick
   diblock copolymer thin films of poly(styrene-block-isoprene) using
   diffraction grating couplers. The measured pattern is decomposed into
   the sum of contributions from ordered grains and grating birefringence.
   Measured patterns are well fitted using a grain model consisting of
   disks lying in the film plane with elliptical cross sections, randomly
   oriented optic axes, and variable-length major axes.}},
