@article{ WOS:000233743500002,
Author = {Maggio, B and Rosetti, CM and Borioli, GA and Fanani, ML and Del Boca, M},
Title = {{Protein-mediated surface structuring in biomembranes}},
Journal = {{BRAZILIAN JOURNAL OF MEDICAL AND BIOLOGICAL RESEARCH}},
Year = {{2005}},
