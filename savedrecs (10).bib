@article{ WOS:000367562900023,
Author = {Nekoeinia, Mohsen and Yousefinejad, Saeed and Abdollahi-Dezaki, Azam},
Title = {{Prediction of E-N(T) Polarity Scale of Ionic Liquids Using a QSPR
   Approach}},
Journal = {{INDUSTRIAL \& ENGINEERING CHEMISTRY RESEARCH}},
Year = {{2015}},
Volume = {{54}},
Number = {{50}},
Pages = {{12682-12689}},
Month = {{DEC 23}},
Abstract = {{A multiparameter quantitative model was developed to establish a
   relationship between structural descriptors of a set of 52 ionic liquids
   and their E-N(T) polarity scale. Theoretical descriptors were extracted
   by Dragon software and the E-N(T) model was obtained using multiple
   linear regression approach. After molecular modeling, four significant
   descriptors were identified which are related to the E-N(T) values of
   the ionic liquids and demonstrates good fit statistics and accurate
   predictions. The stability and prediction ability of the E-N(T) model
   was evaluated using various common statistical methods such as
   cross-validation, external validation, and Y-randomization test. As
   another indicator of model's validity, the leverage and standardized
   residual confirmed the presence of almost all 52 ILs in the
   applicability domain of the proposed model.}},
