@article{ WOS:000597117200022,
Author = {Hua, Jiasong and Ma, Wenjun and Liu, Xiaoyun and Zhuang, Qixin and Wu,
   Zeyang and Huang, Huang and Lin, Shaoliang},
Title = {{Efficient microwave traps with markedly enhanced interfacial
   polarization and impedance matching enabled by dual-shelled, dual-cavity
   magnetic@dielectric hollow nanospheres}},
Journal = {{JOURNAL OF MATERIALS CHEMISTRY C}},
Year = {{2020}},
Volume = {{8}},
Number = {{46}},
Pages = {{16489-16497}},
Month = {{DEC 14}},
Abstract = {{The ability to create robust microwave absorbing materials renders
   efficient protection of electromagnetic radiation. The combination of
   magnetic and dielectric losses has emerged as an appealing strategy to
   yield high-performance microwave absorbers. In this context,
   dual-shelled hollow nanospheres composed of magnetic Fe3O4 inner shell
   and dielectric TiO2 outer shell (denoted DS-Fe3O4@TiO2-HNs) are, for the
   first time, crafted by rationally introducing a sacrificial intermediate
   shell. Notably, such unique dual-shelled nanostructures with a
   relatively large hollow interior within the Fe3O4 inner shell and an
   additional cavity between the Fe3O4 inner shell and the TiO2 outer shell
   (i.e., dual-cavity) manifest an advantageous attribute for producing
   lightweight absorbers. Intriguingly, the reflection loss (RL) of -60.17
   decibel (dB) at 10 GHz and the effective bandwidth of microwave
   absorption of 10.5 GHz at RL <= -10 dB, representing 90\% absorption,
   are achieved for DS-Fe3O4@TiO2-HNs with a thickness of only 1.8 mm. The
   outstanding microwave absorption performance of DS-Fe3O4@TiO2-HNs can be
   attributed to the synergy of efficient trapping of the incident wave,
   markedly enhanced interfacial polarization, and greatly improved
   impedance matching. As such, judiciously crafted uniform dual-shelled,
   dual-cavity nanospheres possessing optimal magnetic and dielectric
   losses hold great promise as highly efficient lightweight microwave
   absorbers for use in electromagnetic interference shielding, including
   mobile communication devices, AM radio, and radar, among other areas.}},
