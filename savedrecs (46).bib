@article{ WOS:000089390000006,
Author = {Iwama, H and Kato, M and Ohmori, S and Akimaru, K and Tsutsumi, K and
   Tohma, J and Nakamura, N},
Title = {{Measurement of soluble fas antigen and ligand in circulating serum and
   intra-abdominal or cerebrospinal fluid during gastrointestinal or
   cerebrovascular surgery}},
Journal = {{JOURNAL OF CRITICAL CARE}},
Year = {{2000}},
Volume = {{15}},
Number = {{3}},
Pages = {{119-125}},
Month = {{SEP}},
Abstract = {{Purpose: Soluble Fas antigen (sFas) and ligand (sFasL), which are
   associated with apoptosis, have not been evaluated in gastrointestinal
   or cerebrovascular surgery. The aim of this study was to measure these
   substances in serum, intra-abdominal fluid, or cerebrospinal fluid, and
   to speculate a pathophysiologic role for Fas-FasL apoptosis in surgery.
   Materials and Methods: Arterial blood and intraabdominal or
   cerebrospinal fluid were collected at intervals from 27 gastrointestinal
   surgical patients, 10 cerebrovascular surgical patients, and 10 spinal
   anesthesia patients.
   Results: Serum sFas levels did not change during and after surgery
   Intra-abdominal and cerebrospinal sFas levels were identical to and
   lower than those in serum. Serum sFasL levels did not change during
   surgery, but decreased after surgery. Intra-abdominal and cerebrospinal
   sFasL levels were higher than and identical to those in serum. In spinal
   anesthesia, cerebrospinal sFas and sFasL levels were lower than those in
   serum.
   Conclusions: Serum sFasL decreases after surgery, whereas
   intra-abdominal or cerebrospinal sFasL increases, although sFas in each
   site does not change, suggesting redistribution of activated lymphocytes
   into local surgical wounds and induction of apoptosis in this site.
