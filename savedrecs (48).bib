@article{ WOS:000088395100008,
Author = {da Cruz, JAMP and Costa, JDM and Borrego, LFP and Ferreira, JAM},
Title = {{Fatigue life prediction in AlMgSil lap joint weldments}},
Journal = {{INTERNATIONAL JOURNAL OF FATIGUE}},
Year = {{2000}},
Volume = {{22}},
Number = {{7}},
Pages = {{601-610}},
Month = {{AUG}},
Abstract = {{The fatigue behaviour of an AlMgSil alloy lap welded joint and the
   improvement in fatigue strength due to post-weld heat treatment were
   investigated. Two series of fatigue tests were performed: one in
   as-welded specimens and another in heat-treated specimens. The
   prediction of crack initiation life under a plane strain condition was
   made based on the local strain approach using Morrow's modified
   equation. The local inelastic stresses and strains at the weld toe were
   calculated for the heat-treated specimens using three methods: Neuber,
   Glinka and finite element analysis. The accuracy of the different
   methods were evaluated and discussed. (C) 2000 Elsevier Science Ltd. All
   rights reserved.}},
