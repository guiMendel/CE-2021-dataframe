@article{ WOS:A1995TJ52100012,
Author = {PALMER, SM and KAUFMAN, RA and SALAMONE, SJ and BLAKECOURTNEY, J and
   BETTE, W and WAHL, HP and FURRER, F},
Title = {{COBAS(R) INTEGRA - CLINICAL LABORATORY INSTRUMENT WITH CONTINUOUS AND
   RANDOM-ACCESS CAPABILITIES}},
Journal = {{CLINICAL CHEMISTRY}},
Year = {{1995}},
Volume = {{41}},
Number = {{12, 1}},
Pages = {{1751-1760}},
Month = {{DEC}},
Abstract = {{The Cobas (R) Integra from Poche Diagnostic Systems is a new clinical
   laboratory analyzer with continuous and random-access features for
   routine chemistries, specific proteins, electrolytes, hormones,
   therapeutic drugs, and drugs of abuse. The system maintains 68
   test-specific reagent cassettes on board, along with multiple
   ion-selective electrodes (ISEs) for electrolyte determinations. This
   gives the Cobas Integra the capability of analyzing as many as 72
   analytes without having to load additional reagents. We describe the
   basic-analyzer configuration and the subsystems for absorbance,
   fluorescence polarization, and ISE measurements. Performance
   characteristics for precision, methods comparison, and on-board
   stability are given for assays representative of the various test
   groups. The 29 Cobas Integra tests evaluated in the present study show
   good agreement (r greater than or equal to 0.98 and slopes generally
   0.90 to 1.12) with the respective methods available on either the
   Olympus AU5000, Hitachi 911 or 717, Behring BNA, or Abbott TDx (R)
   systems. Total assay precision (CV) ranged from 0.8\% to 8.5\%, and
   calibration curves were stable for as long as 20 weeks. Test throughput,
   which is dependent on pipetting sequence, was determined to be up to 600
   tests per hour without ISE and up to 750 tests per hour with ISE; the
   time to first result was 2.0-10.0 min.}},
