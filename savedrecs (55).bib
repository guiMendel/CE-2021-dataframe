@article{ WOS:A1995TR35500004,
Author = {Herrschel, T},
Title = {{Local policy restructuring: A comparative assessment of policy responses
   in England and Germany}},
Journal = {{AREA}},
Year = {{1995}},
Volume = {{27}},
Number = {{3}},
Pages = {{228-241}},
Month = {{SEP}},
Abstract = {{This paper discusses local policy restructuring as a direct response to
   economic change. Using capital expenditure on local economic development
   initiatives by nine cities in England and Germany for the period 1980 to
   1990, a general, if varying shift towards locality-specific measures
   could be observed. These findings support arguments elsewhere in favour
   of more diverse and flexible modes of regulation. The international
   comparison revealed th relatively stronger impact of local policy-makers
   on the type of policy responses than the national policy-making
   environment. Thus, similarities between favoured policy initiatives went
   across international boundaries, suggesting a distinct role for the
   local state in a `flexible' economic world.}},
