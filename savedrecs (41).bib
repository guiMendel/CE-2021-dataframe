@article{ WOS:000165582100031,
Author = {Sekar, P and Umbarkar, S and Scheer, M and Voigt, A and Kirmse, R},
Title = {{Synthesis and properties of novel metal-linked P-n ligand complexes}},
Journal = {{EUROPEAN JOURNAL OF INORGANIC CHEMISTRY}},
Year = {{2000}},
Number = {{12}},
Pages = {{2585-2589}},
Month = {{DEC}},
Abstract = {{The reaction of {[}CpCr(CO)(3)](2) with P-4 in toluene at 90 degreesC
   for 5 h affords the triple-decker sandwich complex {[}CpCr(CO)(eta
   (3)-P-3)(CpCr)(2)(mu,eta (1):eta (5):eta (5)-P-5)] (1) as a new product,
   along with the previously described products with P-2, P-3, and P-10
   units. The structure of the paramagnetic complex 1 has been established
   by single-crystal X-ray diffraction methods. A novel complex
   {[}\{CpCr(CO)(2)\}(2)(mu,eta (1):eta (1):eta
   P-2(2))\{Mo(CO)(4)\}(2)\{(mu,eta (1):eta (1):eta (5):eta
   (5)-P-5)(CpCr)(2)\}] (2) has been obtained from the reaction of
   {[}\{CpCr(CO)(2)\}(2)(mu,eta (2)-P-2)] With {[}Mo-(CO)(4)(NBD)] (NBD =
   norbornadiene). The X-ray structure of 2 reveals a six-membered Mo2P4
   ring combining a Cr2P2 tetrahedral core and a triple-decker sandwich
   Cr2P5 moiety. The EPR spectra of the paramagnetic (S' = 1/2) complexes 1
   and 2 are reported. A noticeable hyperfine interaction with two P-31
   nuclei is observed for 2.}},
