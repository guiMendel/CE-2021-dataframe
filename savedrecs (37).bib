@article{ WOS:000231827600003,
Author = {Loeffler, MJ and Baragiola, RA},
Title = {{The state of hydrogen peroxide on Europa}},
Journal = {{GEOPHYSICAL RESEARCH LETTERS}},
Year = {{2005}},
Volume = {{32}},
Number = {{17}},
Month = {{SEP 10}},
Abstract = {{We report infrared reflectance studies in the laboratory that reveal the
   state of hydrogen peroxide on Europa. Detailed analysis of the 3.5 mm
   infrared absorption band for hydrogen peroxide grown in a number of
   water ice environments shows that, for temperatures and concentrations
   relevant to Europa (80-130 K), hydrogen peroxide molecules exist in
   isolated trimers H2O2 center dot 2H(2)O. If the temperature on an icy
   body containing H2O2 approaches 150 K, as on Ganymede, the trimers
   aggregate into precipitates of crystalline dihydrate H2O2 center dot
   2H(2)O.}},
