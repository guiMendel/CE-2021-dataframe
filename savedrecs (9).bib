@article{ WOS:000594529400016,
Author = {Haggie, S. and Fitzgerald, D. A.},
Title = {{Pro con debates in clinical medicine infection prevention and control in
   cystic fibrosis: One size fits all? The argument in favour}},
Journal = {{PAEDIATRIC RESPIRATORY REVIEWS}},
Year = {{2020}},
Volume = {{36}},
Pages = {{97-99}},
Month = {{NOV}},
Abstract = {{This article advocates for a universal approach to infection control
   measures in cystic fibrosis. The central tenets of infection control
   include hand hygiene, contact precautions, regular microbiological
   surveillance and adopting inpatient, outpatient, domestic and social
   practices to minimise acquisition of common CF pathogens. Infection
   control measures should be proactive and prospective, assuming all
   patients harbour aggressive pathogens, and not relying on past culture
   results. The challenges of implementing these policies include cost,
   equipment, education, consistency, meticulousness all whilst balancing
   additional procedures to a busy clinical workload. Crown Copyright (c)
   2020 Published by Elsevier Ltd. All rights reserved.}},
