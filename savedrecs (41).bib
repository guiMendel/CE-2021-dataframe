@article{ WOS:000166941300147,
Author = {Parveez, GKA and Masri, MM and Zainal, A and Majid, NA and Yunus, AMM
   and Fadilah, HH and Rasid, O and Cheah, SC},
Title = {{Transgenic oil palm: production and projection}},
