@article{ WOS:000367191900002,
Author = {Fleming, Damian},
Title = {{SUNDORHALGAN, WINCHESTER, AND AELFRIC}},
Journal = {{REVIEW OF ENGLISH STUDIES}},
Year = {{2015}},
Volume = {{66}},
Number = {{277}},
Pages = {{822-842}},
Month = {{NOV}},
Abstract = {{This article contextualizes the Old English term sundorhalgan,
   `Pharisees', in the lexical process of loan rendition, the proclivities
   of the school at Winchester, and their particular use of Isidore's
   Etymologies. After plotting all the various ways by which Old English
   authors translate the Latin pharisaei, I demonstrate the limited circle
   of texts which use the term sundorhalgan rather than adopting the Latin
   as a loanword. These sundorhalgan-texts-all from the end of the Old
   English period-are dominated by the anonymous West Saxon Gospels and the
   homilies of AElfric. Based on a close reading of AElfric's pointed use
   and suppression of this term in his homilies, combined with its
   Isidorian basis, I argue that AElfric, or more broadly the school of
   Winchester, is the likeliest origin for the popularisation this term.}},
