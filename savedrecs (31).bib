@article{ WOS:000233187000001,
Author = {Schmitz, PW},
Title = {{Workplace surveillance, privacy protection, and efficiency wages}},
Journal = {{LABOUR ECONOMICS}},
Year = {{2005}},
Volume = {{12}},
Number = {{6}},
Pages = {{727-738}},
Month = {{DEC}},
Abstract = {{Consider an employer who wants her employee to work hard. As is well
   known from the efficiency wage literature, the employer must pay the
   (wealth-constrained) employee a positive rent to provide incentives for
   exerting unobservable effort. Alternatively, the employer could make
   effort observable by costly workplace surveillance. It is argued that a
   privacy protection law preventing surveillance may increase the total
   surplus. While such a law reduces the employer's profit, this loss can
   be overcompensated by the employee's gain, because the employer invests
   in surveillance not only to implement higher effort, but also to reduce
   the employee's rent. (c) 2004 Elsevier B.V. All rights reserved.}},
