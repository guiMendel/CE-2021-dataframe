@article{ WOS:000284647600005,
Author = {Garland, Stephen and Baker, Anthony and Phillott, Andrea D. and
   Skerratt, Lee F.},
Title = {{BSA reduces inhibition in a TaqMan (R) assay for the detection of
   Batrachochytrium dendrobatidis}},
Journal = {{DISEASES OF AQUATIC ORGANISMS}},
Year = {{2010}},
Volume = {{92}},
Number = {{2-3}},
Pages = {{113-116}},
Month = {{NOV 25}},
Abstract = {{A TaqMan (R) assay for the causative agent of chytridiomycosis in
   amphibians (Batrachochytrium dendrobatidis) can be inhibited by phenolic
   compounds, including humic and tannic acids, resulting in false
   negatives. Bovine serum albumin (BSA) is known to reduce inhibition of
   PCR when samples are contaminated with these inhibitors. We assessed the
   effect of BSA in reducing inhibition of the TaqMan (R) assay when
   analyzing skin swabs for B. dendrobatidis. We found that the addition of
   BSA to the TaqMan (R) reaction reduced inhibition to insignificant
   levels. BSA did not appreciably affect the efficiency or analytical
   sensitivity of the TaqMan (R) reaction in the analysis of standard DNA
   solutions free from environmental inhibitors. We recommend the addition
   of 400 ng mu l(-1) of BSA to the standard TaqMan (R) assay to reduce
   inhibition associated with sampling wild amphibians.}},
