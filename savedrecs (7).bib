@article{ WOS:000592958800038,
Author = {Liu, Jiangfan and Fu, Tairan and Shi, Congling},
Title = {{Upconversion Model for Directly Determining the Microscopic
   Energy-Transfer Parameters for beta-NaYF4:Er3+}},
Journal = {{JOURNAL OF PHYSICAL CHEMISTRY C}},
Year = {{2020}},
Volume = {{124}},
Number = {{46}},
Pages = {{25509-25520}},
Month = {{NOV 19}},
Abstract = {{Lanthanide-doped photon upconversion systems are widely used to convert
   two or more low-energy photons into one high-energy photon. Theoretical
   upconversion models are needed to further understand the upconversion
   luminescence dynamics and improve photonic systems. Existing macroscopic
   and microscopic models still have limitations when analyzing
   upconversion systems with finite energy migration (EM) or time-dependent
   energy-transfer (ET) rates. This paper presents a macroscopic
   energy-transfer model based on the ion-pair microscopic state
   probability in an upconversion system. The model uses the microscopic
   dynamic state probability of ion pairs to predict the time-dependent
   macroscopic energy-transfer rates. The model gives the intrinsic
   microscopic and macroscopic parameters for the energy-transfer rates for
   upconversion systems where the EM process may or may not be infinitely
   fast. The model is verified against experimental data for single-doped
   systems of beta-NaYF4:Er3+. The emission decay curves of the
   beta-NaYF4:Er3+ samples with various Er3+ dopant concentrations were
   experimentally measured when excited by a 1.5 mu m pulsed laser to
   determine the microscopic resonant energy-transfer parameters and the
   macroscopic energy-transfer rate. This work provides a theoretical
   method to determine the microscopic intrinsic energy-transfer parameters
   of widely used upconversion luminescence systems.}},
