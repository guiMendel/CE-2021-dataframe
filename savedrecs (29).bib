@article{ WOS:000281115500015,
Author = {Cardone, John and Le Friec, Gaelle and Vantourout, Pierre and Roberts,
   Andrew and Fuchs, Anja and Jackson, Ian and Suddason, Tesha and Lord,
   Graham and Atkinson, John P. and Cope, Andrew and Hayday, Adrian and
   Kemper, Claudia},
Title = {{Complement regulator CD46 temporally regulates cytokine production by
   conventional and unconventional T cells}},
Journal = {{NATURE IMMUNOLOGY}},
Year = {{2010}},
Volume = {{11}},
Number = {{9}},
Pages = {{862-U123}},
Month = {{SEP}},
Abstract = {{In this study we demonstrate a new form of immunoregulation: engagement
   on CD4(+) T cells of the complement regulator CD46 promoted the effector
   potential of T helper type 1 cells (T(H)1 cells), but as interleukin 2
   (IL-2) accumulated, it switched cells toward a regulatory phenotype,
   attenuating IL-2 production via the transcriptional regulator ICER/CREM
   and upregulating IL-10 after interaction of the CD46 tail with the
   serine-threonine kinase SPAK. Activated CD4(+) T cells produced CD46
   ligands, and blocking CD46 inhibited IL-10 production. Furthermore,
   CD4(+) T cells in rheumatoid arthritis failed to switch, consequently
   producing excessive interferon-gamma (IFN-gamma). Finally, gamma delta T
   cells, which rarely produce IL-10, expressed an alternative CD46 isoform
   and were unable to switch. Nonetheless, coengagement of T cell antigen
   receptor (TCR) gamma delta and CD46 suppressed effector cytokine
   production, establishing that CD46 uses distinct mechanisms to regulate
   different T cell subsets during an immune response.}},
