@article{ WOS:000363345400007,
Author = {Arrighi, Yves and Davin, Berengere and Trannoy, Alain and Ventelou,
   Bruno},
Title = {{The non-take up of long-term care benefit in France: A pecuniary motive?}},
Journal = {{HEALTH POLICY}},
Year = {{2015}},
Volume = {{119}},
Number = {{10}},
Pages = {{1338-1348}},
Month = {{OCT}},
Abstract = {{With aging populations, European countries face difficult challenges. In
   2002, France implemented a public allowance program (APA) offering
   financial support to the disabled elderly for their long-term care (LTC)
   needs. Although currently granted to 1.2 million people, it is suspected
   that some of those eligible do not claim it-presenting a non-take-up
   behavior. The granting of APA is a decentralized process, with 94 County
   Councils (CC) managing it, with wide room for local interpretation. This
   spatial heterogeneity in the implementation of the program creates the
   conditions for a ``quasi-natural experiment{''}, and provides the
   opportunity to study the demand for APA in relation to variations in
   CCs' ``generosity{''} in terms of both eligibility and subsidy rate for
   LTC. We use a national health survey and administrative data in a
   multilevel model controlling for geographical, cultural and political
   differences between counties. The results show that claiming for APA is
   associated with the ``generosity{''} of CCs: the population tends to
   apply less for the allowance if the subsidy rate is in average lower.
   This pecuniary trade-off, revealed by our study, can have strong
   implications for the well-being of the elderly and their relatives. (C)
   2015 Elsevier Ireland Ltd. All rights reserved.}},
