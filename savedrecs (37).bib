@article{ WOS:000232230100062,
Author = {Heinzl, T and Kaestner, T and Wipf, A},
Title = {{Effective actions for the SU(2) confinement-deconfinement phase
   transition}},
Journal = {{PHYSICAL REVIEW D}},
Year = {{2005}},
Volume = {{72}},
Number = {{6}},
Month = {{SEP}},
Abstract = {{We compare different Polyakov-loop actions yielding effective
   descriptions of finite-temperature SU(2) Yang-Mills theory on the
   lattice. The actions are motivated by a simultaneous strong coupling and
   character expansion obeying center symmetry and include both Ising and
   Ginzburg-Landau type models. To keep things simple we limit ourselves to
   nearest-neighbor interactions. Some truncations involving the most
   relevant characters are studied within a novel mean-field approximation.
   Using inverse Monte Carlo techniques based on exact geometrical
   Schwinger-Dyson equations we determine the effective couplings of the
   Polyakov-loop actions. Monte Carlo simulations of these actions reveal
   that the mean-field analysis is a fairly good guide to the physics
   involved. Our Polyakov-loop actions reproduce standard Yang-Mills
   observables well up to limitations due to the nearest-neighbor
   approximation.}},
