@article{ WOS:000165674900007,
Author = {Kitatani, K and Akiba, S and Sato, T},
Title = {{Role of phospholipase D-derived phosphatidic acid as a substrate for
   phospholipase A(2) in RBL-2H3 cells}},
Journal = {{BIOLOGICAL \& PHARMACEUTICAL BULLETIN}},
Year = {{2000}},
Volume = {{23}},
Number = {{12}},
Pages = {{1430-1433}},
Month = {{DEC}},
Abstract = {{The implications of phospholipase D (PLD) in cytosolic phospholipase
   A(2) (cPLA(2)) activation were studied in a mast cell line, RBL-2H3,
   upon stimulation with antigen. Antigen-stimulated prostaglandin D-2
   generation was apparently suppressed by ethanol with a concomitant
   decrease in phosphatidic acid (PA) formation. The prostaglandin D-2
   generation was also inhibited almost completely by methyl arachidonyl
   fluorophosphonate (MAFP), an inhibitor of cPLA(2), but not by
   diacylglycerol lipase inhibitor. Furthermore, stimulation with antigen
   resulted in an increase in lysophosphatidic acid formation, which was
   suppressed by MAFP in parallel with an increase in PA formation. These
   results suggest that PA formed by the catalytic action of PLD is used as
   a substrate for cPLA(2), thus PLD regulates cPLA(2) activation in
   antigen-stimulated RBL-2H3 cells.}},
