@article{ WOS:000233947100004,
Author = {Esteves, E and Kleiman, S},
Title = {{The compactified Picard scheme of the compactified Jacobian}},
Journal = {{ADVANCES IN MATHEMATICS}},
Year = {{2005}},
Volume = {{198}},
Number = {{2}},
Pages = {{484-503}},
Month = {{DEC 20}},
Abstract = {{Let C be an integral projective curve in any characteristic. Given an
   invertible sheaf C on C of degree 1, form the corresponding Abel map
   A(L): C -> (J) over bar, which maps C into its compactified Jacobian,
   and form its pullback map A{*}(L) : Pic((J) over bar)(0) -> J, which
   carries the connected component of 0 in the Picard scheme back to the
   Jacobian. If C has, at worst, double points, then A{*}(L) is known to be
