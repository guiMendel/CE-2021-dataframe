@article{ WOS:A1995RM67700025,
Author = {ZHANG, DF and ZHANG, YH and LI, CX and CHEN, YS and ZHU, Y},
Title = {{THERMAL FIXING OF HOLOGRAPHIC GRATINGS IN BATIO3}},
Journal = {{APPLIED OPTICS}},
Year = {{1995}},
Volume = {{34}},
Number = {{23}},
Pages = {{5241-5246}},
Month = {{AUG 10}},
Abstract = {{Fixing of a holographic grating in a single BaTiO3 crystal is studied in
   detail by means of a thermal process. Above T = 78 degrees C,
   oscillations of the diffracted intensity of the sample appear, which are
   related to the fixing process. Different methods to perform and optimize
