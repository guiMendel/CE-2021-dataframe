@article{ WOS:000231188600027,
Author = {Bar-Sinai, A and Bassa, N and Fischette, M and Gottesman, MM and Love,
   DC and Hanover, JA and Hochman, J},
Title = {{Mouse mammary tumor virus env-derived peptide associates with nucleolar
   targets in lymphoma, mammary carcinoma, and human breast cancer}},
Journal = {{CANCER RESEARCH}},
Year = {{2005}},
Volume = {{65}},
Number = {{16}},
Pages = {{7223-7230}},
Month = {{AUG 15}},
Abstract = {{We have previously shown that the leader peptide (p14) of the
   Env-precursor of mouse mammary tumor virus is translocated into the
   nucleoli of murine T cell lymphomas that harbor this virus. Using a
