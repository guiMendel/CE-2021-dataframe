@article{ WOS:000236832500008,
Author = {Vilakazi, RLM and Chikwembani, S},
Title = {{Surface structure study of shrink-resist treated wool fibres using
   atomic force microscopy}},
Journal = {{SOUTH AFRICAN JOURNAL OF SCIENCE}},
Year = {{2005}},
Volume = {{101}},
Number = {{11-12}},
Pages = {{502-506}},
Month = {{NOV-DEC}},
Abstract = {{SURFACES OF WOOL FIBRES WERE INVESTIGATED using atomic force microscopy.
   The fibres were chemically treated with chlorine/Hercosett resin and
   Caro's acid/ Wacker resin, and the fibre surfaces subsequently imaged
   and analysed. Lateral force microscopy was used to identify the effects
   of the chemical treatments and the root-mean-square roughness of
   `flattened' images in the micrcoscope was also measured. Exposure to
   chlorine/Hercosett resin tended to cause formation of etch holes (in the
   absence of resin), which closed up when resin was added. In contrast,
   etching with Caro's acid/ Wacker resin caused no hole formation and
   otherwise the two treatments similarly affected the fibre cuticle
   morphology and therefore its shrink-resist properties. We recommend that
   the less environmentally polluting Caro's acid/Wacker resin combination
   be used to replace the traditionally used chlorine/Hercosett resin in
   the shrinkproofing of wool fibres, because the latter contaminates
   wastewater with organic chlorides.}},
