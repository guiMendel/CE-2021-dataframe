@article{ WOS:000365882900004,
Author = {Bragina, L. G. and Bragin, N. Yu.},
Title = {{Radiolarians from upper Cenomanian sediments of the Aimaki section
   (Dagestan Republic)}},
Journal = {{STRATIGRAPHY AND GEOLOGICAL CORRELATION}},
Year = {{2015}},
Volume = {{23}},
Number = {{6}},
Pages = {{600-606}},
Month = {{NOV}},
Abstract = {{In the Upper Cretaceous Aimaki section (Dagestan), upper Cenomanian
   sediments encompassing to the OAE-2 Event yielded the first radiolarians
   including Archaeospongoprunum sp. cf. A. archaeobipartitum Bragina,
   Crucella irwini Pessagno, Distylocapsa veneta (Squinabol),
   Pseudodictyomitra pseudomacrocephala (Squinabol), and others. By its
   composition, this assemblage is typical of the Mediterranean belt, being
   similar to previously described coeval assemblages from the Crimea,
   western Caucasus, and Turkey. The outburst of radiolarian abundance is
   noted for the OAE-2 interval in sections of many regions and may be
   related to this event.}},
