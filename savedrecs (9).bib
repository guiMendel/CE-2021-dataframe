@article{ WOS:000594230900001,
Author = {Cho, Chang Beck and Kim, Young Jin and Chin, Won Jong and Lee, Jin-Young},
Title = {{Comparing Rubber Bearings and Eradi-Quake System for Seismic Isolation
   of Bridges}},
Journal = {{MATERIALS}},
Year = {{2020}},
Volume = {{13}},
Number = {{22}},
Month = {{NOV}},
Abstract = {{Seismic isolation systems have been used worldwide in bridge structures
   to reduce vibration and avoid collapse. The seismic isolator, damper,
   and Shock Transmission Unit (SUT) are generally adopted in the seismic
   design of bridges to improve their seismic safety with economic
   efficiency. There are several seismic isolation systems, such as Natural
   Rubber Bearing (NRB), Lead Rubber Bearing (LRB), and the Eradi-Quake
   System (EQS). EQS as a new technology is expected to effectively reduce
   both seismic force and displacement, but there is still some need to
   verify whether it might provide an economical and practical strategy for
   a bridge isolation system. Moreover, it is important to guarantee
   consistent performance of the isolators by quality control. A
   comparative evaluation of the basic properties of the available seismic
   isolators is thus necessary to achieve a balance between
   cost-effectiveness and the desired performance of the bridge subjected
   to extreme loading. Accordingly, in this study, the seismic response
   characteristics of the seismic isolation systems for bridges were
   investigated by conducting compressive test and compressive-shear test
   on NRB, LRB, and EQS.}},
