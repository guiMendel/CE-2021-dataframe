@article{ WOS:A1995RJ78600033,
Author = {CANEPA, M and SALVIETTI, M and TRAVERSO, M and MATTERA, L},
Title = {{SURFACE RECONSTRUCTION AND THERMAL-DESORPTION - O/AG(110) - AN
   EXPERIMENTAL-STUDY}},
Journal = {{SURFACE SCIENCE}},
Year = {{1995}},
Volume = {{331}},
Number = {{A}},
Pages = {{183-188}},
