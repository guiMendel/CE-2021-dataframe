@article{ WOS:A1995TL76700011,
Author = {Jaworski, W},
Title = {{Strong approximate transitivity, polynomial growth, and spread out
   random walks on locally compact groups}},
Journal = {{PACIFIC JOURNAL OF MATHEMATICS}},
Year = {{1995}},
Volume = {{170}},
Number = {{2}},
Pages = {{517-533}},
Month = {{OCT}},
Abstract = {{We extend to continuous groups our recent results on strongly
   approximately transitive group actions. We are concerned with locally
   compact second countable groups and standard Borel G-spaces. A G-space
   chi with a sigma-finite quasiinvariant measure alpha is called strongly
   approximately transitive (SAT) if there exists a probability measure nu
   much less than alpha such for every Borel set A with alpha(A) not equal
   0 and every epsilon > 0 one can find g epsilon G with v(gA) > 1 -
   epsilon. Examples of SAT G-spaces include boundaries of spread out
   random walks on G. We prove that when G is compactly generated and has
   polynomial growth then every standard SAT G-space (chi, alpha) is
   necessarily purely atomic; when G is additionally connected, (chi,alpha)
   is a singleton. The Choquet-Deny theorem for spread out random walks on
   G follows as a corollary. For a connected G we establish the equivalence
   of the following conditions: (a) G has polynomial growth; (b) every
   standard SAT G-space is a singleton; (c) every SAT homogeneous space of
   G is a singleton; (d) every homogeneous space of G admits a sigma-finite
   invariant measure; (e) the Choquet-Deny theorem holds for every spread
   out probability measure on G; (f) the Choquet-Deny theorem holds for
   every absolutely continuous compactly supported probability measure on
   G.}},
