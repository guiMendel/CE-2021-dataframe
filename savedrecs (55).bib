@article{ WOS:A1995TB76700012,
Author = {BOURANTAS, K and MITSULISMENTSIKOFF, C and TSIARA, S and FAKITSA, B and
   GALANAKIS, E and CHRISTOU, L and NIKIFORAKIS, E},
Title = {{FOLLOW-UP OF 50 PATIENTS WITH HAIRY-CELL LEUKEMIA (HCL)}},
Journal = {{JOURNAL OF EXPERIMENTAL \& CLINICAL CANCER RESEARCH}},
Year = {{1995}},
Volume = {{14}},
Number = {{3}},
Pages = {{307-312}},
Month = {{SEP}},
Abstract = {{In this retrospective study a follow-up of 50 patients, 35 males and 15
   females, aged between 27 and 82 years, with hairy cell leukemia is
   described. Their survival ranged between 1 and 14 years. The symptoms
   were not specific in most of them. Splenomegaly was present in all but
   eight patients. Pancytopenia of various degree and the appearance of
   hairy cells were the main laboratory findings. Neutropenia was present
   in 38 patients (66\%) and monocytopenia in all of them. Using monoclonal
   antibodies, in six cases' B-lymphocyte origin of the hairy cells in the
   peripheral blood was revealed. The bone. marrow biopsy revealed in most
   of the cases diffuse involvement. Hypergammaglobulinemia was present in
   20 patients (in five of them monoclonal paraprotein was also detected);
   monoclonal paraprotein could also be detected. The levels of beta
   2-microglobulin (beta 2-M) and soluble receptors of interleukin-2 (IL2R)
   represented significant prognostic factors. Treatment for these patients
   was splenectomy, interferon alpha-2b (INF) and chlorambucil. Therapy
   with INF turned out to be the most effective. Patients who underwent
   splenectomy had also a relatively good prognosis. Chlorambucil was of
   limited value in the treatment of these patients.}},
