@article{ WOS:A1995TM49000005,
Author = {Schmidt, K and Miehlke, RK and Rupprecht, R},
Title = {{Open or arthroscopic synovectomy of the knee in rheumatoid arthritis}},
Journal = {{AKTUELLE RHEUMATOLOGIE}},
Year = {{1995}},
Volume = {{20}},
Number = {{6}},
Pages = {{212-220}},
Month = {{NOV}},
Abstract = {{Synovectomy of the knee has settled as a standard procedure in
   rheumatoid patients. The clincal outcome mainly depends on the
   radicality of the removal of synovial tissue. Using modern arthroscopic
   techniques and shaver systems, arthroscopic synovectomy of the knee
   seems to be as radical as the conventional open technique. Do the
   clinical results confirm this hypothesis?
   The clinical findings in 24 knees after conventional open synovectomy
   (group I), in 24 knees following arthroscopic synovectomy (group II) and
   in 24 knees after arthroscopic synovectomy in combination with
   synoviorthesis (group III) were compared.
   The diagnosis was RA in all cases. The average follow-up was five years.
   Patients of all groups reported as well a reduction in complaints and
   swelling as an improvement in walking-ability and in the ability to
   climb up stairs. At follow-up clinical findings showed a slight increase
   of ROM and recurrent swelling in eight knees of group I, in four knees
   of group II and in three knees of group III. The overall results were
   assessed by the Klein-score. An average increase was found in group I
   from 33.9 to 61 points, in group II from 36.6 to 68.5 points and in
   group III from 36.2 to 74 points. Radiographically a slight increase in
   Larsen categories in all groups was observed.
   Five years postoperatively the clinical outcome following careful
   arthroscopical synovectomy is as favourable as after conventional open
   synovectomy. Arthroscopically synovectomized patients seem to profit
   from additional synoviorthesis.}},
