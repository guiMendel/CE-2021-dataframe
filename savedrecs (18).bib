@article{ WOS:000362459900012,
Author = {Crawford, N. and De Roeck, W. and Schuetz, M.},
Title = {{Uniqueness regime for Markov dynamics on quantum lattice spin systems}},
Journal = {{JOURNAL OF PHYSICS A-MATHEMATICAL AND THEORETICAL}},
Year = {{2015}},
Volume = {{48}},
Number = {{42, SI}},
Month = {{OCT 23}},
Abstract = {{We consider a lattice of weakly interacting quantum Markov processes.
   Without interaction, the dynamics at each site is relaxing exponentially
   to a unique stationary state. With interaction, we show that there
   remains a unique stationary state in the thermodynamic limit, i.e.
   absence of phase coexistence, and the relaxation towards it is
   exponentially fast for local observables. We do not assume that the
   quantum Markov process is reversible (detailed balance) w.r.t. a local
   Hamiltonian.}},
