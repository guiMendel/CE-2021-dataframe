@article{ WOS:000231695200005,
Author = {Lin, YC and Chou, CY},
Title = {{On the design of variable sampling intervals (X)over-bar charts under
   non-normality}},
Journal = {{INTERNATIONAL JOURNAL OF INDUSTRIAL ENGINEERING-THEORY APPLICATIONS AND
   PRACTICE}},
Year = {{2005}},
Volume = {{12}},
Number = {{3}},
Pages = {{246-255}},
Month = {{SEP}},
Abstract = {{Recent studies demonstrated that the variable sampling interval (VSI) X
   charts are quicker than the traditional fixed sampling interval (FSI) X
   charts in detecting process shifts. The usual assumption for designing a
   control chart is that the data are normally distributed. However, this
   assumption may not be true. When process data are not normally
   distributed, the effect on the false alarm rate and the detection
   ability of process shifts may be inappropriate. The Burr distribution
   has been used in the literature to represent various non-normal
   distributions. In this paper, the Burr distribution will be employed to
   evaluate and design the VSI X charts under non-normality. Equations for
   determining the performance of the VSI X charts are derived, and then
   the method of designing VSI X charts for non-normal distributions is
   provided. A numerical example is given for illustration of practical
   applications.
   Significance: When process data are not normally distributed, the
   performance of the VSI X chart may be unreasonable. In this paper the
   Burr distribution to is used evaluate and design the VSI X charts under
   non-normality.}},
