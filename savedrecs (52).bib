@article{ WOS:A1995TE20700004,
Author = {FRIEDMAN, EP and MILLER, RF and SEVERN, A and WILLIAMS, IG and SHAW, PJ},
Title = {{CRYPTOCOCCAL PNEUMONIA IN PATIENTS WITH THE
   ACQUIRED-IMMUNODEFICIENCY-SYNDROME}},
Journal = {{CLINICAL RADIOLOGY}},
Year = {{1995}},
Volume = {{50}},
Number = {{11}},
Pages = {{756-760}},
Month = {{NOV}},
Abstract = {{Infection with Cryptococcus neoformans occurs with increased frequency
   in patients with the human immunodeficiency virus (HIV). Despite the
   lungs being the portal of entry for the fungus the commonest
   presentation is with meningitis: Cryptococcal broncho pulmonary
   infection occurs less commonly, The chest radiographs of fourteen HIV
   positive patients with cryptococcal pneumonia were reviewed, The
   commonest radiographic abnormalities mere interstitial infiltrates,
   (nine patients) and focal or widespread alveolar consolidation (seven
   patients), Ground glass shadowing, not previously described in
   cryptococcal pneumonia, occurred in sis radiographs, miliary nodules in
   one, lymphadenopathy in four, and small pleural effusions in three. Two
   patients had a normal chest radiograph.
   In contrast to previous reports, we found alveolar consolidation and
   ground glass shadowing to be common and nodules and cavitation to be
   rare, Cryptococcal pneumonia should be considered in the differential
   diagnosis of Pneumocystis carinii pneumonia, bacterial pneumonia and
   miliary tuberculosis in HIV positive patients.}},
