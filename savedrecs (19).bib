@article{ WOS:000367330600003,
Author = {Daniels, Matthew W. and Guo, Wei and Stocks, G. Malcolm and Xiao, Di and
   Xiao, Jiang},
Title = {{Spin-transfer torque induced spin waves in antiferromagnetic insulators}},
Journal = {{NEW JOURNAL OF PHYSICS}},
Year = {{2015}},
Volume = {{17}},
Month = {{OCT 19}},
Abstract = {{We explore the possibility of exciting spin waves in insulating
   antiferromagnetic films by injecting spin current at the surface. We
   analyze both magnetically compensated and uncompensated interfaces. We
   find that the spin current induced spin-transfer torque can excite spin
   waves in insulating antiferromagnetic materials and that the chirality
   of the excited spin wave is determined by the polarization of the
   injected spin current. Furthermore, the presence of magnetic surface
   anisotropy can greatly increase the accessibility of these excitations.}},
