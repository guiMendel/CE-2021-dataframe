@article{ WOS:000592421700001,
Author = {Kompisova, Anna and Lukot'ka, Obert},
Title = {{Short cycle covers of graphs with at most 77\% vertices of degree two}},
Journal = {{ELECTRONIC JOURNAL OF COMBINATORICS}},
Year = {{2020}},
Volume = {{27}},
Number = {{4}},
Month = {{NOV 13}},
Abstract = {{Let G be a bridgeless multigraph with m edges and n(2) vertices of
   degree two and let cc(G) be the length of its shortest cycle cover. It
   is known that if cc(G) < 1.4m, in bridgeless graphs with n(2) <= m/10,
   then the Cycle Double Cover Conjecture holds. Fan (2017) proved that if
   n(2) = 0, then cc(G) < 1.6258m, and cc(G) < 1.6148m, provided that G is
   loopless; morever, if n(2) <= m/30, then cc(G) < 1.6467m. We show that
   for a bridgeless multigraph with m, edges and n(2) vertices of degree
   two, cc(G) < 1.6148m, + 0.0741n(2). Therefore, if n(2) = 0, then cc(G) <
   1.6148m, even if G has loops; if n(2) <= m/30, then cc(G) < 1.6173m; and
   if n(2) <= m/10, then cc(G) < 1.6223 vertical bar E(G)vertical bar. Our
   improvement is obtained by randomizing Fan's construction.}},
