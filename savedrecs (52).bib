@article{ WOS:A1995TE36300018,
Author = {DORON, E and FRISCHAT, SD},
Title = {{SEMICLASSICAL DESCRIPTION OF TUNNELING IN MIXED SYSTEMS - CASE OF THE
   ANNULAR BILLIARD}},
Journal = {{PHYSICAL REVIEW LETTERS}},
Year = {{1995}},
Volume = {{75}},
Number = {{20}},
Pages = {{3661-3664}},
Month = {{NOV 13}},
Abstract = {{We study quantum-mechanical tunneling between symmetry-related pairs of
   regular phase space regions that are separated by a chaotic layer. We
   consider the annular billiard, and use scattering theory to relate the
   splitting of quasidegenerate states quantized on the two regular regions
   to specific paths connecting them. The tunneling amplitudes involved are
   given a semiclassical interpretation by extending the billiard
   boundaries to complex space and generalizing specular reflection to
   complex rays. We give analytical expressions for the splittings, and
   show that the dominant contributions come from chaos-assisted paths that
   tunnel into and out of the chaotic layer.}},
