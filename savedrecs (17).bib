@article{ WOS:000363464500012,
Author = {Sanchez-Delgado, G. and Cadenas-Sanchez, C. and Mora-Gonzatez, J. and
   Martinez-Tellez, B. and Chillon, P. and Lof, M. and Ortega, B. and Ruiz,
   J. R.},
Title = {{Assessment of handgrip strength in preschool children aged 3 to 5 years}},
Journal = {{JOURNAL OF HAND SURGERY-EUROPEAN VOLUME}},
Year = {{2015}},
Volume = {{40}},
Number = {{9}},
Pages = {{966-972}},
Month = {{NOV}},
Abstract = {{We investigated whether there is an optimal grip span for determining
   the maximum handgrip strength in preschool children and if it is
   influenced by gender, age, or hand size. A total of 292 preschool
   children (3-5years; 59.2\% boys) carried out the handgrip strength test
   with different grip spans (4.0, 4.5, 5.0, 5.5, and 6.0cm). The hand size
   was also measured. We also determined the reliability of the optimal
   grip span in another group of children (n=56, 57\% boys) who did the
   test twice, with a 3-hour difference between tests. The results showed
   that 4.0cm is the optimal grip span to determine the maximum handgrip
   strength in preschool children. This result applied to both genders, all
   age groups, and hand sizes. Paired t-tests showed no significant
   differences between test and retest. These findings may guide clinicians
   and researchers in selecting the optimal grip span when measuring
   handgrip strength in preschool children. Level IV}},
