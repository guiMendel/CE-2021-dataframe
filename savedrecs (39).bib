@article{ WOS:000231612200011,
Author = {Choi, YS and Park, IM and Kim, JH and Cho, KM and Lee, DL},
Title = {{Effect of Si on the thermo-mechanical deformation and spheroidization of
   medium carbon steels}},
Journal = {{METALS AND MATERIALS INTERNATIONAL}},
Year = {{2005}},
Volume = {{11}},
Number = {{4}},
Pages = {{327-332}},
Month = {{AUG}},
Abstract = {{Medium carbon steels (0.45 \%C) having various Si content (0.2 similar
   to 1.5 \%) were thermo-mechanically processed in a controlled hot
   deformation simulator, a Thermecmaster-Z, and subsequent heat-treatment
   was performed for spheroidization. The effects of Si on the
   thermo-mechanical deformation and the spheroidization behavior were
   investigated. The results showed an increase in transformation
   temperatures, A(3) and A(1), with increasing Si content. Also, both
   grain size and fraction of the ferrite after the thermo-mechanical
   deformation increased with increasing Si content. Furthermore, the
   spheroidization of the then-no-mechanically processed medium carbon
   steels was improved by the addition of Si. The non-stop hot
   deformation-spheroidization process, which eliminates extra
   cooling-reheating processing steps, was found to be highly effective for
   Si-added medium carbon steels.}},
