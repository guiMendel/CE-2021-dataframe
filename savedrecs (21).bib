@article{ WOS:000288333900006,
Author = {Granberg, I. G. and Artamonova, M. S. and Dobryshman, E. M.},
Title = {{On Two Small-Scale Circulation Mechanisms of Fine-Aerosol Transport in
   the Atmospheric Surface Layer}},
Journal = {{IZVESTIYA ATMOSPHERIC AND OCEANIC PHYSICS}},
Year = {{2010}},
Volume = {{46}},
Number = {{7}},
Pages = {{865-871}},
Month = {{DEC}},
Abstract = {{Small-scale processes are taken to mean the disturbances of the
   atmospheric basic background which are caused by the thermal
   inhomogeneity of the underlying surface and under which one can neglect
   the effects of both centripetal and Coriolis accelerations. Slight
   disturbances suggest the use of linearized hydrothermodynamic equations
   of a weakly compressible atmosphere. Two models are considered. In one
   of the models, circulation over a weakly sloping barchan is analyzed
   using a refined model of mountain-valley circulation (the well-known
   Prandtl model). The other model, which is a model of a thermal spot in a
   geostrophic flow, can conditionally be called ``anticonvective.{''} This
   problem is solved using the method of universal functions for parabolic
   equations with variable coefficients.}},
