@article{ WOS:000284146100017,
Author = {Glosson, Nicole L. and Gonyo, Patrick and May, Nathan A. and Schneider,
   Christine L. and Ristow, Laura C. and Wang, Qiuhong and Hudson, Amy W.},
Title = {{Insight into the Mechanism of Human Herpesvirus 7 U21-mediated Diversion
   of Class I MHC Molecules to Lysosomes}},
Journal = {{JOURNAL OF BIOLOGICAL CHEMISTRY}},
Year = {{2010}},
Volume = {{285}},
Number = {{47}},
Pages = {{37016-37029}},
Month = {{NOV 19}},
Abstract = {{The U21 open reading frame from human herpesvirus-7 encodes a membrane
   protein that associates with and redirects class I MHC molecules to the
   lysosomal compartment. The mechanism by which U21 accomplishes this
   trafficking excursion is unknown. Here we have examined the contribution
   of localization, glycosylation, domain structure, and the absence of
   substrate class I MHC molecules on the ability of U21 to traffic to
   lysosomes. Our results suggest the existence of a cellular protein
   necessary for U21-mediated rerouting of class I MHC molecules.}},
