@article{ WOS:A1995UM42500011,
Author = {Hidano, K and Tsutaya, K},
Title = {{Global existence and asymptotic behavior of solutions for nonlinear wave
   equations}},
Journal = {{INDIANA UNIVERSITY MATHEMATICS JOURNAL}},
Year = {{1995}},
Volume = {{44}},
Number = {{4}},
Pages = {{1273-1305}},
Month = {{WIN}},
Abstract = {{We study the global existence and asymptotic behavior of solutions to
   semilinear wave equations with two kinds of nonlinear terms. Our main
   purpose is to show the existence of a scattering opera,tor for small
   data. The existence of global solutions is proved in the same manner for
   small initial data given at t = 0. Our assumptions on the order of
   nonlinear terms are optimal in view of the nonexistence theorems due to
   many mathematicians such as F. John. As a consequence, we have succeeded
   in removing the assumption of spherical symmetry on initial data from
   the global existence theorem of T. C. Sideris (Comm. P. D. E. 8 (1983),
   1291-1323).}},
