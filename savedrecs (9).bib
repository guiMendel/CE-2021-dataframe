@article{ WOS:000571108200008,
Author = {Almasi, Miroslav and Walko, Martin and Borzikova, Jana and Gyepes,
   Robert},
Title = {{A novel approach to achieve molecular switching in solid-state driving
   by thermal treatment: A photochromic zinc-coordination polymer}},
Journal = {{INORGANICA CHIMICA ACTA}},
Year = {{2020}},
Volume = {{512}},
Month = {{NOV 1}},
Abstract = {{The molecular switching of complexes based on diarylethenes in a
   solid-state is their most frequently studied property. This switching
   becomes possible when the active carbon atoms acquire a suitable
   interatomic separation in the molecule. Even if the structural analysis
   may suggest conditions disfavouring the occurrence of such switching, it
   may still be enabled by post-synthetic modification of the sample, like
   dehydration. In the present study, a novel one-dimensional coordination
   polymer with composition \{{[}Zn(mu(3)-HA)(H2O)(3)]center dot H2O\}(n)
   (I) was prepared and characterized. In its solid state, however, its
   thiophene rings were arranged in the nonswitchable conformation yielding
   a C center dot center dot center dot C separation 4.148(6) angstrom
   between the active carbon atoms. After fully dehydrating the sample,
   this interatomic separation decreased to 3.814 angstrom, making it
   susceptible to photochromism by UV radiation.}},
