@article{ WOS:000609181500066,
Author = {Zhou, Biao and Huang, Shanshan and Liu, Shuyun and Ren, Lijie and Huang,
   Can and Lian, Zongde},
Title = {{Thrombectomy for Stroke Caused by Cardiac Myxoma}},
Journal = {{JOURNAL OF STROKE \& CEREBROVASCULAR DISEASES}},
Year = {{2020}},
Volume = {{29}},
Number = {{12}},
Month = {{DEC}},
Abstract = {{Large vessel occlusion stroke, caused by cardiac myxoma, is a rare and
   severe condition with poor neurological outcomes. Currently, there are
   no clear guidelines for treating patients with this condition. In our
   case, we describe a rare case of acute ischemic stroke caused by cardiac
   myxoma which was successfully treated with mechanical thrombectomy. At
   the end of a 6 months' follow-up, her National Institutes of Health
   Stroke Scale score (NIHSS) had significantly improved, from 20 to 3.
   This result is encouraging and suggests that mechanical thrombectomy may
   be a feasible therapy for large vessel occlusion stroke induced by
   cardiac myxoma emboli.}},
