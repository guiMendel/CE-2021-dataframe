Title = {{Dynamic mechanisms for bundling and guidance during neural network
   formation}},
Journal = {{PHYSICA A}},
Year = {{2000}},
Volume = {{288}},
Number = {{1-4}},
Pages = {{369-379}},
Month = {{DEC 15}},
Note = {{1st Dynamims Days Asia-Pacific International Conference on Nonlinear
   Science, HONG KONG, PEOPLES R CHINA, JUL 13-16, 1999}},
Organization = {{Hong Kong Baptist Univ; Asia Pacific Ctr Theoret Phys; Climbing Program,
