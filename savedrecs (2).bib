@article{ WOS:000596202000001,
Author = {Chen, Le and Du, Yanxia and Liang, Yi and Li, Jianjun},
Title = {{Research on corrosion behaviour of X65 pipeline steel under dynamic AC
   interference}},
Journal = {{CORROSION ENGINEERING SCIENCE AND TECHNOLOGY}},
Year = {{2021}},
Volume = {{56}},
Number = {{3}},
Pages = {{219-229}},
Month = {{APR 3}},
Abstract = {{To accurately assess the corrosion risk under dynamic AC interference,
   the dynamic fluctuation characteristics of the pipeline near high-speed
   railway were analyzed by field test data. The corrosion rate and
   morphology of corrosion products under natural corrosion, steady AC
   interference, and dynamic AC interference were compared, and the effect
   of AC current density and typical dynamic interference cycle on the
   corrosion rate of X65 steel were investigated. The results show that the
   corrosion rates under steady and dynamic AC interference are higher than
   that under natural corrosion. With the increase of peak AC current
   density and interference time of each cycle, the dynamic AC corrosion
   rate increases. When the interference time increases to 350s in each
   interference cycle, the dynamic AC corrosion rate is equivalent to the
   steady AC corrosion rate.}},
