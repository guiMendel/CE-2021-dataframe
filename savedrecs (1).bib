@article{ WOS:000599083600001,
Author = {Muller, Lars and Nadurata, Vincent L. and Cula, Beatrice and Hoof,
   Santina and Herwig, Christian and Limberg, Christian},
Title = {{Versatile Coordination Behavior of the Asymmetric
   Bis(3-mesityl-pyrazol-1-yl)(5-mesitylpyrazol-1-yl) Hydroborate Ligand
   towards Late 3 d M2+ Ions}},
Journal = {{EUROPEAN JOURNAL OF INORGANIC CHEMISTRY}},
