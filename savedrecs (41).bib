@article{ WOS:000165911600003,
Author = {Oloffs, K and Jeroch, H},
Title = {{Dose-response relation of supplemented microbial phytase to extruded
   wheat diets on phosphorus utilization in laying hens}},
Journal = {{ARCHIV FUR GEFLUGELKUNDE}},
Year = {{2000}},
Volume = {{64}},
Number = {{6}},
Pages = {{251-254}},
Month = {{DEC}},
Abstract = {{In a balance trial the dose-response relation of supplemented phytase to
   extruded wheat diets on phosphorus utilization in laying hens (36 week
   old) were tested. Six different feed mixtures were used, concisting of
   extruded wheat at 90\%; one control ration without any phytase and five
   rations with varying dosages of microbial phytase in a range of 200 to
   1030 units. Prior to application wheat had been processed in an extruder
   to eliminate the native wheat phytase.
   The control ration without any phytase activity showed a phosphorus
   utilization of 23,4\%. In contrast, all other groups with
   supplementation of microbial phytase showed higher values of phosphorus
   utilization. Initially, there is an increase of phosphorus utilization
   with increasing dosage of phytase followed by a stagnation and a more or
   less steady plateau, respectively.}},
