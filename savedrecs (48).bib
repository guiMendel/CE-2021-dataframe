@article{ WOS:000088516300031,
Author = {Kim, CS and Kim, YG and Lu, CD and Morozumi, T},
Title = {{Azimuthal angle distribution in B -> K{*}(-> K pi)l(+) l(-) in the low
   invariant m(l)+(l)- region}},
Journal = {{PHYSICAL REVIEW D}},
Year = {{2000}},
Volume = {{62}},
Number = {{3}},
Month = {{AUG 1}},
Abstract = {{We present the angular distribution of the rare B decay, B-->K{*}(-->K
   pi)l(+)l(-). By studying the azimuthal angle distribution in the low
   invariant mass region of dileptons, we can probe new physics effects
   efficiently. In particular, this distribution is found to be quite
   sensitive to the ratio of the contributions from two independent
   magnetic moment operators, which also contribute to B-->K{*} gamma.
   Therefore, our method can be very useful when new physics is introduced
   without changing the total decay rate of the b-->s gamma. The angular
   distributions are compared with the predictions of the standard model,
   and are shown for the cases when the aforementioned ratio is different
   from the standard model prediction.}},
