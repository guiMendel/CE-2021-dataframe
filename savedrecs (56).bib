@article{ WOS:A1995RV02500007,
Author = {NAKAMURA, Y and SUZUKI, SY and HIROMI, J},
Title = {{GROWTH AND GRAZING OF A NAKED HETEROTROPHIC DINOFLAGELLATE, GYRODINIUM
   DOMINANS}},
Journal = {{AQUATIC MICROBIAL ECOLOGY}},
Year = {{1995}},
Volume = {{9}},
Number = {{2}},
Pages = {{157-164}},
Month = {{AUG 31}},
Abstract = {{Growth and grazing rates of a naked heterotrophic dinoflagellate,
   Gyrodinium dominans {[}equivalent spherical diameter (ESD) = ca 20 mu
   m], were examined in batch cultures. Strains isolated from the Seto
   Inland Sea (Harima strain) and Tokyo Bay (Tokyo strain) in Japan were
   used for the experiments. Both strains grew well when fed amorphous
   materials (such as bacterial flocs with attached nanoflagellates and
   aggregates of a small diatom) and plankton species with ESD of 15 to 26
   mu m. The Tokyo strain also grew rapidly when fed small (ca 5 mu m)
   phytoplankton species at levels higher than 1 x 10(5) ml(-1). However,
   rates of ingestion of Nephroselmis aff. rotunda (ESD = 4.5 mu m) by the
   Tokyo strain were too low to sustain rapid growth of G. dominans at
   natural levels of small (2 to 8 mu m) flagellate abundance (<3 x 10(4)
   ml(-1)). Ingestion of Heterocapsa triquetra (ESD = 15.3 mu m) followed
   Michaelis-Menten kinetics with half-saturation constants of 820 and 180
   ml(-1) and maximum ingestion rates of 0.82 and 0.59 prey predator(-1)
   h(-1) for the Harima and Tokyo strains, respectively. The clearance
   rates of both strains feeding on H. triquetra were about 2 orders of
   magnitude higher than those for N, aff, rotunda. These results indicate
   that G. dominans can play an important role in the decline of red tides
   caused by large (> 15 mu m) nanoflagellates such as Gymnodinium
   mikimotoi.}},
