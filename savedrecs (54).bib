@article{ WOS:A1995TC70100017,
Author = {YOSSIAN, D and GIMPLIN, S and MAHAJNA, S and HOROWITZ, YS},
Title = {{KINETICS OF ISOLATED PEAK-5 IN TLD-100 FOLLOWING 165-DEGREES-C
   POSTIRRADIATION ANNEALING}},
Journal = {{RADIATION MEASUREMENTS}},
Year = {{1995}},
Volume = {{24}},
Number = {{4}},
Pages = {{387-393}},
Month = {{OCT}},
Note = {{International Symposium on Luminescent Detectors and Transformers of
   Ionizing Radiation, TALLINN, ESTONIA, SEP 25-29, 1994}},
Organization = {{Estonian Acad Sci, Inst Geol; Int Sci Fdn; Tartu Univ, Inst Exptl Phys
   \& Technol; Victoreen Inc; Bicron Technol Vertriebs GmbH}},
Abstract = {{Peak 5 in the glow curve of TLD-100 (at a temperature of approximately
   200 degrees C) was studied as a function of post-irradiation annealing
   at 165 degrees C. Computerized glow peak analysis shows that the order
   of kinetics of peak 5 increases from 1.2 +/- 0.05 (1 SD) following 165
   degrees C annealing for five minutes to 1.72 +/- 0.07 (1 SD) following
   annealing at 165 degrees C for thirty minutes. Extrapolation of the
   results to zero annealing time yields a value of kinetic order
   consistent with unity. The glow curve analysis also indicates that peak
   4 (the satellite peak at approximately 175 degrees C) is undetectable
   even at the lowest annealing duration of 5 minutes. The high temperature
   satellite of peak 5 (peak 6 at approximately 225 degrees C) grows with
   increasing annealing time to 10\% of the intensity of peak 5 after
   annealing for 30 min. Glow curve analysis including peak 6, however,
   does not significantly change the conclusions regarding the increase in
   the order of kinetics of peak 5 as a function of annealing time or the
   consistency of the extrapolated results with kinetic order unity at zero
   annealing time. Analysis ofisothermal decay curves at 165 degrees C
   shows that the relative intensity of peak 4 to peak 5 decreases with
   increasing annealing time, from 8\% after 5 min annealing to 0.5\% after
   30 min annealing.}},
