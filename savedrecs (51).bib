@article{ WOS:A1995TT42500011,
Author = {Sudnitsyn, II and Sidorova, MA and Vasilyeva, MI and Yegorov, YV},
Title = {{Deep outflux of water from irrigated Sod-Podzolic soil}},
Journal = {{EURASIAN SOIL SCIENCE}},
Year = {{1995}},
Volume = {{27}},
Number = {{12}},
Pages = {{101-108}},
Month = {{DEC}},
Abstract = {{With sprinkling and subsoil irrigation of Sod-Podzolic soils, an outflux
   of water from the soil profile develops. For the warm season, a reliable
   dependence of the mean monthly outflux (O, mm/month) on the mean monthly
   influx (I, mm/month) has been discovered: O = 0.3 (I = 48) where 55 < I
   < 180. A dependence exists between O/I and I: (O/I)max - (O/I)min =
   11.3/I. For some experiment variants, the dependence (O/I)max - (O/I)min
   = 0.32 - 0.002 I. The highest values for O and I are typical of
   irrigation with a decrease in capillary pressure of the soil moisture
   before irrigation to -10 kPa. With subsoil irrigation, the deep outflux
   is more intensive than with sprinkling.}},
