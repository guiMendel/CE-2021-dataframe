@article{ WOS:A1995TD64900003,
Author = {IM, DD and DUSKA, LR and ROSENSHEIN, NB},
Title = {{ADEQUACY OF CONIZATION MARGINS IN ADENOCARCINOMA IN-SITU OF THE CERVIX
