@article{ WOS:000090068400002,
Author = {Gildfind, A and Gigante, MA and Al-Qaimari, G},
Title = {{Evolving performance control systems for digital puppetry}},
Journal = {{JOURNAL OF VISUALIZATION AND COMPUTER ANIMATION}},
Year = {{2000}},
Volume = {{11}},
Number = {{4}},
Pages = {{169-183}},
Month = {{SEP}},
Abstract = {{We describe a new approach for creating performance control systems for
   digital puppetry. Genetic programming with fitness values specified
   directly by the puppeteer is used. A generic device and model
   representation combined with the inherent domain independence of the
   genetic programming paradigm allows this approach to create control
   systems for arbitrary combinations of input devices and models. In
   addition, a number of unique interaction techniques have been developed
   to support the user-directed search. In this paper we introduce the
   approach, describe the implementation and user interface and present the
   results from a comprehensive evaluation with expert users. We show that
   a search-based approach can provide an effective alternative to manually
   designing performance control systems and an elegant mechanism for
   unifying low-level input devices with a broad range of model control
   modes. Copyright (C) 2000 John Wiley \& Sons, Ltd.}},
