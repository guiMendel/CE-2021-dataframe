@article{ WOS:000231763900008,
Author = {Wang, CA and Zhou, AG and Qi, L and Huang, Y},
Title = {{Quantitative phase analysis in the Ti-Al-C ternary system by X-ray
   diffraction}},
Journal = {{POWDER DIFFRACTION}},
Year = {{2005}},
Volume = {{20}},
Number = {{3}},
Pages = {{218-223}},
Month = {{SEP}},
Abstract = {{Materials in the Ti-Al-C ternary system commonly contain three
   coexisting phases, Ti3AlC2, Ti2AlC, and TiC. Quantitative phase analysis
   in this ternary system was investigated using X-ray diffraction. First,
   nonoverlap diffraction peaks were selected: the (002) peak at 2
   theta=9.5 degrees for Ti3AlC2 (I/I-0 = 26.5), the (002) peak at 2
   theta=13.0 degrees for Ti2AlC (I/I-0 = 39), and the (111) peak at 2
   theta=35.9 degrees for TiC (I/I-0 = 78), respectively. Then, based on
   the mixing-sample method without internal standards, a set of equations
   was derived for determining the amounts of Ti3AlC2, Ti2AlC, and TiC in a
   sample using the intensities of the selected diffraction peaks. Finally,
   the applicability and error sources for this method were investigated.
   The method is simple and straightforward, and is applicable to the
   entire Ti-Al-C ternary system, since the derivation of this equation
   group is self-checking. (c) 2005 International Centre for Diffraction
   Data.}},
