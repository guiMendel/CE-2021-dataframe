@article{ WOS:000587652300001,
Author = {Qomariyah, Nunung Nurul and Sari, Synthia Atas and Fajar, Ahmad Nurul},
Title = {{SONIA: AN INTEGRATED INDONESIA ONLINE TOURISM SYSTEM IN NEW NORMAL ERA}},
Journal = {{INTERNATIONAL JOURNAL OF INNOVATIVE COMPUTING INFORMATION AND CONTROL}},
