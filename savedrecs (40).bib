@article{ WOS:000168714500005,
Author = {Gast, DL and Jacobs, HA and Logan, KR and Murray, AS and Holloway, A and
   Long, L},
Title = {{Pre-session assessment of preferences for students with profound
   multiple disabilities}},
Journal = {{EDUCATION AND TRAINING IN MENTAL RETARDATION AND DEVELOPMENTAL
   DISABILITIES}},
Year = {{2000}},
Volume = {{35}},
Number = {{4}},
Pages = {{393-405}},
Month = {{DEC}},
Abstract = {{This study evaluated the effectiveness of a brief 2 min pre-session
   stimulus preference assessment in predicting the levels of responding of
   four students with profound multiple disabilities during a 5-minute
   experimental session immediately following the assessment. During the
   pre-session assessment, students were presented with four stimuli
   selected from a pool of stimuli identified as either preferred (2
