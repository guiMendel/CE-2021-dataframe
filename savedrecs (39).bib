@article{ WOS:000230766400004,
Author = {Hara, F and Aoe, M and Doihara, H and Taira, N and Shien, T and
   Takahashi, H and Yoshitomi, S and Tsukuda, K and Toyooka, S and Ohta, T
   and Shimizu, N},
Title = {{Antitumor effect of gefitinib ('Iressa') on esophageal squamous cell
   carcinoma cell lines in vitro and in vivo}},
Journal = {{CANCER LETTERS}},
Year = {{2005}},
Volume = {{226}},
Number = {{1}},
Pages = {{37-47}},
Month = {{AUG 8}},
Abstract = {{High expression of epidermal growth factor receptor (EGFR) is thought to
   be correlated with cell proliferation, invasion, metastasis, resistance
   to chemoradiotherapy, and poor prognosis in Various kinds of human
   cancers. Blockade of EGFR signal transduction can be a promising
   strategy for cancer therapy. Approximately 40-70\% of esophageal
   squamous cell carcinomas (ESCCs) show high expression of EGFR. In this
   study, we examined the antitumor effect of gefitinib, an EGFR tyrosine
   kinase inhibitor, against ESCC cells in vitro and in vivo. In three ESCC
   cell lines (TE8, T.T and T.Tn), cell proliferation had been inhibited in
   a dose-dependent manner and IC50 Values (respectively, 8.49, 18.9 and
   17.3 mu M). Gefitinib inhibited EGF-induced autophosphorylation of EGFR
   and its downstream signaling pathways, Ras/Raf/MAPK and PI3K/Akt, and
   caused G(1) arrest of cell cycle and apoptosis confirmed with flow
   cytometry. We examined the effect of gefitinib on nude mice bearing
   established TE8 and T.T xenografts. Gefitimb (100 or 200 mg/kg
   once-daily, p.o.) showed antitumor activity in a dose-dependent manner,
   resulting in a significantly improved survival of treated mice as
   compared with untreated mice. Immunohistochemical examination of the
   harvested tumor was performed to examine the status of phosphorylated
   EGFR, PCNA, Factor VIII and apoptosis. We found inhibition of EGFR
   phosphorylation, cell cycle arrest (by PCNA staining), decrease of
   microvessel density (Factor VIII) and induction of apoptosis by TUNEL
   staining. In conclusion, our findings demonstrate that gefitinib is
   effective for growth inhibition of ESCC cell lines in vitro and in vivo
   and suggest that gefitinib may be one of the new therapeutic options for
   ESSC. (c) 2005 Elsevier Ireland Ltd. All rights reserved.}},
