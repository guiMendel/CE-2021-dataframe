@article{ WOS:A1995RR80200032,
Author = {FURUTA, M and DOHMARU, T and KATAYAMA, T and TORATANI, H and TAKEDA, A},
Title = {{RETENTION OF RADIOLYTIC CO GAS IN IRRADIATED PEPPER GRAINS AND
   IRRADIATION DETECTION OF SPICES AND DRY GRAINS WITH THE LEVEL OF STOCKED
   CO GAS}},
Journal = {{JOURNAL OF AGRICULTURAL AND FOOD CHEMISTRY}},
Year = {{1995}},
Volume = {{43}},
Number = {{8}},
Pages = {{2130-2133}},
Month = {{AUG}},
Abstract = {{The release of radiolytic CO gas from Co-60 gamma-irradiated pepper
   seeds was unexpectedly slower than that of radiolytic Hz gas during a
   storage period after irradiation. These gases were retained in the
   grains and could be recovered by pulverization under gastight condition.
   Using this procedure, 10-kGy-irradiated pepper grains could be
   distinguished from nonirradiated samples for more than 2 months by the
   level of CO and H-2 gases. The patterns of CO change at 10, 20, and 30
   kGy were similar, and the CO amounts were proportional to irradiation
   doses at any point of the storage period after irradiation. Co-60
   gamma-irradiated grains of allspice, cinnamon, cumin, polished rice, and
   wheat could be distinguished from nonirradiated ones by the level of
   retained CO gas even after 2 months of storage at room temperature.
   Thus, radiolytic CO gas could be an effective probe for rapid screening
   of irradiated pepper and dry grains.}},
