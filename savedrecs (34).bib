@article{ WOS:000233607500020,
Author = {Page, PR},
Title = {{New broad Be-8 nuclear resonances}},
Journal = {{PHYSICAL REVIEW C}},
Year = {{2005}},
Volume = {{72}},
Number = {{5}},
Month = {{NOV}},
Abstract = {{Energies, total and partial widths, and reduced width amplitudes of Be-8
   resonances up to an excitation energy of 26 MeV are extracted from a
   coupled-channel analysis of experimental data. The presence of an
   extremely broad J(pi)=2(+) ``intruder{''} resonance is confirmed, and
   new 1(+) and very broad 4(+) resonances are discovered. A previously
   known 22-MeV 2(+) resonance is likely resolved into two resonances. The
   experimental J(pi)T=3((+))? resonance at 22 MeV is determined to be
