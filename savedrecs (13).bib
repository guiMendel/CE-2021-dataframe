@article{ WOS:000363040500018,
Author = {Polat, Ahmet Veysel and Bekci, Tumay and Dabak, Nevzat and Ulu, Esra
   Meltem Kayahan and Selcuk, Mustafa Bekir},
Title = {{Vaccine-induced myositis with intramuscular sterile abscess formation:
   MRI and ultrasound findings}},
Journal = {{SKELETAL RADIOLOGY}},
Year = {{2015}},
Volume = {{44}},
Number = {{12}},
Pages = {{1849-1852}},
Month = {{DEC}},
Abstract = {{Although limb swelling is a well-known complication of vaccination, its
   rarity and wide band of differential diagnosis of limb swelling make it
   a diagnostic challenge. In this case report, we describe three cases of
