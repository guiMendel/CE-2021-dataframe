@article{ WOS:A1995RG29000009,
Author = {DAOUST, G and PLOURDE, A and BEAULIEU, J},
Title = {{INFLUENCES OF CROWN SIZE AND MATURATION ON FLOWER PRODUCTION AND SEX
   EXPRESSION IN PICEA-GLAUCA TREATED WITH GIBBERELLIN A(4/7)}},
Journal = {{TREE PHYSIOLOGY}},
Year = {{1995}},
Volume = {{15}},
