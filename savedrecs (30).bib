@article{ WOS:000233303300011,
Author = {Cerqueira, PM and Coelho, EB and Geleilete, TJM and Goldman, GH and
   Lanchote, VL},
Title = {{Influence of chronic renal failure on stereoselective metoprolol
   metabolism in hypertensive patients}},
Journal = {{JOURNAL OF CLINICAL PHARMACOLOGY}},
Year = {{2005}},
Volume = {{45}},
Number = {{12}},
Pages = {{1422-1433}},
Month = {{DEC}},
Abstract = {{The influence of chronic renal failure on the stereoselective metabolism
   of rac-metoprolol was investigated in 15 hypertensive patients, 7 of
   them with chronic renal failure and 8 with normal renal function. They
   were treated with rac-metoprolol (200 mg) for 7 days. The patients of
   both groups presented stereoselectivity in metoprolol metabolism,
   favoring the formation of 1'R-alpha-hydroxymetoprolol
   (AUC(1.R/1'S)(0-24) approximate to 2.5) and (R)-metoprolol acidic
   metabolite (AUC((S)/(R))(0-24) approximate to 0.8), the latter resulting
   in the plasma accumulation of (S)-metoprolol (AUC((S)/(R))(0-24)
   approximate to 1.2). Patients with chronic renal failure presented
   plasma accumulation of the 4 alpha-hydroxymetoprolol isomers and of both
   metoprolol acidic metabolite enantiomers. A 50\% reduction in CIR does
   not explain the 3- to 4-fold plasma accumulation of metoprolol acidic
   metabolite in this group, suggesting that other pathways of metoprolol
   elimination are affected in chronic renal failure in addition to renal
   excretion. Chronic renal failure does not change the stereoselective
   kinetic disposition of metoprolol but modifies its stereoselective
   metabolism, inducing some of the CYP enzymes involved in the formation
   of the metoprolol acid metabolite.}},
