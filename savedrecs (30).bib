@article{ WOS:000234087200004,
Author = {Cassin, B},
Title = {{Time of deliberation and space of power: Athens and Rome, the first
   conflict}},
Journal = {{JAVNOST-THE PUBLIC}},
Year = {{2005}},
Volume = {{12}},
Number = {{4}},
Pages = {{39-44}},
Month = {{DEC}},
Abstract = {{The conflict between Greece and Rome is one of modern history's first
   international conflicts. Writing in the second century AD, AE lius
   Aristides views it as a discursive, linguistic and rhetorical conflict.
   Essentially, it can even be described as a conflict between silence and
   rhetoric. Rome, unique voice of Empire, is, in being a complying,
   fine-tuned orchestra, even more silent and may be seen as prefiguring
   our contemporary notion of consensus and our version of globalisation?
   Athens talks, debates, and retains the notion of the political as
   continuous creation of dissent. Hence Athens - or rhetoric - is
   victorious even when conquered.}},
