@article{ WOS:A1995TA37500001,
Author = {RICHARDS, RA},
Title = {{IMPROVING CROP PRODUCTION ON SALT-AFFECTED SOILS - BY BREEDING OR
   MANAGEMENT}},
Journal = {{EXPERIMENTAL AGRICULTURE}},
Year = {{1995}},
Volume = {{31}},
Number = {{4}},
Pages = {{395-408}},
Month = {{OCT}},
Abstract = {{The area of salt-affected land is increasing because of irrigation
   practices and changed land use. Breeding crops that tolerate soil
   salinity and yield well in salt-affected soils, acid employing crop
   management practices to counter salinity, have been proposed to maintain
   crop productivity. Here, it is argued that neither breeding nor
   management will adequately counter the effects of salinity. Although
   both offer the potential to maintain yields for a brief period, it is
   inevitable that salinity will continue to increase and crop productivity
   will decline. Only the establishment of a perennial vegetation that will
   maintain evapotranspiration al high levels all year round on both
   salt-affected land and recharge areas will halt the increase in
   salinization.}},
