@article{ WOS:000165609100004,
Author = {Nishikawa, T and Miyahara, K and Nagai, S},
Title = {{Effects of temperature and salinity on the growth of the giant diatom
   Coscinodiscus wailesii isolated from Harima-Nada, Seto Inland Sea, Japan}},
Journal = {{NIPPON SUISAN GAKKAISHI}},
Year = {{2000}},
Volume = {{66}},
Number = {{6}},
Pages = {{993-998}},
Month = {{NOV}},
Abstract = {{The giant diatom Coscinodiscus wailesii is one of the harmful species
   for Nori (Porphyra) culture from autumn to spring in the Harima-Nada,
   eastern part of Seto Inland Sea, Japan. The growth response of C.
   wailesii isolated from Harima-Nada was examined on four strains in 36
   different combinations of temperature (5-30 degreesC) and salinity
   (10-35) under a 130 mu mol m(-2) s(-1) and 16 h: 8 h LD photo cycle. The
   growth of C. wailesii was observed in most of the combinations examined
   and the maximum growth rate averaged for the four strains was 1.02
   divisions d(-1). The optimum temperature and salinity were 20-25
   degreesC and 20-30, respectively. The range of optimum temperature is in
   good accordance with the field data of water temperature in Harima-Nada
   during the period from October to November, when the autumn bloom of C.
   wailesii occurs. However, the growth rate at 5 to 10 degreesC decreased
   more than half the maximum growth rate. Therefore, the low water
