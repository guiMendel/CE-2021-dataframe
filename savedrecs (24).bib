@article{ WOS:000284364300004,
Author = {Ekrem, Torbjorn and Stur, Elisabeth and Hebert, Paul D. N.},
Title = {{Females do count: Documenting Chironomidae (Diptera) species diversity
   using DNA barcoding}},
Journal = {{ORGANISMS DIVERSITY \& EVOLUTION}},
Year = {{2010}},
Volume = {{10}},
Number = {{5}},
Pages = {{397-408}},
Month = {{NOV}},
Abstract = {{Because the family Chironomidae, or non-biting midges, is one of the
   most species-rich groups of macro-invertebrates in freshwater habitats,
   species-level identifications of chironomids are important for
   biodiversity assessments in these ecosystems. Morphology-based species
   identifications from adult female chironomids usually are considerably
   more difficult than from adult males, or even impossible; thus, the
   females are often neglected in community assessments. We used DNA
   barcoding to investigate how inclusion of the females influenced the
   species count from springs and spring brooks at Solendet Nature Reserve
   in Central Norway. By means of the barcodes we were able to identify
   77.6\% of the females to species by associating them with males from the
   study site or from other regions, whereas the remaining, unassociated
   females could be identified to genus level only. The number of recorded
   species increased by 27\% when females were included. We also found that
   DNA barcoding is effective for the detection of taxonomically
   challenging species and species groups. Using DNA barcoding in
   combination with traditional taxonomy, we recognised at least five
   species new to science and three species and one genus new to Norway.}},
