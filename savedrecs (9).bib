@article{ WOS:000570128700001,
Author = {Gibson, Heather D. and Hall, Stephen G. and Petroulas, Pavlos and
   Spiliotopoulos, Vassilis and Tavlas, George S.},
Title = {{The effect of emergency liquidity assistance (ELA) on bank lending
   during the euro area crisis}},
Journal = {{JOURNAL OF INTERNATIONAL MONEY AND FINANCE}},
Year = {{2020}},
Volume = {{108}},
Number = {{SI}},
Month = {{NOV}},
Abstract = {{We examine the impact of emergency liquidity assistance (ELA) on bank
   lending in eleven euro area countries during the financial crisis. With
   the intensification of the crisis, ELA took on a pivotal role in some
   countries. However, assessments of the quantitative impact of ELA in the
   literature are non-existent. We estimate a structural panel model for
   the determination of bank lending, which includes the amount of ELA
   received by each bank, allowing us to investigate the direct effect of
   ELA on lending. Our model corrects a misspecification found in the
   prototype model used in the literature. We then undertake a VAR
   analysis, which allows us to address the effect of ELA on GDP. Finally,
   we examine spillover effects among banks, indicating that ELA generated
   positive spillovers to other banks. (C) 2020 Elsevier Ltd. All rights
   reserved.}},
