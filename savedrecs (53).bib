@article{ WOS:A1995TJ88400010,
Author = {VASILEV, MI and TSEKHOMSKII, VA and GAPONENKO, SV and GERMANENKO, IN},
Title = {{EXCITON EFFECTS IN COPPER HALIDE GLASSES}},
Journal = {{JOURNAL OF OPTICAL TECHNOLOGY}},
Year = {{1995}},
Volume = {{62}},
Number = {{11}},
Pages = {{802-811}},
Month = {{NOV}},
Abstract = {{This paper describes exciton effects in photochromic glasses colored by
   CuCl and CuBr nanocrystals. Exciton absorption and photoluminescence is
   discussed, along with nonlinear absorption at high light intensities.
   The optical properties of the glasses associated with the generation and
   annihilation of excitons in the copper halide crystals are interpreted
   on the basis of spatial quantization effects in the regime of quantized
   motion of the exciton. In this case, the nonlinear effects are
   determined by the interaction in the system of a finite number of
   excitons, localized in one quantum box. A mechanism is proposed for the
   photoburning of stable holes in the absorption spectra, based on the
   effect of a local electric field, which broadens the absorption bands of
   the resonantly excited crystallites. The effect of selective thermal
   action on copper halide crystals in glass is described. (C) 1995 Optical
   Society of America.}},
