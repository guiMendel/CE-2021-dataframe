Title = {{CATECHOLAMINERGIC INHIBITION BY HYPERCORTISOLEMIA IN THE PARAVENTRICULAR
   NUCLEUS OF CONSCIOUS RATS}},
Journal = {{ENDOCRINOLOGY}},
Year = {{1995}},
Volume = {{136}},
Number = {{11}},
