@article{ WOS:000595599500026,
Author = {Leuci, Valeria and Donini, Chiara and Grignani, Giovanni and Rotolo,
   Ramona and Mesiano, Giulia and Fiorino, Erika and Gammaitoni, Loretta
   and D'Ambrosio, Lorenzo and Merlini, Alessandra and Landoni, Elisa and
