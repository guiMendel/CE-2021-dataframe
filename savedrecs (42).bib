@article{ WOS:000166684000001,
Author = {Ageev, ON},
Title = {{On the spectrum of Cartesian powers of classical automorphisms}},
Journal = {{MATHEMATICAL NOTES}},
Year = {{2000}},
Volume = {{68}},
Number = {{5-6}},
Pages = {{547-551}},
Month = {{NOV-DEC}},
Abstract = {{We prove the following statement: the set of all essential spectral
   multiplicities of T-(n) = T x x T (n times) is \{n, n(n - 1),.... n!\}
   on \{const\}(perpendicular to) for Chacon transformations T, or,
   equivalently, the operator T-(n) has a simple spectrum on the subspace
   C-sim of all functions that are invariant with respect to permutations
   of the coordinates. As an immediate consequence of this fact, we have
   the disjointness of all convolution powers of the spectral measure for
   Chacon transformations. If n = 2, then T x T has a homogeneous spectrum
   of multiplicity 2 on \{const\}(perpendicular to),i.e.. this is a
   solution of Rokhlin's problem for Chacon transformations. Similar
   statements are considered for other classical automorphisms.}},
