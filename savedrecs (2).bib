@article{ WOS:000594661800018,
Author = {Alghamdi, Sultan and Markovic, Uros and Stanojev, Ognjen and Schiffer,
   Johannes and Hug, Gabriela and Aristidou, Petros},
Title = {{Wide-area oscillation damping in low-inertia grids under time-varying
   communication delays}},
Journal = {{ELECTRIC POWER SYSTEMS RESEARCH}},
Year = {{2020}},
Volume = {{189}},
Month = {{DEC}},
Abstract = {{Wide-Area Control (WAC) can be efficiently used for oscillation damping
   in power systems. However, to plement a WAC, a communication network is
   required to transmit signals between the generation units and control
   center. In turn, this makes WAC vulnerable to time-varying communication
   delays that, if not appropriately considered in the control design, can
   destabilize the system. Moreover, with the increasing integration
   renewable energy resources into the grid, usually interfaced via power
   electronics, power system dynamics becoming drastically faster and
   making WAC more vulnerable to communication delays. In this paper,
   propose a design procedure for a delay-robust wide-area oscillation
   damping controller for low-inertia systems. Its performance is
   illustrated on the well-known Kundur two-area system. The results
   indicate that the obtained WAC successfully improves the oscillation
   damping while ensuring robustness against time-varying communication
   delays.}},
