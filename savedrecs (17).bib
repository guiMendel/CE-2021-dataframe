@article{ WOS:000366295900004,
Author = {Radu, A. and Niculescu, E. C. and Cristea, M.},
Title = {{Dielectric Modulation of Nonlinear Optical Properties in ZnO Quantum
   Dots Under Intense Laser Fields}},
Journal = {{SCIENCE OF ADVANCED MATERIALS}},
Year = {{2015}},
Volume = {{7}},
Number = {{11}},
Pages = {{2285-2296}},
Month = {{NOV}},
Abstract = {{Third-order nonlinear susceptibilities associated with interlevel
   transitions have been calculated for ZnO quantum dots with different
   dielectric environments under high-frequency laser fields. Our
   calculations show that high nonlinear susceptibilities can be obtained
   when the quantum dots are embedded within a matrix having a large
   dielectric mismatch and a small energy band gap. These structures are
   more sensitive to the laser dressing effect due to an increased spread
   of the wave functions involved in the transitions. Moreover, they
   exhibit a third harmonic generation curve with two peaks of similar
   intensities for specific values of the laser parameters. On the basis of
   the latter effect, ZnO quantum dots could be potentially useful for
   designing efficient two-color photodetectors.}},
