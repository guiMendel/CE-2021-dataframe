@article{ WOS:000235033500060,
Author = {Lubitz, J},
Title = {{Health, technology, and medical care spending}},
Journal = {{HEALTH AFFAIRS}},
Year = {{2005}},
Volume = {{24}},
Number = {{6}},
Pages = {{W5R81-W5R85}},
Month = {{NOV-DEC}},
Abstract = {{The RAND Future Elderly Model illustrates important principles about the
   relation among medical technologies, health spending, and health. New
   technologies add to spending because the costs of the new technologies
   and the health care costs during the added years of life they bring
   outweigh reductions in annual spending from better health. Many
   technologies with a low cost per patient per year result in high
   aggregate costs because of an expanded population being treated.
   However, the jury is still out on whether a better health-risk profile
   among future sixty-five-year-olds could moderate health spending for the
   elderly.}},
