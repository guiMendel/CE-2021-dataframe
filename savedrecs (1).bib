@article{ WOS:000606523800012,
Author = {Farria, Aimee T. and Plummer, Joshua B. and Salinger, Andrew P. and
   Shen, Jianjun and Lin, Kevin and Lu, Yue and McBride, Kevin M. and
   Koutelou, Evangelia and Dent, Sharon Y. R.},
Title = {{Transcriptional Activation of MYC-Induced Genes by GCN5 Promotes B-cell
   Lymphomagenesis}},
Journal = {{CANCER RESEARCH}},
Year = {{2020}},
Volume = {{80}},
Number = {{24}},
Pages = {{5543-5553}},
Month = {{DEC 15}},
Abstract = {{Overexpression of the MYC oncoprotein is an initiating step in the
   formation of several cancers. MYC frequently recruits
   chromatin-modifying complexes to DNA to amplify the expression of
   cancer-promoting genes, including those regulating cell cyde,
   proliferation, and metabolism, yet the roles of specific modifiers in
   different cancer types are not well defined. Here, we show that GCN5 is
   an essential coactivator of cell-cycle gene expression driven by MYC
   overexpression and that deletion of Gcn5 delays or abrogates
   tumorigenesis in the E mu-Myr mouse model of B-cell lymphoma. Our
   results demonstrate that Gcn5 loss impacts both expression and
   downstream functions of Myc.
   Significance: Our results provide important proof of principle for Gcn5
   functions in formation and progression of Myc-driven cancers, suggesting
   that GCN5 may be a viable target for development of new cancer
   therapies.}},
