@article{ WOS:000364026900016,
Author = {Koh, Elizabeth and Cho, Young Hoan and Caleon, Imelda},
Title = {{Learning and Instruction Research in the Asia-Pacific Region from 2002
   to 2011: Examining Trends, Authors, and Institutions}},
Journal = {{ASIA-PACIFIC EDUCATION RESEARCHER}},
Year = {{2015}},
Volume = {{24}},
Number = {{4}},
Pages = {{719-730}},
Month = {{DEC}},
Abstract = {{Although research from the Asia-Pacific region has been increasing, much
   is unknown about research trends, authors, and institutions in the
   region, especially in relation to learning and instruction. It is
   important to have an understanding of current research to identify its
   strengths and weaknesses and to chart future research directions. It is
   also useful to identify prominent and productive authors and
   institutions in the Asia-Pacific to enhance international and regional
   collaboration between researchers. Twelve top international journals
   from 2002 to 2011 in the fields of educational psychology, educational
   technology, and learning sciences were examined. New bibliometric
   measures, such as the q (2) index, as well as co-authorship network
   analysis, and cluster analysis methods were utilized to provide insights
   into Asia-Pacific authors and institutions in the field of learning and
   instruction research.}},
