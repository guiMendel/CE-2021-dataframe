@article{ WOS:000362688800004,
Author = {Tovar, Elizabeth and Rayens, Mary Kay and Gokun, Yevgeniya and Clark,
   Michele},
Title = {{Mediators of adherence among adults with comorbid diabetes and
   depression: The role of self-efficacy and social support}},
Journal = {{JOURNAL OF HEALTH PSYCHOLOGY}},
Year = {{2015}},
Volume = {{20}},
Number = {{11}},
Pages = {{1405-1415}},
Month = {{NOV}},
Abstract = {{Depression and diabetes have been linked in a variety of ways, and the
   presence of depression in those with diabetes can negatively affect
   adherence to care recommendations. A sample of 201 participants with
   Type 2 Diabetes completed a cross-sectional survey that assessed
   depressive symptoms, adherence, self-efficacy, social support, and
   personal characteristics. Multiple regression analysis was used to test
   whether self-efficacy and social support mediate the relationship
   between depressive symptoms and adherence. The findings suggest complete
   mediation via self-efficacy and some types of social support.
   Intervening to bolster self-efficacy and social support may decrease the
   negative effect of depression on adherence.}},
