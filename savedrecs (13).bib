@article{ WOS:000365446600025,
Author = {Razzaghi, Parvin and Samavi, Shadrokh},
Title = {{Image retargeting using nonparametric semantic segmentation}},
Journal = {{MULTIMEDIA TOOLS AND APPLICATIONS}},
