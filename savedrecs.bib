@article{ WOS:000608049800002,
Author = {Chiu, P-Y and Lidsky, D. and Chuang, Y. and Su, Y-H and Li, J-Y and
   Harris, C. T. and Lu, T. M.},
Title = {{Post-growth modulation doping by ion implantation}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2020}},
Volume = {{117}},
Number = {{26}},
Month = {{DEC 28}},
Abstract = {{Modulation doping is a commonly adopted technique to create
   two-dimensional (2D) electrons or holes in semiconductor
   heterostructures. One constraint, however, is that the intentional
   dopants required for modulation doping are controlled and incorporated
   during the growth of heterostructures. Using undoped strained germanium
   quantum wells as the model material system, we show, in this work, that
   modulation doping can be achieved post-growth of heterostructures by ion
   implantation and dopant-activation anneals. The carrier density is
   controlled ex situ by varying the ion fluence and implant energy, and an
   empirical calibration curve is obtained. While the mobility of the
   resulting 2D holes is lower than that in undoped heterostructure
   field-effect transistors built using the same material, the achievable
   carrier density is significantly higher. Potential applications of this
   modulation-doping technique are discussed.}},
