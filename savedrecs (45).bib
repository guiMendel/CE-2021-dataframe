@article{ WOS:000089301900001,
Author = {Martini, IB and Ma, B and Da Ros, T and Helgeson, R and Wudl, F and
   Schwartz, BJ},
Title = {{Ultrafast competition between energy and charge transfer in a
   functionalized electron donor/fullerene derivative}},
Journal = {{CHEMICAL PHYSICS LETTERS}},
Year = {{2000}},
Volume = {{327}},
Number = {{5-6}},
Pages = {{253-262}},
Month = {{SEP 15}},
Abstract = {{The fact that fullerenes are good electron accepters has generated
   interest in covalently linked complexes between electron donors and
   fullerenes: photoinduced charge transfer in these dyads has great
   potential for use in photovoltaic devices. In this Letter, we
   investigate the excited-state properties of a
   perylene-fulleropyrrolidine dyad using steady-state and femtosecond
   time-resolved spectroscopies. Following photoexcitation, charge
   separation and energy transfer occur in nearly equal proportion; both
   processes take place on a sub-picosecond timescale. This suggests that
   competition between energy and charge transfer is common in these
   molecular systems, so that the best molecules for device applications
   are not necessarily those with the fastest electron transfer rates. (C)
   2000 Published by Elsevier Science B.V.}},
