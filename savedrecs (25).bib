@article{ WOS:000284233600075,
Author = {Babilotte, P. and Ruello, P. and Vaudel, G. and Pezeril, T. and Mounier,
   D. and Breteau, J. -M. and Gusev, V.},
Title = {{Picosecond acoustics in p-doped piezoelectric semiconductors}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2010}},
Volume = {{97}},
Number = {{17}},
Month = {{OCT 25}},
Abstract = {{We demonstrate by experiment and theoretical analysis that the presence
   of built-in electric fields near the (111) and ((1) over bar(1) over
   bar(1) over bar) surfaces of p-doped GaAs causes efficient generation of
   acoustic waves due to the laser-induced inverse piezoelectric effect. At
   the same time, the generation efficiency from the electron-hole-phonon
   deformation potential is shown to be reduced. The polarity of the
   acoustic pulse is inverted when changing the laser irradiated surface
   from (111) to ((1) over bar(1) over bar(1) over bar). The results have
   ramifications for optically controlled piezoelectric ultrasound
   transducers. (C) 2010 American Institute of Physics. {[}doi:
   10.1063/1.3501125]}},
