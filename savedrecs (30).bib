@article{ WOS:000234415300008,
Author = {Brown, ED},
Title = {{Conserved P-loop GTPases of unknown function in bacteria: an emerging
   and vital ensemble in bacterial physiology}},
Journal = {{BIOCHEMISTRY AND CELL BIOLOGY-BIOCHIMIE ET BIOLOGIE CELLULAIRE}},
Year = {{2005}},
Volume = {{83}},
Number = {{6}},
Pages = {{738-746}},
