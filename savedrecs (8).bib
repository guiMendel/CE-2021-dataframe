@article{ WOS:000591685200001,
Author = {Wulf, Dallas and Jaeckel, Felix and McCammon, Dan and Chervenak, James
   A. and Eckart, Megan E.},
Title = {{Optimal filtering of overlapped pulses in microcalorimeter data}},
Journal = {{JOURNAL OF APPLIED PHYSICS}},
Year = {{2020}},
Volume = {{128}},
Number = {{17}},
Month = {{NOV 7}},
Abstract = {{In this study, we present a general algorithm for processing
   microcalorimeter data with special applicability to data with high
   photon count rates. Conventional optimal filtering, which has become
   ubiquitous in microcalorimeter data processing, suffers from its
   inability to recover overlapped pulses without sacrificing spectral
   resolution. The technique presented here was developed to address this
   particular shortcoming and does so without imposing any assumptions
   beyond those made by the conventional technique. We demonstrate the
   performance of the algorithm with a dataset that approximately satisfies
   these assumptions and which is representative of a wide range of
   microcalorimeter applications. We also apply the technique to a highly
   non-linear dataset, examining the impact on performance in the limit
   that these assumptions break down.}},
