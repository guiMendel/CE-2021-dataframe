@article{ WOS:A1995RR74200003,
Author = {KUMANO, T and KUBOTA, T and TAKAHASHI, N},
Title = {{TEXTURE OF PRIMARY RECRYSTALLIZATION ON NONORIENTED ELECTRICAL STEEL
   SHEET WITH PHASE-TRANSFORMATION}},
