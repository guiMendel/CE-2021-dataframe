@article{ WOS:000233603400125,
Author = {Rangan, C and Murray, RJA},
Title = {{Theory of detection of angular momentum states in Rydberg atoms using
   half-cycle pulses}},
Journal = {{PHYSICAL REVIEW A}},
Year = {{2005}},
Volume = {{72}},
Number = {{5}},
Month = {{NOV}},
Abstract = {{The creation and detection of specific angular momentum states in
   Rydberg atoms is a significant advance in quantum-information
   processing. Detection of high angular momentum states in alkali-metal
   atoms is difficult because they are nearly-degenerate in energy. We
   present theoretical results on the detection of such states using
   terahertz-frequency half-cycle pulses (HCPs). Calculations are done for
   n=15,l=0,...,14 states in hydrogen and cesium using a grid-based
   pseudopotential method. Our results indicate that HCP redistribution is
   a reliable method of distinguishing angular momentum Rydberg states.}},
