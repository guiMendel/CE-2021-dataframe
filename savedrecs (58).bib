@article{ WOS:A1995RL05600010,
Author = {ABE, S and ITO, Y and DAVIES, E},
Title = {{ISOLATION OF A HEPARIN SENSITIVE, RIBOSOME SEDIMENTING FACTOR FROM THE
   CYTOSKELETON FRACTIONS OF PEAS AND CORN}},
Journal = {{PLANT PHYSIOLOGY AND BIOCHEMISTRY}},
Year = {{1995}},
Volume = {{33}},
Number = {{4}},
Pages = {{463-470}},
Month = {{JUL-AUG}},
Abstract = {{The cytoskeleton pellet from etiolated pea stems was resuspended in a
   buffer containing 400 mM potassium acetate and a neutral detergent,
   ultracentrifuged to remove ribosomes, and dialyzed against a
   cytoskeleton-stabilizing buffer. This caused the formation of a
   colloidal complex containing actin, tubulin, streptavidin-binding
   proteins and some other unidentified proteins. The complex enabled
   ribosomes from pea stem and wheat germ to sediment at somewhat lower g
   forces. Heparin, high concentrations of K+, and protease K strongly
   inhibited this ribosome-sedimenting activity. Treatment of the complex
   with 400 mM KOAc caused the release of a predominant 42.5 kDa protein
   and the simultaneous release of ribosome-sedimenting activity. We also
   isolated a similar protein having ribosome-sedimenting activity from the
   cytoskeleton of corn endosperm using the same method.}},
Publisher = {{GAUTHIER-VILLARS}},
Address = {{DEPT UNIV PROFESSIONNEL REVUES SCIENTIFIQUES TECHNIQUE 11 RUE GOSSIN,
   F-92543 MONTROUGE, FRANCE}},
