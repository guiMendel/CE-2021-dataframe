@article{ WOS:A1995RT43900025,
Author = {YOUNG, SJ and JAMES, AS and MATTHEWS, A},
Title = {{AN INVESTIGATION INTO THE THICKNESS AND COATING STRUCTURE UNIFORMITY OF
   ZIRCONIA FILMS ON FLAT SURFACES}},
