@article{ WOS:000279096500010,
Author = {Cahoy, Dexter O.},
Title = {{A bootstrap test for equality of variances}},
Journal = {{COMPUTATIONAL STATISTICS \& DATA ANALYSIS}},
Year = {{2010}},
Volume = {{54}},
Number = {{10}},
Pages = {{2306-2316}},
Month = {{OCT 1}},
Abstract = {{We introduce a bootstrap procedure to test the hypothesis H-0 that K + 1
   variances are homogeneous. The procedure uses a variance-based
   statistic, and is derived from a normal-theory test for equality of
   variances. The test equivalently expressed the hypothesis as H-0 : eta =
   (eta(1),...,eta(k+1))(T) = 0, where eta(i)'s are log contrasts of the
   population variances. A box-type acceptance region is constructed to
   test the hypothesis H-0. Simulation results indicated that our method is
   generally superior to the Shoemaker and Levene tests, and the
   bootstrapped version of the Levene test in controlling the Type I and
   Type II errors. (C) 2010 Elsevier By. All rights reserved.}},
