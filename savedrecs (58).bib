@article{ WOS:A1995RJ76800025,
Author = {AHMED, M and HASNAIN, K and NASIM, I and AYUB, H},
Title = {{MAGNETIC-PROPERTIES OF MARAGING STEELS IN RELATION TO NICKEL
   CONCENTRATION}},
