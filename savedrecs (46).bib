@article{ WOS:000089710300030,
Author = {Daimaruya, M and Kobayashi, H},
Title = {{Measurements of impact tensile strength of concrete and mortar using
   reflected tensile stress waves}},
Journal = {{JOURNAL DE PHYSIQUE IV}},
Year = {{2000}},
Volume = {{10}},
