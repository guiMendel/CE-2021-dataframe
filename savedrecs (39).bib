@article{ WOS:000231310900038,
Author = {Lung, MT and Lam, CH and Sander, LM},
Title = {{Island, pit, and groove formation in strained heteroepitaxy}},
Journal = {{PHYSICAL REVIEW LETTERS}},
Year = {{2005}},
Volume = {{95}},
Number = {{8}},
Month = {{AUG 19}},
Abstract = {{We study the morphological evolution of strained heteroepitaxial films
   using a kinetic Monte Carlo method in three dimensions. The elastic part
   of the problem uses a Green's function method. Isolated islands are
   observed under deposition conditions for deposition rates slow compared
   with intrinsic surface roughening rates. They are hemispherical and
   truncated conical for high and low temperature cases, respectively.
