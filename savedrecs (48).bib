@article{ WOS:000088413600006,
Author = {Deshpande, AH and Munshi, MM},
Title = {{Rhinocerebral mucormycosis diagnosis by aspiration cytology}},
Journal = {{DIAGNOSTIC CYTOPATHOLOGY}},
Year = {{2000}},
Volume = {{23}},
Number = {{2}},
Pages = {{97-100}},
Month = {{AUG}},
Abstract = {{Rhinocerebral mucormycosis is a rapidly progressing, often fatal fungal
   infection that occurs commonly in diabetics and immunocompromised
   individuals. We present 2 cases of rhinocerebral mucormycosis with a
   paranasal mass. One patient had art intracranial extension. Nasal
   scrapings and fine-needle aspiration cytology (FNAC) of the paranasal
   masses showed fungal hyphae morphologically resembling Mucor. Surgical
   material showed features of mucormycosis. FNAC and scrape smears can
   give a conclusive diagnosis of mucormycosis, and the patient can be
   treated with appropriate antifungal therapy and surgical debridement.
   Preoperative cytology is an effective technique to establish a diagnosis
   of mucormycosis and obviates the need for a preoperative biopsy. Diagn.
   Cytopathol. 2000;23:97-100. (C) 2000 Wiley-Liss, Inc.}},
