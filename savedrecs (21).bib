@article{ WOS:000286572200001,
Author = {Dieckhoener, C. and Lochner, S. and Lindenberger, D.},
Title = {{Simulating the Effects of European Natural Gas Infrastructure
   Developments}},
Journal = {{OIL GAS-EUROPEAN MAGAZINE}},
Year = {{2010}},
Volume = {{36}},
Number = {{4}},
Pages = {{174+}},
Month = {{DEC}},
Abstract = {{The projected increasing natural gas import dependency of the European
   Union requires investments in additional transport infrastructure
   capacities. To analyze where investments might be required, an analysis
   of potential gas flows in addition to a consideration of transport
   capacities within the European pipeline network is conducive to
   investigate the level of market integration and to identify bottlenecks
   within the future European gas market under different import
   infrastructure assumptions. With the European natural gas infrastructure
   model TIGER different scenarios have been simulated showing that the
   Western European gas market would be fairly well integrated in 2019
   whereas some severe bottlenecks are identified in Eastern Europe.
   However, major infrastructure projects in South-Eastern Europe would
   help to eliminate some of these bottlenecks.}},
