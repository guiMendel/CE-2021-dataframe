@article{ WOS:000364254800008,
Author = {Plank, J. and Sakai, E. and Miao, C. W. and Yu, C. and Hong, J. X.},
Title = {{Chemical admixtures - Chemistry, applications and their impact on
   concrete microstructure and durability}},
Journal = {{CEMENT AND CONCRETE RESEARCH}},
Year = {{2015}},
Volume = {{78}},
Number = {{A, SI}},
Pages = {{81-99}},
Month = {{DEC}},
Note = {{14th International Congress on the Chemistry of Cement (ICCC 2015),
   Beijing, PEOPLES R CHINA, OCT 13-16, 2015}},
Organization = {{China Building Mat Acad Chinese Ceram Soc}},
Abstract = {{An overview of current PCE compositions and synthesis methods is
   provided, followed by novel applications for PCEs including G-S-H-PCE
   nano-composites and a description of still unresolved challenges for PCE
   technology. In addition, the functionality of chemical admixtures in
   specific applications for low-carbon cements and concrete systems is
   discussed. The action mechanisms of retarders and the recycling system
   of sludge water by using retarder are introduced. Furthermore, the
   influence of fluoride ion and the effectiveness of PCE polymers in
   blended cements and the effect of non-adsorbed polymer are presented.
   And the impact of special interface modifying materials, of a refined
   pore structure and of chemical admixtures, particularly
   shrinkage-reducing agents, is described. The article concludes that more
   accurate quantitative micro-analytical methods and modeling tools will
   be needed to obtain a holistic understanding of factors affecting the
   microstructure of concrete, with the final goal of achieving a more
   durable concrete. (C) 2015 Elsevier Ltd. All rights reserved.}},
