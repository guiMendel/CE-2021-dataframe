@article{ WOS:000596240600001,
Author = {Van Lith, Theresa and Beerse, Megan and Smalley, Quinn},
Title = {{A qualitative inquiry comparing mindfulness-based art therapy versus
   neutral clay tasks as a proactive mental health solution for college
