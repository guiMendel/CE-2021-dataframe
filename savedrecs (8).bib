@article{ WOS:000590779900001,
Author = {Purser, Gregory L.},
Title = {{The Combined Effect of Functional Independence, Loneliness, and Social
   Engagement on Older Adult Drinking Levels}},
Journal = {{JOURNAL OF SOCIAL WORK PRACTICE IN THE ADDICTIONS}},
Abstract = {{Loneliness has long been associated with older adult alcohol use;
   however, the relationship between loneliness and alcohol use remains
   complex and poorly understood. The purpose of this study is to examine a
   possible causal pathway between functional independence, social
   engagement, loneliness, and drinking among older adults using structural
   equation modeling. A lower level of functional independence was a
   significant predictor of higher levels of loneliness and lower levels of
   social engagement. Additionally, higher feelings of loneliness predicted
   higher levels of drinking and higher levels of social engagement
   predicted lower levels of drinking.}},
