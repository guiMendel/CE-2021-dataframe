@article{ WOS:A1995TC91300010,
Author = {OKUMURA, Y},
Title = {{SO(10) GRAND UNIFIED THEORY IN NONCOMMUTATIVE DIFFERENTIAL GEOMETRY ON
   THE DISCRETE SPACE M(4)XZ(N)}},
Journal = {{PROGRESS OF THEORETICAL PHYSICS}},
Year = {{1995}},
Volume = {{94}},
Number = {{4}},
Pages = {{607-619}},
Month = {{OCT}},
Abstract = {{S0(10) grand unified theory (GUT) is reconstructed using the generalized
   differential calculus extended on the discrete space M(4)xZ(N), which
   was previously proposed by the present author. The standard model, SU(5)
   GUT and the left-right symmetric gauge theory have already been
   reconstructed according to this formalism in which the unified picture
   of the gauge field and Higgs field as the generalized connection in
   non-commutative geometry is realized. Four sheets (N=4) are prepared for
   the reconstruction of S0(10) GUT in which Higgs fields responsible for
   symmetry breakdowns are placed separately in the generalized gauge field
   on the discrete space. The seesaw mechanism is well incorporated to give
   the Majorana mass to the right-handed neutrino, and up and down quarks
   acquire different masses, respectively. The detailed investigation of
   the Higgs potential and interacting terms with complicated forms will be
   performed in the future.}},
