@article{ WOS:000227210000007,
Author = {Chang, SY and Hwang, HC and Park, S},
Title = {{A two-dimensional vector packing model for the efficient use of coil
   cassettes}},
Journal = {{COMPUTERS \& OPERATIONS RESEARCH}},
Year = {{2005}},
Volume = {{32}},
Number = {{8}},
Pages = {{2051-2058}},
Month = {{AUG}},
Abstract = {{We consider the problem of efficiently packing steel products, known as
   coils, into special containers, called cassettes for shipping. The
   objective is to minimize the number of cassettes used for packing all
   the given coils where each cassette has capacity limits on both total
   payload weight and size. We model this problem as a two-dimensional
   vector packing problem and propose a heuristic. We also analyze the
   worst-case performance of the proposed algorithm under a special
   condition which, in fact, holds for the particular real-world case that
   we handled. Our computational experiment with real production data shows
   that the proposed algorithm performs quite satisfactorily in practice.
