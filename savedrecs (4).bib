@article{ WOS:000610112600001,
Author = {Lyzinski, Vince and Sussman, Daniel L.},
Title = {{Matchability of heterogeneous networks pairs}},
Journal = {{INFORMATION AND INFERENCE-A JOURNAL OF THE IMA}},
Year = {{2020}},
Volume = {{9}},
Number = {{4}},
Pages = {{749-783}},
Month = {{DEC}},
Abstract = {{We consider the problem of graph matchability in non-identically
   distributed networks. In a general class of edge-independent networks,
   we demonstrate that graph matchability can be lost with high probability
   when matching the networks directly. We further demonstrate that under
   mild model assumptions, matchability is almost perfectly recovered by
   centering the networks using universal singular value thresholding
   before matching. These theoretical results are then demonstrated in both
   real and synthetic simulation settings. We also recover analogous
   core-matchability results in a very general core-junk network model,
   wherein some vertices do not correspond between the graph pair.}},
