@article{ WOS:A1995TE01700005,
Author = {ENOMOTO, EK and ANGUS, DL},
Title = {{AFRICAN-AMERICAN SCHOOL ATTENDANCE IN THE 19TH-CENTURY - EDUCATION IN A
   RURAL NORTHERN COMMUNITY, 1850-1880}},
Journal = {{JOURNAL OF NEGRO EDUCATION}},
Year = {{1995}},
Volume = {{64}},
Number = {{1}},
Pages = {{42-51}},
Month = {{WIN}},
Abstract = {{This study characterizes the school attendance of African Americans in a
   rural northern community prior to and following the Civil War by
   examining the settlement patterns and educational activities of African
   Americans in Cass County, Michigan, in the 19th century. Analysis of
   Census records, annual school district reports, and area maps for the
   years 1850 to 1880 yield comparative data on the township's White and
   African American families and their school-going children. The nature of
   schooling and the condition of educational facilities for Blacks and
   Whites during this era are also described and compared. These data
   reveal that before the War, a substantial difference in school
   attendance rates existed between the races in Calvin Township (24\% for
   African Americans compared to 71\% for Whites). However, African
   American school enrollments in the township increased during the 1860s
   and 1870s, eventually superseding that of Whites by the 1880s (77\% for
   African Americans compared to 67\% for Whites.)}},
