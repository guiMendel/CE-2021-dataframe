@article{ WOS:A1995TB92100039,
Author = {MOREAU, A},
Title = {{DETECTION OF ACOUSTIC 2ND-HARMONICS IN SOLIDS USING A HETERODYNE LASER
   INTERFEROMETER}},
Journal = {{JOURNAL OF THE ACOUSTICAL SOCIETY OF AMERICA}},
Year = {{1995}},
Volume = {{98}},
Number = {{5, 1}},
Pages = {{2745-2752}},
Month = {{NOV}},
Abstract = {{Acoustic second harmonic generation in solids is difficult to measure
   and often requires careful sample preparation. In this paper, a
   heterodyne interferometer modified to measure acoustic fundamental and
   second harmonics is presented. It requires relatively little sample
   preparation and it provides noncontact, wideband, absolute, and
   pointlike measurements. It is shown that the output of an ideal
   heterodyne interferometer does not contain any even harmonics of a
   single-frequency acoustic signal. Odd harmonics, however, are produced
   in calculable amounts. A detailed model of the interferometer is
   introduced to account for various sources of spurious second harmonics
   and the model is experimentally verified. Three methods of calibration
   for the absolute measurement of surface displacement are presented.
   Measurements of the nonlinearity parameter, beta, of a fused quartz
   sample are shown to agree with literature values.}},
