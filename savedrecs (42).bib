@article{ WOS:000165751500006,
Author = {Halhal, M and Naciri, M and Berbich, O and Benabdellah, W and Berraho, A},
Title = {{Osteoma of the orbit. A case report}},
Journal = {{JOURNAL FRANCAIS D OPHTALMOLOGIE}},
Year = {{2000}},
Volume = {{23}},
Number = {{9}},
Pages = {{888-891}},
Month = {{NOV}},
Abstract = {{Osteoma of the orbit is a benign tumor arising from the sinus. The tumor
   progresses slowly and can cause serious ophthalmological and
   neurosurgical complications.
   We present a case of osteoma of the orbit in a 24-year-old patient with
   an uneventful past history. The patient had a tumor of the left superior
   inner angle with a lateralized grade 1 exophthalmos and normal ocular
   function. Orbital osteoma was confirmed on the CT scan. ENT and
   digestive investigations were normal. Surgical excision was made via
   anterior medial acces.
   We analyzed the clinical, epidermological and therapeutic problems
   raised by osteomas of the orbit situated in the superior oblique muscle.}},
