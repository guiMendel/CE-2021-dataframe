@article{ WOS:000366288100022,
Author = {Nazarova, E. and Balchev, N. and Nenkov, K. and Buchkov, K. and
   Kovacheva, D. and Zahariev, A. and Fuchs, G.},
Title = {{Improvement of the superconducting properties of polycrystalline FeSe by
   silver addition}},
Journal = {{SUPERCONDUCTOR SCIENCE \& TECHNOLOGY}},
Year = {{2015}},
Volume = {{28}},
Number = {{12}},
Month = {{DEC}},
Abstract = {{We investigated the influence of different Ag additions (up to 10 wt\%)
   on the superconducting properties of FeSe0.94. The structural
   investigations (XRD and SEM) indicated that Ag is present in three
   different forms. Ag at grain boundaries supports the excellent
   intergrain connections and reduces Delta T to values smaller than 1 K at
   B = 0 and Delta T <= 2.74 K at B = 14 T. Ag insertion in the crystal
   lattice unit cell provides additional carriers and changes the
   electron-hole imbalance in FeSe0.94. This results in an increase in the
   magnetoresistive effect (MR) and critical temperature (T-c). Reacted Ag
   forms a small amount (similar to 1\%) of Ag2Se impurity phase, which may
   increase the pinning energy in comparison with that of the undoped
   sample. The enhanced upper critical field (B-c2) is also a result of the
   increased impurity scattering. Thus, unlike cuprates, Ag addition
   enhances the T-c, B-c2, pinning energy and MR making the properties of
   polycrystalline FeSe0.94 similar to those of single crystals.}},
Publisher = {{IOP PUBLISHING LTD}},
Address = {{TEMPLE CIRCUS, TEMPLE WAY, BRISTOL BS1 6BE, ENGLAND}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Nazarova, E (Corresponding Author), Bulgarian Acad Sci, Inst Solid State Phys, Blvd Lenin 72, BU-1784 Sofia, Bulgaria.
   Nazarova, E.; Balchev, N.; Buchkov, K.; Zahariev, A., Bulgarian Acad Sci, Inst Solid State Phys, BU-1784 Sofia, Bulgaria.
