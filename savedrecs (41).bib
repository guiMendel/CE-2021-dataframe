@article{ WOS:000174949300019,
Author = {Malbran, S},
Title = {{The development of pulse synchrony: An exploratory study of
   three-year-old children}},
Journal = {{BULLETIN OF THE COUNCIL FOR RESEARCH IN MUSIC EDUCATION}},
Year = {{2000}},
Number = {{147}},
Pages = {{109-115}},
Month = {{WIN}},
Note = {{18th ISME International Research Seminar, UNIV UTAH, SALT LAKE CITY,
   UTAH, JUL 08-14, 2000}},
Organization = {{ISME}},
Abstract = {{The study describes the performances of three-year-old children tapping
   the pulse of a musical fragment in moderate tempo. Tapping
   synchronically to musical stimuli was assessed in terms of beat
   correspondence, sustaining correspondence, tempo accuracy and
   regularity. The test consisted of tapping the pulse on a digital drum
   according to the beats of Prokofieff's `March 10' (from Music for
   children, op.65). Computer software was used to record the musical
   example, to present the stimuli during the administration of the test,
   and to record and measure the children's responses. Results illustrate
   some characteristics related to the relevance of the selected variables
   and provide some empirical evidence about rhythmic performance at this
   age. Delayed performance was the most common trait, and discontinuity in
   tapping and fluctuating in accuracy and regularity were also frequently
   observed. Anticipation and strict accuracy of the onset were rare. Some
   theoretical and practical questions are posed. Future developments are
   outlined.}},
