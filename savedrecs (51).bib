@article{ WOS:A1995TJ24700102,
Author = {TATSUMISAGO, M and KATO, S and MINAMI, T and KOWADA, Y},
Title = {{HIGH-TEMPERATURE STRUCTURE AND CRYSTALLIZATION KINETICS OF LI2O-TEO2
   GLASSES}},
