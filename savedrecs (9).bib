@article{ WOS:000600528700009,
Author = {Boehr, Christoph},
Title = {{Thomistic Foundation, Phenomenological Orientation and Anthropological
   Objective: Karol Wojtyla}},
Journal = {{INTERNATIONALE KATHOLISCHE ZEITSCHRIFT COMMUNIO}},
Year = {{2020}},
Number = {{6}},
Pages = {{661-670}},
Month = {{NOV-DEC}},
Abstract = {{In bridging between the philosophy of being following Thomism and the
   philosophy of consciousness following phenomenology Karol Wojtyla finds
   an independent synthesis, as a `philosophy of reflection', that has
   hardly been appreciated to this day, although it is an outstanding work
   of thought. He draws from the sources of Thomism and phenomenology two
   currents that have particularly influenced him, especially in dealing
   with Max Scheler an anthropology that attempts to understand man as a
   person decisively about his deed.}},
