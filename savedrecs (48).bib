@article{ WOS:000089067400002,
Author = {Hultmark, P and Karrholm, J and Stromberg, C and Herberts, P and Mose,
   CH and Malchau, H},
Title = {{Cemented first-time revisions of the femoral component - Prospective 7
   to 13 years' follow-up using second-generation and third-generation
   technique}},
Journal = {{JOURNAL OF ARTHROPLASTY}},
Year = {{2000}},
Volume = {{15}},
Number = {{5}},
Pages = {{551-561}},
Month = {{AUG}},
Abstract = {{In a prospective, consecutive study of first-time cemented femoral
   revisions, 109 hips were evaluated for a minimum of 7 years. There were
   87 Charnley (18 standard, 69 long), 21 Spectron (3 standard, 18 long),
   and 1 Brunswick long-stem arthroplasties. There was an increased risk
   for rerevision in younger patients (P = .0001) and with use of standard
   stems (P = .002). Factors of importance for development of mechanical
   failure were younger age (P = .0001), presence of more pronounced bone
   defects (P < .005), use of standard stems (P < .0005), and presence of
   radiolucencies on the postoperative radiographs (Gruen zones 4 and 6) (P
   < .01). Our results confirm that cemented femoral revisions are a
   durable option in elderly patients, when improved cementing techniques
   and long-stem prostheses are used.}},
