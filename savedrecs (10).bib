@article{ WOS:000366687200049,
Author = {Ye, Mengyuan and Yu, Yu and Chen, Guanyu and Luo, Yuchan and Zhang,
   Xinliang},
Title = {{On-chip WDM mode-division multiplexing interconnection with optional
   demodulation function}},
Journal = {{OPTICS EXPRESS}},
Year = {{2015}},
Volume = {{23}},
Number = {{25}},
Pages = {{32130-32138}},
Month = {{DEC 14}},
Abstract = {{We propose and fabricate a wavelength-division-multiplexing (WDM)
   compatible and multi-functional mode-division-multiplexing (MDM)
   integrated circuit, which can perform the mode conversion and
   multiplexing for the incoming multipath WDM signals, avoiding the
   wavelength conflict. An phase-to-intensity demodulation function can be
   optionally applied within the circuit while performing the mode
   multiplexing. For demonstration, 4 x 10 Gb/s non-return-to-zero
   differential phase shift keying (NRZ-DPSK) signals are successfully
   processed, with open and clear eye diagrams. Measured bit error ratio
   (BER) results show less than 1dB receive sensitivity variation for three
   modes and four wavelengths with demodulation. In the case without
   demodulation, the average power penalties at 4 wavelengths are -1.5, -3
   and -3.5 dB for TE0-TE0, TE0-TE1 and TE0-TE2 mode conversions,
   respectively. The proposed flexible scheme can be used at the interface
   of long-haul and on-chip communication systems. (C)2015 Optical Society
   of America}},
