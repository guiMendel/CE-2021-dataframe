@article{ WOS:A1995TC72700010,
Author = {FU, YP and CAO, Y and HERTZBERG, KM and GRIENINGER, G},
Title = {{FIBRINOGEN ALPHA-GENES - CONSERVATION OF BIPARTITE TRANSCRIPTS AND
   CARBOXY-TERMINAL-EXTENDED ALPHA-SUBUNITS IN VERTEBRATES}},
