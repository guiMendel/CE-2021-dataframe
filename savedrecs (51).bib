@article{ WOS:A1995TK40100022,
Author = {Maldonado, EP and Vieira, ND},
Title = {{Optimization of the active medium length in longitudinally pumped
   continuous-wave lasers}},
Journal = {{JOURNAL OF THE OPTICAL SOCIETY OF AMERICA B-OPTICAL PHYSICS}},
Year = {{1995}},
Volume = {{12}},
Number = {{12}},
Pages = {{2482-2485}},
Month = {{DEC}},
Abstract = {{Using a space-dependent rate-equation analysis and considering the gain
   medium within the confocal region, we developed a method to calculate
   the output power in longitudinally pumped cw lasers as a function of a
   single parameter: the active medium length. It is possible to find an
   optimum laser design for maximum output power for a specified laser
   medium and pump wavelength. Some well-known longitudinally pumped laser
   systems, Ti:Al2O3, KCl:Tl-0(1), and Cr:LiSAF, are discussed. In
   particular, we also analyzed the Nd:YLF laser under two pumping
   conditions: pumped by a diode laser at lambda p = 797 nm and pumped by
   an argon-ion laser at lambda p = 514.5 nm. For the diode-pumped
   configuration, efficiencies as high as 50\% can be expected. (C) 1995
   Optical Society of America}},
