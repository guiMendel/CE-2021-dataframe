@article{ WOS:000232417900009,
Author = {Freytag, A},
Title = {{The credibility of monetary reform - New evidence}},
Journal = {{PUBLIC CHOICE}},
Year = {{2005}},
Volume = {{124}},
Number = {{3-4}},
Pages = {{391-409}},
Month = {{SEP}},
Abstract = {{The history of monetary policy is characterised by crisis and reform.
   The paper is dedicated to an explanation of what makes monetary reforms
   successful. A cross-sectional econometric analysis is chosen to deal
   with this problem. It is based on a standard macroeconomic model of
   commitment and credibility. As the dependent variable, we calculate a
   post-reform inflation rate. The exogenous variables are the degree of
   legal commitment, the constraining influence of institutions and a new
   variable for ex-ante credibility of the reform. The paper allows for the
   conclusion that monetary commitment, the consideration of institutional
   constraints and abstinence from the money press are crucial for the
   success of a monetary reform.}},
