@article{ WOS:000231939500016,
Author = {Miyagawa, M and Anton, M and Wagner, B and Haubner, R and Souvatzoglou,
   M and Gansbacher, B and Schwaiger, M and Bengel, FM},
Title = {{Non-invasive imaging of cardiac transgene expression with PET:
   comparison of the human sodium/iodide symporter gene and HSV1-tk as the
   reporter gene}},
Journal = {{EUROPEAN JOURNAL OF NUCLEAR MEDICINE AND MOLECULAR IMAGING}},
Year = {{2005}},
Volume = {{32}},
Number = {{9}},
Pages = {{1108-1114}},
Month = {{SEP}},
Abstract = {{Purpose: Genes encoding for intracellular enzymes or transmembrane
   proteins are suitable as reporters, but may differ in terms of
   applicability for cardiac imaging. The aim of this study was to compare
   the human sodium iodide symporter gene (hNIS) with the herpes simplex
   virus type 1 thymidine kinase gene (HSV1-tk) as the reporter gene in
   non-invasive imaging of cardiac transgene expression with positron
   emission tomography ( PET).
   Methods: Equal doses of adenoviral vectors encoding for hNIS, wild-type
   HSV1-tk, mutant HSV1-sr39tk or LacZ as the control gene were directly
   injected into the myocardium of 34 animals. Two days later, dynamic PET
   was formed with a clinical scanner, using reporter probes specific for
   the respective reporter gene. Imaging with N-13-ammonia was also
   performed to identify cardiac regions of interest.
   Results: Kinetics differed significantly: I-124 as the probe for hNIS
   showed rapid early uptake, remaining stable overtime. Maximal myocardial
   concentration was 3.61 +/- 1.15\%. The nucleoside F-18-FHBG, as the
   specific probe for HSV1-sr39tk, showed increasing uptake over time, but
   maximal accumulation was significantly lower (1.45 +/- 0.54\%, P=
   0.0009). I-124-FIAU, as the specific probe for wild- type HSV1-tk,
   showed early uptake with subsequent washout. Maximal accumulation was
   lowest (0.63 +/- 0.23\%, P< 0.0001). Post-mortem analysis by
   autoradiography and gamma counting confirmed the in vivo data.
   Conclusion: Reporter genes encoding for transporter proteins such as
   hNIS are an attractive alternative to overexpression of intracellular
   enzymes for cardiac gene product imaging. hNIS yielded higher signal
   intensity and imaging contrast for PET than did HSV1-tk and HSV1-sr39tk.
   Therefore, this approach may be preferable for the future monitoring of
   cardiac gene- or cell-based therapy.}},
