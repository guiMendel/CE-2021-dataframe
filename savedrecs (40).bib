@article{ WOS:000166530900005,
Author = {Baeriswyl, D},
Title = {{Variational scheme for the Mott transition}},
Journal = {{FOUNDATIONS OF PHYSICS}},
Year = {{2000}},
Volume = {{30}},
Number = {{12}},
Pages = {{2033-2048}},
Month = {{DEC}},
Abstract = {{The Hubbard model is studied at half filling, using two complementary
   variational wave functions, the Gutzwiller ansatz for the metallic phase
   at small values of the interaction parameter U and its analog for the
   insulating phase at large values of U. The metallic phase is
   characterized by the Drude weight, which exhibits a jump at the critical
   point U-c. In the insulating phase the system behaves as a collection of
   dipoles which increase both in number and in size as U gets smaller. The
   two wave functions are able to describe the two asymptotic regimes
   (small and large values of U, respectively), but they can no longer be
   trusted in the region of the Mott transition (U approximate to U-c).
   More powerful methods are needed to study, for instance, the divergence
   of the electric susceptibility for U --> U-c.}},
