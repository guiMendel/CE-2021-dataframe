@article{ WOS:000284835700004,
Author = {Barengolts, S. A. and Mesyats, G. A. and Tsventoukh, M. M.},
Title = {{The ecton mechanism of unipolar arcing in magnetic confinement fusion
   devices}},
Journal = {{NUCLEAR FUSION}},
Year = {{2010}},
Volume = {{50}},
Number = {{12}},
Month = {{DEC}},
Abstract = {{It has been shown that the source of current and erosion plasma in a
   unipolar arc is explosive electron emission, which occurs as ejection of
   individual portions of electrons named ectons. This phenomenon is
   responsible for the numerous microcraters left by unipolar arcs on metal
   surfaces. An arc of this type is self-sustained due to the interaction
   of the erosion plasma with the electrode surface. The duration of an arc
   is determined by the conditions of its initiation: the higher the arc
   current, and hence, the number of cells in the spot formed on arc
   initiation, the longer the arc operation.}},
