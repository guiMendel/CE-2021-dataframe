Title = {{SYNTHESIS AND PROPERTIES OF NEW LIPOPHILIC MACROTRICYCLIC CYLINDRICAL
   CRYPTANDS}},
Journal = {{JOURNAL OF ORGANIC CHEMISTRY}},
Year = {{1995}},
Volume = {{60}},
