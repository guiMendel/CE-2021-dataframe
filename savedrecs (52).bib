Language = {{English}},
Affiliation = {{UNIV FLORIDA,DEPT MICROBIOL \& CELL SCI,GAINESVILLE,FL 32611.}},
DOI = {{10.1021/bp00036a003}},
ISSN = {{8756-7938}},
Keywords-Plus = {{RECOMBINANT ESCHERICHIA-COLI; ZYMOMONAS-MOBILIS GENES; CRYSTALLINE
