@article{ WOS:000234096800073,
Author = {Baldwin, E and Thomas, B and Lee, JW and Rabiei, A},
Title = {{Processing TiPdNi base thin-film shape memory alloys using ion beam
   assisted deposition}},
Journal = {{SURFACE \& COATINGS TECHNOLOGY}},
Year = {{2005}},
Volume = {{200}},
Number = {{7}},
Pages = {{2571-2579}},
Month = {{DEC 21}},
Abstract = {{A new generation of thin-film shape memory alloys (SMA) has been
   developed with less than 2 gin thickness for MEMS microactuator
   applications. In this study, thin-film TiPdNi SMA was processed using
   ion beam assisted deposition (IBAD) with and without in situ heat
   treatment. As-deposited films were found to be amorphous. Postdeposition
   annealing lead to bulk diffusion of palladium to the substrate interface
   and silicon into the bulk film, creating a porous cross section. Various
   forms of tensile failures were observed including decohesion and
   delamination as a result of postdeposition annealing. Effect of heating
   and cooling rates was studied as well as in situ heat treatment during
   deposition. Deposition using the MAD technique with in situ heat
   treatment was successful in producing fully martensitic films 1.5 gm
   thick and with reduced grain size and film defects, compared to the
   other sputter deposited films and EBAD deposited followed by
   postdeposition heat treatment. The effects of various processing
   parameters, and heat treatment conditions, on film properties have been
   studied. (c) 2004 Elsevier B.V. All rights reserved.}},
