@article{ WOS:000231550000021,
Author = {Shaporenko, A and Cyganik, P and Buck, M and Ulman, A and Zharnikov, A},
Title = {{Self-assembled monolayers of semifluorinated alkaneselenolates on noble
   metal substrates}},
