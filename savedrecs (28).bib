@article{ WOS:000282210600002,
Author = {Barragan, P. and Errea, L. F. and Mendez, L. and Rabadan, I.},
Title = {{Isotope effect in ion-atom collisions}},
Journal = {{PHYSICAL REVIEW A}},
Year = {{2010}},
Volume = {{82}},
Number = {{3}},
Month = {{SEP 28}},
Abstract = {{We explain the origin of the unusual large isotopic dependence found in
   charge-transfer cross sections for H( D, T)(+) + Be collisions. We show
   that this large effect appears in a semiclassical treatment as a
   consequence of the mass dependence of the charge-transfer transition
   probabilities, which is due to the variation of the radial velocity in
   the region where the nonadiabatic transitions take place. The
   possibility of finding such a large isotope effect in other collision
   systems is discussed.}},
