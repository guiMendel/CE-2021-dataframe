@article{ WOS:A1995TN25600009,
Author = {Ugbodaga, P and Oviasu, E and Ojogwu, LI},
Title = {{Serum lipoprotein pattern in nephrotic Nigerians}},
Journal = {{JOURNAL OF NEPHROLOGY}},
Year = {{1995}},
Volume = {{8}},
Number = {{6}},
Pages = {{331-333}},
Month = {{NOV-DEC}},
Abstract = {{A study to assess lipid abnormalities in nephrotic Nigerians was carried
   out on 30 consecutive nephrotic patients aged between 15 and 56 years,
   by measuring the serum total cholesterol, LDL-cholesterol
   VLDL-cholesterol and triglycerides, as well as HDL-cholesterol. For this
   study, 30 healthy subjects, aged and sex matched, served as controls.
   The serum total cholesterol, LDL-cholesterol, VLDL-cholesterol and
   tryglycerides in the nephrotic group were significantly higher than the
   controls (P<0.001), whereas the HDL-cholesterol was significantly lower
   (P<0.001). The HDL-cholesterol to total cholesterol ratio was also found
   to be significantly decreased (P<0.001).
   The data from this study suggest that despite the low incidence of
   atherosclerotic heart disease in Nigerians, patients with the nephrotic
   syndrome appear to be at high risk of developing this disorder,
   especially considering the well known relationship between the same and
   hyperlipidemia.}},
