@article{ WOS:000613015900019,
Author = {Davardoostmanesh, Maryam and Ahmadzadeh, Hossein and Goharshadi, Elaheh
   K. and Meshkini, Azadeh and Sistanipour, Elnaz},
Title = {{Electrophoretic extraction of highly monodispersed graphene quantum dots
   from widely polydispersed bulk and its cytotoxicity effect against
   cancer cells}},
Journal = {{MICROCHEMICAL JOURNAL}},
Year = {{2020}},
Volume = {{159}},
Month = {{DEC}},
Abstract = {{In this work, we used a rapid (less than 10 s), inexpensive, and
   efficient method for exteraction of graphene quantum dots, GQDs, after
   thermal pyrolysis preparation from citric acid. The electrophoretic size
   fractionation was performed following the citric acid pyrolysis without
   using dialysis membrane. The last separated fraction was GQDs with a
   narrow size distribution and average size of similar to 60 nm. The
   characterization techniques showed that the GQDs exhibit strong
   photoluminescence properties which are highly desired for several
   applications in various fields such as cancer therapy. The cytotoxicity
   assesments demonstrated that the GQDs have higher toxicity (IC50 of
   31.08 +/- 2.71 mu g/mL after 24 h) against human bone carcinoma cell
   line, Saos-2, compared to the bulk (IC50 of 65.06 +/- 5.41 mu g/mL after
