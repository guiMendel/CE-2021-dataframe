@article{ WOS:000367380800004,
Author = {Lebed, Richard F.},
Title = {{Do the P-c(+) pentaquarks have strange siblings?}},
Journal = {{PHYSICAL REVIEW D}},
Year = {{2015}},
Volume = {{92}},
Number = {{11}},
Month = {{DEC 29}},
Abstract = {{The recent LHCb discovery of states P-c(+)(4380), P-c(+)(4450), believed
   to be c (c) over bar uud pentaquark resonances, begs the question of
   whether equivalent states with c (c) over bar -> s (s) over bar exist,
   and how they might be produced. The precise analogue to the P-c(+)
   discovery channel Lambda(b) -> J/psi K- p, namely, Lambda(c) -> phi
   pi(0)pi, is feasible for this study and indeed is less Cabibbo
   suppressed, although its limited phase space suggests that evidence of a
   s (s) over bar uud resonance P-s(+) would be confined to the kinematic
   end-point region.}},
