@article{ WOS:000233905700006,
Author = {Boskovic, L and Altman, IS and Agranovski, IE and Braddock, RD and
   Myojo, T and Choi, M},
Title = {{Influence of particle shape on filtration processes}},
Journal = {{AEROSOL SCIENCE AND TECHNOLOGY}},
Year = {{2005}},
Volume = {{39}},
Number = {{12}},
Pages = {{1184-1190}},
Month = {{DEC}},
Abstract = {{The influence of particle shape on filtration processes was
   investigated. Two types of particles, including spherical polystyrene
   latex (PSL) and iron oxide, and perfect cubes of magnesium oxide, were
   examined. It was found that the removal efficiency of spherical
   particles on fibrous filters is very similar for corresponding sizes
   within the range of 50-300 nm, regardless of the fact that the densities
   of PSL and iron oxide differ by a factor of five. On the other hand, the
   removal efficiency of magnesium oxide cubic particles was measured, and
   found to be much lower than the removal efficiency for the
   aerodynamically similar spheres. Such disparity was ascribed to the
   different nature of the motion of the spherical and cubic particles
   along the fiber surface, following the initial collision. After touching
   the fiber surface and before coming to rest, the spherical particles
   could either slide or roll compared to the cubic ones, which could
   either slide or tumble. During tumbling, the area of contact between the
   particle and the fiber changes significantly, thus affecting the bounce
   probability, whilst for the spheres, the area of contact remains the
   same for any point of the particle trajectory. The extra probability of
   particle bounce by the cubes was derived fromthe experimental data. The
   particle kinetic energy was proposed to be responsible for the
   difference in removal efficiency of particles with alternative shapes,
   if all other process parameters remain the same. The increase in kinetic
