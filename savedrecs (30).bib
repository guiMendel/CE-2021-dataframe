@article{ WOS:000234118500023,
Author = {Heidenreich, BJ and Elliott, OT and Charney, ND and Virgien, KA and
   Bridges, AW and McKeon, MA and Peck, SK and Krause, D and Gordon, JE and
   Hunter, LR and Lamoreaux, SK},
Title = {{Limit on the electron electric dipole moment in gadolinium-iron garnet}},
Journal = {{PHYSICAL REVIEW LETTERS}},
Year = {{2005}},
Volume = {{95}},
Number = {{25}},
Month = {{DEC 16}},
Abstract = {{A new method for the detection of the electron electric dipole moment
   (EDM) using a solid is described. The method involves the measurement of
   a voltage induced across the solid by the alignment of the sample's
   magnetic dipoles in an applied magnetic field, H. A first application of
   the method to GdIG has resulted in a limit on the electron EDM of
   5x10(-24)e cm, which is a factor of 40 below the limit obtained from the
   only previous solid-state EDM experiment. The result is limited by the
   imperfect discrimination of an unexpectedly large voltage that is even
   upon the reversal of the sample magnetization.}},
