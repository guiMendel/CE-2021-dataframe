@article{ WOS:000285899400029,
Author = {Vallejo, A. and Fernandez, L. A. and Olivares, M. and Prieto, A. and
   Etxebarria, N. and Usobiaga, A. and Zuloaga, O.},
Title = {{Optimization of large volume injection-programmable temperature
   vaporization-gas chromatography-mass spectrometry analysis for the
   determination of estrogenic compounds in environmental samples}},
Journal = {{JOURNAL OF CHROMATOGRAPHY A}},
Year = {{2010}},
Volume = {{1217}},
Number = {{52}},
Pages = {{8327-8333}},
Month = {{DEC 24}},
Abstract = {{Large volume injection-programmable temperature vaporization-gas
   chromatography-mass spectrometry (LVI-PTV-GC-MS) was optimized for the
   determination of estrone (E1), 17 beta-estradiol (E2), 17 alpha-ethynyl
   estradiol (EE2), mestranol (MeEE2) and estriol (E3) for their
   determination in environmental samples (estuarine water, wastewater,
   fish bile and fish homogenate) after derivatization with 25 mu L (BSTFA
   + 1\%TMCS) and 125 mu L of pyridine. Experimental designs such as
   Plackett-Burman (PBD) and central composite designs (CCDs) were used to
   optimize the LVI-PTV variables (cryo-focusing temperature, vent time,
   vent flow, vent pressure, injection volume, purge flow to split vent,
   splitless time and injection speed). Optimized conditions were as
   follows: 45 mu L of n-hexane extract are injected at 60 degrees C and 6
   mu L/s with a vent flow and a vent pressure of 50 mL/min and 7.7 psi,
   respectively, during 5 min; then the split valve is closed for 1.5 min
   and afterwards the injector is cleaned at 100 mL/min before the next
   injection. The method was applied to the determination of estrogenic
   compounds in environmental samples such as estuarine water, wastewater,
   and fish homogenate and bile. Limits of detection (0.04-0.15 ng/L for
   water samples, 0.04-0.67 ng/g for fish bile and 0.1-7.5 ng for fish
   homogenate) obtained were approx. ten times lower than those obtained by
   means of a common split/splitless inlet. (C) 2010 Elsevier B.V. All
   rights reserved.}},
