@article{ WOS:000609196200011,
Author = {Quijano-Gomez, Estefania},
Title = {{Gentrification, social classes and new urban actors in Bogota Centre
   (Colombia)}},
Journal = {{CIUDAD Y TERRITORIO-ESTUDIOS TERRITORIALES-CYTET}},
Year = {{2020}},
Volume = {{52}},
Number = {{206}},
Pages = {{887-900}},
Month = {{WIN}},
Abstract = {{During the last years, and more specifically after the transition to the
   service economy, social classes and urban characters in cities have been
   transformed according to the new postmodern way of life from their
   residential trajectories and spatial practices. In this article we aim
   to take a look at this new class division encouraged by globalization
   and reinforced by contemporary urban processes such as gentrification,
   and also try to identify the urban actors who star in the current scene.
   The study focuses in the case of Bogota Centre, a place that has
   recently been involved in a socio-cultural and demographic restructuring
   that reflects the new residential consumption habits and the recent
   labor market trends. Through the analysis of the Multipurpose Survey
   2017, we establish the methodological basis for determining the
   localization patterns of these new social classes in the central urban
   space.}},
