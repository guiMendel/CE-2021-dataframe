@article{ WOS:000593706700011,
Author = {Yang, Xue and Wu, Yiman and Yu, Hongen and Sun, Min and Zheng, Jie and
   Li, Xingguo and Lin, Wei and Wu, Yong},
Title = {{A YH3 promoted palladium catalyst for reversible hydrogen storage of
   N-ethylcarbazole}},
Journal = {{INTERNATIONAL JOURNAL OF HYDROGEN ENERGY}},
Year = {{2020}},
Volume = {{45}},
Number = {{58}},
Pages = {{33657-33662}},
Month = {{NOV 27}},
Abstract = {{N-ethylcarbazole (NEC) is a promising liquid organic hydrogen carrier,
   while sluggish kinetics of hydrogen absorption and desorption restrict
   its application. To overcome that, a YH3 promoted palladium catalyst
   Pd/Al2O3-YH3 is developed in this work by taking advantage of the fast
   reversible hydrogenation and dehydrogenation kinetics of YH3. With the
   Pd/Al2O3-YH3, NEC can reversibly store 5.5 wt\% hydrogen in 4 h below
   473 K. The performance is the best compared to that of all the reported
   catalysts for both hydrogen absorption and desorption. Moreover, there
   are no gaseous impurities produced and no performance decay during three
   hydrogen storage cycles. The excellent performance derives from the
   intrinsic high catalytic activity of Pd/Al2O3 and the promoting effect
   of YH3 by providing a new hydrogen transfer path, making NEC more
   attractive for practical application. (C) 2020 Hydrogen Energy
   Publications LLC. Published by Elsevier Ltd. All rights reserved.}},
