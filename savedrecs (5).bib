@article{ WOS:000595190800016,
Author = {Thau, Matthew R. and Bhatraju, Pavan K.},
Title = {{Sub-Phenotypes of Acute Kidney Injury: Do We Have Progress for
   Personalizing Care?}},
Journal = {{NEPHRON}},
Year = {{2020}},
Volume = {{144}},
Number = {{12}},
Pages = {{677-679}},
Month = {{DEC}},
Note = {{10th Annual UAB-UCSD O'Brien Center Symposium / 25th International
   Conference on Advances in Critical Care Nephrology / 25th Annual
   Continuous Renal Replacement Therapy Conference, San Diego, CA, FEB
   24-27, 2020}},
Organization = {{Univ Alabama Birmingham; Univ Calif San Diego, OBrien Ctr Acute Kidney
   Injury Res}},
Abstract = {{Acute kidney injury (AKI) is the most common form of organ dysfunction
   occurring in patients admitted to the intensive care unit and
   contributes significantly to poor long-term outcomes. Despite this
   public health impact, no effective pharmacotherapy exists for AKI. One
   reason may be that heterogeneity is present within AKI as currently
   defined, thereby concealing unique pathophysiologic processes specific
   to certain AKI populations. Supporting this notion, we and others have
   shown that diversity within the AKI clinical syndrome exists, and the
   ``one-size-fits-all{''} approach by current diagnostic guidelines may
   not be ideal. A ``precision medicine{''} approach that exploits an
   individual's genetic, biologic, and clinical characteristics to identify
   AKI sub-phenotypes may overcome such limitations. Identification of AKI
   sub-phenotypes may address a critical unmet clinical need in AKI by (1)
   improving risk prognostication, (2) identifying novel pathophysiology,
   and (3) informing a patient's likelihood of responding to current
   therapeutics or establishing new therapeutic targets to prevent and
   treat AKI. This review discusses the current state of phenotyping AKI
   and future directions.}},
