@article{ WOS:000285530300008,
Author = {Kim, Minki and Lee, Jin Kyung and Hong, Young Jun and Hong, Seek-Il and
   Kang, Hye Jin and Chang, Yoon Hwan},
Title = {{Late-onset Neutropenia Following Rituximab Therapy as a Treatment of
   Diffuse Large B-cell Lymphoma: A Single Institution Study}},
Journal = {{KOREAN JOURNAL OF LABORATORY MEDICINE}},
Year = {{2010}},
Volume = {{30}},
Number = {{6}},
Pages = {{575-579}},
Month = {{DEC}},
Abstract = {{Background : Late-onset neutropenia (LON) following rituximab therapy
   has been reported in recent years. However, its incidence has not been
   reported in Korea. The aim of this study is to investigate the incidence
   of LON after rituximab therapy in Korean patients with diffuse large
   B-cell lymphoma (DLBCL).
   Methods : Ninety-eight cases of DLBCL treated with rituximab between
   2004 and 2008 were evaluated. We identified LON as defined by the
   neutrophil count of <1.5 x 10(9)/L without apparent cause after the
   recovery of neutrophil count following rituximab therapy. Bone marrow
   aspiration and biopsy specimens at the time of neutropenia were
   available for retrospective review in only 5 of the patients.
   Results : LON was observed in 15 (15.3\%) of the 98 patients. In the
   bone marrow specimens of the 5 patients, promyelocytes were relatively
   increased and the maturation index of the granulopoiesis was 2:1-3:1,
   which reflects maturation arrest.
   Conclusions : The incidence of LON following rituximab therapy was
   15.3\% in Korean patients with DLBCL. Although there are several
   hypotheses about the causative mechanisms of LON, we suggest that
   maturation arrest at the promyelocyte stage of granulopoiesis may be one
   of the mechanisms involved in the development of LON. (Korean J Lab Med
   2010;30:575-9)}},
