@article{ WOS:000577307800022,
Author = {Arbelaez, Lizeth and Geffroy, Pierre-Marie and Aimable, Anne and Fukuda,
   Koichiro and Bechade, Emilie},
Title = {{High-purity synthesis of La2SiO5 by solid-state reaction between La2O3
   and different characteristics of SiO2}},
Journal = {{CERAMICS INTERNATIONAL}},
Year = {{2020}},
Volume = {{46}},
Number = {{16, A}},
Pages = {{25546-25555}},
Month = {{NOV}},
Abstract = {{The solid-state reaction between lanthanum oxide and different
   characteristics of silica has been investigated to define new process
   conditions that favor the formation of La2SiO5 and to characterize the
   La2O3: SiO2 reaction mechanisms. Silica with different crystalline
   structures (amorphous or quartz-type silica) and various average
   particle sizes of similar to 36, 12, 1, and 0.02 mu m were used. The 1:1
   M ratio mixtures between La2O3 and different types of SiO2 were ground
   and subsequently heated at 1200 and 1500 degrees C for 10 h. The
   morphology of the ground mixtures, their thermal properties, and the
   phases obtained after heating were determined in this study. The phases
   identified after each solid-state synthesis were quantified using
   Rietveld refinements. The results obtained indicate that an amorphous
   silica with a small particle size (< 1 mu m) is more advantageous in the
   La2SiO5 powder synthesis. An alternative synthesis method of using an
   amorphous colloidal silica with a particle size of 0.02 mu m is
   proposed, and the purity level achieved at 1500 degrees C after 10 h is
   higher than 90 vol\%. A reaction mechanism was suggested that highlights
   the strong effect of the size ratio (La2O3/SiO2: 1 mu m/0.02 mu m) to
   improve the reactivity and La2SiO5 phase formation.}},
