@article{ WOS:000360864600010,
Author = {Roque-Malherbe, R. and Carballo, E. and Polanco, R. and Lugo, F. and
   Lozano, C.},
Title = {{Structure and adsorption properties of a porous cooper hexacyanoferrate
   polymorph}},
Journal = {{JOURNAL OF PHYSICS AND CHEMISTRY OF SOLIDS}},
Year = {{2015}},
Volume = {{86}},
Pages = {{65-73}},
Month = {{NOV}},
Abstract = {{The key questions addressed here were: the structure elucidation and the
   investigation of the adsorption space and framework expansion effect of
   a Cu(II) hexacyanoferrate (III) polymorph (labeled Cu-PBA-I). The
   structural analysis was performed with a broad set of characterization
   methods. Additionally, a low and high pressure carbon dioxide adsorption
   investigation was performed, assuming, to comprehend the adsorption
   experiments, that the adsorbent plus the adsorbed phase were a solid
   solution. We concluded: that the Cu-PBA-I presented the following
   composition, K1/4Cu(II){[}Fe(III)(CN)(6)](3/4)circle(1/4)nH(2)O,
   exhibited an antiferromagnetic behavior and displayed a thermally stable
   I4m2 space group lattice in the degassed state. Moreover, the low
   pressure adsorption study allowed the calculation of the micropore
   volume, W=0.09 cm(3)/g and the isosteric heat of adsorption, q(iso)=19
   kJ/mol; further, the high pressure adsorption data revealed an extremely
   high adsorption capacity owing to a framework expansion effect. Finally,
   the DRIFTS spectrum of adsorbed CO2 displayed peaks corresponding to
   carbon dioxide physically adsorbed and interacting with electron
   accepting Lewis acid sites. Hence, was produced an excellent adsorbent
   which combine porosity and anti-ferromagnetism, antagonist properties
   rarely found together. (C) 2015 Published by Elsevier Ltd.}},
