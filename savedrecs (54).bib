@article{ WOS:A1995TC88900008,
Author = {ROSENTHAL, J and WEIDENHAMMER, W and LOW, A},
Title = {{ANTIHYPERTENSIVE THERAPY WITH AN IMIDAZOLINE RECEPTOR AGONIST EFFECTS ON
   QUALITY-OF-LIFE OF PATIENTS}},
Journal = {{MEDIZINISCHE WELT}},
Year = {{1995}},
Volume = {{46}},
Number = {{10}},
Pages = {{498-505}},
Month = {{OCT}},
Abstract = {{A postmarketing surveillance study on patients with essential
   hypertension has been carried out to investigate the efficacy and
   tolerance of Moxonidine (daily dose: 0.2 mg up to 0.6 mg, duration: 12
   weeks). On a subset of 4132 patients there was a questionnaire regarding
