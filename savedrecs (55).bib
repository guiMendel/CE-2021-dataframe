@article{ WOS:A1995RV60900009,
Author = {JURGENSEN, C},
Title = {{MODELING OF NUTRIENT RELEASE FROM THE SEDIMENT IN A TIDAL INLET,
   KERTINGE-NOR, FUNEN, DENMARK}},
Journal = {{OPHELIA}},
Year = {{1995}},
Volume = {{42}},
Pages = {{163-178}},
