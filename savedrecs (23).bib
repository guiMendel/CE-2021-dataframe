@article{ WOS:000285767900054,
Author = {Cao, Juanjuan and McMechan, George},
Title = {{Multiple prediction and subtraction from apparent slowness relations in
   2D synthetic and field ocean-bottom cable data}},
Journal = {{GEOPHYSICS}},
Year = {{2010}},
Volume = {{75}},
Number = {{6}},
Pages = {{V89-V99}},
Month = {{NOV-DEC}},
Abstract = {{A target-oriented algorithm is developed for the prediction of multiples
   recorded on ocean-bottom cables by utilizing apparent slowness relations
   in common-source and common-receiver gathers. It is based on combining
   offsets and times of direct waves and primary reflections to predict
   multiples by matching apparent slownesses at all source and receiver
   locations; all higher-order multiples can be predicted by matching
   apparent slownesses alternately in common-source and common-receiver
   gathers. No knowledge of the subsurface velocity is required.
   Traveltimes of the direct waves and primary reflections need to be
   picked from common-source gathers. The subtraction of multiples involves
   flattening the predicted times of the multiple events, subtracting a
   local spatial average trace from each trace, within a fixed time window
   containing the wavelet of the multiple, and then shifting the data back
   to its original times. Tests of synthetic and field data indicate that
   the proposed method predicts multiples very well and removes them from
   seismic data efficiently with negligible affect on the primary
   reflections, as long as the primary and multiple reflections do not
   overlap in time and slowness over substantial windows in the domain in
   which the removal is done.}},
