@article{ WOS:000089356500011,
Author = {Lambertucci, JR and Serufo, JC and Gerspacher-Lara, R and Rayes, AAM and
   Teixeira, R and Nobre, V and Antunes, CMF},
Title = {{Schistosoma mansoni: assessment of morbidity before and after control}},
