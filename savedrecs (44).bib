@article{ WOS:000167599900004,
Author = {Frey, C and Roberts, NE},
Title = {{Sinus tarsi dysfunction: What is it and how is it treated?}},
Journal = {{SPORTS MEDICINE AND ARTHROSCOPY REVIEW}},
Year = {{2000}},
Volume = {{8}},
Number = {{4}},
Pages = {{336-342}},
Month = {{OCT-DEC}},
Abstract = {{Sinus tarsi syndrome has been described as pain over the sinus tarsi,
   perceived hindfoot instability, and pain relief after injection of local
   anesthetics. This syndrome is really a diagnosis of exclusion. A more
   appropriate term is sinus tarsi dysfunction. Patients have the same
   symptoms, but it can be attributed to one of many differential diagnoses
   that include fractures, ligament injuries, and coalitions. Interosseous
   ligament tears are the most common cause of sinus tarsi dysfunction.
   With the advancement of imaging techniques, small joint arthroscopy, and
   clinical experience, an exact diagnosis can be made and appropriated
   treatment can be implemented.}},
