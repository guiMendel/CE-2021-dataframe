@article{ WOS:000232133100014,
Author = {Malec, E and Wylezek, G},
Title = {{The Huygens principle and cosmological gravitational waves in the
   Regge-Wheeler gauge}},
Journal = {{CLASSICAL AND QUANTUM GRAVITY}},
Year = {{2005}},
Volume = {{22}},
Number = {{17}},
Pages = {{3549-3553}},
Month = {{SEP 7}},
Abstract = {{We study the propagation of axial gravitational waves in Friedmann
   universes. The evolution equation is obtained in the Regge-Wheeler
   gauge. The gravitational waves obey the Huygens principle in the
   radiation-dominated era, but in the matter-dominated universe their
   propagation depends on their wavelengths, with the scale fixed
   essentially by the Hubble radius. Short waves practically satisfy the
   Huygens principle while long waves can backscatter off the curvature of
   a spacetime.}},
