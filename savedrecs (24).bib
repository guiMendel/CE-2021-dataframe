@article{ WOS:000284962600071,
Author = {Chen, Chuan-Hung and Geng, Chao-Qiang and Wang, Wei},
Title = {{Z-mediated charge and CP asymmetries and FCNCs in Bd,s processes}},
Journal = {{JOURNAL OF HIGH ENERGY PHYSICS}},
Year = {{2010}},
Number = {{11}},
Month = {{NOV}},
Abstract = {{We show model-independently that the negative like-sign charge asymmetry
   (-A(sl)(b)) is less than 3.16 x 10(-3) when the constraints from the B-q
   - (B) over bar (q) mixings and the time-dependent CP asymmetries (CPAs)
   for Bq -> J/Psi M-q with M-q = K, phi and q = d, s are taken into
   account. Although the result is smaller than the measured value by the
   DO Collaboration at Fermilab, there is still plenty of room to have new
   physics, which is sensitive to new CP violating effects, as the standard
   model (SM) prediction is (2.3(-0.5)(+0.6)) x 10(-4). To illustrate the
   potential large broken vertical bar A(sl)(b)broken vertical bar, we show
   the influence of new SU(2) L singlet exotic quarks in the vector-like
   quark model, where the Z-mediated flavor changing neutral currents
   (FCNCs) are generated at tree level. In particular, we demonstrate that
   (a) the like-sign charge asymmetry could be enhanced by a factor of two
   in magnitude; (b) the CPA of sin 2 beta(J/Psi phi)(s) s could reach to
   -15\%; (c) the CPA of sin 2 beta(phi KS) could be higher than sin 2 beta
   J/Psi(KS) when vertical bar A(sl)(b)vertical bar is larger than the SM
   prediction; and (d) the branching ratio for B-s -> mu(+)mu(-) could be
   as large as 0.6 x 10(-8).}},
