@article{ WOS:A1995RX07000003,
Author = {KEGL, M and BUTINAR, BJ and OBLAK, MM},
Title = {{SHAPE OPTIMAL-DESIGN OF ELASTIC PLANAR FRAMES WITH NONLINEAR RESPONSE}},
