@article{ WOS:000366066500031,
Author = {Hiruta, Akihiro and Ishizaki, Osamu and Tuchinaga, Kazuhiro and Snyder,
   Glen T. and Matsumoto, Ryo},
Title = {{Influence of the carbon isotopic composition of methane and the
   proportion of methane-derived bicarbonate on the C-13/C-12 ratio of
   dissolved inorganic carbon at the sulfate-methane transition in the
   Joetsu Basin area, eastern margin of the Sea of Japan}},
Journal = {{MARINE AND PETROLEUM GEOLOGY}},
Year = {{2015}},
Volume = {{67}},
Pages = {{468-480}},
Month = {{NOV}},
Abstract = {{The geochemistry of interstitial waters associated with active marine
   hydrocarbon seepage provides insight into the source and ongoing
   dynamics of such systems. In this investigation of interstitial waters
   obtained from sediments in a gas hydrate region of the Sea of Japan, we
   compare carbon isotopic composition (delta C-13) of dissolved inorganic
   carbon (DIC) at the sulfate-methane transition (SMT) with delta C-13 of
   deeply sourced methane (as inferred from the deepest samples obtained
   beneath the SMT). Based on this comparison, we distinguish two causes
   for lower values of delta C-13(DIC) at the SMT. In Group A (17 piston
   cores), delta C-13(DIC) values at the SMT (-30.0 to -9.7 parts per
   thousand VPDB) correlate positively with delta C-13 of deep methane
   (-90.7 to -61.4 parts per thousand VPDB). Lowering of these delta
   C-13(DIC) values was independent of methane flux, which controls the
   generation rate of methane-derived bicarbonate by anaerobic oxidation of
   methane, owing to the influence of a high alkalinity flux from depth. In
   the Group B core samples, delta C-13(DIC) at the SMT (-41.6 to - 14.2
   parts per thousand VPDB) relative to delta C-13 of deep methane (-68.2
   to -40.5 parts per thousand VPDB) is lower than that of Group A,
   implying a greater contribution of methane-derived HCO3-. With only two
   exceptions, the Group B core sites were located at areas of active gas
   seepage. The admixture of methane from migrating methane bubbles with
   deep methane in the interstitial waters of sediments has the potential
   to significantly increase the overall methane flux at gas seep sites.
   Although the admixture may reflect higher delta C-13 values of deep
   methane, the increase in methane flux results in a higher proportion of
   methane-derived HCO3- and lower delta C-13(DIC) at the SMT. Recovery of
   two group B cores adjacent a dormant pockmark suggests that the
   depression with seismic blanking beneath it, similar to that of the gas
   seep sites, is a previous gas seepage site. (C) 2015 Elsevier Ltd. All
   rights reserved.}},
