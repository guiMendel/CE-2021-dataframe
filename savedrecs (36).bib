@article{ WOS:000232257800007,
Author = {Schuetz, YB and Naik, A and Guy, RH and Vuaridel, E and Kalia, YN},
Title = {{Transdermal lontophoretic delivery of triptorelin in vitro}},
Journal = {{JOURNAL OF PHARMACEUTICAL SCIENCES}},
Year = {{2005}},
Volume = {{94}},
Number = {{10}},
Pages = {{2175-2182}},
Month = {{OCT}},
Abstract = {{The feasibility of delivering triptorelin ({[}D-Trp(6)]LHRH) by
   transdermal iontophoresis was evaluated in vitro. Peptide
   electrotransport at different current densities and donor concentrations
   was measured across porcine ear skin. The concomitant delivery of an
   electroosmotic marker enabled calculation of the respective
   contributions of electromigration (EM) and electroosmosis (EO) to
   iontophoretic delivery. At a given concentration (3 mM), a threefold
   increase in current density produced a corresponding increase in the
   cumulative amount of peptide present in the receptor compartment.
   Conversely, doubling the concentration to 6 mM produced a twofold
   reduction in the amount of peptide delivered, partly due to a
   concentration-dependent inhibition of EO. EM was revealed to be the
   predominant transport mechanism, accounting for 80\% of overall
   delivery. Finally, despite the inhibition of EO, the results indicate
   that application of an iontophoretic current of 0.8 mA over a relatively
   small contact area (4 cm(2)) would provide a delivery rate of 36 mu g/h,
   largely sufficient for therapeutic requirements. (c) 2005 Wiley-Liss,
   Inc.}},
