@article{ WOS:000366098000004,
Author = {Anacleto, M. A. and Salako, I. G. and Brito, F. A. and Passos, E.},
Title = {{Analogue Aharonov-Bohm effect in neo-Newtonian theory}},
Journal = {{PHYSICAL REVIEW D}},
Year = {{2015}},
Volume = {{92}},
Number = {{12}},
Month = {{DEC 10}},
Abstract = {{We address the issues of the scattering of massless planar scalar waves
   by an acoustic black hole in neo-Newtonian hydrodynamics. We then
   compute the differential cross section through the use of the partial
   wave approach in the neo-Newtonian theory which is a modification of the
   usual Newtonian theory that correctly incorporates the effects of
   pressure. We mainly show that the scattering of planar waves leads to a
   modified analogue Aharonov-Bohm effect due to a nontrivial response of
   the parameters defining the equation of state.}},
