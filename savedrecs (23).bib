@article{ WOS:000283573100019,
Author = {Koziol, James A.},
Title = {{The rank product method with two samples}},
Journal = {{FEBS LETTERS}},
Year = {{2010}},
Volume = {{584}},
Number = {{21}},
Pages = {{4481-4484}},
Month = {{NOV 5}},
Abstract = {{Breitling et al. (2004) {[}1] introduced a statistical technique, the
   rank product method, for detecting differentially regulated genes in
   replicated microarray experiments. The technique has achieved widespread
   acceptance and is now used more broadly, in such diverse fields as RNAi
   analysis, proteomics, and machine learning. In this note, we extend the
