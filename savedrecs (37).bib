@article{ WOS:000232447200002,
Author = {Zvyagin, AA},
Title = {{Fluctuators and qubits: coherent quantum oscillations}},
Journal = {{JOURNAL OF PHYSICS-CONDENSED MATTER}},
Year = {{2005}},
Volume = {{17}},
Number = {{36}},
Pages = {{L385-L391}},
Month = {{SEP 14}},
Abstract = {{Recent experiments observed coherent quantum oscillations for a system
   of Josephson phase qubits and critical-current fluctuators, which
   demonstrated the ability to measure two-qubit interactions. Here we
   propose a new scheme of manipulations with a system consisting of
   coupled qubits and fluctuators. In such a scheme one can, on the one
   hand, observe quantum coherent Rabi-like oscillations. On the other
   hand, the scheme permits us to distinguish responses from qubits with
   different eigenvalues. Also, it will be useful for testing the system
   and extracting its parameters.}},
