@article{ WOS:000166369600011,
Author = {Pick, S and Givaudan, M and Brown, J},
Title = {{Quietly working for school-based sexuality education in Mexico:
   Strategies for advocacy}},
Journal = {{REPRODUCTIVE HEALTH MATTERS}},
Year = {{2000}},
Volume = {{8}},
Number = {{16}},
Pages = {{92-102}},
Month = {{NOV}},
Abstract = {{In the past decade, there have been dramatic changes in Mexico in public
   opinion and support for sexuality education. The growing recognition
   that young people are sexually active before marriage and need
   information in order to make responsible choices is the result of
