@article{ WOS:A1995TC96200003,
Author = {MASTROBATTISTA, JM and DOLLE, P and BLANTON, SH and NORTHRUP, H},
Title = {{EVALUATION OF CANDIDATE GENES FOR FAMILIAL BRACHYDACTYLY}},
Journal = {{JOURNAL OF MEDICAL GENETICS}},
Year = {{1995}},
Volume = {{32}},
Number = {{11}},
Pages = {{851-854}},
Month = {{NOV}},
Abstract = {{Type Al brachydactyly in humans is a recognisable syndrome characterised
   by shortening of the middle phalanx of all digits with occasional fusion
   of the middle and terminal phalanges. The purpose of this study was to
   evaluate candidate genes for type Al brachydactyly in two families with
   multiple affected members. Several classes of genes have been implicated
   in the control of distal limb development including homeobox containing
   genes (MSX1, MSX2), some members of the homeobox gene family, and genes
   encoding growth factors of the FGF, TGF, and PDGF families. Homeobox
   (Hox) genes are a family of developmental control genes activated early
   in embryogenesis that encode positional information along the
   anterior-posterior body axis and specify distinct spatial domains within
   developing limbs. Growth factor genes can regulate the proliferation and
   differentiation of various embryonic structures including limb buds and
   have been shown to influence Hox gene expression. Candidate genes HOXD,
   MSX1, MSX2, FGF-1, and FGF-2 were excluded in one family. The
   brachydactyly type Al gene or locus was not found in either of the two
   families studied.}},
