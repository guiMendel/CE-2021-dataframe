@article{ WOS:000089542100045,
Author = {Uehara, Y and Kuwahara, M and Ushioda, S},
Title = {{Comparison of light-emission efficiencies from
   Si-metal-oxide-semiconductor junctions and from Si in scanning tunneling
   microscopy}},
Journal = {{JAPANESE JOURNAL OF APPLIED PHYSICS PART 1-REGULAR PAPERS SHORT NOTES \&
   REVIEW PAPERS}},
Year = {{2000}},
Volume = {{39}},
Number = {{8}},
Pages = {{4904-4909}},
Month = {{AUG}},
Abstract = {{We have investigated the mechanism of scanning tunneling microscope
   (STM) visible light emission from n-type Si(100). The current
   fluctuation theory that explained the light emission characteristics of
   the Si-metal-oxide-semiconductor (Si-MOS) tunnel junction is applied to
   the STM light emission from n-type Si(100). This theory reproduces the
   observed STM light emission spectra, and predicts that the light
   emission efficiency from the STM geometry is similar to 1.5 x 10(4)
   times greater than that from the Si-MOS junctions. This prediction is
   consistent with our experimental result. Experimentally, the light
   emission intensity from the Si surface under the STM is comparable to
   that from the Si-MOS junction, although the tunneling current in the STM
   is 10(-3) of the current in the Si-MOS junction. That is, the light
   emission efficiency from the STM is at least 10(3) times greater than
   that from the Si-MOS.}},
