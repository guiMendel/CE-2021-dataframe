@article{ WOS:000165879500005,
Author = {Ma, YQ},
Title = {{Wetting-driven structure formation of a binary mixture in the presence
   of a mobile particle pinning potential}},
Journal = {{PHYSICAL REVIEW E}},
Year = {{2000}},
Volume = {{62}},
Number = {{6, B}},
Pages = {{8207-8211}},
Month = {{DEC}},
Abstract = {{We investigate the pattern formation on the solid substrate of
   phase-separating films containing mobile wetting particles with a
   preferential attraction for one component of the mixtures. The presence
   of mobile particles under the surface-particle interaction modulation
   breaks the isotropy of the bulk phase-separating process, leading to the
   formation of orientational structure due to the interplay between phase
   separation and wetting particle ordering under a modulated pinning
   potential at the late stage. Simulations suggest that the
   phase-separation morphology can be changed through the adjustment of the
   wettable-phase-particle interaction and the surface-particle
