@article{ WOS:000286634100011,
Author = {Derbin, A. V. and Fomenko, K. A. and Borexino Collaboration},
Title = {{New Experimental Limits on the Probabilities of Pauli-Forbidden
   Transitions in the C-12 Nucleus from Data Obtained with the Borexino
   Detector}},
Journal = {{PHYSICS OF ATOMIC NUCLEI}},
Year = {{2010}},
Volume = {{73}},
Number = {{12}},
Pages = {{2064-2073}},
Month = {{DEC}},
Abstract = {{The Pauli exclusion principle was tested for nucleons in the C-12
   nucleus by using data from the Borexino detector. The approach used
   consisted in seeking photons, neutrons, and protons, as well as
   electrons and positrons, emitted in the Pauli-forbidden transitions of
   nucleons from the 1P(3/2) shell to the filled 1S(1/2) shell. Owing to a
   uniquely low background level in the Borexino detector and its large
   mass, the currently most stringent experimental limits were obtained for
   the probabilities and relative intensities of Pauli-forbidden
   transitions for the electromagnetic, strong, and weak channels.}},
