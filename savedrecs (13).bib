@article{ WOS:000372047700002,
Author = {Moscoso, Claudia and Matusiak, Barbara and Svensson, U. Peter},
Title = {{IMPACT OF WINDOW SIZE AND ROOM REFLECTANCE ON THE PERCEIVED QUALITY OF A
   ROOM}},
Journal = {{JOURNAL OF ARCHITECTURAL AND PLANNING RESEARCH}},
Year = {{2015}},
Volume = {{32}},
Number = {{4}},
Pages = {{294-306}},
Month = {{WIN}},
Abstract = {{This paper reports the findings from a full-scale laboratory experiment
   about the assessment of architectural quality attributes in luminous
   environments. The presented data are part of the findings from a larger
   methodological experiment. This paper discusses the impact of window
   size and room reflectance on the perceived quality of a small room and
   the correlations between various architectural quality attributes.
   Paired attributes and semantic differential scale were used to assess
   nine attributes. Twenty-six participants evaluated six different
   scenarios: two similar rooms with different surface reflectances
   (created using achromatic colored surfaces: white and black) and three
   different window sizes. Results from the two-way, repeated-measures
   analysis of variance suggested that, separately, window size and room
   reflectance had statistically significant effects on the evaluation of
   the attributes. However, despite the considerable difference in absorbed
   light between the white room and the black room, little support was
   found in the data for the interaction of window size and room
   reflectance together Subsequent analyses of mean ratings suggested that
   window size was more important than the color of the room for positive
   evaluations of certain, attributes. Additionally, the study found very
   strong, positive correlations between the attributes.}},
