@article{ WOS:A1995TK34100005,
Author = {GIELEN, CCAM and VANBOLHUIS, BM and THEEUWEN, M},
Title = {{ON THE CONTROL OF BIOLOGICALLY AND KINEMATICALLY REDUNDANT MANIPULATORS}},
Journal = {{HUMAN MOVEMENT SCIENCE}},
