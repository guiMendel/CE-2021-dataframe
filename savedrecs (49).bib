@article{ WOS:000088880000010,
Author = {Yamada, H and Kuwahara, Y and Takamatsu, Y and Hayase, T},
Title = {{A new sensitive determination method of estradiol in plasma using
   peroxyoxalate ester chemiluminescence combined with an HPLC system}},
Journal = {{BIOMEDICAL CHROMATOGRAPHY}},
Year = {{2000}},
Volume = {{14}},
Number = {{5}},
Pages = {{333-337}},
Month = {{AUG}},
Abstract = {{A new sensitive determination method of estradiol in a plasma sample
   using peroxyoxalate ester chemiluminescence was developed. Estradiol,
   which was extracted by liquid-liquid extraction using ethyl acetate from
   plasma, was derivatized with dansyl-chloride (DNS-Cl) and separated by
