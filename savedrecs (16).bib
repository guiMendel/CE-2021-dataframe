@article{ WOS:000365308400009,
Author = {Kakehashi, Rie and Tokai, Naoji and Maeda, Hiroshi},
Title = {{Effects of the spacer length on the aggregate formation and the gelation
   of alkylamide amine oxides}},
Journal = {{COLLOID AND POLYMER SCIENCE}},
Year = {{2015}},
Volume = {{293}},
Number = {{11, SI}},
Pages = {{3157-3165}},
Month = {{NOV}},
Abstract = {{Three kinds of alkylamide amine oxides having one, two, and three amides
   were synthesized, and their solution properties such as the surface
   tension and the viscosity were examined. The effects of the spacer group
   length n between amide and dimethyamine oxide group as well as the
   number of amide groups were examined with respect to the aggregation
   behavior. For short spacer lengths, amide groups join the hydrophilic
   head group, while for long spacer lengths, amide groups and a part of
   the spacer groups are suggested to reside in the micelle palisade layer.
   In a series of amphiphile having three amide groups, gels formed at a
   high concentration of 50 mM were tentatively grouped into two types: A
   and B, depending on the spacer length n. For Gel-A, the gelling
   temperature T (gel) increased linearly with n but it was scarcely
   affected by the protonation of amine oxide (AO) group. For Gel-A,
   fiber-like aggregates were observed in cryo-TEM images. For Gel-B,
   values of T (gel) were higher by 18 +/- 2 K than those of Gel-A for a
   given n. Cryo-TEM images of Gel-B showed wide ribbon-like aggregates. On
   ionization of AO group, Gel-B transformed into Gel-A.}},
