@article{ WOS:000287071100005,
Author = {Chen, Joseph C. and Kirby, E. Daniel},
Title = {{Comparing Classical DOE and Taguchi Parameter Design in a Learning
   Environment}},
Journal = {{JOURNAL OF ENGINEERING TECHNOLOGY}},
Year = {{2010}},
Volume = {{27}},
Number = {{2}},
Pages = {{42-52}},
Month = {{FAL}},
Abstract = {{Proper application of Design of experiments (DOE) methodologies is a
   critical component of Six Sigma and other quality management programs
   However, classical DOE methodologies are typically considered loo
   cumbersome or difficult, and many quality engineers resort to guesswork
   and trial-and-error when optimizing a design or process. It is therefore
   imperative that engineering and technology programs that train students
   to be competent in Six Sigma provide training in It DOE methodologies,
   including not only classical styles but alternatives such as Taguchi
   Parameter Design (TPD). Students need to understand the application of
   various DOE methodologies in a Six Sigma program, including which
   specific methodology is ideal in a given situation. This stiffly
   outlines the differences between classical DOE and TPD in optimization.
   Students learning Six Sigma techniques receive an optimization problem
   and split into two project teams, one using a full factorial design and
   one using TPD. The results support the literature indicating that TPD is
   a very capable and much more efficient method for optimization than
   traditional DOE full factorial designs.}},
