@article{ WOS:000363268800001,
Author = {Duke, Angela N. and Johnson, Matthew W. and Reissig, Chad J. and
   Griffiths, Roland R.},
Title = {{Nicotine reinforcement in never-smokers}},
Journal = {{PSYCHOPHARMACOLOGY}},
Year = {{2015}},
Volume = {{232}},
Number = {{23}},
Pages = {{4243-4252}},
Month = {{DEC}},
Abstract = {{Global tobacco-related mortality dwarfs that of all other drugs.
   Nicotine is believed to be the primary agent responsible for tobacco use
   and addiction. However, nicotine is a relatively weak and inconsistent
   reinforcer in nonhumans and nicotine reinforcement has not been
   demonstrated in never-smokers.
   This study investigated the discriminative, subjective, and reinforcing
   effects of nicotine in never-smokers.
   Eighteen never-smokers (< 50 lifetime nicotine exposures) participated
   in a double-blind study. During a drug discrimination phase, volunteers
   ingested oral nicotine and placebo capsules (quasi-random order) at
   least 2 h apart and rated subjective effects repeatedly for 2 h after
   ingestion in daily sessions. Blocks of 10 sessions were continued until
   significant discrimination was achieved (p a parts per thousand currency
   signaEuro parts per thousand 0.05, binomial test; a parts per thousand
   yen8 of 10). Following discrimination, nicotine choice was tested by
   having volunteers choose which capsule set to ingest on each daily
   session. Successive blocks of 10 sessions were conducted until choice
   for nicotine or placebo met significance within each volunteer (a parts
   per thousand yen8 of 10 sessions).
   All 18 volunteers significantly discriminated nicotine from placebo; the
   lowest dose discriminated ranged from 1.0 to 4.0 mg/70 kg. Nine
   volunteers significantly chose nicotine (choosers) and nine
   significantly chose placebo (nicotine avoiders). The choosers reported
   predominately positive nicotine subjective effects (e.g.,
   alert/attentive, good effects, liking), while avoiders tended to report
   negative effects (e.g., dizzy, upset stomach, disliking). Both choosers
   and avoiders attributed their choice to the qualitative nature of drug
   effects.
   These results provide the first evidence that nicotine can function as a
   reinforcer in some never-smokers.}},
