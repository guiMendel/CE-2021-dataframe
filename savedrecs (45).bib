@article{ WOS:000089766400003,
Author = {Morck, R and Nakamura, M and Shivdasani, A},
Title = {{Banks, ownership structure, and firm value in Japan}},
Journal = {{JOURNAL OF BUSINESS}},
Year = {{2000}},
Volume = {{73}},
Number = {{4}},
Pages = {{539-567}},
Month = {{OCT}},
Abstract = {{We investigate the relation between firms' ownership structures and q
   ratios in Japan. At low levels of ownership by main banks, firms' q
   ratios fall. as bank equity ownership rises. At higher levels of bank
   ownership, this relationship is mitigated and, in some specifications,
   even reversed. We argue that this relation reflects both costs and
   benefits of equity holdings by banks. In Japan, unlike the United
   States, firm value rises monotonically with increased managerial
   ownership. Equity ownership by corporate blockholders is also positively
   related to firm value in Japan.}},
