@article{ WOS:000284430700007,
Author = {Sonntag, U. and Henkel, J. and Renneberg, B. and Bockelbrink, A. and
   Braun, V. and Heintze, C.},
Title = {{Counseling overweight patients: analysis of preventive encounters in
   primary care}},
Journal = {{INTERNATIONAL JOURNAL FOR QUALITY IN HEALTH CARE}},
Year = {{2010}},
Volume = {{22}},
Number = {{6}},
Pages = {{486-492}},
Month = {{DEC}},
Abstract = {{The increasing prevalence of obesity requires particularly primary care
   providers to take action. The aim of this study was to analyze general
   practitioners (GPs) encounters with overweight and obese patients in
   primary care to test the hypothesis that patients with a BMI >= 30
   kg/m(2) would have longer consultations focusing on lifestyle-related
   issues like nutrition and physical activity than those with a BMI < 30
   kg/m(2).
   Cross sectional comparison of audiotaped encounters of patients with a
   BMI >= 30 kg/m(2) and those with a BMI < 30 kg/m(2).
   Twelve GP surgeries in Berlin, Germany.
   Fifty patients who agreed to have preventive check-up encounters
   audiotaped.
   Based on the Roter Interaction Analysis System (RIAS) we assessed
   duration of encounter and the prevalence of GP statements regarding
   cardiovascular risks, nutrition and physical activity.
   An increased BMI was found to be a predictor for the length of
   encounters (P = 0.01), whereas the content of talks was mainly
   determined by the individual of GP and sex of the GP. Statements
   regarding cardiovascular risks were most frequent, followed by those
   regarding nutrition and physical activity. In this study the assessed
   physiological parameters were not associated with the specific contents
   of preventive encounters like nutrition or physical activity (P > 0.05).
   Our results indicate that GPs rarely use the check-up program to conduct
   lifestyle consultations with obese patients. Barriers to lifestyle
   counseling and possible solutions are discussed with a view to promoting
   individualized and target management of overweight patients.}},
Publisher = {{OXFORD UNIV PRESS}},
Address = {{GREAT CLARENDON ST, OXFORD OX2 6DP, ENGLAND}},
Type = {{Article}},
