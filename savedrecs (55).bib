Title = {{COMMUNITY PROGRAMS FOR OBESITY PREVENTION - THE MINNESOTA HEART
   HEALTH-PROGRAM}},
Journal = {{OBESITY RESEARCH}},
Year = {{1995}},
Volume = {{3}},
Number = {{2}},
Pages = {{S283-S288}},
