@article{ WOS:000235527400003,
Author = {Fingerson, L},
Title = {{Do mothers' opinions matter in teens' sexual activity?}},
Journal = {{JOURNAL OF FAMILY ISSUES}},
Year = {{2005}},
Volume = {{26}},
Number = {{7}},
Pages = {{947-974}},
Month = {{OCT}},
Note = {{95th Annual Meeting of the American-Sociological-Association,
   Washington, DC, AUG 11-16, 2000}},
Organization = {{Amer Sociol Assoc}},
Abstract = {{Using the Add Health data (N = 9,530 dyads), this study explores sexual
   socialization in the family using the theory of reasoned action by
   assessing how mothers' opinions are associated with their children's
   sexual behavior. Findings suggest that the more sexually liberal teens
   think their mothers are, the more likely the teens are to have higher
   numbers of sex partners. Talking about sex and having a strong
   connection between mothers and teens contribute to sexual norm
   transmission. The more talk about sex there is within the dyad, the more
   likely the teen is to have had sex. The stronger the connection between
   mothers and teens, the less likely the teen is to have had sex. Finally,
   for boys, verbal communication with their mothers is more important in
   sexual norm transmission; and for girls, having a good connection in the
   dyad is more important.}},
