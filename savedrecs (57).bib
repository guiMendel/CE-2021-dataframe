@article{ WOS:A1995RH48000014,
Author = {HUNTINGFORD, C},
Title = {{NON-DIMENSIONALISATION OF THE PENMAN-MONTEITH MODEL}},
Journal = {{JOURNAL OF HYDROLOGY}},
