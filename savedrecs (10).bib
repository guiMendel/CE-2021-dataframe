@article{ WOS:000367630700035,
Author = {Cantwell, Mark G. and Sullivan, Julia C. and Katz, David R. and Burgess,
   Robert M. and Hubeny, J. Bradford and King, John},
Title = {{Source determination of benzotriazoles in sediment cores from two urban
   estuaries on the Atlantic Coast of the United States}},
Journal = {{MARINE POLLUTION BULLETIN}},
Year = {{2015}},
Volume = {{101}},
Number = {{1}},
Pages = {{208-218}},
Month = {{DEC 15}},
Abstract = {{Benzotriazoles (BZTs) are used in a broad range of commercial and
   industrial products, particularly as metal corrosion inhibitors and as
   ultraviolet (UV) light stabilizer additives in plastics and polymers. In
   this study, dated sediment cores from two east coast estuaries were
   analyzed for commonly used BZTs. In Narragansett Bay, UV stabilizing
   BZTs (UV-BZTs) were present at high levels from 1961 on, reflecting
   their patent date, local production and long-term preservation in
   sediment In Salem Sound, UV-BZTs were present at concentrations
   consistent with other coastal marine locations not influenced by BZT
   production. Anticorrosive BZTs (AC-BZTs) were found in both cores, with
   the highest levels reported to date present in Narragansett Bay,
   indicating sorption to, and preservation in, sediments. This study
   revealed that both classes of BZTs have remained structurally intact
   over time in coastal sediment cores, demonstrating their resistance to
   degradation and persistence in environmental compartments. Published by
   Elsevier Ltd.}},
