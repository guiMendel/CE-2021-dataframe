@article{ WOS:A1995TF86900019,
Author = {GOLEMBIOWSKA, K and WHITE, TD and SAWYNOK, J},
Title = {{MODULATION OF ADENOSINE RELEASE FROM RAT SPINAL-CORD BY
   ADENOSINE-DEAMINASE AND ADENOSINE KINASE INHIBITORS}},
Journal = {{BRAIN RESEARCH}},
Year = {{1995}},
Volume = {{699}},
Number = {{2}},
Pages = {{315-320}},
Month = {{NOV 20}},
Abstract = {{Adenosine, a modulator of pain processing in the spinal cord, is
   metabolized by adenosine kinase and adenosine deaminase. In this study
   we determined which of these mechanisms is more important for the
   regulation of endogenous adenosine levels in the rat spinal cord. The
   effects of the adenosine kinase inhibitors, 5'-amino-5'-deoxyadenosine
   (NH(2)dAD) and iodotubercidin (IOT), and the adenosine deaminase
   inhibitor, 2'-deoxycoformycin (DCF), on adenosine release in a spinal
   cord superfusion model were studied. DCF markedly increased basal
   adenosine levels detected in perfusates and was more potent than
