Author = {Almeida, AP and Correa, B and Mallozzi, MAB and Sawazaki, E and Soares,
   LMV},
Title = {{Mycoflora and aflatoxin/fumonisin production by fungal isolates from
   freshly harvested corn hybrids}},
Journal = {{BRAZILIAN JOURNAL OF MICROBIOLOGY}},
