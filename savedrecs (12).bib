@article{ WOS:000379976700006,
Author = {Lawlor, Andrea and Crandall, Erin},
Title = {{Questioning Judges with a Questionable Process: An Analysis of Committee
   Appearances by Canadian Supreme Court Candidates}},
Journal = {{CANADIAN JOURNAL OF POLITICAL SCIENCE-REVUE CANADIENNE DE SCIENCE
   POLITIQUE}},
Year = {{2015}},
Volume = {{48}},
Number = {{4}},
Pages = {{863-883}},
Month = {{DEC}},
Abstract = {{In 2006, the Canadian government introduced a new component to its
   process of selecting Supreme Court justices, a review committee composed
   of members of Parliament. Tasked with interviewing justices prior to
   their appointment to the bench, the committee met four times,
   interviewing only five of the eight judicial candidates appointed to the
   bench before the Conservative government announced the committee's
   termination in 2014. This study offers the first comprehensive analysis
   of the performance of this ad hoc judicial review committee. Using an
   original dataset, we find that MPs asked little by way of probing
   questions, such as those related to policy or a candidate's previous
   jurisprudence. However, we do find some evidence that the hearing
   process was used to further the political aims of the participating
   political parties.}},
