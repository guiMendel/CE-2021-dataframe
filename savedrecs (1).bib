@article{ WOS:000600721900001,
Author = {Balakrishnan, Narayanaswamy and Buono, Francesco and Longobardi, Maria},
Title = {{On weighted extropies}},
Journal = {{COMMUNICATIONS IN STATISTICS-THEORY AND METHODS}},
Abstract = {{The extropy is a measure of information introduced as dual to entropy.
   It is a shift-independent information measure just as the entropy. We
   introduce here the notion of weighted extropy, a shift-dependent
   information measure which gives higher weights to larger values of
   random variables. We also study the weighted residual and past extropies
   as weighted versions of extropy for residual and past lifetimes.
   Bivariate versions extropy and weighted extropy are also described.
   Several examples are presented through out to illustrate all the
   concepts introduced here.}},
