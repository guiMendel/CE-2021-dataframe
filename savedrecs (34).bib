Title = {{Highly sensitive and renewable amperometric urea sensor based on
   self-assembled monolayer using porous silicon substrate}},
Journal = {{JOURNAL OF THE KOREAN PHYSICAL SOCIETY}},
Year = {{2005}},
Volume = {{47}},
