@article{ WOS:A1995RT70600006,
Author = {HUCKER, T and KRANZ, HG},
Title = {{REQUIREMENTS OF AUTOMATED PD DIAGNOSIS SYSTEMS FOR FAULT IDENTIFICATION
   IN NOISY CONDITIONS}},
