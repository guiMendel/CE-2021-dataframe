@article{ WOS:000597869900003,
Author = {Valenti, Gianluca},
Title = {{Request strategies in some letters of Bartolomeo Ammannati (1563-1578)}},
Journal = {{LINGUA E STILE}},
Year = {{2020}},
Volume = {{55}},
Number = {{2}},
Pages = {{261-285}},
Month = {{DEC}},
Abstract = {{In the recent decades, linguistic analyses of semi-educated writers have
   increasingly been applied not only to medieval documents but also to
   texts composed between the Renaissance and the present day. On this
   subject, new questions have recently emerged and still await an answer.
   One such question concerns the language of artisans/artists who
   increasingly made use of writing in documents of various kinds between
   the 15th and 16th centuries. Within this framework, I propose a
   sociopragmatic study of the request strategies used by Bartolomeo
   Ammannati in a corpus of 54 letters, sent to Giovanni Caccini between
   1563 and 1578. In so doing, I also test a new method of analysis which
   seeks to objectify the rank of imposition of these requests.}},
