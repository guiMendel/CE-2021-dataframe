@article{ WOS:000366885500008,
Author = {Mazel, Claire},
Title = {{Claire Mazel: The Portrait of Antoine Coysevox from the Pen of
   Fermel'huis. A Sculptor in Motion}},
Journal = {{REVUE DE L ART}},
Year = {{2015}},
Number = {{190}},
Pages = {{67-74}},
Month = {{DEC}},
Abstract = {{The biography of Antoine Coysevox, which Jean-Baptiste Fermel' huis,
   established when the sculptor died in 1720, underscores the essential
   features of the work of this artist who was Louis XIV's principal
   sculptor during the second half of his reign. This portrait relates the
   status, the craft, and the career of Coysevox, the principle esthetics
   that presided over his art, the pursuance of the complex rivalry between
   the Ancients and the Moderns, as well as the mechanism of the artistic
   collaborations on the major royal building sites. Restituted through
   this account is the ambition, but also the normality of the career of a
   sculptor whose fame was rapidly tarnished by the more exceptional
   figures of his contemporaries, Pierre Puget and Francois Girardon, or
   his more audacious successors, such as Guillaume Coustou.}},
