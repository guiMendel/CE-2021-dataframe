@article{ WOS:000089001100028,
Author = {Byrne, B and Abramson, J and Jansson, M and Holmgren, E and Iwata, S},
Title = {{Fusion protein approach to improve the crystal quality of cytochrome
   bo(3) ubiquinol oxidase from Escherichia coli}},
Journal = {{BIOCHIMICA ET BIOPHYSICA ACTA-BIOENERGETICS}},
Year = {{2000}},
Volume = {{1459}},
Number = {{2-3}},
Pages = {{449-455}},
Month = {{AUG 15}},
Note = {{11th European Bioenergetics Conference (EBEC), UNIV SUSSEX, BRIGHTON,
   ENGLAND, SEP 09-14, 2000}},
Abstract = {{Crystals of cytochrome bos ubiquinol oxidase from E. coli diffract
   X-rays to 3.5 Angstrom and the structure determination is in progress.
   The limiting factor to the elucidation of the structural detail is the
   quality of the crystals; the diffraction spots from the crystals are
   diffused which leads to difficulties in processing the data beyond 4.0
   Angstrom. Weak protein-protein contacts within the crystal lattice is
   assumed to be the cause of this problem. To improve these contacts, we
   have introduced protein Z to the C-terminal end of the subunit IV of
   cytochrome bo(3) and expressed both proteins as a single fusion. We have
   successfully obtained crystals of this fusion protein. The spot shape
   problem has clearly been solved in the crystals of the fusion protein
   although further optimization is necessary to obtain higher resolution.
