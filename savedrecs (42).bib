@article{ WOS:000165341600035,
Author = {Das, A and Kloster, S},
Title = {{Analytical solutions of a spherically symmetric collapse of an
   anisotropic fluid body into a regular black hole}},
Journal = {{PHYSICAL REVIEW D}},
Year = {{2000}},
Volume = {{62}},
Number = {{10}},
Month = {{NOV 15}},
Abstract = {{Einstein's equations are investigated for a spherically symmetric
   anisotropic fluid body. Tolman-Bondi types of coordinates are employed.
   A class of exact solutions are found. On the boundary of the collapsing
   star, both Synge-O'Brien and Israel junction conditions are satisfied.
   The metric pertaining to the interior of the star collapsing into a
   black hole satisfies three energy conditions. The interior metric of the
