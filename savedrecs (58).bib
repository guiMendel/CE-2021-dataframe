DOI = {{10.1093/nar/23.13.2464}},
ISSN = {{0305-1048}},
Keywords-Plus = {{TRANSCRIPTION FACTOR-IIIA; 5S RNA GENE; DIRECTS SPECIFIC INITIATION;
