@article{ WOS:000280204800001,
Author = {Kim, Sun Kyu and Van Le, Vinh},
Title = {{Deposition of nanolayered CrN/AlBN thin films by cathodic arc
   deposition: Influence of cathode arc current and bias voltage on the
   mechanical properties}},
Journal = {{SURFACE \& COATINGS TECHNOLOGY}},
Year = {{2010}},
Volume = {{204}},
Number = {{24}},
Pages = {{3941-3946}},
Month = {{SEP 15}},
Abstract = {{Thin films of CrAlBN were deposited on SKD 11 tool steel substrate using
   Cr and AlB cathodes in a cathodic arc plasma deposition system. The
   influence of AlB cathode arc current and substrate bias voltage on the
   mechanical and the structural properties of the films was investigated.
   The CrAlBN thin films had a multilayered structure in which the
   nano-crystalline CrN layer alternated with the amorphous AIBN layer. The
   hardness of the films increased as the AlB cathode arc current was
   raised from 35 to 45 A. and then decreased with further increase of the
   current. The hardness of the films increased rapidly with the increase
   of the bias voltage from -50 to -150 V. Further increase in the bias
   voltage decreased the hardness. The maximum hardness of 48 GPa was
   obtained at the bias voltage of -150 V. With the increase of bias
   voltage, a good correlation between the residual stress and the hardness
   of the films was observed. (C) 2009 Elsevier B.V. All rights reserved.}},
