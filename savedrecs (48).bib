@article{ WOS:000089210000009,
Author = {Greenland, RG},
Title = {{Optimum height at which to kill barley used as a living mulch in onions}},
Journal = {{HORTSCIENCE}},
Year = {{2000}},
Volume = {{35}},
Number = {{5}},
Pages = {{853-855}},
Month = {{AUG}},
Abstract = {{Planting barley (Hordeum vulgare L.) as a living mulch with onions
   (Allium cepa L.) reduces soil erosion and protects the onions from wind
   damage. It can also reduce yield and size of onion bulbs if not managed
   correctly. In a ii-year study at the Oakes Irrigation Research Site in
   North Dakota, barley was planted in the spring at the same time that
   onions were direct-seeded. Barley rows were planted either parallel with
   or perpendicular to the onion ro,vs. Barley was killed with fluazifop-P
   herbicide when approximate to 13, 18, 23, or 30 cm tall. Onion size and
   yields were reduced when barley was allowed to grow taller then 18 cm
   before killing it. Total onion yield was usually greater when barley was
   planted parallel with, rather than perpendicular to, onion rows.
   Chemical name used:
   (R)-2-{[}4-{[}{[}5(trifluoromethyl)-2-pyridinyl]oxy]phenoxy]propanoic
   acid (fluazifop-P).}},
