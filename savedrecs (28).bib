@article{ WOS:000282754800003,
Author = {Yubero, Santiago and Ovejero, Anastasio and Larranaga, Elisa},
Title = {{Perceived social support in school and peer victimization during
   adolescence}},
Journal = {{REVISTA DE PSICOLOGIA SOCIAL}},
Year = {{2010}},
Volume = {{25}},
Number = {{3}},
Pages = {{283-293}},
Month = {{OCT}},
Abstract = {{The present study analyses social support as a protective factor against
   victimization among peers. This research examines the respective
   influence of perceived support from peers and from teacher on
   victimization among peers in school. Participants in the study were 2961
   adolescents (mean age: 13.94) of whom 50.1\% were boys and 49.9\% were
   girls, studying in schools of the Castilla-La Mancha Community (Spain).
   To explore the relationship among variables we used chi-square
   coefficients, as well as hierarchical regression models to assess the
   variables predicting the victimization among peers. The results
   highlight the importance of perceived peers' and teacher's support.
   Finally, these findings and their implications are discussed in relation
   to previous research.}},
