@article{ WOS:000232551200004,
Author = {Hodge, DR},
Title = {{Spiritual assessment in marital and family therapy: A methodological
   framework for selecting from among six qualitative assessment tools}},
Journal = {{JOURNAL OF MARITAL AND FAMILY THERAPY}},
Year = {{2005}},
Volume = {{31}},
Number = {{4}},
Pages = {{341-356}},
Month = {{OCT}},
Abstract = {{Emerging professional and cultural developments underscore the
   importance of spiritual assessment. Most marriage and family therapists,
   however, appear to have received little training on the topic.
   Consequently, this article overviews a complementary family of six
   recently developed qualitative assessment approaches or tools, an
   initial brief assessment tool and five comprehensive assessment tools,
   and presents a methodological framework for using the six tools. To help
   decide whether or not a comprehensive assessment is warranted after
   conducting an initial brief assessment, two general guidelines are
   offered. Suggestions for selecting the appropriate comprehensive
   assessment tool are also presented, and summarized graphically in the
   form of a decision tree. The overall methodological framework is
   designed to be congruent with the recent spiritual assessments
   recommendations issued by the Joint Commission on Accreditation of
   Healthcare Organizations (JCAHO), the largest healthcare accrediting
   organization in the United States.}},
