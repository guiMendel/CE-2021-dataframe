@article{ WOS:000604799100002,
Author = {Shishegaran, Aydin and Shokrollahi, Mohammad and Mirnorollahi, Ali and
   Shishegaran, Arshia and Khani, Mohammadreza Mohammad},
Title = {{A novel ensemble model for predicting the performance of a novel
   vertical slot fishway}},
Journal = {{FRONTIERS OF STRUCTURAL AND CIVIL ENGINEERING}},
Year = {{2020}},
Volume = {{14}},
Number = {{6}},
Pages = {{1418-1444}},
Month = {{AUG}},
Abstract = {{We investigate the performance of a novel vertical slot fishway by
   employing finite volume and surrogate models. Multiple linear
   regression, multiple log equation regression, gene expression
   programming, and combinations of these models are employed to predict
   the maximum turbulence, maximum velocity, resting area, and water depth
   of the middle pool in the fishway. The statistical parameters and error
   terms, including the coefficient of determination, root mean square
   error, normalized square error, maximum positive and negative errors,
   and mean absolute percentage error were employed to evaluate and compare
   the accuracy of the models. We also conducted a parametric study. The
   independent variables include the opening between baffles (OBB), the
   ratio of the length of the large and small baffles, the volume flow
   rate, and the angle of the large baffle. The results show that the key
   parameters of the maximum turbulence and velocity are the volume flow
   rate and OBB.}},
