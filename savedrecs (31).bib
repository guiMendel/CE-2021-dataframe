@article{ WOS:000233958900024,
Author = {Richter, S and Alonso, A and De Bolle, W and Kuhn, H and Verbruggen, A
   and Wellum, R and Taylor, PDP},
Title = {{Preparation and certification of synthetic uranium isotope mixtures with
   U-236/U-238 of 10(-6), 10(-7), 10(-8)}},
Journal = {{JOURNAL OF ANALYTICAL ATOMIC SPECTROMETRY}},
Year = {{2005}},
Volume = {{20}},
Number = {{12}},
Pages = {{1381-1385}},
Month = {{DEC}},
Abstract = {{A set of 3 new synthetic uranium isotope mixtures with U-236/U-238
   isotope ratios in the order of 10(-6), 10(-7) and 10(-8) has been
   produced and certified for the isotopic composition. For the preparation
   of these 3 mixtures a natural uranium starting material ( U-235/U-238 =
   0.007 25) is mixed in 3 different proportions with a non-natural
   starting material, which is made up of a 1 : 1 mixture of IRMM-187
   (U-235/U-238 = 0.0475) with a material highly enriched in U-235. The
   U-236/U-238 ratios of the 3 mixtures are calculated using the measured
   U-236/U-238 ratios of the natural (close to detection limit) and the
   non-natural starting materials. The U-236 isotope intensity of the
   latter material was measured on a Faraday collector. The minor isotope
   abundances of the 3 mixtures were calculated from the minor isotope
   abundances of the starting materials and the measured U-235/U-238 ratios
   of the mixtures. Direct measurements of the U-236/U-238 ratios for all 3
   mixtures were performed by TIMS using a secondary electron multiplier
   operated in ion counting mode for the U-236 ion beam. The results agree
   well with the ratios obtained from the mixing calculations. This
   confirmation is considered a proof for the reliability of our TIMS
   procedure used to measure U-236/U-238 ratios within a wide dynamic
   range. Based on the confidence provided by the synthetic uranium isotope
   mixtures, the minor isotope ratios U-234/U-238 and U-236/U-238 were
   re-measured and re-certified by TIMS for the reference materials
   IRMM-183-187. On the new certificates for IRMM-183-187 the minor ratios
   U-234/ U-238 and U-236/U-238 are certified with a circa ten-times lower
   uncertainty compared to previous values.}},
