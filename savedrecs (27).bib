Title = {{Scanning laser microscope for imaging nanostructured superconductors}},
Journal = {{PHYSICA C-SUPERCONDUCTIVITY AND ITS APPLICATIONS}},
Year = {{2010}},
Volume = {{470}},
Number = {{19, SI}},
Pages = {{730-733}},
