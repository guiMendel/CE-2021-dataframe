@article{ WOS:000281376500008,
Author = {Lamb, Dane T. and Ming, Hui and Megharaj, Mallavarapu and Naidu, Ravi},
Title = {{Relative Tolerance of a Range of Australian Native Plant Species and
   Lettuce to Copper, Zinc, Cadmium, and Lead}},
Journal = {{ARCHIVES OF ENVIRONMENTAL CONTAMINATION AND TOXICOLOGY}},
Year = {{2010}},
Volume = {{59}},
Number = {{3}},
Pages = {{424-432}},
Month = {{OCT}},
Abstract = {{The tolerance of wild flora to heavy-metal exposure has received very
   little research. In this study, the tolerance of four native tree
   species, four native grass species, and lettuce to copper (Cu), zinc
   (Zn), cadmium (Cd), and lead (Pb) was investigated in a root-elongation
   study using Petri dishes. The results of these studies show a diverse
   range of responses to Cu, Zn, Cd, and Pb amongst the tested plant
   species. Toxicity among metals decreased in the following order: Cd
   similar to Cu > Pb > Zn. Metal concentrations resulting in a 50\%
   reduction in growth (EC50) varied considerably, ranging from (mu M) 30
   (Dichanthium sericeum) to > 2000 (Acacia spp.) for Cu; from 260 (Lactuca
   sativa) to 2000 (Acacia spp.) for Zn; from 27 (L. sativa) to 940 (Acacia
   holosericea) for Cd; and from 180 (L. sativa) to > 1000 (Acacia spp.)
   for Pb. Sensitive native plant species identified included D. sericeum,
   Casuarina cunninghamiana, and Austrodanthonia caespitosa. However, L.
   sativa (lettuce) was also among the most sensitive to all four metals.
   Acacia species showed a high tolerance to metal exposure, suggesting
   that the Acacia genus shows potential for use in contaminated-site
   revegetation.}},
