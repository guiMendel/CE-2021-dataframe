@article{ WOS:A1995TB43400010,
Author = {KAPLAN, LA and NEWBOLD, JD},
Title = {{MEASUREMENT OF STREAMWATER BIODEGRADABLE DISSOLVED ORGANIC-CARBON WITH A
   PLUG-FLOW BIOREACTOR}},
Journal = {{WATER RESEARCH}},
Year = {{1995}},
Volume = {{29}},
Number = {{12}},
Pages = {{2696-2706}},
Month = {{DEC}},
Abstract = {{Plug-flow biofilm reactors were colonized by microorganisms indigenous
   to streamwater and used to measure concentrations of biodegradable
   dissolved organic carbon (DOC) in streamwater. We performed experiments
   to determine the influence of physical, chemical, and biological factors
   on the operation of the bioreactors. Colonization required several
   months and the biological removal of DOC within the reactors was
   influenced by hydraulic residence time, dissolved organic carbon
   concentration, and water temperature. The large microbial biomass within
   a bioreactor buffered bioreactor performance from changes in chemical
   and physical parameters.}},
