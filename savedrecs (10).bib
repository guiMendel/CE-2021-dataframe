@article{ WOS:000369172100008,
Author = {Miyata, Non and Miyoshi, Takuya and Yamaguchi, Takanori and Nakazono,
   Toshimitsu and Tani, Motohiro and Kuge, Osamu},
Title = {{VID22 is required for transcriptional activation of the PSD2 gene in the
   yeast Saccharomyces cerevisiae}},
Journal = {{BIOCHEMICAL JOURNAL}},
Year = {{2015}},
Volume = {{472}},
Number = {{3}},
Pages = {{319-328}},
Month = {{DEC 15}},
Abstract = {{Phosphatidylethanolamine (PE) in the yeast Saccharomyces cerevisiae is
   synthesized through decarboxylation of phosphatidylserine (PS),
   catalysed by PS decarboxylase 1 (Psd1p) and 2 (Psd2p) and the cytidine
   5'-diphosphate (CDP)-ethanolamine (CDP-Etn) pathway. PSD1 null (psd1
   Delta) and PSD2 null (psd2 Delta) mutants are viable in a synthetic
   minimal medium, but a psd1 Delta psd2. double mutant exhibits Etn
   auxotrophy, which is incorporated into PE through the CDP-Etn pathway.
   We have previously shown that psd1. is synthetic lethal with deletion of
   VID22 (vid22 Delta) {[}Kuroda et al. (2011) Mol. Microbiol. 80, 248265].
   In the present study, we found that vid22 Delta mutant exhibits Etn
   auxotrophy under PSD1-depressed conditions. Deletion of VID22 in
   wild-type and PSD1-depressed cells caused partial defects in PE
   formation through decarboxylation of PS. The enzyme activity of PS
   decarboxylase in an extract of vid22 Delta cells was similar to 70\% of
   that in wild-type cells and similar to that in psd2. cells and the PS
   decarboxylase activity remaining in the PSD1-depressed cells became
   almost negligible with deletion of vid22 Delta Thus, the vid22 Delta
   mutation was suggested to cause a defect in the Psd2p activity.
   Furthermore, vid22 Delta cells were shown to be defective in expression
   of the PSD2 gene tagged with 6xHA, the defect being ameliorated by
   replacement of the native promoter of the PSD2 gene with a CYC1
   promoter. In addition, an alpha-galactosidase reporter assay revealed
   that the activity of the promoter of the PSD2 gene in vid22 Delta cells
   was similar to 5\% of that in wild-type cells. These results showed that
   VID22 is required for transcriptional activation of the PSD2 gene.}},
