@article{ WOS:000600364300002,
Author = {Chen, Haoran and Guan, Kaifang and Qi, Xiaofen and Wang, Rongchun and
   Ma, Ying},
Title = {{alpha-Lactalbumin ameliorates hepatic lipid metabolism in high-fat-diet
   induced obese C57BL/6J mice}},
Journal = {{JOURNAL OF FUNCTIONAL FOODS}},
Year = {{2020}},
Volume = {{75}},
Month = {{DEC}},
Abstract = {{Bovine alpha-lactalbumin (alpha-LA) is a critical component of whey
   protein (WP). It has regulatory effects on obesity, but the regulatory
   mechanism remains unclear. The present study investigated the beneficial
   effect of alpha-lactalbumin (alpha-LA) on obesity-related hepatic lipid
   metabolism in high-fat-diet (HFD)-induced obese C57BL/6J mice. The
   results showed that supplementation with alpha-LA for 8 weeks improved
   body weight gain, serum lipids, and hepatic steatosis and alleviated
   glucose tolerance and insulin resistance in obese mice. Additionally,
   alpha-LA upregulated the expression of fatty acid beta-oxidation-related
   genes and downregulated the expression of lipogenesis-related genes in
   the livers of obese mice. Our study suggested that the benefit of
   alpha-LA on improving hepatic lipid metabolism may be mainly associated
   with the activation of peroxisome proliferator-activated receptor alpha
   (PPAR alpha) pathways in liver tissues. Therefore, alpha-LA may be used
   as a dietary therapy for obesity and related metabolic disorders.}},
