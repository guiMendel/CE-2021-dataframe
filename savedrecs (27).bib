@article{ WOS:000282587800005,
Author = {Fortin, Mathieu and DeBlois, Josianne},
Title = {{A statistical estimator to propagate height prediction errors into a
   general volume model}},
Journal = {{CANADIAN JOURNAL OF FOREST RESEARCH-REVUE CANADIENNE DE RECHERCHE
   FORESTIERE}},
Year = {{2010}},
Volume = {{40}},
Number = {{10}},
Pages = {{1930-1939}},
Month = {{OCT}},
Abstract = {{In most large-scale inventories, volume models rely on predicted heights
   instead of measured heights. The effect of height predictions over
   volume estimate uncertainty is therefore a crucial issue. In this study,
   we designed a statistical estimator that enables the propagation of tree
   height prediction errors into a general volume model. The proposed
   estimator relies on the assumption that both the volume model and the
   height-diameter model that provides the height predictions do not
   exhibit any lack of fit. This estimator was tested against a Monte Carlo
   simulation for 90 plots from three typical forest types in Quebec,
   Canada (hardwood, mixedwood, and softwood). The results show that the
   estimator provides plot volume predictions with error variances that fit
   those estimated using the Monte Carlo simulation. The estimator
   developed in this study may be useful to quickly provide the uncertainty
   associated with plot volume predictions in large-scale inventories.}},
