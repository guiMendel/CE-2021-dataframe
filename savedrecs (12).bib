@article{ WOS:000367232600015,
Author = {Lu, Dan and Qian, Jing and Li, Wei and Feng, Qianqian and Pan, Shu and
   Zhang, Siquan},
Title = {{beta -hydroxyisovaleryl-shikonin induces human cervical cancer cell
   apoptosis via PI3K/AKT/mTOR signaling}},
Journal = {{ONCOLOGY LETTERS}},
Year = {{2015}},
Volume = {{10}},
Number = {{6}},
Pages = {{3434-3442}},
Month = {{DEC}},
Abstract = {{The present study aimed to investigate the inhibitory ability of
   -hydroxyisovaleryl-shikonin (beta-HIVS) on the proliferation of human
   cervical cancer HeLa cells and to identify the mechanism of this effect.
   The HeLa cells were treated with -HIVS and the inhibition of cell growth
   was detected by an MTT assay. Flow cytometry was performed to analyze
   the apoptosis rate and cell cycle distribution of HeLa cells. Reverse
   transcription-polymerase chain reaction and western blot analysis were
   used to examine the expression of the phosphatidylinositol 3-kinase
   (PI3K)/protein kinase B (AKT)/mammalian target of rapamycin (mTOR)
   signaling pathway proteins. The results revealed that -HIVS inhibited
   HeLa cell proliferation in a dose- and time-dependent manner. With the
   administration of increasing concentrations of -HIVS, the apoptotic rate
   of HeLa cells was also increased. The cell cycle was slightly arrested
   at the S phase, with similar to 6\% of cells in this phase, subsequent
   to treatment with 10 mu M -HIVS. In addition, -HIVS markedly reduced the
   expression levels of PI3K, AKT, mTOR and 70-kDa ribosomal protein S6
   kinase in HeLa cells. -HIVS promoted cervical cancer cell apoptosis by
   inhibiting the PI3K/AKT/mTOR signaling pathway and suppressing
   downstream gene expression. The present study is expected to lead to the
   development of molecular targeted therapy for this signaling pathway as
   a novel method of cervical cancer treatment.}},
