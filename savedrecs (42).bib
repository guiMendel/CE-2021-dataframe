@article{ WOS:000167539900010,
Author = {Banks, WM and Nash, DH and Flaherty, AE and Fok, WC and Tooth, AS},
Title = {{A simplified design approach to determine the maximum strains in a GRP
   vessel supported on twin saddles}},
Journal = {{INTERNATIONAL JOURNAL OF PRESSURE VESSELS AND PIPING}},
Year = {{2000}},
Volume = {{77}},
Number = {{13}},
Pages = {{837-842}},
Month = {{NOV}},
Note = {{9th International Conference on Pressure Vessel Technology, SYDNEY,
   AUSTRALIA, 2000}},
Abstract = {{The maximum strain in a glass reinforced plastic (GRP) vessel, used for
   unpressurised liquid storage and supported on rigid saddle supports,
   occurs in the immediate region of the saddle horn on the outside
   surface. An existing Fourier series solution of this problem has been
   used to conduct a parametric study and extend the previous work given by
   Tooth et al. (Proc Inst Mech Engr, Part E: Process Mech Engng 208 (1994)
   59). To provide the results for the maximum strain in a `closed form',
   an equation fitting technique has been used. This work, presented
   originally at the ICPVT-9, Sydney, gives a simplified design approach
   for the symmetric laminate. Following the preparation of the work for
   the ICPVT-9, a more comprehensive treatment, covering a wider range of
   vessel support locations, was carried out, and presented by Nash et al.
   (Proc Inst Mech Engr, Part L, J Mater: Design Applications, in press).
   Recognising the value of the simplified approach presented in the
   ICPVT-9 paper, use was made of the work of Tooth et al. and Nash et al.
   to extend its range of applicability to provide confidence in using this
