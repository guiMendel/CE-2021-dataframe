@article{ WOS:000089298300010,
Author = {Marquez, E and Wagner, T and Gonzalez-Leal, JM and Bernal-Oliva, AM and
   Prieto-Alcon, R and Jimenez-Garay, R and Ewen, PJS},
Title = {{Controlling the optical constants of thermally-evaporated Ge10Sb30S60
   chalcogenide glass films by photodoping with silver}},
