@article{ WOS:000284485300006,
Author = {Blumm, J. and Lindemann, A. and Meyer, M. and Strasser, C.},
Title = {{Characterization of PTFE Using Advanced Thermal Analysis Techniques}},
Journal = {{INTERNATIONAL JOURNAL OF THERMOPHYSICS}},
Year = {{2010}},
Volume = {{31}},
Number = {{10}},
Pages = {{1919-1927}},
Month = {{OCT}},
Abstract = {{Polytetrafluoroethylene (PTFE) is a synthetic fluoropolymer used in
   numerous industrial applications. It is often referred to by its
   trademark name, Teflon. Thermal characterization of a PTFE material was
   carried out using various thermal analysis and thermophysical properties
   test techniques. The transformation energetics and specific heat were
   measured employing differential scanning calorimetry. The thermal
   expansion and the density changes were determined employing pushrod
   dilatometry. The viscoelastic properties (storage and loss modulus) were
   analyzed using dynamic mechanical analysis. The thermal diffusivity was
   measured using the laser flash technique. Combining thermal diffusivity
   data with specific heat and density allows calculation of the thermal
   conductivity of the polymer. Measurements were carried out from - 125 A
   degrees C up to 150 A degrees C. Additionally, measurements of the
   mechanical properties were carried out down to -A 170 A degrees C. The
   specific heat tests were conducted into the fully molten regions up to
   370 A degrees C.}},
