@article{ WOS:000089486800008,
Author = {Papic, I},
Title = {{Mathematical analysis of FACTS devices based on a voltage source
   converter - Part II: steady state operational characteristics}},
Journal = {{ELECTRIC POWER SYSTEMS RESEARCH}},
Year = {{2000}},
Volume = {{56}},
Number = {{2}},
Pages = {{149-157}},
Month = {{NOV 1}},
Abstract = {{This paper systematically presents the steady state operational
   characteristics of FACTS devices based on a voltage source converter.
   The characteristics of various static compensator designs or the
   individual parts of a unified power flow controller are determined using
   derived mathematical models or state space equations in a rotating
   orthogonal coordinate system. The characteristics of both shunt and
   series compensators in different modes of operation are derived. This
   leads us to the conclusion that the UPFC is in fact a unified device
   combining the advantages of all individual controllers. Analysis of
   steady state operational characteristics resulted in key findings for
   the derivation of control algorithms and for further examination of the
   FACTS devices under dynamic operating conditions. (C) 2000 Elsevier
