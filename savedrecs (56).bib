@article{ WOS:A1995TE57200012,
Author = {SKLAR, C},
Title = {{GROWTH AND ENDOCRINE DISTURBANCES AFTER BONE-MARROW TRANSPLANTATION IN
   CHILDHOOD}},
Journal = {{ACTA PAEDIATRICA}},
Year = {{1995}},
Volume = {{84}},
Number = {{411}},
Pages = {{57-62}},
Month = {{SEP}},
Note = {{19th International Symposium on Growth Hormone and Growth Factors in
   Endocrinology and Metabolism, PRAGUE, CZECH REPUBLIC, APR 21-22, 1995}},
Abstract = {{Bone marrow transplantation (BMT) is now considered the first choice of
   treatment for aplastic anaemia and certain haematopoietic malignancies.
   In addition, BMT is being utilized in the treatment of an increasing
   number of individuals with haematological and non-haematological
   disorders, such as thalassaemia and sickle cell anaemia,
   immunodeficiency syndromes, solid tumours and metabolic diseases. Recent
   advances in transplantation biology, such as the use of unrelated
   donors, partially matched marrow and peripheral and cord blood stem
   cells, have substantially increased the number of potential recipients
   for BMT. Although BMT is often a life-saving procedure, patients are
   exposed to potent therapies that may later cause serious complications.
   The preparative regimens commonly employed in the treatment of aplastic
   anaemia include high-dose cyclophosphamide (50 mg/kg/day for 4 days),
   given alone or in combination with total lymphoid irradiation (TLI) (1).
   In general, children undergoing BMT as a treatment for acute leukaemia
   receive high-dose chemotherapy (usually one or more alkylating agents,
   such as cyclophosphamide) plus total body irradiation (TBI) (2). The
   radiation can be given in a single dose or, more recently, has been
   given in multiple fractions, one to three times daily for several days
   (3). Busulphan and cyclophosphamide are being employed as an alternative
   to TBI by many centres, particularly in the treatment of non-lymphocytic
   leukaemias and thalassaemia.
   This article will review the effects of BMT during childhood on linear
   growth and endocrine function. As the commonest use of BMT is in the
   treatment of aplastic anaemia and acute leukaemia, most of the data
   available have been derived from studies performed in such patients.
   This review will therefore focus primarily an the endocrine sequelae
   following BMT in patients with aplastic anaemia and acute leukaemias.}},
Publisher = {{SCANDINAVIAN UNIVERSITY PRESS}},
Address = {{PO BOX 2959 TOYEN, JOURNAL DIVISION CUSTOMER SERVICE, N-0608 OSLO,
   NORWAY}},
Type = {{Article; Proceedings Paper}},
Language = {{English}},
