@article{ WOS:000359329000092,
Author = {Shaha, S. K. and Czerwinski, F. and Kasprzak, W. and Friedman, J. and
   Chen, D. L.},
Title = {{Microstructure and mechanical properties of Al-Si cast alloy with
   additions of Zr-V-Ti}},
Journal = {{MATERIALS \& DESIGN}},
Year = {{2015}},
Volume = {{83}},
Pages = {{801-812}},
Month = {{OCT 15}},
Abstract = {{The microstructure and tensile properties at temperatures up to 300
   degrees C of an experimental Al-7Si-1Cu-0. 5Mg (wt.\%) cast alloy with
   additions of Ti, V and Zr were assessed and compared with those of the
   commercial A380 grade. The microstructure of both alloys consisted of Al
   dendrites surrounded by Al Si eutectic containing, within its structure,
   the ternary Al-Al2Cu-Si phase. Whereas the Al-15(FeCrMn)(3)Si-2 phases
   were present in the A380 alloy, Ti/Zr/V together with Al and Si phases,
