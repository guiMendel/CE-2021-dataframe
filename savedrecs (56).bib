@article{ WOS:A1995RU35500003,
Author = {SEVICK, EM and WILLIAMS, DRM},
Title = {{COIL-STRETCH TRANSITIONS FOR GRAFTED POLYMERS IN SPATIALLY VARYING FLOWS}},
Journal = {{EUROPHYSICS LETTERS}},
Year = {{1995}},
Volume = {{31}},
Number = {{7}},
Pages = {{357-362}},
Month = {{SEP 1}},
Abstract = {{We study a single grafted or tethered polymer chain in the mushroom
   regime in a spatially varying flow. Unlike the case of constant flows
   discussed previously, a flow with a weakly spatially increasing velocity
   can cause a coil-stretch transition. In particular for velocity fields
   of the form upsilon(x) = V(x/l)(a), where V and l are constants, there
   is a critical value of the exponent a above which coil-stretch
   transitions take place. This is a(c) = (2 nu - 1)/(1 - nu), where nu is
   the Flory exponent for the chain. We also study the case of shear flows,
   where a similar relation holds. We then study more general flows. In the
   particular case of a chain in a good solvent, a(c) = 1/2 and a flow in a
   coverging cone causes a coil-stretch transition for cones above a
   critical angle and for fluid fluxes above a critical value.}},
