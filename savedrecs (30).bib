@article{ WOS:000234336400013,
Author = {De Wever, V and Reiter, W and Ballarini, A and Ammerer, G and Brocard, C},
Title = {{A dual role for PP1 in shaping the Msn2-dependent transcriptional
   response to glucose starvation}},
Journal = {{EMBO JOURNAL}},
Year = {{2005}},
Volume = {{24}},
Number = {{23}},
Pages = {{4115-4123}},
Month = {{DEC 7}},
Abstract = {{in yeast, glucose depletion elicits a quick response in the
   transcription of stress-related genes. The main transcriptional
   activator that orchestrates this response is Msn2, whose nuclear
   localization and DNA binding are negatively controlled by the
   cAMP-dependent protein kinase (PKA). Msn2 activation by sudden glucose
   depletion correlates with a fast but transient decrease in
   phosphorylation of several sites in its nuclear localization signal
   (NLS). Here we show that protein phosphatase 1 (PP1) is the direct
   antagonist of PKA-dependent phosphorylation at the Msn2 nuclear import
   domain and therefore a potential mediator of glucose starvation signals
   that target this transcription factor. Apart from PKA, the protein
   kinase Snf1 can also directly modify one of the Msn2 phosphorylation
   sites (S582) and thereby repress Msn2 function. Consequently, in snf1
   mutants, rephosphorylation of the NLS happens to be much slower during
   prolonged starvation. Thus, a second, Reg1-dependent form of PP1
   indirectly influences Msn2 functionality by modulating Snf1 kinase
   activation and repression. Different activities of PP1 are therefore
   involved in shaping induction and adaptation of the transcriptional
   stress response during acute glucose starvation.}},
