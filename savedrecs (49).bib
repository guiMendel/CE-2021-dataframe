@article{ WOS:000089037400003,
Author = {Yang, JG and Uchiyama, T},
Title = {{Hydroxycinnamic acids and their dimers involved in the cessation of cell
   elongation in Mentha suspension culture}},
Journal = {{BIOSCIENCE BIOTECHNOLOGY AND BIOCHEMISTRY}},
Year = {{2000}},
Volume = {{64}},
Number = {{8}},
Pages = {{1572-1579}},
Month = {{AUG}},
Abstract = {{The contents of mall-bound hydroxycinnamic acids and their dimers were
   compared between elongated and non-elongated cells of
   suspension-cultured Mentha, Wall-bound peroxidase activity was also
   investigated. The main hydroxycinnamic acids esterified to these two
   kinds of cell walls were ferulic and caffeic acids. Eleven
   dehydrodicaffeic acid isomers and six dehydrodiferulic acid isomers
   formed through C-C and C-O-C coupling processes, were detected by GC-MS
   from the extracts released from the walls of non-elongated cells. On the
   other hand, only four dehydrodicaffeic acid isomers and three
   dehydrodiferulic acid isomers were found in the walls of elongated
   cells. Amounts of monomers of ferulic and caffeic acids and their
