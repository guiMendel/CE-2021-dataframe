@article{ WOS:000282360300002,
Author = {Berger, C. F. and Bern, Z. and Dixon, L. J. and Febres Cordero, F. and
   Forde, D. and Gleisberg, T. and Ita, H. and Kosower, D. A. and Maitre,
   D.},
Title = {{Next-to-leading order QCD predictions for Z,gamma{*}+3-jet distributions
   at the Tevatron}},
Journal = {{PHYSICAL REVIEW D}},
Year = {{2010}},
Volume = {{82}},
Number = {{7}},
Month = {{OCT 1}},
Abstract = {{Using BLACKHAT in conjunction with SHERPA, we have computed
   next-to-leading order QCD predictions for a variety of distributions in
   Z,gamma{*} + 1, 2, 3-jet production at the Tevatron, where the Z boson
   or off-shell photon decays into an electron-positron pair. We find good
   agreement between the next-to-leading order results for jet p(T)
   distributions and measurements by CDF and D0. We also present
