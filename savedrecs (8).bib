@article{ WOS:000573278200007,
Author = {Zhou, Hao and Ai, Jing and Gao, Hongyu and Zhang, Weijun and Wang,
   Dongsheng},
Title = {{Removal of arsenic in groundwater using Slag based calcined layered
   double hydroxides (CLDHs) with dual functions of adsorption and
   photo-catalysis}},
Journal = {{COLLOIDS AND SURFACES A-PHYSICOCHEMICAL AND ENGINEERING ASPECTS}},
Year = {{2020}},
Volume = {{604}},
Month = {{NOV 5}},
Abstract = {{We employed Ti-bearing blast furnace slag (Ti-BFS) as raw material to
   prepare calcined TiO2 -loaded layered double hydroxides (TiO2-CLDHs)
   through acidolysis-alkali precipitation coupled with pyrolysis strategy.
   During the pyrolysis process, the involvement of TiO2 and LDHs made the
   TiO2-CLDHs composites have superior photocatalytic ability and abundant
   porosities, resulting in highly-efficient removal of arsenic (As) from
   groundwater. Meanwhile, center dot OH played crucial role in the As(III)
   oxidation process through the results of Electronspin resonance
   spectroscopy (ESR) and X-ray photoelectron spectroscopy (XPS)
   measurements. In addition, the co-existing of humic acid in groundwater
   was found to compete for center dot OH and adsorption sites of
   TiO2-CLDHs, leading to a decrease in As removal. This study not only
   provided a novel strategy for Ti-BFS disposal, but also evaluated the
   application of TiO2-CLDHs in the treatment of As contaminants in
   groundwater environment.}},
