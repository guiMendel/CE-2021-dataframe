@article{ WOS:000165112700003,
Author = {Sakuraba, H and Horii, M and Takezutsumi, T},
Title = {{Hydroxylation of benzene with hydrogen peroxide catalyzed by Fe(III)
   complexes of beta-cyclodextrin-protocatechuate}},
Journal = {{NIPPON KAGAKU KAISHI}},
Year = {{2000}},
Number = {{10}},
Pages = {{685-694}},
Month = {{OCT}},
Abstract = {{beta -Cyclodextrin derivatives as ligands (beta -CD-6-PA and beta
   -CD-2-PA) were prepared by the monoesterification of the hydroxy group
   at the C-6 or C-2 position of beta -CD with 3,4-dihydroxybenzoic acid
   (protocatechuic acid, PAH) with KF in DMF. Direct hydroxylation of
   benzene with hydrogen peroxide was performed by the iron(III)
   coordinated the beta -CD ligands (the improved Hamilton system). The
   beta -CD-6-PA and iron(III) formed a highly active catalyst for the
   hydroxylation, composed of 1 : 2 complex (Fe : ligand) at pH of 2.5, but
   an inert one (1 : 3 complex) at pH of 6.0. The stability of the iron
   (III)-beta -CD complex (1 : 3 (lambda (max) 536 nm, epsilon = 2030, log
   K' = 13.8)) was higher than that of 1 : 2 complex (lambda (max) 667 nm,
   epsilon = 860, log K' = 8.66). At the optimum pH of 2.5, when the beta
   -CD-6-PA-Fe(III) catalyst was used for the hydroxylation, yields of
   catechol, hydroquinone, and phenol were 73, 5 and 22\%, respectively. On
   the other hand,when the beta -CD-2-PA-Fe(III) catalyst was used, these
   yields were 10, 24 and 12\%, respectively. The observed selectivity of
   products in the present hydroxylation is discussed in terms of a
   position of PA substituent of beta -CD-PA-Fe(III) catalyst.}},
