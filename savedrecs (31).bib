@article{ WOS:000233416400008,
Author = {Renshaw, AA and Gould, EW},
Title = {{Comparison of disagreement and error rates for three types of
   interdepartmental consultations}},
Journal = {{AMERICAN JOURNAL OF CLINICAL PATHOLOGY}},
Year = {{2005}},
Volume = {{124}},
Number = {{6}},
Pages = {{878-882}},
Month = {{DEC}},
Abstract = {{Previous studies have documented a relatively high rate of disagreement
   for interdepartmental consultations, but follow-up is limited. We
   reviewed the results of 3 types of interdepartmental consultations in
   out-hospital during a 2-year period, including 328 incoming, 928
   pathologist-generated outgoing, and 22 7 patient- or clinician-generated
   outgoing consults. The disagreement rate was significantly higher for
   incoming consults (10.7\%) than for outgoing pathologist-generated
   consults (5.9\%) (P =.06). Disagreement rates for outgoing patient- or
   clinician-generated consults were not significantly different from
   either other type (7.9\%). Additional consultation, biopsy, or testing
   follow-up was available for 19 (54\%) of 35, 14 (25\%) of 55, and 6
   (33\%) of 18 incoming, outgoing pathologist-generated, and outgoing
   patient or clinician-generated consults with disagreements,
   respectively; the percentage of errors varied widely (15/19 {[}79\%],
