@article{ WOS:000232728000010,
Author = {Noguchi, Y and Yashima, T and Hatanaka, A and Uzawa, M and Yasunami, M
   and Kimura, A and Kitamura, K},
Title = {{A mutation in Wolfram syndrome type 1 gene in a Japanese family with
   autosomal dominant low-frequency sensorineural hearing loss}},
Journal = {{ACTA OTO-LARYNGOLOGICA}},
Year = {{2005}},
Volume = {{125}},
Number = {{11}},
Pages = {{1189-1194}},
