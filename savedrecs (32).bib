@article{ WOS:000233243500068,
Author = {Skoko, D and Yan, J and Johnson, RC and Marko, JF},
Title = {{Low-force DNA condensation and discontinuous high-force decondensation
   reveal a loop-stabilizing function of the protein Fis}},
Journal = {{PHYSICAL REVIEW LETTERS}},
Year = {{2005}},
Volume = {{95}},
Number = {{20}},
Month = {{NOV 11}},
Abstract = {{We report single-DNA-stretching experiments showing that the protein
   Fis, an abundant bacterial chromosome protein of E. coli, mediates a
   dramatic DNA condensation to zero length. This condensation occurs
   abruptly when DNA tension is reduced below a
   protein-concentration-dependent threshold f({*})< 1 pN. Following
   condensation, reopening under larger forces proceeds via a series of
   discrete jumps, indicating that Fis is able to stabilize DNA crossings.
   Our experiments suggest that Fis may play a role in vivo stabilizing the
   ``loop-domain{''} structure of the bacterial chromosome.}},
