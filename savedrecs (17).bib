@article{ WOS:000369851900035,
Author = {Lin, Feng and Zhang, Xianzhao and Cai, Zengxia},
Title = {{Approximation Algorithms for Scheduling with Rejection on Two Unrelated
   Parallel Machines}},
Journal = {{INTERNATIONAL JOURNAL OF ADVANCED COMPUTER SCIENCE AND APPLICATIONS}},
Year = {{2015}},
Volume = {{6}},
Number = {{11}},
Pages = {{260-264}},
Month = {{NOV}},
Abstract = {{In this paper, we study the scheduling problem with rejection on two
   unrelated parallel machines. We may choose to reject some jobs, thus
   incurring the corresponding penalty. The goal is to minimize the
   makespan plus the sum of the penalties of the rejected jobs. We first
   formulate this scheduling problem into an integer program, then relax it
   into a linear program. From the optimal solution to the linear program,
   we obtain the two algorithms using the technique of linear programming
   rounding. In conclusion, we present a deterministic 3-approximation
   algorithm and a randomized 3-approximation algorithm for this problem.}},
