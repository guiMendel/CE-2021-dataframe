@article{ WOS:A1995RT87100007,
Author = {TANG, YX and JIANG, ZH},
Title = {{PHASE-DIAGRAM MODEL FOR THE PHYSICAL-PROPERTIES OF SILICATE AND
   BOROSILICATE GLASS SYSTEMS - DENSITY AND REFRACTIVE-INDEX ANALYSIS}},
Journal = {{JOURNAL OF NON-CRYSTALLINE SOLIDS}},
Year = {{1995}},
Volume = {{189}},
Number = {{3}},
Pages = {{251-257}},
Month = {{SEP}},
Abstract = {{The density, d, and refractive index, n(D), have been investigated in
   terms of the phase diagram model in the Na2O-B2O3-SiO2, BaO-B2O3-SiO2
   and Na2O-K2O-SiO2 glass systems. The calculated values of density and
   refractive index are in good agreement with the experimental results
   taken from the literature. The effect of glass composition on the
   difference between experiment and calculation is discussed. It is
   proposed that the structural unit of a single-component glass resembles
   that of the related congruent melting compound, a multicomponent glass
   can be regarded as the melting mixture of some adjacent congruent
   melting compounds in the phase diagram, and in a ternary system the
   amounts of three congruent melting compounds in that glass can be
   calculated by the level rule.}},
