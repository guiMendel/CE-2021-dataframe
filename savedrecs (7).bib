@article{ WOS:000589448500001,
Author = {Lin, Qian and Fu, Qingqing and Li, Xiang and Luo, Yuheng and Luo, Junqiu
   and Chen, Daiwen and Mao, Xiangbing and Yu, Bing and Zheng, Ping and
   Huang, Zhiqing and Yu, Jie and Yan, Hui and He, Jun},
Title = {{Human beta-Defensin 118 Attenuates Escherichia coli K88-Induced
   Inflammation and Intestinal Injury in Mice}},
Journal = {{PROBIOTICS AND ANTIMICROBIAL PROTEINS}},
Year = {{2021}},
Volume = {{13}},
Number = {{2}},
Pages = {{586-597}},
Month = {{APR}},
Abstract = {{Antibiotics are widely used to treat various inflammatory bowel diseases
   caused by enterotoxigenic Escherichia coli (ETEC). However, continuous
   use of antibiotics may lead to drug resistance. In this study, we
   investigated the role of human beta-defensin 118 (DEFB118) in regulating
   the ETEC-induced inflammation and intestinal injury. ETEC-challenged or
   non-challenged mice were treated by different concentrations of DEFB118.
   We show that ETEC infection significantly increased fecal score (P <
   0.05) and serum concentrations of interlukin-6 (IL-6) and tumor necrosis
   factor-alpha (TNF-alpha). Moreover, the concentrations of D-lactic acid,
   C-reactive protein (CRP), creatinine (CREA), and urea (P < 0.05) were
   both increased in the ETEC-challenged mice. However, DEFB118
   significantly decreased their concentrations in the serum (P < 0.05).
   DEFB118 not only alleviated tissue damage in spleen upon ETEC challenge,
   but also increased the villus height in duodenum and ileum (P < 0.05).
   Moreover, DEFB118 improved the localization and abundance of tight
   junction protein ZO-1 in jejunal epithelium. Interestingly, DEFB118
   decreased the expression levels of critical genes involving in mucosal
   inflammatory responses (NF-kappa B, TLR4, IL-1 beta, and TNF-alpha) and
   the apoptosis (caspase3) upon ETEC challenge (P < 0.05), whereas DEFB118
   significantly upregulated the expression of mucosa functional genes such
   as the mucin1 (MUC1) and sodium-glucose transporter-1 (SGLT-1) in the
   ETEC-challenged mice (P < 0.05). These results indicated a novel
   function of the DEFB118. The anti-inflammatory effect of DEFB118 should
   make it an attractive candidate to prevent various bacteria-induced
   inflammatory bowel diseases.}},
