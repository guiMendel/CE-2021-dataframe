@article{ WOS:000233951100011,
Author = {Roychoudhury, S and Castaldi, M and Lyubovsky, M and LaPierre, R and
   Ahmed, S},
Title = {{Microlith catalytic reactors for reforming iso-octane-based fuels into
   hydrogen}},
