@article{ WOS:000232570700050,
Author = {Takahashi, J and Kageyama, K and Kiyono, H and Nakano, H and Itoh, H},
Title = {{Cation substitution for BaTiO3 phase in low-fired glass-ceramics for
   tunable microwave applications}},
Journal = {{JAPANESE JOURNAL OF APPLIED PHYSICS PART 1-REGULAR PAPERS BRIEF
   COMMUNICATIONS \& REVIEW PAPERS}},
