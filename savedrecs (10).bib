@article{ WOS:000361123700005,
Author = {Li, Fuqiang and Wang, Zhenyu and Liu, Guohua and Fu, Chunjiang and Wang,
   Jianjiang},
Title = {{Hydrostatic seasonal state model for monitoring data analysis of
   concrete dams}},
