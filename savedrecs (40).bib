@article{ WOS:000166114700014,
Author = {de la Cal, J and Valle, AM},
Title = {{A generalization of Bernstein-Kantorovic operators}},
Journal = {{JOURNAL OF MATHEMATICAL ANALYSIS AND APPLICATIONS}},
Year = {{2000}},
Volume = {{252}},
Number = {{2}},
Pages = {{750-766}},
Month = {{DEC 15}},
Abstract = {{In this paper, we use a probabilistic setting to introduce a double
   sequence (L-n({[}k])) Of linear polynomial operators which includes, as
   particular cases, the classical Bernstein operators, the Kantorovic
   operators, and the operators recently introduced by Cao. For these
   operators, we discuss several approximation properties. In particular,
   we deal with the convergence properties according to the way in which
   the different parameters vary, and the preservation of global smoothness
   and classes of functions determined by concave moduli of continuity. A
   remarkable feature of our approach is that if f is differentiable, the
   approximation properties of both L(n)({[}k])f and its derivatives can be
   discussed simultaneously. Throughout the paper, probabilistic methods
   play an important role. (C) 2000 Academic Press}},
