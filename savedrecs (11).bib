@article{ WOS:000365802200002,
Author = {Bateson, Melissa and Robinson, Rebecca and Abayomi-Cole, Tim and
   Greenlees, Josh and O'Connor, Abby and Nettle, Daniel},
Title = {{Watching eyes on potential litter can reduce littering: evidence from
   two field experiments}},
Journal = {{PEERJ}},
Year = {{2015}},
Volume = {{3}},
Month = {{DEC 1}},
Abstract = {{Littering constitutes a major societal problem, and any simple
   intervention that reduces its prevalence would be widely beneficial. In
   previous research, we have found that displaying images of watching eyes
   in the environment makes people less likely to litter. Here, we
   investigate whether the watching eyes images can be transferred onto the
   potential items of litter themselves. In two field experiments on a
   university campus, we created an opportunity to litter by attaching
   leaflets that either did or did not feature an image of watching eyes to
   parked bicycles. In both experiments, the watching eyes leaflets were
   substantially less likely to be littered than control leaflets (odds
   ratios 0.22-0.32). We also found that people were less likely to litter
   when there other people in the immediate vicinity than when there were
   not (odds ratios 0.04-0.25) and, in one experiment but not the other,
   that eye leaflets only reduced littering when there no other people in
   the immediate vicinity. We suggest that designing cues of observation
   into packaging could be a simple but fruitful strategy for reducing
   littering.}},
