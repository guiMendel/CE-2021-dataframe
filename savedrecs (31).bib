@article{ WOS:000234051200015,
Author = {Ng, SHS and Chang, A and Brown, GD and Koop, BF and Davidson, WS},
Title = {{Type I microsatellite markers from Atlantic salmon (Salmo salar)
   expressed sequence tags}},
Journal = {{MOLECULAR ECOLOGY NOTES}},
Year = {{2005}},
Volume = {{5}},
Number = {{4}},
Pages = {{762-766}},
Month = {{DEC}},
Abstract = {{The detection of simple sequence repeats (SSRs) within expressed
   sequence tags (ESTs) connects potential microsatellite markers with
   specific genes, generating Type I markers. Using an in silico approach,
   we identified 1975 SSRs from the Genome Research on Atlantic Salmon
   Project EST database. We designed primers to amplify 158 SSRs, of which
   65 amplified 76 loci (including 11 duplicated loci). Sixty-one of the 76
   loci were variable in 24 Atlantic salmon from seven populations, and
   96\% of these markers also amplify DNA from other salmonids. Functions
   for 16 of the SSR associated ESTs have been determined, confirming them
   as Type I markers.}},
