@article{ WOS:A1995TR20700028,
Author = {Olmstead, JA and Zhu, JH and Gray, DG},
Title = {{Fluorescence spectroscopy of mechanical pulps .3. Effect chlorite
   delignification}},
Journal = {{CANADIAN JOURNAL OF CHEMISTRY-REVUE CANADIENNE DE CHIMIE}},
Year = {{1995}},
Volume = {{73}},
Number = {{11}},
Pages = {{1955-1959}},
Month = {{NOV}},
Abstract = {{Many paper and wood samples fluoresce, but the sources of the emission
   are not well understood. Fluorescence excitation and emission spectra of
   paper sheets prepared from thermomechanical pulp (TMP) and bleached
   chemithermomechanical pulp (BCTMP) showed that the emission from the
   BCTMP was significantly higher than that from the TMP. Removing almost
   all of the lignin from both pulps by means of an acid chlorite treatment
   did not reduce the fluorescence significantly. By means of an
   approximate correction for changes in sheet reflectivity caused by the
   chlorite treatment, the fluorescence intensity was found to increase
   with lignin removal. Clearly, fluorescence is not simply related to
   lignin content.}},
