@article{ WOS:000233350100013,
Author = {Zhao, TX and Jackson, DR and Williams, JT},
Title = {{2-D periodic leaky-wave antennas - Part II: Slot design}},
Journal = {{IEEE TRANSACTIONS ON ANTENNAS AND PROPAGATION}},
Year = {{2005}},
Volume = {{53}},
Number = {{11}},
Pages = {{3515-3524}},
Month = {{NOV}},
Abstract = {{The far-field radiation patterns of a two-dimensional (2-D) periodic
   slot leaky-wave antenna (LWA) are studied. The antenna consists of a
   two-dimensional periodic array of slots in a conducting plane that is
   printed on top of a grounded dielectric slab. The antenna is excited by
   a simple source such as a dipole inside the slab. Reciprocity along with
   the spectral-domain method is used to calculate the far-field pattern,
   and the radiation characteristics of the structure are investigated. A
   comparison between the present periodic slot LWA and a 2-D periodic
   patch LWA discussed in Part I is given to show the advantages of the
   slot antenna for certain applications. The slot LWA can achieve high
   directivity patterns, and a circularly-polarized version of the antenna
   can achieve good circular-polarization at broadside.}},
