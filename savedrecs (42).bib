@article{ WOS:000165281100012,
Author = {Carlson, CA and Ethier, SP},
Title = {{Lack of RB protein correlates with increased sensitivity to
   UV-radiation-induced apoptosis in human breast cancer cells}},
Journal = {{RADIATION RESEARCH}},
Year = {{2000}},
Volume = {{154}},
Number = {{5}},
Pages = {{590-599}},
Month = {{NOV}},
Abstract = {{The underlying causes for different apoptotic responses in neoplastic
   cells are still not fully understood. We demonstrate here that a human
   breast cancer cell line, MDA-ME-468, which lacks the retinoblastoma
   protein (RB), is particularly sensitive to low doses of ultraviolet (UV)
   radiation. These cells are 15-20-fold more sensitive to UV radiation
   than RE-positive cell lines, as measured by both apoptosis and
   clonogenic assays. In addition, a prostate cancer cell line that lacks
   functional RE, DU-145, was found to have a similar apoptotic response to
   low doses of UV radiation. Based on these data, we hypothesized that the
   lack of RE is responsible for the extreme sensitivity of these cells to
   UV-radiation-induced apoptosis, To further examine the role of RE in
   apoptosis, cells of RE-positive human breast cancer and normal cell
   lines were infected with the human papilloma virus type 16 (HPV-16) E7
   and assessed for UV-radiation sensitivity, The HPV-16 E7 protein is
   known to decrease levels of free RE in cells. Infection of RE-positive
   human breast cancer or normal cells with E7 resulted in a 4-5-fold
   increase in sensitivity to UV radiation compared to controls. The above
   data suggest a role for the RE protein in protecting cells from
   undergoing apoptosis in response to UV radiation. (C) 2000 by Radiation
   Research Society.}},
