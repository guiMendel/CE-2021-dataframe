@article{ WOS:000232077000040,
Author = {Stefanova, MS and Pramatarov, PM and Karelin, AV},
Title = {{Enhanced IR hollow cathode laser in a He-3-Ne gas mixture}},
Journal = {{JOURNAL OF PHYSICS D-APPLIED PHYSICS}},
Year = {{2005}},
Volume = {{38}},
Number = {{17}},
Pages = {{3326-3332}},
Month = {{SEP 7}},
Note = {{10th International Symposium on the Science and Technology of Light
   Sources, Toulouse, FRANCE, JUL 18-22, 2004}},
Organization = {{European COST 529 Network; GE; Philips; Heraeus; Osram; SLi; Welch Allyn}},
Abstract = {{An experimental and theoretical study on He-3-Ne and He-4-Ne helical
   hollow cathode lasers is presented. Enhanced laser operation on the near
   IR NeI lines is observed when the natural isotope He-4 is substituted by
   the lighter isotope He-3. A four-fold increase in the laser output power
   and a three-fold increase in the laser gain for the strongest NeI 1.1523
   mu m line is measured in the He-3-Ne gas mixture compared to the He-4-Ne
   gas mixture.
   On the basis of the theoretical analysis done by means of a
   non-stationary kinetic model for the negative glow plasma of He-3-Ne and
   He-4-Ne hollow cathode lasers, a study on the changes in the particle
   kinetics is carried out and an explanation of the experimental results
   is proposed. In the He-3-Ne mixture the electron temperature is lower
   than in the He-4-Ne mixture, while the gas temperature is higher. As a
   result the helium triplet metastable density and the rate constant for
   excitation transfer to neon atoms are higher in the He-3-Ne mixture. The
   lower laser level de-excitation due to intra-multiplet mixing of
   2p(1-10)levels by He-3 atoms is more efficient.}},
