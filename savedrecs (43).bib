@article{ WOS:000165377400020,
Author = {Lorrain, J and Briand, V and Favennec, E and Duval, N and Grosset, A and
   Janiak, P and Hoornaert, C and Cremer, G and Latham, C and O'Connor, SE},
Title = {{Pharmacological profile of SL 59.1227, a novel inhibitor of the
   sodium/hydrogen exchanger}},
Journal = {{BRITISH JOURNAL OF PHARMACOLOGY}},
Year = {{2000}},
Volume = {{131}},
Number = {{6}},
Pages = {{1188-1194}},
Month = {{NOV}},
Abstract = {{1 The NHE1 isoform of the Na+/H+ exchanger plays an important role in
   the regulation of intracellular pH and in cardiac cell injury caused by
   ischaemia and reperfusion. SL 59.1227 is a novel imidazolypiperidine
   Na+/H+ antiport inhibitor which is structurally unrelated to previously
   described acylguanidine inhibitors such as cariporide.
   2 Recovery of pH(i) following an intracellular acid load was measured in
   CCL39-derived PS120 variant cells, selectively expressing either NHE1 or
   NHE2 isoforms of the Na+/H+ exchanger, pH(i) recovery was potently and
   selectively slowed by SL 59.1227 in NHE1-expressing cells (IC50 3.3 +/-
   1.3 nM) versus NHE2-expressing cells (2.3 +/- 1.0 muM). The respective
   IC50 values for cariporide were 103 +/- 28 nhl (NHE1) and 73 +/- 46 muM
   (NHE2).
   3 In anaesthetized rats following left coronary artery occlusion (7 min)
   and reperfusion (10 min) SL 59.1227 (10-100 mug kg(-1) min(-1) i.v.)
   inhibited ischaemia-mediated ventricular tachycardia (71-100\%) and
   reperfusion-induced ventricular fibrillation (75-87\%) and prevented
   mortality. Bolus i.v. administration of SL 59.1227 (1 mg kg(-1))
   produced anti-arrhythmic effects when administered either before or
   during ischaemia.
   4 Cardiac infarct size was determined in anaesthetized rabbits following
   left coronary artery occlusion (30 min) and reperfusion (120 min).
   Infarct size measured as a percentage of the area at risk was 36.2 +/-
   3.4\% (control group) versus 15.3 +/- 3.9\% (SL 59.1227 0.6 mg kg(-1)
   i.v.).
   5 SL 59.1227 is the first example of a potent and NHE1-selective
   non-acylguanidine Na+/H+ exchanger inhibitor. It possesses marked
   cardioprotective properties.}},
