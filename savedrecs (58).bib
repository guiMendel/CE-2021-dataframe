@article{ WOS:A1995RR58500021,
Author = {HOLSA, J and KESTILA, E},
Title = {{STUDY OF THE CRYSTAL-FIELD EFFECT IN REOF-EU3+ (RE=LA, GD AND Y) USING
   OPTICAL SPECTROSCOPY AND X-RAY-POWDER DIFFRACTION}},
Journal = {{JOURNAL OF ALLOYS AND COMPOUNDS}},
Year = {{1995}},
Volume = {{225}},
Number = {{1-2}},
