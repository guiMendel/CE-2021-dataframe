@article{ WOS:A1995RN41800020,
Author = {JENSEN, S and PADERANGA, DC and CHEN, PC and OLSON, K and EDWARDS, M and
   IAVORONE, A and ISRAEL, MA and SHANNON, K},
Title = {{MOLECULAR ANALYSIS AT THE NF1 LOCUS IN ASTROCYTIC BRAIN-TUMORS}},
Journal = {{CANCER}},
Year = {{1995}},
Volume = {{76}},
Number = {{4}},
Pages = {{674-677}},
Month = {{AUG 15}},
Abstract = {{Background. Patients with neurofibromatosis type 1 (NF1) are at
   increased risk for developing malignant neural crest tumors and juvenile
   myeloid leukemia, Although the normal allele of the NF1 tumor-suppressor
   gene is frequently deleted in some of the malignant tumors that arise in
   patients with NF1, the role of NF1 alterations in the sporadic forms of
   these cancers is unclear.
   Methods. A series of intragenic sequence polymorphisms was used to
   investigate lymphocyte and tumor DNA samples from 22 adults with high
   grade malignant gliomas for loss of heterozygosity (LOH) al NF1. In
   addition, an assay based on the polymerase chain reaction was used to
   screen these tumors for point mutations at codon 1423.
   Results. One recurrent anaplastic astrocytoma showed LOH within NF but
   not with a flanking marker located near the gene. Of 21 informative
   tumors, none showed point mutations affecting codon 1423 of NF1.
   Conclusion, These data suggest that LOH at NF1 is uncommon in sporadic
   high grade astrocytoma, and codon 1423 is not a `'hot spot'' for
   activating point mutations in these tumors.}},
