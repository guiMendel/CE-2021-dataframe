@article{ WOS:000365912300004,
Author = {Gladman, Dafna D.},
Title = {{Toward Treating to Target in Psoriatic Arthritis}},
Journal = {{JOURNAL OF RHEUMATOLOGY}},
Year = {{2015}},
Volume = {{42}},
Number = {{93}},
Pages = {{14-16}},
Month = {{NOV}},
Abstract = {{The concept ``treat to target{''} in rheumatology was first developed
   for rheumatoid arthritis. A similar attempt to develop such an approach
   for spondyloarthritis was unsuccessful because the assessment tools and
   target of therapy had not been developed. In psoriatic arthritis (PsA),
   composite indices to assess disease activity, disease state, and
   responsiveness have been developed and can be used as targets. There are
   a number of definitions for remission, but none are widely accepted.
   However, a state of minimal disease activity has been defined. There is
   evidence now that the treat-to-target approach is feasible, using the
   minimal disease activity state as a target and devising a tight control
   approach, which is superior to standard of care. Further work will
   determine the best target and the best approach to reach it.}},
