@article{ WOS:A1995RU23000028,
Author = {ASBECK, P},
Title = {{DEVELOPMENT OF HBT STRUCTURE TO MINIMIZE PARASITIC ELEMENTS}},
Journal = {{SOLID-STATE ELECTRONICS}},
Year = {{1995}},
