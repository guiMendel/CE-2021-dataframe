@article{ WOS:000285431100028,
Author = {Lee, Seul-Yi and Park, Soo-Jin},
Title = {{Influence of CO2 activation on hydrogen storage behaviors of
   platinum-loaded activated carbon nanotubes}},
Journal = {{JOURNAL OF SOLID STATE CHEMISTRY}},
Year = {{2010}},
Volume = {{183}},
Number = {{12}},
Pages = {{2951-2956}},
Month = {{DEC}},
Abstract = {{In this work, platinum (Pt) metal loaded activated multi-walled carbon
   nanotubes (MWNTs) were prepared with different structural
   characteristics for hydrogen storage applications. The process was
   conducted by a gas phase CO2 activation method at 1200 degrees C as a
   function of the CO2 flow time. Pt-loaded activated MWNTs were also
   formulated to investigate the hydrogen storage characteristics. The
   microstructures of the Pt-loaded activated MWNTs were characterized by
   XRD and TEM measurements. The textural properties of the samples were
   analyzed using N-2 adsorption isotherms at 77 K. The BET, D-R, and BJH
   equations were used to observe the specific surface areas and the
   micropore and mesopore structures. The hydrogen storage capacity of the
   Pt-loaded activated MWNTs was measured at 298 K at a pressure of 100
   bar. The hydrogen storage capacity was increased with CO2 flow time. It
   was found that the micropore volume of the activated MWNTs plays a key
   role in the hydrogen storage capacity. (c) 2010 Elsevier Inc. All rights
   reserved.}},
