@article{ WOS:A1995RH44700008,
Author = {CHUNG, C and CHEN, CY and WEI, YY and HSU, CN},
Title = {{MONITORING OF ENVIRONMENTAL RADIATION ON THE SPRATLY ISLETS IN THE SOUTH
   CHINA SEA}},
Journal = {{JOURNAL OF RADIOANALYTICAL AND NUCLEAR CHEMISTRY-ARTICLES}},
Year = {{1995}},
Volume = {{194}},
Number = {{2}},
Pages = {{291-296}},
Month = {{JUL}},
Note = {{3rd International Conference on Methods and Applications of
   Radioanalytical Chemistry (MARC-III), KAILUA KONA, HI, APR 10-16, 1994}},
Organization = {{Amer Nucl Soc, Div Isotopes \& Radiat; Amer Nucl Soc, Div Biol \& Med;
   Amer Nucl Soc, NO Calif Sect}},
Abstract = {{The concentration of radionuclides in environmental samples and the
   environmental radiation level (including cosmic rays) was monitored on
   Spratly Islets in South China Sea prior to the commercial operation of
   nearby Chinese nuclear power plants. Samples of coral sand, sea food
   vegetation, water, and accumulated fallout on the islet were obtained
   and measured for radioactivity. Except for some exposure originating
   from minute amounts of Co-60 and Cs-137, the radiation background is
   mainly emitted from naturally occurring radionuclides. Radiation levels
   including beta, gamma, and neutron on the islet have also monitored.
   Gamma dose rates for outdoor exposure averaged approximately 0.09 mu
   Sv/h; the neutron dose rate was about 0.003 mu Sv/h. Automatic radiation
   surveillance and a routine sampling program on the Spratly Islets are
   viewed as essential to provide an early warning in the Far East in case
   of a nuclear emergency.}},
