@article{ WOS:000282868900010,
Author = {Wiedemeier, Heribert},
Title = {{Novel correlation of Schottky constants with lattice energies for II-VI
   and I-VII compounds}},
Journal = {{JOURNAL OF SOLID STATE CHEMISTRY}},
Year = {{2010}},
Volume = {{183}},
Number = {{10}},
Pages = {{2317-2323}},
Month = {{OCT}},
Abstract = {{Correlations of computed Schottky constants (K(S)
   ={[}V(Zn)'']{[}V(S)'']) with structural and thermodynamic properties
   showed linear dependences of log K(S) on the lattice energies for the
   Zn-, Cd-, Hg-, Mg-, and Sr-chalcogenides and for the Na- and K-halides.
   These findings suggest a basic relation between the Schottky constants
   and the lattice energies for these families of compounds from different
   parts of the Periodic Table, namely, Delta H(T,L)(0) = (2.303nRT log
   K(S))+2.303nRm(b)+2.303nRTi(b).
   Delta H(T,L)(0) is the experimental (Born-Haber) lattice energy
   (enthalpy), n is a constant approximately equal to the formal valence
   (charge) of the material, m(b) and i(b) are the slope and intercept,
   respectively, of the intercept b (of the log K(S) versus Delta H(L)(0)
   linear relation) versus the reciprocal temperature. The results of this
   work also provide an empirical correlation between the Gibbs free energy
   of vacancy formation and the lattice energy. (C) 2010 Elsevier Inc. All
   rights reserved.}},
