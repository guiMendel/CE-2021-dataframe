@article{ WOS:000600116200017,
Author = {Breidenthal, Amy P. and Liu, Dong and Bai, Yuntao and Mao, Yina},
Title = {{The dark side of creativity: Coworker envy and ostracism as a response
   to employee creativity}},
Journal = {{ORGANIZATIONAL BEHAVIOR AND HUMAN DECISION PROCESSES}},
Year = {{2020}},
Volume = {{161}},
Pages = {{242-254}},
Month = {{NOV}},
Abstract = {{Globally, organizations are encouraging employees to be more creative in
   order to enhance organizational innovation. However, the potential
   negative coworker consequences stemming from employee creativity are
   widely unknown. Thus, we extend the creativity literature by drawing on
   social comparison theory and taking an interpersonal approach to
   investigate whether, when, and how employee relative creativity (an
   employee's creativity compared to the average creativity in his or her
   team) may lead to coworker envy, which then results in coworkers
   ostracizing the focal employee. The findings reveal that coworker envy
   (Time 2) mediates the relationship between employee relative creativity
   (Time 1) and coworker ostracism (Time 3). When an employee has a
   relatively higher quality leader-member exchange relationship with his
   or her team leader (Time 1), his or her relative creativity (Time 1) is
   more likely to trigger coworker envy (Time 2) and ostracism (Time 3). In
   contrast, an employee's relative creativity (Time 1) is more likely to
   fuel coworker envy (Time 2) and ostracism (Time 3) when he or she
   possesses relatively lower quality exchange relationships with coworkers
   (Time 1).}},
