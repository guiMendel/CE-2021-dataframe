@article{ WOS:000232142100005,
Author = {Kagedal, K and Johansson, AC and Johansson, U and Heimlich, G and
   Roberg, K and Wang, NS and Jurgensmeier, JM and Ollinger, K},
Title = {{Lysosomal membrane permeabilization during apoptosis - involvement of
