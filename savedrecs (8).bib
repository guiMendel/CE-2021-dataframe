@article{ WOS:000590257300001,
Author = {Sherlock, Laura G. and Sjostrom, Kara and Sian, Lei and Delaney, Cassidy
   and Tipple, Trent E. and Krebs, Nancy F. and Nozik-Grayck, Eva and
   Wright, Clyde J.},
Title = {{Hepatic-Specific Decrease in the Expression of Selenoenzymes and Factors
