@article{ WOS:000364094300022,
Author = {Cai, Weili and Takenaka, Mitsuru and Takagi, Shinichi},
Title = {{Effectiveness of Surface Potential Fluctuation for Representing
   Inversion-Layer Mobility Limited by Coulomb Scattering in MOSFETs}},
Journal = {{IEEE ELECTRON DEVICE LETTERS}},
Year = {{2015}},
Volume = {{36}},
Number = {{11}},
Pages = {{1183-1185}},
Month = {{NOV}},
Abstract = {{In this letter, the effectiveness of surface potential fluctuation
   (sigma(s)), experimentally extracted from the conductance method, in
   representing the mobility limited by Coulomb scattering (mu(coulomb))
   due to MOS interface charges has been experimentally examined in Si
   MOSFETs. It is found that mu(coulomb) can be described as a universal
   line proportional to sigma(-2)(s), irrespective of the conditions of MOS
   interface charges, even under the coexistence of positive and negative
   charges, where threshold voltage shift (Delta V-th) cannot represent
   mu(coulomb). This result can be explained by considering that
   sigma(2)(s) more directly corresponds to the total amount of Coulomb
   scattering centers.}},
